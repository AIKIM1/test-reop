                                                            CanUserAddRows="True" CanUserRemoveRows="True"
                                                            BeginningEdit="dgTestRoute_BeginningEdit"
                                                            BeginningNewRow="dgTestRoute_BeginningNewRow" BeganEdit="dgTestRoute_BeganEdit"
                                                            LoadedCellPresenter="dgTestRoute_LoadedCellPresenter" UnloadedCellPresenter="dgTestRoute_UnloadedCellPresenter" 
                                                            ctrlLib:DataGridExtension.IsAlternatingRow="False"  AlternatingRowBackground="White" CommittingEdit="dgTestRoute_CommittingEdit" 
                                                            MouseDoubleClick="dgRoute_MouseDoubleClick" Margin="10,0,-10,0">

                                                    <c1:C1DataGrid.Columns>
