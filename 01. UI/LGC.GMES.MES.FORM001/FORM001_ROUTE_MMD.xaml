                                                        <Button Margin="4,0,0,0" x:Name="btnExportTp102" Content="{Binding ConverterParameter=Export, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelImportStyle}" VerticalAlignment="Center" Click="btnExportTp102_Click"/>
                                                        <Border Margin="4,0,0,0" Style="{StaticResource Content_Separator_BorderStyle}" />
                                                        <!--<c1:C1NumericBox Margin="4,0,0,0" Name="numAddCountTp102" Minimum="1" Value="1" CornerRadius="0" Width="50" Height="23"/>
                                                        <Button Margin="4,0,0,0" x:Name="btnPlusTp102" Click="btnPlusTp102_Click"   Content="{Binding ConverterParameter=Plus,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonAddStyle}"/>
                                                        <Button Margin="4,0,0,0" x:Name="btnMinusTp102" Click="btnMinusTp102_Click" Content="{Binding ConverterParameter=Minus,  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonDeleteStyle}" />-->
                                                        <!--<Border Margin="4,0,0,0" Style="{StaticResource Content_Separator_BorderStyle}" />-->
                                                        <!--<Button Margin="4,0,0,0" x:Name="btnTestRouteSave" Click="btnTestRouteSave_Click" Content="{Binding ConverterParameter=저장,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}"/>-->
                                                    </StackPanel>
                                                </Grid>

