<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.FORM001.FORM001_ROUTE_MMD"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="1024" d:DesignWidth="1280"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="FCS I/F Error 이력 조회">
            <Grid>

                <Grid x:Name="Content" >
                    <Grid.Resources>
                        <DataTemplate x:Key="ResultTemplate">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="{Binding}" HorizontalAlignment="Center"></TextBlock>
                            </Grid>
                        </DataTemplate>
                    </Grid.Resources>

                    <c1:C1TabControl x:Name="TapRoute">

                        <c1:C1TabItem x:Name="tpDefRoute" Header="{Binding ConverterParameter='DEFAULT_ROUTE_INF', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="33*"/>
                                    <ColumnDefinition Width="127*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <Grid Grid.Row="0" Grid.ColumnSpan="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="51"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>

                                    <!--조회영역-->
                                    <Grid Grid.Row="0" Grid.Column="0" Margin="0,8,0,8">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="13"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                            <ColumnDefinition Width="13"/>
                                        </Grid.ColumnDefinitions>

                                        <!--<Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="12" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>-->
                                        <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" Grid.ColumnSpan="12" BorderBrush="#FF9DA0A3" Background="#FFE0E0E0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>

                                        <TextBlock Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='AREA', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <TextBox   Grid.Row="0" Grid.Column="2" x:Name="txtTp1AREAID" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" IsReadOnly="True"/>

                                        <TextBlock Grid.Row="0" Grid.Column="3" Text="{Binding ConverterParameter='생산라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <TextBox   Grid.Row="0" Grid.Column="4" x:Name="txtTp1EQSGID" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" IsReadOnly="True"/>

                                        <TextBlock Grid.Row="0" Grid.Column="5" Text="{Binding ConverterParameter='모델', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <TextBox   Grid.Row="0" Grid.Column="6" x:Name="txtTp1MDLLOT_ID" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" IsReadOnly="True"/>

                                        <TextBlock Grid.Row="0" Grid.Column="7" Text="{Binding ConverterParameter='ROUTE_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <TextBox   Grid.Row="0" Grid.Column="8" x:Name="txtTp1ROUTID" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" IsReadOnly="True"/>

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="280"/>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>

                                        <!--<Border Grid.Row="0" Grid.Column="0"  BorderThickness="1" BorderBrush="#FF9DA0A3" Background="#FFE0E0E0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">-->
                                        <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#FF9DA0A3" Background="#FFE0E0E0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>

                                        <!--Route Tree-->
                                        <Grid Grid.Row="0" Grid.Column="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>

                                            <Grid Grid.Row="0" Margin="10,13,11,0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="115"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="*"/>
                                                </Grid.RowDefinitions>

                                                <TextBlock   Grid.Row="0"  Grid.Column="0" Margin="0,0,0,0" Style="{StaticResource SearchCondition_LabelStyle}" Text="{Binding ConverterParameter='FACTORY', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                                <c1:C1ComboBox  Grid.Row="0" Grid.Column="1" Margin="8,0,0,0" x:Name="cboPlant"  DisplayMemberPath="CMCDNAME1" SelectedValuePath="CMCODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboPlant_SelectedValueChanged" IsEnabled="False" />

                                                <TextBlock   Grid.Row="1"  Grid.Column="0" Margin="0,10,0,0" Style="{StaticResource SearchCondition_LabelStyle}" Text="{Binding ConverterParameter='AREA', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                                <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" Margin="8,10,0,0" x:Name="cboArea"  DisplayMemberPath="CMCDNAME1" SelectedValuePath="CMCODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboArea_SelectedValueChanged" IsEnabled="False" />

                                                <TextBlock   Grid.Row="2"  Grid.Column="0" Margin="0,10,0,0" Style="{StaticResource SearchCondition_LabelStyle}" Text="{Binding ConverterParameter='USE_FLAG', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                                <c1:C1ComboBox  Grid.Row="2" Grid.Column="1" Margin="8,10,0,0" x:Name="cboUseFlag"  DisplayMemberPath="CMCDNAME1" SelectedValuePath="CMCODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                                <Button Grid.Row="3" Grid.Column="1" Margin="0,20,0,20" x:Name="btnSearch" Click="btnSearch_Click" Content="{Binding ConverterParameter='조회',    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" VerticalAlignment="Top" HorizontalAlignment="Right" Grid.RowSpan="2"   Cursor="Hand"/>

                                            </Grid>

                                            <Grid Grid.Row="1" Margin="1,0,1,0">

                                                <c1:C1TreeView x:Name="tvLineModelRoute" 
                                                               AllowDrop="False" 
                                                               IsVirtualizing="False" 
                                                               VirtualizingPanel.VirtualizationMode="Recycling"
                                      VirtualizingPanel.IsVirtualizing="True"  
                                   AllowDragDrop="False" BorderThickness="0" MinWidth="250" HorizontalAlignment="Stretch" 
                                                           MouseRightButtonDown="tvLineModelRoute_MouseRightButtonDown" 
                                                           ItemExpanding="tvLineModelRoute_ItemExpanding" >
                                                </c1:C1TreeView>
                                            </Grid>
                                        </Grid>

                                        <GridSplitter Grid.Row="0" Grid.Column="1" x:Name="slDefRouteCol01" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" 
                                                            DragStarted="slDefRouteCol01_DragStarted"
                                                            DragCompleted="slDefRouteCol01_DragCompleted"/>

                                        <Grid Grid.Row="0" Grid.Column="2" Margin="10,0,0,0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="8"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>

                                            <!--Route Grid-->
                                            <Grid Grid.Row="0" Grid.Column="0">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="40"/>
                                                    <RowDefinition Height="*"/>
                                                </Grid.RowDefinitions>

                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*" />
                                                </Grid.ColumnDefinitions>

                                                <Grid Grid.Row="0">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*"/>
                                                        <ColumnDefinition Width="Auto" />
                                                    </Grid.ColumnDefinitions>

                                                    <StackPanel Grid.Column="0" Orientation="Horizontal">
                                                        <TextBlock x:Name="txtRowTitleTp101" Text="{Binding ConverterParameter='MP_ROUTE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_ModuleTitleStyle}" />
                                                        <TextBlock x:Name="txtRowCntTp101" Text="{Binding Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_Number_LabelStyle}" Margin="5,0,0,0"/>
                                                    </StackPanel>

                                                    <StackPanel Grid.Column="1" Orientation="Horizontal">
                                                        <!--<Button Margin="4,0,0,0" x:Name="btnComCode" Content="{Binding ConverterParameter=CMCDMNGT, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonFormChangeStyle}" VerticalAlignment="Center" Click="btnComCode_Click"/>-->
                                                        <Border Margin="4,0,0,0" x:Name="btnComCode_Sepa" Style="{StaticResource Content_Separator_BorderStyle}" />
                                                        <!--<Button Margin="4,0,0,0" x:Name="btnImportTp101" Content="{Binding ConverterParameter=Import, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelImportStyle}" VerticalAlignment="Center" Click="btnImportTp101_Click"/>-->
                                                        <Button Margin="4,0,0,0" x:Name="btnExportTp101" Content="{Binding ConverterParameter=Export, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelImportStyle}" VerticalAlignment="Center" Click="btnExportTp101_Click"/>
                                                        <Border Margin="4,0,0,0" Style="{StaticResource Content_Separator_BorderStyle}" />
                                                        <!--<c1:C1NumericBox Margin="4,0,0,0" Name="numAddCountTp101" Minimum="1" Value="1" CornerRadius="0" Width="50" Height="23"/>
                                                        <Button Margin="4,0,0,0" x:Name="btnPlusTp101" Click="btnPlusTp101_Click"   Content="{Binding ConverterParameter=Plus,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonAddStyle}"/>
                                                        <Button Margin="4,0,0,0" x:Name="btnMinusTp101" Click="btnMinusTp101_Click" Content="{Binding ConverterParameter=Minus,  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonDeleteStyle}" />-->
                                                        <!--<Border Margin="4,0,0,0" Style="{StaticResource Content_Separator_BorderStyle}" />-->
                                                        <!--<Button Margin="4,0,0,0" x:Name="btnRouteSave" Click="btnRouteSave_Click" Content="{Binding ConverterParameter=저장,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}"/>
                                                        <Button Margin="4,0,0,0" x:Name="btnRouteCopy" Click="btnRouteCopy_Click" Content="{Binding ConverterParameter=복사,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}"/>-->
                                                    </StackPanel>
                                                </Grid>

                                                <uc:UcBaseDataGrid x:Name="dgRoute" Grid.Row="1" Grid.Column="0" HeadersVisibility="Column"
                                                            FrozenColumnCount="2" SelectionMode="SingleRow"
                                                            CanUserAddRows="True" CanUserRemoveRows="True"
                                                            BeginningEdit="dgRoute_BeginningEdit"
                                                            BeginningNewRow="dgRoute_BeginningNewRow" BeganEdit="dgRoute_BeganEdit"
                                                            LoadedCellPresenter="dgRoute_LoadedCellPresenter" UnloadedCellPresenter="dgRoute_UnloadedCellPresenter" 
                                                            ctrlLib:DataGridExtension.IsAlternatingRow="False"  AlternatingRowBackground="White" CommittingEdit="dgRoute_CommittingEdit" 
                                                            MouseDoubleClick="dgRoute_MouseDoubleClick">

                                                    <c1:C1DataGrid.Columns>
                                                        <!--<c1:DataGridCheckBoxColumn Binding = "{Binding CHK, Mode=TwoWay}" Width="Auto"  HorizontalAlignment="Center" CanUserSort="False" >
                                                            <c1:DataGridCheckBoxColumn.Header>
                                                                <StackPanel Orientation="Horizontal" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                                                                    <CheckBox x:Name="chkHeaderAllTp101" Margin="0" Width="19" Checked="chkHeaderAllTp101_Checked" Unchecked="chkHeaderAllTp101_Unchecked"></CheckBox>
                                                                </StackPanel>
                                                            </c1:DataGridCheckBoxColumn.Header>
                                                        </c1:DataGridCheckBoxColumn>-->
                                                        <c1:DataGridComboBoxColumn       Header="USE_FLAG"                   Binding="{Binding USE_FLAG}"                HorizontalAlignment="Left" EditOnSelection="True" DisplayMemberPath="CMCDNAME1" SelectedValuePath="CMCODE" IsReadOnly="True"/>
                                                        <c1:DataGridComboBoxColumn       Header="FACTORY"                      Binding="{Binding SHOPID}"                  HorizontalAlignment="Left" EditOnSelection="True" DisplayMemberPath="CMCDNAME1" SelectedValuePath="CMCODE" IsReadOnly="True" Visibility="Collapsed" />
                                                        <c1:DataGridComboBoxColumn       Header="(*)AREAID"                  Binding="{Binding AREAID}"                  HorizontalAlignment="Left" EditOnSelection="True" DisplayMemberPath="CMCDNAME1" SelectedValuePath="CMCODE" IsReadOnly="True" Visibility="Collapsed" />
                                                        <c1:DataGridComboBoxColumn       Header="EQSGID"                     Binding="{Binding EQSGID}"                  HorizontalAlignment="Left" EditOnSelection="True" DisplayMemberPath="CMCDNAME1" SelectedValuePath="CMCODE" IsReadOnly="True"/>
                                                        <c1:DataGridComboBoxColumn       Header="MDLLOT_ID"                  Binding="{Binding MDLLOT_ID}"               HorizontalAlignment="Left" EditOnSelection="True" DisplayMemberPath="CMCDNAME1" SelectedValuePath="CMCODE" IsReadOnly="True"/>
                                                        <c1:DataGridTextColumn           Header="(*)ROUTE_ID"                Binding="{Binding ROUTID}"                  HorizontalAlignment="Left" IsReadOnly="True"/>
                                                        <ctrlLib:DataGridMultiLangColumn Header="ROUTE_DESC"                 Binding="{Binding ROUT_NAME, Mode=TwoWay}"  HorizontalAlignment="Left" VerticalAlignment="Center" IsReadOnly="True"/>
                                                        <c1:DataGridComboBoxColumn       Header="ROUTE_TYPE_CD"              Binding="{Binding ROUT_TYPE_CODE}"          HorizontalAlignment="Left" EditOnSelection="True" DisplayMemberPath="CMCDNAME"  SelectedValuePath="CMCODE" IsReadOnly="True"/>
                                                        <c1:DataGridComboBoxColumn       Header="ROUT_GR_CODE"               Binding="{Binding ROUT_GR_CODE}"            HorizontalAlignment="Left" EditOnSelection="True" DisplayMemberPath="CMCDNAME1" SelectedValuePath="CMCODE" IsReadOnly="True"/>
                                                        <c1:DataGridComboBoxColumn       Header="PERF_GRP"                   Binding="{Binding ROUT_RSLT_GR_CODE}"       HorizontalAlignment="Left" EditOnSelection="True" DisplayMemberPath="CMCDNAME1" SelectedValuePath="CMCODE" IsReadOnly="True"/>

                                                        <c1:DataGridTextColumn           Header="REWORK_GRADE"               Binding="{Binding RWK_AVAIL_GRD_CODE}"      HorizontalAlignment="Left" IsReadOnly="True"/>
                                                        <c1:DataGridTemplateColumn Name="RwkAvailGrd" Width="Auto" Header="REWORK_GRADE_CHOICE">
                                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <Button  x:Name="btnRwkAvailGrd" Content="{Binding ConverterParameter=CHOICE,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnRwkAvailGrd_Click"/>
                                                                </DataTemplate>
                                                            </c1:DataGridTemplateColumn.CellTemplate>
                                                        </c1:DataGridTemplateColumn>
                                                        <c1:DataGridTextColumn           Header="SLT_GRD"                    Binding="{Binding SLT_GRD}"                 HorizontalAlignment="Left" IsReadOnly="True"/>
                                                        <c1:DataGridTemplateColumn Name="SlectorGrd" Width="Auto" Header="SLT_GRD_CHOICE">
                                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <Button  x:Name="btnRoutSltGrd" Content="{Binding ConverterParameter=CHOICE,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnRoutSltGrd_Click"/>
                                                                </DataTemplate>
                                                            </c1:DataGridTemplateColumn.CellTemplate>
                                                        </c1:DataGridTemplateColumn>
                                                        <c1:DataGridCheckBoxColumn  Header="MAVD_USE_FLAG" Binding="{Binding MAVD_USE_FLAG, Mode=TwoWay}" HorizontalAlignment="Center" Width="Auto" CanUserFilter="False"/>
                                                        <c1:DataGridTextColumn Header="INSUSER" Binding="{Binding INSUSER}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                                        <c1:DataGridDateTimeColumn Header="INSDTTM" Binding="{Binding INSDTTM}"  Format="yyyy-MM-dd HH:mm:ss" HorizontalAlignment="Center" IsReadOnly="True"></c1:DataGridDateTimeColumn>
                                                        <c1:DataGridTextColumn Header="UPDUSER" Binding="{Binding UPDUSER}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                                        <c1:DataGridDateTimeColumn Header="UPDDTTM" Binding="{Binding UPDDTTM}"  Format="yyyy-MM-dd HH:mm:ss" HorizontalAlignment="Center" IsReadOnly="True"></c1:DataGridDateTimeColumn>
                                                    </c1:C1DataGrid.Columns>

                                                </uc:UcBaseDataGrid>
                                                <ctrlLib:LoadingIndicator Grid.ColumnSpan="1" HorizontalAlignment="Left" Height="100" Margin="282,241,0,0" Grid.Row="1" Grid.RowSpan="1" VerticalAlignment="Top" Width="100"/>
                                            </Grid>

                                            <GridSplitter Grid.Row="1" Grid.Column="0" x:Name="slDefRouteRow01" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" 
                                                            DragStarted="slDefRouteRow01_DragStarted"
                                                            DragCompleted="slDefRouteRow01_DragCompleted"/>

                                            <!--Test Route Grid-->
                                            <Grid Grid.Row="2" Grid.Column="0">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="40"/>
                                                    <RowDefinition Height="*"/>
                                                </Grid.RowDefinitions>

                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*" />
                                                </Grid.ColumnDefinitions>

                                                <Grid Grid.Row="0">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*"/>
                                                        <ColumnDefinition Width="Auto" />
                                                    </Grid.ColumnDefinitions>

                                                    <StackPanel Grid.Column="0" Orientation="Horizontal">
                                                        <TextBlock x:Name="txtRowTitleTp102" Text="{Binding ConverterParameter='TEST_ROUTE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_ModuleTitleStyle}" />
                                                        <TextBlock x:Name="txtRowCntTp102" Text="{Binding Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_Number_LabelStyle}" Margin="5,0,0,0"/>
                                                    </StackPanel>


                                                    <StackPanel Grid.Column="1" Orientation="Horizontal">
