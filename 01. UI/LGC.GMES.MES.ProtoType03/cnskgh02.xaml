<UserControl x:Class="LGC.GMES.MES.ProtoType03.cnskgh02"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" >

    <Grid x:Name="grid1" HorizontalAlignment="Left" Height="154" VerticalAlignment="Top" Width="156">
        <Image x:Name="ImgCart" HorizontalAlignment="Left" Height="44" VerticalAlignment="Top" Width="60" />
        <Label x:Name="lblCartType" Content="S" HorizontalAlignment="Center"  VerticalAlignment="Top" FontWeight="Bold" Background="{x:Null}" Height="26" Margin="7,1,129,0" Width="20"/>
        <Label x:Name="lblCartNo" Content="Cart No" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,32,0,0" Background="{x:Null}" Height="26" Cursor="Hand" MouseDown="lblCartNo_MouseDown"/>
        <Label x:Name="lblCartID" Content="Cart ID" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Height="26" Margin="0,72,0,0" Visibility="Hidden"/>
    </Grid>


</UserControl>
