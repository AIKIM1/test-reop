<UserControl x:Class="LGC.GMES.MES.ProtoType03.cnskgh04"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" >

    <Grid x:Name="grid1" HorizontalAlignment="Left" Height="80" VerticalAlignment="Top" Width="100" Margin="0,5,0,0">
        <Grid.Background>
            <ImageBrush/>
        </Grid.Background>
        <!--       <Image x:Name="ImgCart" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="20" Margin="39,0,0,0" />
        -->
        <Label x:Name="lblZoneID" Content="Zone ID" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="22,5,0,0" Background="{x:Null}" Height="30" FontSize="13" Cursor="Hand" MouseDown="lblZoneID_MouseDown"/>
        <Label x:Name="lblZoneNM" Content="Zone Name" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="22,5,0,0" Background="{x:Null}" Height="30" FontSize="13" Cursor="Hand" Visibility="Hidden" MouseDown="lblZoneID_MouseDown" />
        <Label x:Name="lblCartCnt" Content="Cart Count" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="21,24,0,0" Background="{x:Null}" Height="25" FontSize="11"/>

    </Grid>


</UserControl>
