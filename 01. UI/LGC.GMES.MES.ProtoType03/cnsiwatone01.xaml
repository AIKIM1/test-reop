<UserControl x:Class="LGC.GMES.MES.ProtoType03.cnsiwatone01"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" >

            <ctrlLib:LGCLayout.Content>

                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="81*" />
                        <RowDefinition Height="202*" />
                        <RowDefinition Height="485*" />
                    </Grid.RowDefinitions>
                    <!-- Search -->
                    <Grid x:Name="SearchArea" Grid.Row="0" Margin="10,0,10,0">
                        <Grid Margin="0">
                            <!--#region  Grid Definition -->
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="2*" />
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="2*" />
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="2*" />
                            </Grid.ColumnDefinitions>
                            <!--#endregion-->
                            <!--#region Header -->
                            <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                                    <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="Mixer"/>
                                </StackPanel>
                            </Border>
                            <Border Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="4" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <Button Grid.Row="0" x:Name="btnSearch" Content="{Binding ConverterParameter=Search, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="1" Width="67"/>
                                    <Button Grid.Row="0" x:Name="btnStart" Content="{Binding ConverterParameter=Start, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="1" Width="67"/>
                                    <Button Grid.Row="0" x:Name="btnCancel" Content="{Binding ConverterParameter=Cancel, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="1" Width="67"/>
                                    <Button Grid.Row="0" x:Name="btnEnd" Content="{Binding ConverterParameter=End, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="1" Width="67"/>
                                    <Button Grid.Row="0" x:Name="btnMove" Content="{Binding ConverterParameter=Move, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="1" Width="67"/>
                                </StackPanel>
                            </Border>

                            <!--#endregion-->

                            <!--Border 1 Row-->
                            <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <!-- 필수 검색 조건은 아래와 같이 StackPanel을 이용하여 필수 마크를 붙여 줍니다 -->
                                    <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="Resource"/>
                                </StackPanel>
                            </Border>
                            <Border Grid.Row="1" Grid.Column="1" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                <ComboBox x:Name="cboResource" Margin="8,3" Style="{StaticResource SearchCondition_ComboBoxStyle}" DisplayMemberPath="PRODNAME" SelectedValuePath="PRODID"/>
                            </Border>
                            <Border Grid.Row="1" Grid.Column="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}" Margin="0,10.27" />
                                    <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" d:LayoutOverrides="TopMargin, BottomMargin" Text="작업일자"/>
                                </StackPanel>
                            </Border>
                            <Border Grid.Row="1" Grid.Column="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}" Grid.ColumnSpan="2" BorderBrush="White" Margin="0"/>

                            <!--Control 1 Row-->
                            <Border Grid.Row="1" Grid.Column="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderBrush="White">
                                <DatePicker Margin="8,3" Style="{StaticResource DatePickerBaseStyle}" x:Name="dtpDateFrom"/>
                            </Border>
                        </Grid>

                    </Grid>
                    <!--#region 작업지시-->
                    <Grid x:Name="Condition"  Grid.Row="1" Margin="10,0,10,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <!-- -->
                        <Border Grid.ColumnSpan="2" Height="5"></Border>
                        <!-- 작업지시 -->
                        <Grid x:Name="Workorder" Grid.Row="1" Grid.Column="0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <!-- 작업지시 Title -->
                                <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='작업지시', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <!-- 작업지시 Mon. -->
                                <Grid Grid.Row="1">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>
                                        <!-- 생산계획수량 -->
                                        <Border Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <TextBlock Style="{StaticResource Content_InputForm_LabelStyle}" Text="Plan"/>
                                            </StackPanel>
                                        </Border>
                                        <Border Grid.Column="1">
                                            <TextBox x:Name="txtPlanqty" Style="{StaticResource SearchCondition_TextBoxStyle}" ></TextBox>
                                        </Border>
                                        <!-- 설비할당수량 -->
                                        <Border Grid.Column="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <TextBlock Style="{StaticResource Content_InputForm_LabelStyle}" Text="Assign"/>
                                            </StackPanel>
                                        </Border>
                                        <Border Grid.Column="3">
                                            <TextBox x:Name="txtAssignqty" Style="{StaticResource SearchCondition_TextBoxStyle}"></TextBox>
                                        </Border>
                                        <!-- 생산실적수량-->
                                        <Border Grid.Column="4" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <TextBlock Style="{StaticResource Content_InputForm_LabelStyle}" Text="Result"/>
                                            </StackPanel>
                                        </Border>
                                        <Border Grid.Column="5">
                                            <TextBox x:Name="txtResultqty" Style="{StaticResource SearchCondition_TextBoxStyle}"></TextBox>
                                        </Border>
                                    </Grid>
                                </Grid>
                                <!-- 작업지시 현황 -->
                                <c1:C1DataGrid x:Name="_dgWorkorderView" Grid.Row="3">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridCheckBoxColumn Header="Chk"      Binding="{Binding Chk}" HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn     Header="작업지시" Binding="{Binding WORKORDER}" />
                                        <c1:DataGridTextColumn     Header="순번"     Binding="{Binding OPERCODE}" />
                                        <c1:DataGridTextColumn     Header="제품ID"   Binding="{Binding PRODID}" />
                                        <c1:DataGridTextColumn     Header="제품명"   Binding="{Binding PRODNAME}" />
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>
                        </Grid>
                        <Border Grid.Column="1" Width="5"></Border>
                        <Grid x:Name="ProductLot" Grid.Row="1" Grid.Column="2">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <!-- Product Lot Title -->
                                <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='Product Lot', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <c1:C1DataGrid x:Name="_dgProductLot" Grid.Row="1">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn        Header="버전"       Binding="{Binding VERSION}"/>
                                        <c1:DataGridTextColumn        Header="LOT ID"     Binding="{Binding LOT_ID}"/>
                                        <c1:DataGridTextColumn        Header="상태"       Binding="{Binding STATUS}"/>
                                        <c1:DataGridTextColumn Header="재공" Binding="{Binding WIPQTY}"/>
                                        <c1:DataGridTextColumn Header="장비수량" Binding="{Binding EQPENDQTY}" />
                                        <c1:DataGridTextColumn Header="제품명" Binding="{Binding PRODID}" />
                                        <c1:DataGridTextColumn Header="시작시간" Binding="{Binding START_TIME}" />
                                        <c1:DataGridTextColumn Header="종료시간" Binding="{Binding END_TIME}" />
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>
                        </Grid>
                    </Grid>
                    <!--#endregion-->

                    <!--#region 실적-->
                    <Grid x:Name="Content" Grid.Row="2" Margin="10,0,10,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <!-- 실적상세 -->
                        <Border Grid.Row="0" Height="5"/>

                        <Grid x:Name="ResultView" Grid.Row="1" Grid.Column="0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="1*"/>
                                </Grid.RowDefinitions>
                                <!-- 실적상세 Title -->
                                <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='실적상세', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <!-- 실적상세 View -->
                                <Grid Grid.Row="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>

                                    <!--#region Row 0 -->
                                    <!-- LOT ID -->
                                    <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="LOT ID"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtLotID" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="99"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <!-- 제품ID -->
                                    <Border Grid.Row="0" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Style="{StaticResource Content_InputForm_LabelStyle}" Text="제품ID"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="0" Grid.Column="4" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtPRODID" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="99"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <!--#endregion-->
                                    <!--#region Row 1 -->
                                    <!-- 제품명 -->
                                    <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="제품명"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="5" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtProdName" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="266"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <!-- 변환율 -->
                                    <Border Grid.Row="1" Grid.Column="6" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="변환율"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="1" Grid.Column="7" Grid.ColumnSpan="5" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtChangeRate" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="96"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <!--#endregion-->
                                    <!--#region Row 2 -->
                                    <!-- 작업지시 -->
                                    <Border Grid.Row="2" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="시작시간"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtWork" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="98" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="2" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="종료시간"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="2" Grid.Column="4" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtEndTime" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="96"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="2" Grid.Column="6" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="가동시간"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="2" Grid.Column="7" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtRunTime" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="96"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <!--#endregion-->
                                    <!--#region Row 3 -->
                                    <!-- Version -->
                                    <Border Grid.Row="3" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="Version"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtVersion" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="98" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                            <Button x:Name="btnVersion" Content="{Binding ConverterParameter=V, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="5,10" Width="20" Height="20"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="3" Grid.Column="4" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="작업지시"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="3" Grid.Column="6" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtWorkorder" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="98" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                            <Button x:Name="btnVWorkorder" Content="{Binding ConverterParameter=V, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="5,10" Width="20" Height="20"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="생산량"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtResult" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                        </StackPanel>                                        
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtResultQty" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtResultQty1" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="양품량"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="4" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtGoodQty" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="5" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtGoodQty1" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="6" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="Loss량"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="7" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtLossQty" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="8" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtLossQty1" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                        </StackPanel>
                                    </Border>
                                    <!--#endregion-->
                                    <!--#region Row 4 -->
                                    <Border Grid.Row="4" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="작업조"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtShift" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="98" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                            <Button x:Name="btnShift" Content="{Binding ConverterParameter=V, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="5,10" Width="20" Height="20"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="4" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="작업자"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="4" Grid.Column="6" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtOperator" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,17.086,0,17.085" Width="98" d:LayoutOverrides="TopMargin, BottomMargin"></TextBox>
                                            <Button x:Name="btnVOperator" Content="{Binding ConverterParameter=V, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="5,10" Width="20" Height="20"/>
                                        </StackPanel>
                                    </Border>
                                    <!--#endregion-->
                                    <!--#region Row 5 -->
                                    <Border Grid.Row="5" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="종료시간"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="4" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <DatePicker Margin="8,3" Style="{StaticResource DatePickerBaseStyle}" x:Name="dtpDateEnd"/>
                                    </Border>
                                    <Border Grid.Row="5" Grid.Column="5" Grid.ColumnSpan="4"  Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                    </Border>
                                    <!--#endregion-->
                                    <!--#region Row 6 -->
                                    <Border Grid.Row="6" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock Margin="0,0,9,0" Style="{StaticResource Content_InputForm_LabelStyle}" Text="특이사항"/>
                                        </StackPanel>
                                    </Border>
                                    <Border Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="8" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox x:Name="txtRemark" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="0,42" Width="421"></TextBox>
                                        </StackPanel>
                                    </Border>                                    
                                    <!--#endregion-->
                                    
                                </Grid>
                            </Grid>
                        </Grid>
                        <Border Grid.Column="1" Width="5" />
                        <Grid x:Name="ResultDetail" Grid.Row="1" Grid.Column="2">
                            <!--#region Lot Detail -->
                            <c1:C1TabControl x:Name="tabDetail" >
                                <!--#region 불량정보 -->
                                <c1:C1TabItem x:Name="tabDefect" Header="불량정보">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height ="Auto"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="5"/>
                                        </Grid.RowDefinitions>
                                        <!--타이틀-->
                                        <Grid Grid.Row="0">
                                            <Border BorderBrush="Black">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="Auto" />
                                                    </Grid.ColumnDefinitions>
                                                    <Border Grid.Column="1">
                                                        <Button x:Name="btnModel" Content="{Binding ConverterParameter=Model종료, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="5,10" Width="100" Height="20"/>
                                                    </Border>
                                                    <Border Grid.Column="2" Margin="0,0,3,0">
                                                        <Button x:Name="btnDefect" Content="{Binding ConverterParameter=저장, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="5,10" Width="100" Height="20"/>
                                                    </Border>
                                                </Grid>
                                            </Border>
                                        </Grid>
                                        <!--불량항목-->
                                        <Grid Grid.Row="1" >
                                            <c1:C1DataGrid Margin="0,5,0,0" x:Name="dgDefect">
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn Binding="{Binding DEFECT_CODE}" Header="불량코드"/>
                                                    <c1:DataGridTextColumn Binding="{Binding DEFECT_NAME}" Header="불량명"/>
                                                    <c1:DataGridTextColumn Binding="{Binding DEFECT_QTY}" Header="불량수량"/>
                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>
                                        </Grid>
                                    </Grid>
                                </c1:C1TabItem>
                                <!--#endregion-->
                                <!--#region 품질정보 -->
                                <c1:C1TabItem x:Name="tabQuality" Header="품질정보">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height ="Auto"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="5"/>
                                        </Grid.RowDefinitions>
                                        <!--타이틀-->
                                        <Grid Grid.Row="0">
                                            <Border BorderBrush="Black">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="Auto" />
                                                    </Grid.ColumnDefinitions>
                                                    <Border Grid.Column="1">

                                                    </Border>
                                                    <Border Grid.Column="2">
                                                        <Button x:Name="btnQualit" Content="{Binding ConverterParameter=저장, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="5,10" Width="100" Height="20"/>
                                                    </Border>
                                                </Grid>
                                            </Border>
                                        </Grid>
                                        <!--품질정보-->
                                        <Grid Grid.Row="1" >
                                            <c1:C1DataGrid Margin="0,5,0,0" x:Name="dgMaterial">
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn Binding="{Binding QUALITY_CODE}" Header="측정코드"/>
                                                    <c1:DataGridTextColumn Binding="{Binding QUALITY_NAME}" Header="측정명"/>
                                                    <c1:DataGridTextColumn Binding="{Binding QUALITY_USL}" Header="USL"/>
                                                    <c1:DataGridTextColumn Binding="{Binding QUALITY_LSL}" Header="LSL"/>
                                                    <c1:DataGridTextColumn Binding="{Binding QUALITY_UNIT}" Header="UNIT"/>
                                                    <c1:DataGridTextColumn Binding="{Binding QUALITY_VALUE}" Header="VALUE"/>
                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>
                                        </Grid>
                                    </Grid>
                                </c1:C1TabItem>
                                <!--#endregion-->                                
                                <!--#region 투입자재 -->
                                <c1:C1TabItem x:Name="tabMaterial" Header="투입자재">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height ="Auto"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="5"/>
                                        </Grid.RowDefinitions>
                                        <!--타이틀-->
                                        <Grid Grid.Row="0">
                                            <Border BorderBrush="Black">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="Auto" />
                                                    </Grid.ColumnDefinitions>
                                                    <Border Grid.Column="1">

                                                    </Border>
                                                    <Border Grid.Column="2">
                                                        <Button x:Name="btnMaterial" Content="{Binding ConverterParameter=저장, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_SubTaskSpecialButtonStyle}" Cursor="Hand" Margin="5,10" Width="100" Height="20"/>
                                                    </Border>
                                                </Grid>
                                            </Border>
                                        </Grid>
                                        <!--투입자재-->
                                        <Grid Grid.Row="1" >
                                            <c1:C1DataGrid Margin="0,5,0,0" x:Name="dgQuality">
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridComboBoxColumn Binding="{Binding MTRL_ID}" Header="자재ID"/>
                                                    <c1:DataGridTextColumn Binding="{Binding QUALITY_NAME}" Header="자재Lot"/>
                                                    <c1:DataGridNumericColumn Binding="{Binding USAGE}" Header="사용량"/>
                                                    <c1:DataGridDateTimeColumn Binding="{Binding INPUT_TIME}" Header="투입시간"/>
                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>
                                        </Grid>                                        
                                    </Grid>                                    
                                </c1:C1TabItem>
                                <!--#endregion-->

                            </c1:C1TabControl>
                            <!--#endregion-->
                        </Grid>
                    </Grid>
                    <!--#endregion-->

                </Grid>

            </ctrlLib:LGCLayout.Content>

        </ctrlLib:LGCLayout>

        <ctrlLib:LoadingIndicator x:Name="loadingIndicator" Grid.Row="999" Grid.Column="999" />
    </Grid>
</UserControl>
