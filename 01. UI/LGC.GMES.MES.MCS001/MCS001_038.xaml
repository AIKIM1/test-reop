<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             x:Class="LGC.GMES.MES.MCS001.MCS001_038"
             mc:Ignorable="d" 
             d:DesignHeight="850" d:DesignWidth="1920" 
             Loaded="UserControl_Loaded"  
             >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="STK 재공현황 Summary" >
            <ctrlLib:LGCLayout.Content>
                <Grid >
                    <c1:C1TabControl x:Name="tabStock">
                        <!--창고-->
                        <c1:C1TabItem x:Name="ctbStock" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='창고'}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <!--조회영역-->
                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="13" />
                                    </Grid.ColumnDefinitions>

                                    <!--백그라운드용-->
                                    <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                    <Grid Grid.Row="1" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="3*"/>
                                            <ColumnDefinition Width="7*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock                       Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                        <c1:C1ComboBox  x:Name="cboArea" Grid.Column="1" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                    </Grid>
                                    
                                    <Grid Grid.Row="1" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="3*"/>
                                            <ColumnDefinition Width="7*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock                              Grid.Column="0" Text="{Binding ConverterParameter='WH_TYPE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                        <c1:C1ComboBox  x:Name="cboStockerType" Grid.Column="1" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="3*"/>
                                            <ColumnDefinition Width="7*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock                                 Grid.Column="0" Text="{Binding ConverterParameter='극성', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                        <c1:C1ComboBox  x:Name="cboElectrodeType"  Grid.Column="1" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="7">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="3*"/>
                                            <ColumnDefinition Width="7*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock                                     Grid.Column="0" Text="{Binding ConverterParameter='창고', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                        <ctrlLib:MultiSelectionBox x:Name="cboStocker" Grid.Column="3"  MinWidth="140"  DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" isAllUsed="False" Height="36" VerticalAlignment="Bottom"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="9">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="3*"/>
                                            <ColumnDefinition Width="7*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock                             Grid.Column="0" Text="{Binding ConverterParameter='구분', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                        <c1:C1ComboBox  x:Name="cboSearchType" Grid.Column="1" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                    </Grid>

                                    <Button Grid.Row="1" Grid.Column="12" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                                </Grid>

                                <!--조회목록-->
                                <Grid Grid.Row="2">
                                    <!--라미외 권치별-->
                                    <c1:C1DataGrid x:Name="dgStockListE" 
                                                   Grid.Row="1" 
                                                   IsReadOnly="True" 
                                                   HeadersVisibility="None"
                                                   FrozenTopRowsCount="2"
                                                   FrozenColumnCount="4"
                                                   CanUserFilter="False"
                                                   CanUserSort="False"
                                                   SelectionMode="MultiRange" 
                                                   LoadedCellPresenter="dgStockList_LoadedCellPresenter"
                                                   UnloadedCellPresenter="dgStockList_UnloadedCellPresenter">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn      Header = "WH_TYPE"                     Binding = "{Binding EQGRNAME}"                    HorizontalAlignment="Left"   ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn      Header = "구분"                        Binding = "{Binding ELTR_TYPE_NAME}"              HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridNumericColumn   Header = "LEVEL"                       Binding = "{Binding EQPT_FLOOR_NO}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn      Header = "창고명"                      Binding = "{Binding EQPTNAME}"                    HorizontalAlignment="Left"   ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridNumericColumn   Header = "RACK수(EA)"                  Binding = "{Binding CNT_RACK}"                    HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "NO_MERGE_COL1"               Binding = "{Binding NO_MERGE_COL1}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "[적재율(%),전체]"            Binding = "{Binding ALL_RATE_LOADING}"            HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "[적재율(%),NO_MERGE_COL2]"   Binding = "{Binding NO_MERGE_COL2}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "[적재율(%),양품]"            Binding = "{Binding GOOD_RATE_LOADING}"           HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "[적재율(%),NO_MERGE_COL3]"   Binding = "{Binding NO_MERGE_COL3}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "[적재율(%),HOLD]"            Binding = "{Binding EMPTY_RATE_LOADING}"          HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "NO_MERGE_COL4"               Binding = "{Binding NO_MERGE_COL4}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "공CARRIER(EA)"               Binding = "{Binding CNT_EMPTY_CST}"               HorizontalAlignment="Right" Format="###,###,##0.#"  ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "NO_MERGE_COL5"               Binding = "{Binding NO_MERGE_COL5}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "오류Carrier"                 Binding = "{Binding CNT_ERROR_CST}"               HorizontalAlignment="Right" Format="###,###,##0.#"  ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "모델"                        Binding = "{Binding PRJT_NAME}"                   HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridNumericColumn   Header = "합계(EA)"                    Binding = "{Binding CNT_LOT}"                     HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양산(EA),999M]"             Binding = "{Binding CNT_PROD_GOOD_LE1000}"        HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양산(EA),1000M-1999M]"      Binding = "{Binding CNT_PROD_GOOD_1000_1999}"     HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양산(EA),2000M-2999M]"      Binding = "{Binding CNT_PROD_GOOD_2000_2999}"     HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양산(EA),3000M]"            Binding = "{Binding CNT_PROD_GOOD_QV3000}"        HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양산(EA),HOLD]"             Binding = "{Binding CNT_PROD_GOOD_HOLD}"          HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[시생산(EA),999M]"           Binding = "{Binding CNT_PILOT_GOOD_LE1000}"       HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[시생산(EA),1000M-1999M]"    Binding = "{Binding CNT_PILOT_GOOD_1000_1999}"    HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[시생산(EA),2000M-2999M]"    Binding = "{Binding CNT_PILOT_GOOD_2000_2999}"    HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[시생산(EA),3000M]"          Binding = "{Binding CNT_PILOT_GOOD_OV3000}"       HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[시생산(EA),HOLD]"           Binding = "{Binding CNT_PILOT_GOOD_HOLD}"         HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[기타(EA),999M]"             Binding = "{Binding CNT_ETC_GOOD_LE1000}"         HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[기타(EA),1000M-1999M]"      Binding = "{Binding CNT_ETC_GOOD_1000_1999}"      HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[기타(EA),2000M-2999M]"      Binding = "{Binding CNT_ETC_GOOD_2000_2999}"      HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[기타(EA),3000M]"            Binding = "{Binding CNT_ETC_GOOD_OV3000}"         HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[기타(EA),HOLD]"             Binding = "{Binding CNT_ETC_GOOD_HOLD}"           HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridTextColumn      Header = "EQPT_SORT_NO"                Binding = "{Binding EQPT_SORT_NO}"                HorizontalAlignment="Left"  Visibility="Collapsed"/>
                                        </c1:C1DataGrid.Columns>
                                        <c1:C1DataGrid.TopRows>
                                            <c1:DataGridColumnHeaderRow />
                                            <c1:DataGridColumnHeaderRow />
                                        </c1:C1DataGrid.TopRows>
                                    </c1:C1DataGrid>

                                    <!--라미외 가용구분별-->
                                    <c1:C1DataGrid x:Name="dgStockListU" 
                                                   Visibility="Collapsed"
                                                   Grid.Row="1" 
                                                   IsReadOnly="True" 
                                                   HeadersVisibility="None"
                                                   FrozenTopRowsCount="2"
                                                   FrozenColumnCount="4"
                                                   CanUserFilter="False"
                                                   CanUserSort="False"
                                                   SelectionMode="MultiRange" 
                                                   LoadedCellPresenter="dgStockList_LoadedCellPresenter"
                                                   UnloadedCellPresenter="dgStockList_UnloadedCellPresenter">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn      Header = "WH_TYPE"                     Binding = "{Binding EQGRNAME}"                    HorizontalAlignment="Left"   ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn      Header = "구분"                        Binding = "{Binding ELTR_TYPE_NAME}"              HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridNumericColumn   Header = "LEVEL"                       Binding = "{Binding EQPT_FLOOR_NO}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn      Header = "창고명"                      Binding = "{Binding EQPTNAME}"                    HorizontalAlignment="Left"   ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridNumericColumn   Header = "RACK수(EA)"                  Binding = "{Binding CNT_RACK}"                    HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "NO_MERGE_COL1"               Binding = "{Binding NO_MERGE_COL1}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "[적재율(%),전체]"            Binding = "{Binding ALL_RATE_LOADING}"            HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "[적재율(%),NO_MERGE_COL2]"   Binding = "{Binding NO_MERGE_COL2}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "[적재율(%),양품]"            Binding = "{Binding GOOD_RATE_LOADING}"           HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "[적재율(%),NO_MERGE_COL3]"   Binding = "{Binding NO_MERGE_COL3}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "[적재율(%),HOLD]"            Binding = "{Binding EMPTY_RATE_LOADING}"          HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "NO_MERGE_COL4"               Binding = "{Binding NO_MERGE_COL4}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "공CARRIER(EA)"               Binding = "{Binding CNT_EMPTY_CST}"               HorizontalAlignment="Right" Format="###,###,##0.#"  ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "NO_MERGE_COL5"               Binding = "{Binding NO_MERGE_COL5}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "오류Carrier"                 Binding = "{Binding CNT_ERROR_CST}"               HorizontalAlignment="Right" Format="###,###,##0.#"  ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "모델"                        Binding = "{Binding PRJT_NAME}"                   HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridNumericColumn   Header = "합계(EA)"                    Binding = "{Binding CNT_LOT}"                     HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양산(EA),소계]"             Binding = "{Binding CNT_PROD}"                    HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양산(EA),가용]"             Binding = "{Binding CNT_PROD_GOOD}"               HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양산(EA),HOLD]"             Binding = "{Binding CNT_PROD_GOOD_HOLD}"          HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[시생산(EA),소계]"           Binding = "{Binding CNT_PILOT}"                   HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[시생산(EA),가용]"           Binding = "{Binding CNT_PILOT_GOOD}"              HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[시생산(EA),HOLD]"           Binding = "{Binding CNT_PILOT_GOOD_HOLD}"         HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[기타(EA),소계]"             Binding = "{Binding CNT_ETC}"                     HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[기타(EA),가용]"             Binding = "{Binding CNT_ETC_GOOD}"                HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[기타(EA),HOLD]"             Binding = "{Binding CNT_ETC_GOOD_HOLD}"           HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridTextColumn      Header = "EQPT_SORT_NO"                Binding = "{Binding EQPT_SORT_NO}"                HorizontalAlignment="Left"  Visibility="Collapsed"/>
                                        </c1:C1DataGrid.Columns>
                                        <c1:C1DataGrid.TopRows>
                                            <c1:DataGridColumnHeaderRow />
                                            <c1:DataGridColumnHeaderRow />
                                        </c1:C1DataGrid.TopRows>
                                    </c1:C1DataGrid>
                                    
                                    <!--라미-->
                                    <c1:C1DataGrid x:Name="dgStockListL" 
                                                   Visibility="Collapsed"
                                                   Grid.Row="1" 
                                                   IsReadOnly="True" 
                                                   HeadersVisibility="None"
                                                   FrozenTopRowsCount="3"
                                                   FrozenColumnCount="4"
                                                   CanUserFilter="False"
                                                   CanUserSort="False"
                                                   SelectionMode="MultiRange" 
                                                   LoadedCellPresenter="dgStockList_LoadedCellPresenter"
                                                   UnloadedCellPresenter="dgStockList_UnloadedCellPresenter">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn      Header = "WH_TYPE"                     Binding = "{Binding EQGRNAME}"                    HorizontalAlignment="Left"   ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridNumericColumn   Header = "LEVEL"                       Binding = "{Binding EQPT_FLOOR_NO}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "창고명"                      Binding = "{Binding EQPTNAME}"                    HorizontalAlignment="Left"   ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridNumericColumn   Header = "RACK수(EA)"                  Binding = "{Binding CNT_RACK}"                    HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "NO_MERGE_COL1"               Binding = "{Binding NO_MERGE_COL1}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "[적재율(%),전체,전체]"       Binding = "{Binding ALL_RATE_LOADING}"            HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "[적재율(%),NO_MERGE_COL2]"   Binding = "{Binding NO_MERGE_COL2}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "[적재율(%),양품,양품]"       Binding = "{Binding GOOD_RATE_LOADING}"           HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "[적재율(%),NO_MERGE_COL3]"   Binding = "{Binding NO_MERGE_COL3}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "[적재율(%),HOLD,HOLD]"       Binding = "{Binding EMPTY_RATE_LOADING}"          HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "NO_MERGE_COL4"               Binding = "{Binding NO_MERGE_COL4}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "극성비율(양/음 %)"           Binding = "{Binding RATE_CA_BY_AN}"               HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridNumericColumn   Header = "공CARRIER(EA)"               Binding = "{Binding CNT_EMPTY_CST}"               HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "NO_MERGE_COL5"               Binding = "{Binding NO_MERGE_COL5}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "오류Carrier"                 Binding = "{Binding CNT_ERROR_CST}"               HorizontalAlignment="Right" Format="###,###,##0.#" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTextColumn      Header = "모델"                        Binding = "{Binding PRJT_NAME}"                   HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn   Header = "[양극(EA),합계,합계]"        Binding = "{Binding CNT_CA_WIP}"                  HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양극(EA),양산,양품]"        Binding = "{Binding CNT_CA_PROD_GOOD}"            HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양극(EA),양산,HOLD]"        Binding = "{Binding CNT_CA_PROD_HOLD}"            HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양극(EA),시생산,양품]"      Binding = "{Binding CNT_CA_PILOT_GOOD}"           HorizontalAlignment="Right" Format="###,###,##0.#" />
                                            <c1:DataGridNumericColumn   Header = "[양극(EA),시생산,HOLD]"      Binding = "{Binding CNT_CA_PILOT_HOLD}"           HorizontalAlignment="Right" Format="###,###,##0.#" />
