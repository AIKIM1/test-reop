<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:cmm001="clr-namespace:LGC.GMES.MES.CMM001;assembly=LGC.GMES.MES.CMM001"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             x:Class="LGC.GMES.MES.MCS001.MCS001_078"
             mc:Ignorable="d" 
             d:DesignHeight="850" d:DesignWidth="1920" 
             Loaded="UserControl_Loaded" 
             Unloaded="UserControl_Unloaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.CMM001;component/PopupFindControlStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="활성화 창고 수동출고 예약" >
            <ctrlLib:LGCLayout.Content>
                <Grid >
                    <Grid.RowDefinitions>
                        <!--TopSearch-->
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8"/>
                        <!--Contents-->
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="8"/>
                        <!--Bottom-->
                        <RowDefinition Height="42"/>
                    </Grid.RowDefinitions>

                    <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                    <Grid x:Name="TopSearch" Grid.Row="0" Grid.ColumnSpan="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="8" />
                        </Grid.RowDefinitions>
                        <!-- 조회 버튼 영역-->
                        <!--2024.08.16, E20240805-000350, taufiqhadi: Changed column definition for new ComboBox placement-->
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="13"/>
                            <ColumnDefinition Width="0.9*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="0.5*" />
                            <ColumnDefinition Width="10"/>
                            <!--DEFINITION FOR BOX FLOOR-->
                            <ColumnDefinition Width="0.5*" />
                            <!--DEFINITION FOR BOX FLOOR-->
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="0.5*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="0.5*" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="0.6*" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="0.2*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="13" />
                        </Grid.ColumnDefinitions>

                        <StackPanel Grid.Row="1" Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Left" >
                            <RadioButton Margin="5,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='실Pallet'}" GroupName="PalletType" Name="rdoRealPallet" Style="{StaticResource SearchCondition_RadioButtonStyle}" Checked="rdoPalletType_Checked" />
                            <RadioButton Margin="0,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='공Pallet'}" GroupName="PalletType" Name="rdoNoPallet" Style="{StaticResource SearchCondition_RadioButtonStyle}" Checked="rdoPalletType_Checked" />
                            <RadioButton Margin="0,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='비정상Pallet'}" GroupName="PalletType" Name="rdoAbnormalPallet" Style="{StaticResource SearchCondition_RadioButtonStyle}" Checked="rdoPalletType_Checked" />
                            <RadioButton Margin="0,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='오류Pallet'}" GroupName="PalletType" Name="rdoErrorPallet" Style="{StaticResource SearchCondition_RadioButtonStyle}" Checked="rdoPalletType_Checked" />
                        </StackPanel>
                        <Grid Grid.Row="1" Grid.Column="3">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" IsEnabled="False" />
                            </StackPanel>
                            <c1:C1ComboBox Grid.Row="0"  Grid.Column="1" x:Name="cboArea" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboArea_SelectedValueChanged" />
                        </Grid>

                        <!--2024.08.16, E20240805-000350, taufiqhadi: Added ComboBox for floor-->
                        <Grid Grid.Row="1" Grid.Column="5">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock Text="{Binding ConverterParameter='층', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" IsEnabled="False" />
                            </StackPanel>
                            <c1:C1ComboBox Grid.Row="0"  Grid.Column="1" x:Name="cboFloor" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboFloor_SelectedValueChanged" />
                        </Grid>

                        <Grid Grid.Row="1" Grid.Column="7">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock Text="{Binding ConverterParameter='창고', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            </StackPanel>
                            <c1:C1ComboBox Grid.Row="0"  Grid.Column="1" x:Name="cboStocker"  DisplayMemberPath="EQPTNAME" SelectedValuePath="EQPTID"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStocker_SelectedValueChanged" />
                        </Grid>

                        <Grid Grid.Row="1" Grid.Column="9">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock Text="{Binding ConverterParameter='Pallet ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            </StackPanel>
                            <TextBox   Grid.Row="0"  Grid.Column="1" x:Name="txtPalletId" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="120" KeyDown="txtPalletId_KeyDown" PreviewKeyDown="txtPalletId_PreviewKeyDown" />
                            <cmm001:PopupFindControl Grid.Row="0" Grid.Column="1" x:Name="popPalletId" SelectedValuePath="CBO_CODE" DisplayMemberPath="CBO_NAME" PopupDisplayMode="ValueAndText" KeyDown="popPalletId_KeyDown" Visibility="Collapsed" />
                        </Grid>

                        <Grid Grid.Row="1" Grid.Column="12">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock Text="{Binding ConverterParameter='Carrier ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            </StackPanel>
                            <TextBox   Grid.Row="0"  Grid.Column="1" x:Name="txtCarrierId" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="120" KeyDown="txtCarrierId_KeyDown" PreviewKeyDown="txtCarrierId_PreviewKeyDown" />
                        </Grid>

                        <Button Grid.Row="1" Grid.Column="15" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                    </Grid>

                    <!--Contents-->
                    <Grid x:Name="Contents" Grid.Row="2">
                        <Grid Grid.Row="0"  x:Name="ContentsRow">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2.1*"/>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="7.9*" />
                            </Grid.ColumnDefinitions>
                            <!--Left Grid Area-->
                            <Grid Grid.Row="0" x:Name="Left">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34" />
                                    <RowDefinition Height="1.2*" />
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="0.8*" />
                                </Grid.RowDefinitions>

                                <Grid Grid.Row="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter=창고재고, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    </Border>
                                </Grid>

                                <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Grid Grid.Row="1" x:Name="grdLeft">
                                    <uc:UcBaseDataGrid x:Name="dgRealPallet" 
                                                   Grid.Row="0" 
                                                   Grid.Column="0" 
                                                   IsReadOnly="True" 
                                                   HeadersVisibility="None"
                                                   FrozenTopRowsCount="3"
                                                   FrozenColumnCount="2"
                                                   SelectionMode="SingleCell"
                                                   AlternatingRowBackground="White"  
                                                   CanUserSort="False"
                                                   LoadedCellPresenter="dgRealPallet_LoadedCellPresenter" 
                                                   UnloadedCellPresenter="dgRealPallet_UnloadedCellPresenter" 
                                                   MouseLeftButtonUp="dgRealPallet_MouseLeftButtonUp"
                                                   Margin="8"
                                                   >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn      Header="[모델LOT, 모델LOT, 모델LOT]"              Binding="{Binding MDLLOT_ID}"            HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn      Header="[프로젝트명, 프로젝트명, 프로젝트명]"     Binding="{Binding PRJT_NAME}"            HorizontalAlignment="Center"/>
                                            <c1:DataGridNumericColumn   Header="[Pallet 수량, 출하가능, 출하가능]"        Binding="{Binding SHIP_PLT_QTY}"         HorizontalAlignment="Right"  Format="#,##0" />
                                            <c1:DataGridNumericColumn   Header="[Pallet 수량, 출하불가능, HOLD]"          Binding="{Binding HOLD_PLT_QTY}"         HorizontalAlignment="Right"  Format="#,##0"  />
                                            <c1:DataGridNumericColumn   Header="[Pallet 수량, 출하불가능, 검사대기]"      Binding="{Binding INSP_WAIT_PLT_QTY}"    HorizontalAlignment="Right"  Format="#,##0" />
                                            <c1:DataGridNumericColumn   Header="[Pallet 수량, 출하불가능, 불합격]"        Binding="{Binding NG_PLT_QTY}"           HorizontalAlignment="Right"  Format="#,##0" />
                                            <c1:DataGridNumericColumn   Header="[Pallet 수량, 출하불가능, 장기재고]"      Binding="{Binding VLD_DATE_PLT_QTY}"    HorizontalAlignment="Right"  Format="#,##0" />
                                            <c1:DataGridNumericColumn   Header="[Cell 수량, 출하가능, 출하가능]"          Binding="{Binding SHIP_CELL_QTY}"        HorizontalAlignment="Right"  Format="#,##0"  />
                                            <c1:DataGridNumericColumn   Header="[Cell 수량, 출하불가능, HOLD]"            Binding="{Binding HOLD_CELL_QTY}"        HorizontalAlignment="Right"  Format="#,##0"  />
                                            <c1:DataGridNumericColumn   Header="[Cell 수량, 출하불가능, 검사대기]"        Binding="{Binding INSP_WAIT_CELL_QTY}"   HorizontalAlignment="Right"  Format="#,##0"  />
                                            <c1:DataGridNumericColumn   Header="[Cell 수량, 출하불가능, 불합격]"          Binding="{Binding NG_CELL_QTY}"          HorizontalAlignment="Right"  Format="#,##0"  />
                                            <c1:DataGridNumericColumn   Header="[Cell 수량, 출하불가능, 장기재고]"        Binding="{Binding VLD_DATE_CELL_QTY}"   HorizontalAlignment="Right"  Format="#,##0"  />
                                            <c1:DataGridNumericColumn   Header="[이동중 수량, 이동중 수량, 이동중 수량]"  Binding="{Binding OUT_PLT_CNT}"          HorizontalAlignment="Right"  Format="#,##0" />
                                            <c1:DataGridTextColumn      Header="[PRODID, PRODID, PRODID]"                 Binding="{Binding PRODID}"               HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        </c1:C1DataGrid.Columns>
                                        <c1:C1DataGrid.TopRows>
                                            <c1:DataGridColumnHeaderRow />
                                            <c1:DataGridColumnHeaderRow />
                                            <c1:DataGridColumnHeaderRow />
                                        </c1:C1DataGrid.TopRows>
                                    </uc:UcBaseDataGrid>

                                    <uc:UcBaseDataGrid x:Name="dgNoPallet" 
                                                   HeadersVisibility="None" 
                                                   IsReadOnly="True"
                                                   Grid.Row="0" 
                                                   FrozenTopRowsCount="2"
                                                   FrozenColumnCount="2"
                                                   Visibility="Collapsed"
                                                   SelectionMode="SingleCell"
                                                   AlternatingRowBackground="White"  
                                                   CanUserSort="False"
                                                   LoadedCellPresenter="dgNoPallet_LoadedCellPresenter" 
                                                   UnloadedCellPresenter="dgNoPallet_UnloadedCellPresenter" 
                                                   MouseLeftButtonUp="dgNoPallet_MouseLeftButtonUp"
                                                   Margin="8"
                                                   >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn      Header="[Tray Type, Tray Type]"       Binding="{Binding CSTPROD_NAME}"                HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridNumericColumn   Header="[공Pallet, 공Pallet]"         Binding="{Binding E_PLT_CNT}"                   HorizontalAlignment="Right" Format="#,##0" />
                                            <c1:DataGridNumericColumn   Header="[공Tray Pallet, 세정전]"      Binding="{Binding T_PLT_CLEAN_BEFORE_CNT}"      HorizontalAlignment="Right" Format="#,##0" />
                                            <c1:DataGridNumericColumn   Header="[공Tray Pallet, 세정후]"      Binding="{Binding T_PLT_CLEAN_AFTER_CNT}"       HorizontalAlignment="Right" Format="#,##0" />
                                            <c1:DataGridNumericColumn   Header="[이동중 수량, 이동중 수량]"   Binding="{Binding OUT_PLT_CNT}"                 HorizontalAlignment="Right" Format="#,##0" />
                                            <c1:DataGridNumericColumn   Header="[세정기, 공Pallet]"           Binding="{Binding C_PLT_E_CNT}"                 HorizontalAlignment="Right" Format="#,##0" Visibility="Hidden" />
                                            <c1:DataGridNumericColumn   Header="[세정기, 공Tray Pallet]"      Binding="{Binding C_PLT_T_CNT}"                 HorizontalAlignment="Right" Format="#,##0" Visibility="Hidden" />
                                            <c1:DataGridTextColumn      Header="[Tray Type, Tray Type]"       Binding="{Binding CSTPROD}"                     HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed" />
                                        </c1:C1DataGrid.Columns>
                                        <c1:C1DataGrid.TopRows>
                                            <c1:DataGridColumnHeaderRow />
                                            <c1:DataGridColumnHeaderRow />
                                        </c1:C1DataGrid.TopRows>
                                    </uc:UcBaseDataGrid>

                                    <uc:UcBaseDataGrid x:Name="dgAbnormalPallet" 
                                                   HeadersVisibility="Column" 
                                                   IsReadOnly="True"
                                                   Grid.Row="0" 
                                                   Visibility="Collapsed"
                                                   SelectionMode="SingleCell"
                                                   AlternatingRowBackground="White"  
                                                   CanUserSort="False"
                                                   FrozenColumnCount="2"
                                                   MergingCells="dgAbnormalPallet_MergingCells"
                                                   LoadedCellPresenter="dgAbnormalPallet_LoadedCellPresenter" 
                                                   UnloadedCellPresenter="dgAbnormalPallet_UnloadedCellPresenter" 
                                                   MouseLeftButtonUp="dgAbnormalPallet_MouseLeftButtonUp"
                                                   Margin="8"
                                                   >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn      Header="EQPTID"         Binding="{Binding EQPTID}"                      HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header="창고명"         Binding="{Binding EQPT_NAME}"                   HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header="정보불일치유형" Binding="{Binding ABNORM_TRF_RSN_CODE}"         HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header="정보불일치유형" Binding="{Binding ABNORM_TRF_RSN_NAME}"         HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn   Header="Pallet 수량"    Binding="{Binding PLT_CNT}"                     HorizontalAlignment="Right" Format="#,##0" />
                                            <c1:DataGridNumericColumn   Header="이동중 수량"     Binding="{Binding OUT_PLT_CNT}"                 HorizontalAlignment="Right" Format="#,##0" />
                                        </c1:C1DataGrid.Columns>
                                    </uc:UcBaseDataGrid>

                                    <uc:UcBaseDataGrid x:Name="dgErrorPallet" 
                                                   HeadersVisibility="Column" 
                                                   IsReadOnly="True"
                                                   Grid.Row="0" 
                                                   Visibility="Collapsed"
                                                   SelectionMode="SingleCell"
                                                   AlternatingRowBackground="White"  
                                                   FrozenColumnCount="2"
                                                   CanUserSort="False"
                                                   LoadedCellPresenter="dgErrorPallet_LoadedCellPresenter" 
                                                   UnloadedCellPresenter="dgErrorPallet_UnloadedCellPresenter" 
                                                   MouseLeftButtonUp="dgErrorPallet_MouseLeftButtonUp"
                                                   Margin="8"
                                                   >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn      Header="창고"         Binding="{Binding EQPTID}"           HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header="창고"         Binding="{Binding EQPT_NAME}"        HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header="오류유형"     Binding="{Binding CST_TYPE_CODE}"    HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header="오류유형"     Binding="{Binding CST_TYPE_NAME}"    HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn   Header="Pallet 수량"  Binding="{Binding PLT_CNT}"          HorizontalAlignment="Right" Format="#,##0" />
                                        </c1:C1DataGrid.Columns>
                                    </uc:UcBaseDataGrid>
                                </Grid>

                                <!--Splitter-->
                                <GridSplitter Grid.Row="2" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                                <Grid Grid.Row="3">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="2.*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                        <TextBlock x:Name="txtPortInfo" Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter=목적지 포트 정보, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                        <Button x:Name="btnRefresh" Margin="4,4,4,2" Grid.Row="0" Grid.Column="2"  Content="{Binding ConverterParameter='새로고침', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnRefresh_Click"/>
                                    </Grid>

                                    <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                    <Grid Grid.Row="1" >
                                        <uc:UcBaseDataGrid x:Name="dgPortInfo" 
                                                Grid.Row="0" 
                                                Grid.Column="0" 
                                                HeadersVisibility="Column" 
                                                FrozenColumnCount="2"
                                                SelectionMode="SingleRow"
                                                CanUserRemoveRows="True" 
                                                CanUserAddRows="True" 
                                                IsReadOnly="True"
                                                AlternatingRowBackground="White" 
                                                LoadedCellPresenter="dgPortInfo_LoadedCellPresenter" 
                                                UnloadedCellPresenter="dgPortInfo_UnloadedCellPresenter"
                                                MouseLeftButtonUp="dgPortInfo_MouseLeftButtonUp"
                                                Margin="8"
                                                >
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridTextColumn Header="포트명"             Binding="{Binding PORT_NAME}"            HorizontalAlignment="Left" />
                                                <c1:DataGridTextColumn Header="출고예약 수량"      Binding="{Binding REQ_CNT}"              HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn Header="입출고"             Binding="{Binding LOC_TRF_TP_NAME}"      HorizontalAlignment="Left" />
                                                <c1:DataGridTextColumn Header="상태"               Binding="{Binding TRF_STAT_NAME}"        HorizontalAlignment="Left" />
                                                <c1:DataGridTextColumn Header="운영상태"           Binding="{Binding ACCESS_MODE_NAME}"     HorizontalAlignment="Left" />
                                                <c1:DataGridTextColumn Header="캐리어 유무"        Binding="{Binding CARRIER_EXIST}"        HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn Header="캐리어 요청 상태"   Binding="{Binding REQ_TRF_TP_NAME}"      HorizontalAlignment="Left" Visibility="Collapsed" />
                                                <c1:DataGridTextColumn Header="PORT_ID"            Binding="{Binding PORT_ID}"              HorizontalAlignment="Center" Visibility="Collapsed" />
                                                <c1:DataGridTextColumn Header="LOC_TRF_TP"         Binding="{Binding LOC_TRF_TP}"           HorizontalAlignment="Center" Visibility="Collapsed"  />
                                                <c1:DataGridTextColumn Header="TRF_STAT_CODE"      Binding="{Binding TRF_STAT_CODE}"        HorizontalAlignment="Center" Visibility="Collapsed"  />
                                                <c1:DataGridTextColumn Header="ACCESS_MODE_CODE"   Binding="{Binding ACCESS_MODE_CODE}"     HorizontalAlignment="Center" Visibility="Collapsed"  />
                                                <c1:DataGridTextColumn Header="CARRIER_EXIST"      Binding="{Binding CARRIER_EXIST_NAME}"   HorizontalAlignment="Center" Visibility="Collapsed"  />
                                                <c1:DataGridTextColumn Header="REQ_TRF_TP"         Binding="{Binding REQ_TRF_TP}"           HorizontalAlignment="Center" Visibility="Collapsed"  />
                                                <!--<c1:DataGridTextColumn Header="LOC_DETL_TP"        Binding="{Binding LOC_DETL_TP}"          HorizontalAlignment="Center" Visibility="Collapsed"  />-->
                                            </c1:C1DataGrid.Columns>
                                        </uc:UcBaseDataGrid>
                                        <uc:UcBaseDataGrid x:Name="dgStkInfo" 
                                                   Grid.Row="0" 
                                                   Grid.Column="0" 
                                                   FrozenColumnCount="2"
                                                   HeadersVisibility="Column" 
                                                   SelectionMode="SingleRow"
                                                   CanUserRemoveRows="True" 
                                                   CanUserAddRows="True" 
                                                   IsReadOnly="True"
                                                   AlternatingRowBackground="White" 
                                                   LoadedCellPresenter="dgStkInfo_LoadedCellPresenter" 
                                                   UnloadedCellPresenter="dgStkInfo_UnloadedCellPresenter"
                                                   MouseLeftButtonUp="dgStkInfo_MouseLeftButtonUp"
                                                   Visibility="Collapsed"
                                                   Margin="8"
                                                   >
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridTextColumn Header="창고"               Binding="{Binding EQPTNAME}"            HorizontalAlignment="Left" />
                                                <c1:DataGridTextColumn Header="반송예약수량"       Binding="{Binding MOVE_IN_QTY}"         HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn Header="적재율(%)"          Binding="{Binding LOAD_RATE, StringFormat=\{0:n2\}}"           HorizontalAlignment="Right" />
                                            </c1:C1DataGrid.Columns>
                                        </uc:UcBaseDataGrid>
                                    </Grid>
                                </Grid>
                            </Grid>

                            <!--Splitter-->
                            <GridSplitter Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"  />
                            <Grid x:Name="HMIAera" Grid.Column="2" >
                                <Grid x:Name="RightArea">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}"></Border>
                                    <Grid Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="334*"/>
                                            <ColumnDefinition Width="1177*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="34"/>
