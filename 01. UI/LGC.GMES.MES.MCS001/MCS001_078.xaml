                                                                    <c1:DataGridTextColumn		Header="반송요청상태"	  Binding="{Binding ORD_STAT}"    	      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                                    <c1:DataGridTextColumn      Header="창고"             Binding="{Binding EQPTID}"              HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                                    <c1:DataGridTextColumn		Header="CARRIERID"	      Binding="{Binding CARRIERID}"	          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                                    <c1:DataGridTextColumn		Header="ORDID"	          Binding="{Binding ORDID}"	              HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                                    <c1:DataGridTextColumn		Header="출고유형"	      Binding="{Binding STK_ISS_TYPE}"	      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                                    <c1:DataGridTextColumn		Header="출고유형"	      Binding="{Binding STK_ISS_TYPE_NAME}"	  HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                                </c1:C1DataGrid.Columns>
                                                            </uc:UcBaseDataGrid>
                                                        </Grid>
                                                    </c1:C1TabItem>

                                                    <c1:C1TabItem x:Name="tabItemOutRackList" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='이동중 현황'}" >
                                                        <Grid Grid.Row="0">
                                                            <uc:UcBaseDataGrid x:Name="dgOutRacktDetail" 
                                                               HeadersVisibility="Column" 
                                                               FrozenColumnCount="2"
                                                               IsReadOnly="True"
                                                               Margin="8"
                                                               >
                                                                <c1:C1DataGrid.Columns>
                                                                    <c1:DataGridTextColumn Header="NO"                     Binding="{Binding TRF_NO}"                HorizontalAlignment="Center" />
                                                                    <c1:DataGridTextColumn Header="Carrier ID"             Binding="{Binding CARRIERID}"             HorizontalAlignment="Center" />
                                                                    <c1:DataGridTextColumn Header="JOB_TP"                 Binding="{Binding JOB_CREATE_TP}"         HorizontalAlignment="Center" />
                                                                    <c1:DataGridTextColumn Header="PATH_TRF_TP"            Binding="{Binding CARRIER_STRUCT}"        HorizontalAlignment="Center" Visibility="Collapsed" />
                                                                    <c1:DataGridTextColumn Header="PATH_TRF_TP"            Binding="{Binding CARRIER_STRUCT_NAME}"   HorizontalAlignment="Center" />
                                                                    <c1:DataGridTextColumn Header="Tray Type"              Binding="{Binding MDL_TP}"                HorizontalAlignment="Center" Visibility="Collapsed" />
                                                                    <c1:DataGridTextColumn Header="Tray Type"              Binding="{Binding TRAY_TYPE}"             HorizontalAlignment="Center" />
                                                                    <c1:DataGridTextColumn Header="MACRO_SRC_LOC_NAME"     Binding="{Binding SRC_NAME}"              HorizontalAlignment="Center" />
                                                                    <c1:DataGridTextColumn Header="MACRO_DST_LOC_NAME"     Binding="{Binding DST_NAME}"              HorizontalAlignment="Center" />
                                                                    <c1:DataGridTextColumn Header="현재위치"               Binding="{Binding CURR_NAME}"             HorizontalAlignment="Center" />
                                                                    <c1:DataGridTextColumn Header="경과시간"               Binding="{Binding MACRO_EXETIME}"         HorizontalAlignment="Center" />
                                                                </c1:C1DataGrid.Columns>
                                                            </uc:UcBaseDataGrid>
                                                        </Grid>
                                                    </c1:C1TabItem>

                                                    <c1:C1TabItem x:Name="tabItemCleanerList" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='세정기 현황'}" Visibility="Hidden" >
                                                        <Grid Grid.Row="0">
                                                            <uc:UcBaseDataGrid x:Name="dgCleanerDetail" 
                                                               HeadersVisibility="Column" 
                                                               FrozenColumnCount="2"
                                                               IsReadOnly="True"
                                                               Margin="8"
                                                               >
                                                                <c1:C1DataGrid.Columns>
                                                                    <c1:DataGridTextColumn      Header = "NO"             Binding = "{Binding TRF_NO}"           HorizontalAlignment="Center" IsReadOnly="True" />
                                                                    <c1:DataGridTextColumn      Header = "설비명"         Binding = "{Binding EQPTNAME}"         HorizontalAlignment="Center" IsReadOnly="True" />
                                                                    <c1:DataGridTextColumn      Header = "포트명"         Binding = "{Binding PORT_NAME}"        HorizontalAlignment="Center" IsReadOnly="True" />
                                                                    <c1:DataGridTextColumn		Header = "Carrier ID"	  Binding = "{Binding CSTID}"	         HorizontalAlignment="Center" IsReadOnly="True" />
                                                                    <c1:DataGridTextColumn		Header = "Tray Type"	  Binding = "{Binding CSTPROD_NAME}"	 HorizontalAlignment="Center" IsReadOnly="True" />
                                                                    <c1:DataGridTextColumn		Header = "Carrier상태"	  Binding = "{Binding CSTSTAT_NAME}"	 HorizontalAlignment="Center" IsReadOnly="True" />
                                                                    <c1:DataGridTextColumn      Header = "입고일시"       Binding = "{Binding UPDDTTM}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                                </c1:C1DataGrid.Columns>
                                                            </uc:UcBaseDataGrid>
                                                        </Grid>
                                                    </c1:C1TabItem>
                                                </c1:C1TabControl>
                                                <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='경과일수', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" FontSize="12" Margin="0,0,0,0" />
                                                <Border    Grid.Row="1" Grid.Column="3" Background="#F2CB61" BorderThickness="0.5" Style="{StaticResource BorderSubTitleStyle}" />
                                                <TextBlock Grid.Row="1" Grid.Column="5" Text="{Binding ConverterParameter='D+7이상', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                                <Border    Grid.Row="1" Grid.Column="7" Background="Orange" BorderThickness="0.5" Style="{StaticResource BorderSubTitleStyle}" />
                                                <TextBlock Grid.Row="1" Grid.Column="9" Text="{Binding ConverterParameter='D+15이상', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                                <Border    Grid.Row="1" Grid.Column="11" Background="Red" BorderThickness="0.5" Style="{StaticResource BorderSubTitleStyle}" />
                                                <TextBlock Grid.Row="1" Grid.Column="13" Text="{Binding ConverterParameter='D+30이상', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                            </Grid>
                                        </Grid>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>
                    </Grid>

                    <!--Bottom-->
                    <Border Grid.Row="4" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                    <Grid x:Name="BottomArea" Grid.Row="4" >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="13"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="13" />
                        </Grid.ColumnDefinitions>

                        <Grid Grid.Row="0" Grid.Column="3">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock  x:Name="txtDstType" Text="{Binding ConverterParameter='목적지유형', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" Visibility="Collapsed" />
                            </StackPanel>
                            <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboDstType"  DisplayMemberPath="LOC_NAME" SelectedValuePath="LOCID"  AutoComplete="False" SelectedValueChanged="cboDstType_SelectedValueChanged" Visibility="Collapsed" />
                        </Grid>

                        <Grid Grid.Row="0" Grid.Column="5">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock  x:Name="txtShipType" Text="{Binding ConverterParameter='출고유형', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" Visibility="Collapsed" />
                            </StackPanel>
                            <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboShipType"  DisplayMemberPath="LOC_NAME" SelectedValuePath="LOCID"  AutoComplete="False" SelectedIndexChanged="cboOutPort_SelectedIndexChanged" Visibility="Collapsed" />
                        </Grid>

                        <Grid Grid.Row="0" Grid.Column="7">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock  Text="{Binding ConverterParameter='목적지명', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            </StackPanel>
                            <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboOutPort"  DisplayMemberPath="LOC_NAME" SelectedValuePath="LOCID"  AutoComplete="False" SelectedIndexChanged="cboOutPort_SelectedIndexChanged" />
                        </Grid>

                        <Grid Grid.Row="0" Grid.Column="9">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock  x:Name="txtCheckingQty" Text="{Binding ConverterParameter='체크된 수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            </StackPanel>
                            <TextBox Grid.Row="0" Grid.Column="1" x:Name="txtCheckingQtyData" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" TextAlignment="Right" MinWidth="50" />
                        </Grid>

                        <Button x:Name="btnManualIssue" Grid.Column="11" Content="{Binding ConverterParameter=수동반송예약, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnManualIssue_Click"  />
                        <Button x:Name="btnTransferCancel" Grid.Column="13" Content="{Binding ConverterParameter=반송예약취소, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnTransferCancel_Click" />
                        <Button x:Name="btnManualClean" Grid.Column="14" Content="{Binding ConverterParameter=수동세정완료, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnManualClean_Click"  Margin="4,0,0,0" Visibility="Collapsed" />
                        <!--<Button x:Name="btnDataIssue" Grid.Column="15" Content="{Binding ConverterParameter=데이터출고, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnDataIssue_Click" Margin="4,0,0,0" Visibility="Hidden" />-->
                    </Grid>

                </Grid>
            </ctrlLib:LGCLayout.Content>
        </ctrlLib:LGCLayout>
        <ctrlLib:LoadingIndicator x:Name="loadingIndicator"/>
    </Grid>
</UserControl>