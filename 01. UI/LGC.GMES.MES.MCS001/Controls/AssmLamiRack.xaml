<UserControl x:Class="LGC.GMES.MES.MCS001.Controls.AssmLamiRack"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LGC.GMES.MES.MCS001.Controls"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             Height="28" Width="45" Foreground="White">

	<UserControl.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</UserControl.Resources>
	<Grid x:Name="craneActive" x:FieldModifier="private" Margin="2,2,2,2" >
		<Border BorderThickness="1" BorderBrush="#FFA0A0A0">
		       <Grid >
					<Grid.ColumnDefinitions>
                    
                        <ColumnDefinition Width="*"/>
					
					</Grid.ColumnDefinitions>
                <Button  Grid.Column="0" x:Name="btnUserControl" Content="" Width="45" Height="28" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="7" FontWeight="Bold"/>
            </Grid>
      </Border>
	</Grid>
</UserControl>
