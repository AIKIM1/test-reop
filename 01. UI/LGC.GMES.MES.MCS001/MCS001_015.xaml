<UserControl x:Class="LGC.GMES.MES.MCS001.MCS001_015" 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	 	     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		     xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
		     xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary" 
             mc:Ignorable="d" d:DesignHeight="768"
		     d:DesignWidth="1024" Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded" 
             >

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="Foil 공급 현황 모니터링">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="8" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <!--백그라운드용-->
                <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />

                <!--조회영역-->
                <Grid Grid.Row="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <!--조회조건-->

                    <!--ROW1-->
                    <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                    <Grid x:Name="TopSearch" Grid.Row="0" Grid.ColumnSpan="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="8" />
                        </Grid.RowDefinitions>
                        <!-- 조회 버튼 영역-->
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="13" />
                        </Grid.ColumnDefinitions>

                        <Button Grid.Row="1" Grid.Column="2" x:Name="btnMaterialSupply" Content="{Binding ConverterParameter='FOIL 공급 요청 현황', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Cursor="Hand" Click="btnMaterialSupply_Click" />
                        <TextBlock Grid.Row="1"  Grid.Column="4" Text="{Binding ConverterParameter='자동조회', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" />
                        <c1:C1ComboBox Grid.Row="1"  Grid.Column="6" x:Name="cboAutoSearch" Margin="4,3,0,3" AutoComplete="False" DisplayMemberPath="NAME" SelectedValuePath="CODE" SelectedValueChanged="cboAutoSearch_SelectedValueChanged"  Width="Auto" MinWidth="40"/>
                        <Button Grid.Row="1" Grid.Column="8" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                    </Grid>
                </Grid>


                <Grid Grid.Row="2" Grid.Column="0" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="4*" />
                        <ColumnDefinition Width="8" />
                        <ColumnDefinition Width="6*" />
                    </Grid.ColumnDefinitions>
                    <GridSplitter Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                    <Grid x:Name="LeftContents" Grid.Row="0" Grid.Column="0" >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="34"/>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <Grid Grid.Row="0" x:Name="grdCatingTitle">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='Coating 생산계획', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                            </Border>
                            <Button x:Name="btnSupplyRequest" Grid.Column="2" Content="{Binding ConverterParameter='자재 공급 요청/요청 취소', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Cursor="Hand" Click="btnFoilSupplyRequest_Click" Margin="0,0,4,0"/>
                        </Grid>

                        <Grid Grid.Row="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="2*" />
                            </Grid.RowDefinitions>

                            <Border Grid.Row="0" Grid.Column="0" Background="#EFF1F3" BorderThickness="1" BorderBrush="#9F9F9F"></Border>
                            <Grid Grid.Row="0">
                                <c1:C1DataGrid x:Name="dgList" 
                                               Grid.Row="0"
                                               IsReadOnly="True" 
                                               HeadersVisibility="Column"
                                               SelectionMode="SingleRow"
                                               LoadedCellPresenter="dgList_LoadedCellPresenter" 
                                               UnloadedCellPresenter="dgList_UnloadedCellPresenter"
                                               >
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn Header="설비" Binding="{Binding EQPTNAME}" HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL"/>
                                        <c1:DataGridTextColumn Header="PJT" Binding="{Binding PRJT_NAME}" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn Header="PRODID" Binding="{Binding PRODID}" HorizontalAlignment="Center" />
                                        <c1:DataGridNumericColumn Header="계획수량" Binding="{Binding C_ROLL_QTY}" HorizontalAlignment="Right" Format="#,##0" />
                                        <c1:DataGridNumericColumn	Header = "생산수량"	Binding = "{Binding OUTQTY}" HorizontalAlignment="Right" Format="#,##0" />
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>

                            <Grid Grid.Row="2" x:Name="grdMaterialSupplyTitle">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='자재 공급 요청', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                </Border>
                                <!--<Button x:Name="btnFoilSupplyChoice" Grid.Row="0" Grid.Column="2" Margin="0,0,4,0"  Content="{Binding ConverterParameter='Foil 공급 선택', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Click="btnFoilSupplyChoice_Click" />-->
                            </Grid>

                            <Grid Grid.Row="3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="1.4*" />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="0.8*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="4" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="7" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid Grid.Row="0" Grid.Column="1" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="2.5*"/>
                                        <ColumnDefinition Width="7.5*"/>
                                    </Grid.ColumnDefinitions>
                                    <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                        <TextBlock  Text="{Binding ConverterParameter='조회기간',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1" >
                                        <ctrlLib:LGCDatePicker DatepickerType="Date" x:Name="dtpDateFrom"  />
                                        <TextBlock  Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0"/>
                                        <ctrlLib:LGCDatePicker DatepickerType="Date" x:Name="dtpDateTo" />
                                    </StackPanel>
                                </Grid>

                                <StackPanel Grid.Row="0" Grid.Column="3" Orientation="Horizontal" HorizontalAlignment="Left" >
                                    <RadioButton Margin="0,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=양극}" GroupName="RadioButton_Type" Name="rdoAnode" Style="{StaticResource SearchCondition_RadioButtonStyle}" IsChecked="True" />
                                    <RadioButton Margin="0,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=음극}" GroupName="RadioButton_Type" Name="rdoCathode" Style="{StaticResource SearchCondition_RadioButtonStyle}"  />
                                </StackPanel>

                                <Button Grid.Row="0" Grid.Column="4" x:Name="btnMaterialSupplyRequest" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnMaterialSupplyRequest_Click" />
                                <Button x:Name="btnFoilSupplyChoice" Grid.Row="0" Grid.Column="6" Margin="0,0,4,0"  Content="{Binding ConverterParameter='Foil 공급 선택', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Click="btnFoilSupplyChoice_Click" />
                            </Grid>

                            <Border Grid.Row="4" Grid.Column="0" Background="#EFF1F3" BorderThickness="1" BorderBrush="#9F9F9F"></Border>
                            <c1:C1DataGrid x:Name="dgMaterialSupply" 
                                           Grid.Row="4"
                                           IsReadOnly="True" 
                                           HeadersVisibility="Column"
                                           SelectionMode="SingleRow" 
                                           >
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridTemplateColumn  EditOnSelection="False" Name="CHK" Width="Auto" HorizontalAlignment="Center" CanUserFilter="False" CanUserSort="False" CanUserResize="False" IsReadOnly="True" >
                                        <c1:DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <RadioButton GroupName="dgMaterialSupplyChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" x:Name="dgaterialSupplyChoice" Width="Auto" HorizontalAlignment="Center" Checked="dgMaterialSupplyChoice_Checked" />
                                                <!---->
                                            </DataTemplate>
                                        </c1:DataGridTemplateColumn.CellTemplate>
                                    </c1:DataGridTemplateColumn>
                                    <c1:DataGridTextColumn Header="SPLY_TRGT_FLAG" Binding="{Binding SPLY_TRGT_FLAG}" HorizontalAlignment="Center" IsReadOnly="True" />
                                    <c1:DataGridTextColumn Header="순위" Binding="{Binding ROW_NUM}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="설비명" Binding="{Binding EQPTNAME}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="공급요청일시" Binding="{Binding MTRL_SPLY_REQ_DTTM}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="MTRLID" Binding="{Binding MTRLID}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="MTRLNAME" Binding="{Binding MTRLDESC}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="진행상태" Binding="{Binding REQ_STAT_NAME}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="Foil ID" Binding="{Binding MLOTID}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="작업지시상세" Binding="{Binding WO_DETL_ID}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="요청자" Binding="{Binding MTRL_SPLY_REQ_USERID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn Header="요청자" Binding="{Binding REQ_USERNAME}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="Port ID" Binding="{Binding PORT_ID}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="요청 ID" Binding="{Binding MTRL_SPLY_REQ_ID}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                    <c1:DataGridTextColumn Header="EQPTID" Binding="{Binding EQPTID}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                    <c1:DataGridTextColumn Header="MTRL_SPLY_REQ_STAT_CODE" Binding="{Binding MTRL_SPLY_REQ_STAT_CODE}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                    <c1:DataGridTextColumn Header="EQPTID" Binding="{Binding EQPTID}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                    
                                </c1:C1DataGrid.Columns>
                            </c1:C1DataGrid>
                        </Grid>
			        </Grid>

			        <Grid x:Name="RightContents" Grid.Row="0" Grid.Column="2" >
			            <Grid.RowDefinitions>
			                <RowDefinition Height="120"/>
			                <RowDefinition Height="34"/>
			                <RowDefinition Height="*"/>
			            </Grid.RowDefinitions>

                        <Border Grid.Row="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                        <Grid Grid.Row="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid x:Name="PortInfoTitle" Grid.Row="0">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='Port 정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    </Border>
                                </Grid>
                            </Grid>
                            
                            <Border Grid.Row="1" Grid.Column="0" Background="#EFF1F3" BorderThickness="1" BorderBrush="#9F9F9F"></Border>
                            <Grid  Grid.Row="1" x:Name="PortInfo">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <c1:C1DataGrid x:Name="dgPortInfo" 
                                               Grid.Row="0" Grid.Column="0" 
                                               IsReadOnly="True" 
                                               HeadersVisibility="Column" 
                                               FrozenColumnCount="5" 
                                               AlternatingRowBackground="White"  
                                               CanUserFilter="False" 
                                               CanUserSort="False" 
                                               SelectionMode="SingleRow" 
                                               >
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn		Header = "Port ID"		Binding = "{Binding PORT_ID}"	          HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "PORT명"	    Binding = "{Binding PORT_NAME}"	          HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "극성"	        Binding = "{Binding ELTR_TYPE_CODE}"	  HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "Carrier ID"	Binding = "{Binding MCS_CST_ID}"          HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "MTRLID"		Binding = "{Binding MTRLID}"	          HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "MTRLNAME"	    Binding = "{Binding MTRLDESC}"	          HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "Foil ID"	    Binding = "{Binding MLOTID}"              HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "REQ_EQPTNAME"	    Binding = "{Binding REQ_EQPTNAME}"        HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "MTRL_SPLY_REQ_DTTM"	    Binding = "{Binding MTRL_SPLY_REQ_DTTM}"  HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "REQ_EQPTID"	    Binding = "{Binding REQ_EQPTID}"          HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "MTRL_SPLY_REQ_ID"	    Binding = "{Binding MTRL_SPLY_REQ_ID}"    HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn		Header = "REVISION"	    Binding = "{Binding BOMREV}"	          HorizontalAlignment="Center" Visibility="Collapsed" />
                                        <c1:DataGridNumericColumn	Header = "수량"		    Binding = "{Binding MLOTQTY_CUR}"	      HorizontalAlignment="Center" Format="#,##0" Visibility="Collapsed"/>
                                        <c1:DataGridNumericColumn	Header = "입고수량"		Binding = "{Binding MLOTQTY_STOCKED}"	  HorizontalAlignment="Center" Format="#,##0" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn		Header = "자재 입고 일시"	Binding = "{Binding MLOTDTTM_STOCKED}"            HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn		Header = "PORT_TYPE_CODE"	Binding = "{Binding PORT_TYPE_CODE}"              HorizontalAlignment="Center" Visibility="Collapsed" />
                                        
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>
                        </Grid>

                        <Border Grid.Row="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}"></Border>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='Foil 공급 현황', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                            </Border>

                            <StackPanel Grid.Column="2" Orientation="Horizontal" Margin="0,0,4,0" >
                                <!--<Button x:Name="btnSupplyRequest" Content="{Binding ConverterParameter='자재 공급 요청/요청 취소', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Cursor="Hand" Click="btnFoilSupplyRequest_Click" />-->
                                                <Button x:Name="btnCarrierMoveOrder" Content="{Binding ConverterParameter='Carrier 이동 지시', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Cursor="Hand" Margin="4,0,0,0" Click="btnCarrierMoveOrder_Click" />
                                                </StackPanel>
                        </Grid>

                        <Border Grid.Row="2" Grid.Column="0" Background="White"  BorderThickness="1" BorderBrush="#9F9F9F"></Border>
                        <Grid x:Name="gdFoilStock" Grid.Row="2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="15"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="1.*"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="1.*"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="15"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="10"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="30" />
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="10"/>
                            </Grid.ColumnDefinitions>

                            <Grid Grid.Row="1" Grid.Column="1" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="60"/>
                                    <RowDefinition Height="60"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>

                                <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" >
                                    <Border Grid.Row="0" Grid.Column="0" Background="#FFF9F9F9" BorderBrush="#ffd8d8d8" BorderThickness="1" ></Border>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='양극 코터 2호기 투입', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}"  HorizontalAlignment="Center" FontWeight="Bold" />
                                </Grid>
                                <Border Grid.Row="0" Grid.Column="2" BorderThickness="0,1,1,1" BorderBrush="#ffd8d8d8" ></Border>
                                <Grid Grid.Row="0" Grid.Column="2" x:Name="gdEBE001">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"></RowDefinition>
                                        <RowDefinition Height="*"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Grid.Row="0" Grid.Column="0" x:Name="checkEBE001" Content="{Binding '', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}"  Margin="4,0,0,0" Checked="checkFoilStock_Checked" Unchecked="checkFoilStock_Unchecked"/>
                                    <TextBlock x:Name="txtCSTEBE001" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="4,0,0,0"/>
                                    <TextBlock x:Name="txtMLOTEBE001" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="4,0,0,0" />

                                    <Grid Grid.Row="1" Grid.Column="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"></RowDefinition>
                                            <RowDefinition Height="*"></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBlock x:Name="txtInoutTypeEBE001" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBlock x:Name="txtPortStateCodeEBE001" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    </Grid>

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" >
                                    <Border Grid.Row="0" Grid.Column="0" Background="#FFF9F9F9" BorderBrush="#ffd8d8d8" BorderThickness="1" ></Border>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='양극 코터 1호기 투입', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}"  HorizontalAlignment="Center" FontWeight="Bold" />
                                </Grid>
                                <Border Grid.Row="1" Grid.Column="2" BorderThickness="0,1,1,1" BorderBrush="#ffd8d8d8" ></Border>
                                <Grid Grid.Row="1" Grid.Column="2" x:Name="gdEBE004">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"></RowDefinition>
                                        <RowDefinition Height="*"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Grid.Row="0" Grid.Column="0" x:Name="checkEBE004" Content="{Binding '', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}"  Margin="4,0,0,0" HorizontalAlignment="Center" Checked="checkFoilStock_Checked" Unchecked="checkFoilStock_Unchecked"/>
                                    <TextBlock Grid.Row="0" x:Name="txtCSTEBE004" Grid.Column="1" HorizontalAlignment="Left" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="4,0,0,0"/>
                                    <TextBlock Grid.Row="1" x:Name="txtMLOTEBE004" Grid.Column="1" HorizontalAlignment="Left" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="4,0,0,0"/>

                                    <Grid Grid.Row="1" Grid.Column="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"></RowDefinition>
                                            <RowDefinition Height="*"></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBlock x:Name="txtInoutTypeEBE004" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBlock x:Name="txtPortStateCodeEBE004" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    </Grid>

                                </Grid>
                            </Grid>

                            <Grid Grid.Row="3" Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1.0*"></RowDefinition>
                                    <RowDefinition Height="1.0*"></RowDefinition>
                                    <RowDefinition Height="0.4*"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"></ColumnDefinition>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
                                    <ColumnDefinition Width="1.*"></ColumnDefinition>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
                                    <ColumnDefinition Width="1.*"></ColumnDefinition>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
