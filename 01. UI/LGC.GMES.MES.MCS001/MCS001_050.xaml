<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             x:Class="LGC.GMES.MES.MCS001.MCS001_050"
             mc:Ignorable="d" 
             d:DesignHeight="850" d:DesignWidth="1920" 
             Loaded="UserControl_Loaded" 
             Unloaded="UserControl_Unloaded" 
             >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="활성화 창고 적재 현황" >
            <ctrlLib:LGCLayout.Content>
                <Grid >
                    <Grid.RowDefinitions>
                        <!--TopSearch-->
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8"/>
                        <!--Contents-->
                        <RowDefinition Height="*"/>
                        <!--<RowDefinition Height="8"/>-->
                    </Grid.RowDefinitions>

                    <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                    <Grid x:Name="TopSearch" Grid.Row="0" Grid.ColumnSpan="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="8" />
                        </Grid.RowDefinitions>
                        <!-- 조회 버튼 영역-->
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="13"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="13" />
                        </Grid.ColumnDefinitions>

                        <Grid Grid.Row="1" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock      Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            <c1:C1ComboBox  Grid.Column="1"  x:Name="cboArea" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboArea_SelectedValueChanged" />
                        </Grid>

                        <Grid Grid.Row="1" Grid.Column="3">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock  Text="{Binding ConverterParameter='창고', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            </StackPanel>
                            <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboStocker"  DisplayMemberPath="EQPTNAME" SelectedValuePath="EQPTID" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStocker_SelectedValueChanged" />
                        </Grid>

                        <Button Grid.Row="1" Grid.Column="10" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                    </Grid>

                    <!--Contents-->
                    <Grid x:Name="Contents" Grid.Row="2">
                        <Grid Grid.Row="0"  x:Name="ContentsRow">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2.4*"/>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="7.6*" />
                            </Grid.ColumnDefinitions>

                            <!--Left Grid Area-->
                            <Grid Grid.Row="0" x:Name="LeftAreaLami">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <Grid Grid.Row="0" x:Name="grdCapacitySummaryTitle">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='창고 적재 현황', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    </Border>
                                </Grid>

                                <Border Grid.Row="1" Background="#e8ebed" BorderThickness="0" BorderBrush="#8a8a8a"></Border>
                                <Grid Grid.Row="1" >
                                    <c1:C1DataGrid x:Name="dgCapacitySummary" 
                                                   Grid.Row="0" 
                                                   Grid.Column="0" 
                                                   IsReadOnly="True" 
                                                   HeadersVisibility="None"
                                                   FrozenTopRowsCount="2"
                                                   CanUserFilter="False" 
                                                   CanUserSort="False" 
                                                   SelectionMode="SingleCell" 
                                                   AlternatingRowBackground="White"  
                                                   Margin="8"
                                                   LoadedCellPresenter="dgCapacitySummary_LoadedCellPresenter" 
                                                   UnloadedCellPresenter="dgCapacitySummary_UnloadedCellPresenter" 
                                                   MouseLeftButtonUp="dgCapacitySummary_MouseLeftButtonUp" 
                                                   MergingCells="dgCapacitySummary_MergingCells" 
                                                   >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn    Header="[창고, 창고]"                   Binding="{Binding EQPTNAME}"           HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn Header="[용량, 용량]"                   Binding="{Binding RACK_MAX_QTY}"       HorizontalAlignment="Right" Format="#,##0" />
                                            <c1:DataGridNumericColumn Header="[적재율(%), 적재율(%)]"         Binding="{Binding RACK_RATE}"          HorizontalAlignment="Right" Format="###,###,##0.##" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn    Header="[적재율(%), 적재율(%)]"         Binding="{Binding RACK_RATE3}"         HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn    Header="[프로젝트명, 프로젝트명]"       Binding="{Binding PRJT_NAME}"          HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn Header="[출하가능, 출하가능]"           Binding="{Binding SHIP_PLT_QTY}"       HorizontalAlignment="Right"  Format="#,##0" />
                                            <c1:DataGridNumericColumn Header="[출하불가능, HOLD]"             Binding="{Binding HOLD_PLT_QTY}"       HorizontalAlignment="Right"  Format="#,##0"  />
                                            <c1:DataGridNumericColumn Header="[출하불가능, 검사대기]"         Binding="{Binding INSP_WAIT_PLT_QTY}"  HorizontalAlignment="Right"  Format="#,##0" />
                                            <c1:DataGridNumericColumn Header="[출하불가능, 불합격]"           Binding="{Binding NG_PLT_QTY}"         HorizontalAlignment="Right"  Format="#,##0" />
                                            <c1:DataGridNumericColumn Header="[출하불가능, 장기재고]"         Binding="{Binding VLD_DATE_PLT_QTY}"   HorizontalAlignment="Right"  Format="#,##0" />
                                            <c1:DataGridNumericColumn Header="[공Pallet, 공Pallet]"           Binding="{Binding EMPTY_QTY}"          HorizontalAlignment="Right" Format="#,##0" />
                                            <c1:DataGridNumericColumn Header="[비정상Pallet, 비정상Pallet]"   Binding="{Binding ABNORM_QTY}"         HorizontalAlignment="Right" Format="#,##0" />
                                            <c1:DataGridNumericColumn Header="[오류Pallet, 오류Pallet]"       Binding="{Binding ERROR_QTY}"          HorizontalAlignment="Right" Format="#,##0" />
                                            
                                            <c1:DataGridTextColumn    Header="[창고, 창고]"                   Binding="{Binding EQPTID}"             HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn    Header="[PRODID, PRODID]"               Binding="{Binding PRODID}"             HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                        </c1:C1DataGrid.Columns>
                                        <c1:C1DataGrid.TopRows>
                                            <c1:DataGridColumnHeaderRow />
                                            <c1:DataGridColumnHeaderRow />
                                        </c1:C1DataGrid.TopRows>
                                    </c1:C1DataGrid>
                                </Grid>

                            </Grid>

                            <!--Splitter-->
                            <GridSplitter x:Name="Splitter" Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"  DragStarted="Splitter_DragStarted" DragCompleted="Splitter_DragCompleted"/>
                            <!--<Border Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"></Border>-->
                            <Grid x:Name="Right" Grid.Column="2" >
                                <c1:C1TabControl x:Name="C1TabControl" >
                                    <c1:C1TabItem x:Name="tabProduct" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='실Pallet'}" >
                                        <Grid >
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="7" />
                                                <RowDefinition Height="18" />
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>

                                            <Border Grid.Row="0" Grid.RowSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}"></Border>
                                            <Grid Grid.Row="1">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="10" />

                                                    <ColumnDefinition Width="20" />
                                                    <ColumnDefinition Width="3.5" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="8" />
                                                    <ColumnDefinition Width="20" />
                                                    <ColumnDefinition Width="3.5" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="8" />
                                                    <ColumnDefinition Width="20" />
                                                    <ColumnDefinition Width="3.5" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="8" />
                                                </Grid.ColumnDefinitions>
                                                <TextBlock  Grid.Column="1" Text="{Binding ConverterParameter='경과일수', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" FontSize="12" Margin="0,0,0,0" />

                                                <Border    Grid.Row="0" Grid.Column="3" Background="#F2CB61" BorderThickness="0.5" Style="{StaticResource BorderSubTitleStyle}" />
                                                <TextBlock Grid.Row="0" Grid.Column="5" Text="{Binding ConverterParameter='D+7이상', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                                <Border    Grid.Row="0" Grid.Column="7" Background="Orange" BorderThickness="0.5" Style="{StaticResource BorderSubTitleStyle}" />
                                                <TextBlock Grid.Row="0" Grid.Column="9" Text="{Binding ConverterParameter='D+15이상', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                                <Border    Grid.Row="0" Grid.Column="11" Background="Red" BorderThickness="0.5" Style="{StaticResource BorderSubTitleStyle}" />
                                                <TextBlock Grid.Row="0" Grid.Column="13" Text="{Binding ConverterParameter='D+30이상', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                            </Grid>

                                            <c1:C1DataGrid x:Name="dgRealPallet" 
                                                           Grid.Row="2"
                                                           IsReadOnly="True" 
                                                           HeadersVisibility="None" 
                                                           FrozenTopRowsCount="2"
                                                           FrozenColumnCount="4" 
                                                           SelectionMode="SingleRow" 
                                                           Margin="8"
                                                           MouseDoubleClick="dgRealPallet_MouseDoubleClick" 
                                                           LoadedCellPresenter="dgRealPallet_LoadedCellPresenter" 
                                                           UnloadedCellPresenter="dgRealPallet_UnloadedCellPresenter" 
                                                           PreviewMouseLeftButtonDown="dgRealPallet_PreviewMouseLeftButtonDown"
                                                           >
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn	    Header="[NO, NO]"		                   Binding = "{Binding SEQ}"		                HorizontalAlignment="Right" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[창고, 창고]"		               Binding = "{Binding EQPTID}"	                    HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header="[창고, 창고]"		               Binding = "{Binding EQPTNAME}"	                HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[Rack, Rack]"		               Binding = "{Binding RACK_ID}"	                HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header="[Rack, Rack]"		               Binding = "{Binding RACK_NAME}"	                HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[Carrier ID, Carrier ID]"		   Binding = "{Binding CSTID}"	                    HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn      Header="[입고일시, 입고일시]"              Binding = "{Binding CSTINDTTM}"                  HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[경과일수, 경과일수]"	           Binding = "{Binding PAST_DAY}"	                HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[Pallet ID, Pallet ID]"	           Binding = "{Binding BOXID}"	                    HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[LINE, LINE]"	                   Binding = "{Binding EQSGNAME}"	                HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridNumericColumn	Header="[수량, 수량]"		               Binding = "{Binding TOTAL_QTY}"	                HorizontalAlignment="Center" Format="#,##0" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[프로젝트, 프로젝트]"	           Binding = "{Binding PRJT_NAME}"	                HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[모델LOT, 모델LOT]"	               Binding = "{Binding MDLLOT_ID}"	                HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[제품, 제품]"		               Binding = "{Binding PRODID}"	                    HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[출하가능여부, 출하가능여부]"	   Binding = "{Binding SHIPPING_YN}"	            HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[HOLD, MES]"		               Binding = "{Binding MES_HOLD_YN}"	            HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[HOLD, QMS]"		               Binding = "{Binding QMS_HOLD_YN}"	            HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[HOLD, CELL]"		               Binding = "{Binding SUBLOT_HOLD_YN}"	            HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[HOLD, PALLET]"		               Binding = "{Binding PACK_HOLD_YN}"	            HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[QA검사, 성능검사]"		           Binding = "{Binding PROD_INSP_RESULT}"	        HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[QA검사, 치수검사]"		           Binding = "{Binding MEASR_INSP_RESULT}"	        HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[QA검사, 한계불량율 (저전압)]"     Binding = "{Binding LOW_VOLT_INSP_RESULT}"	    HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[QA검사, 출하검사]"		           Binding = "{Binding OQC_INSP_YN}"	            HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[장기재고, 장기재고]"		       Binding = "{Binding OLD_YN}"	                    HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header="[CSTSTAT, CSTSTAT]"		           Binding = "{Binding CSTSTAT}"	                HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header="[CSTPROD, CSTPROD]"		           Binding = "{Binding CSTPROD}"	                HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header="[CARRIERID, CARRIERID]"	           Binding = "{Binding CARRIERID}"	                HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header="[ORDID, ORDID]"	                   Binding = "{Binding ORDID}"	                    HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                </c1:C1DataGrid.Columns>
                                                <c1:C1DataGrid.TopRows>
                                                    <c1:DataGridColumnHeaderRow />
                                                    <c1:DataGridColumnHeaderRow />
                                                </c1:C1DataGrid.TopRows>
                                            </c1:C1DataGrid>
                                        </Grid>
                                    </c1:C1TabItem>

                                    <c1:C1TabItem x:Name="tabEmptyPallet" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='공Pallet'}" >
                                        <Grid >
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="0.3*"/>
                                                <ColumnDefinition Width="8"/>
                                                <ColumnDefinition Width="0.7*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Row="0" Grid.RowSpan="2" Grid.Column="0" Style="{StaticResource Content_InputForm_ContentBorderStyle}"></Border>
                                            <c1:C1DataGrid x:Name="dgEmptyPallet" 
                                                           HeadersVisibility="None" 
                                                           FrozenTopRowsCount="2"
                                                           IsReadOnly="True"
                                                           SelectionMode="SingleCell" 
                                                           Margin="8"
                                                           Grid.Column="0">
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn      Header="[Tray Type, Tray Type]"       Binding="{Binding CSTPROD}"                     HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn      Header="[Tray Type, Tray Type]"       Binding="{Binding CSTPROD_NAME}"                HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                                    <c1:DataGridTextColumn      Header="[공Pallet, 공Pallet]"         Binding="{Binding E_PLT_CNT}"                   HorizontalAlignment="Right" Format="#,##0" />
                                                    <c1:DataGridTextColumn      Header="[공Tray Pallet, 세정전]"      Binding="{Binding T_PLT_CLEAN_BEFORE_CNT}"      HorizontalAlignment="Right" Format="#,##0" />
                                                    <c1:DataGridNumericColumn   Header="[공Tray Pallet, 세정후]"      Binding="{Binding T_PLT_CLEAN_AFTER_CNT}"       HorizontalAlignment="Right" Format="#,##0" />
                                                </c1:C1DataGrid.Columns>
                                                <c1:C1DataGrid.TopRows>
                                                    <c1:DataGridColumnHeaderRow />
                                                    <c1:DataGridColumnHeaderRow />
                                                </c1:C1DataGrid.TopRows>
                                            </c1:C1DataGrid>
                                            <GridSplitter x:Name="SplitterEmpty" Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
                                            <Border Grid.Row="0" Grid.RowSpan="2" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"></Border>
                                            <c1:C1DataGrid x:Name="dgEmptyPalletList" 
                                                           HeadersVisibility="Column" 
                                                           IsReadOnly="True"
                                                           SelectionMode="SingleCell" 
                                                           Margin="8"
                                                           Grid.Column="2">
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn	    Header = "NO"		      Binding = "{Binding SEQ}"		                      HorizontalAlignment="Right" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn      Header = "창고"           Binding = "{Binding EQPTID}"                        HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn      Header = "창고"           Binding = "{Binding EQPT_NAME}"                     HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn      Header = "Rack"           Binding = "{Binding RACK_ID}"                       HorizontalAlignment="Center" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn      Header = "Rack"           Binding = "{Binding RACK_NAME}"                     HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header = "Carrier ID"	  Binding = "{Binding CSTID}"	                      HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header = "Tray Type"	  Binding = "{Binding CSTPROD_NAME}"	              HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header = "공Tray 유무"	  Binding = "{Binding CSTSTAT_YN}"	                  HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header = "세정여부"	      Binding = "{Binding CST_CLEAN_FLAG}"	              HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn      Header = "입고일시"       Binding = "{Binding CSTINDTTM}"                     HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn      Header = "비고"           Binding = "{Binding NOTE}"                          HorizontalAlignment="Left"   IsReadOnly="True" />
                                                    <c1:DataGridTextColumn		Header = "CSTSTAT"		  Binding = "{Binding CSTSTAT}"	                      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header = "CSTPROD"		  Binding = "{Binding CSTPROD}"	                      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header = "CARRIERID"	  Binding = "{Binding CARRIERID}"	                  HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header = "ORDID"	      Binding = "{Binding ORDID}"	                      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header = "출고유형"	      Binding = "{Binding STK_ISS_TYPE}"	              HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header = "출고유형"	      Binding = "{Binding STK_ISS_TYPE_NAME}"	          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>

                                        </Grid>
                                    </c1:C1TabItem>

                                    <c1:C1TabItem x:Name="tabAbNormalPallet" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='비정상Pallet'}" >
                                        <Grid >
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Border Grid.Row="0" Grid.RowSpan="2" Grid.Column="0" Style="{StaticResource Content_InputForm_ContentBorderStyle}"></Border>
                                            <c1:C1DataGrid x:Name="dgAbNormalPallet" 
                                                           IsReadOnly="True" 
                                                           HeadersVisibility="Column" 
                                                           FrozenColumnCount="4" 
                                                           SelectionMode="SingleRow" 
                                                           Margin="8">
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn	    Header = "NO"		      Binding = "{Binding SEQ}"		            HorizontalAlignment="Right" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn      Header = "창고"           Binding = "{Binding EQPTID}"              HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn      Header = "창고"           Binding = "{Binding EQPT_NAME}"           HorizontalAlignment="Left" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn      Header = "Rack ID"        Binding = "{Binding RACK_ID}"             HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn      Header = "Rack"           Binding = "{Binding RACK_NAME}"           HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn      Header = "입고일시"       Binding = "{Binding UPDDTTM}"             HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn      Header = "Carrier ID"     Binding = "{Binding CSTID}"               HorizontalAlignment="Center" IsReadOnly="True" />
                                                    <c1:DataGridTextColumn      Header = "Carrier상태"    Binding = "{Binding CSTSTAT_NAME}"        HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn      Header = "Tray Type"      Binding = "{Binding CSTPROD_NAME}"        HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn      Header = "정보불일치유형" Binding = "{Binding ABNORM_TRF_RSN_CODE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
