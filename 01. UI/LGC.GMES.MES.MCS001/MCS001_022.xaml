<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             x:Class="LGC.GMES.MES.MCS001.MCS001_022"
             mc:Ignorable="d" 
             d:DesignHeight="850" d:DesignWidth="1920" 
             Loaded="UserControl_Loaded" 
             Unloaded="UserControl_Unloaded" 
             >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="노칭대기창고모니터링" >
            <ctrlLib:LGCLayout.Content>
                <Grid >
                    <Grid.RowDefinitions>
                        <!--TopSearch-->
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8"/>
                        <!--Contents-->
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="4"/>
                        <!--Bottom-->
                        <!--<RowDefinition Height="42"/>-->
                    </Grid.RowDefinitions>

                    <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                    <Grid x:Name="TopSearch" Grid.Row="0" Grid.ColumnSpan="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="8" />
                        </Grid.RowDefinitions>
                        <!-- 조회 버튼 영역-->
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>

                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>

                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>

                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>                            
                            <ColumnDefinition Width="4"/>
                        </Grid.ColumnDefinitions>

                        <TextBlock Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox Grid.Row="1" Grid.Column="2" x:Name="txtLotId" CharacterCasing="Upper" Style="{StaticResource Content_InputForm_TextBoxStyle}" MinWidth="120"/>

                        <Button Grid.Row="1" Grid.Column="4" x:Name="btnSelect" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSelect_Click" />

                        <Button Grid.Row="1" Grid.Column="6" x:Name="btnPancakeWarehousingHistory" Content="{Binding ConverterParameter='노칭대기창고입출고이력조회',    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Cursor="Hand" Click="btnPancakeWarehousingHistory_Click" />
                    
                        <Button x:Name="btnSKIDSearch" Grid.Row="1" Grid.Column="17"  Content="{Binding ConverterParameter='SKID 현황조회', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnSKIDSearch_Click" />
                        <Button x:Name="btnInputLotSearch" Grid.Row="1" Grid.Column="19"  Content="{Binding ConverterParameter='입고LOT조회', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnInputLotSearch_Click"  />
                        <Button x:Name="btnSkidInfo" Grid.Row="1" Grid.Column="21"  Content="{Binding ConverterParameter='SKID조회', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnSkidInfo_Click" Visibility="Collapsed" />
                        <TextBlock Grid.Row="1"  Grid.Column="23" Text="{Binding ConverterParameter='자동조회', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" />
                        <c1:C1ComboBox Grid.Row="1"  Grid.Column="25" x:Name="cboAutoSearch" Margin="4,3,0,3" AutoComplete="False" DisplayMemberPath="NAME" SelectedValuePath="CODE" SelectedValueChanged="cboAutoSearch_SelectedValueChanged"  Width="Auto" MinWidth="40"/>
                        <Button Grid.Row="1" Grid.Column="27" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" PreviewMouseDown="btnSearch_PreviewMouseDown"  />
                    </Grid>
                    <!--Contents-->
                    <Grid x:Name="Contents" Grid.Row="2">
                        <Grid Grid.Row="0"  x:Name="ContentsRow">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2.4*"/>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="7.6*" />
                            </Grid.ColumnDefinitions>
                            <!--Left Grid Area-->
                            <Grid Grid.Row="0" x:Name="Left">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="32" />
                                    <RowDefinition Height="142" />
                                    <RowDefinition Height="4" />
                                    <!-- SPLITTER-->
                                    <RowDefinition Height="32" />
                                    <RowDefinition Height="225"/>
                                    <RowDefinition Height="4" />
                                    <!-- SPLITTER-->
                                    <RowDefinition Height="32" />
                                    <RowDefinition Height="1*" />
                                    <RowDefinition Height="5" />
                                </Grid.RowDefinitions>

                                <Grid Grid.Row="0" x:Name="grdStoreTitle">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter=창고현황, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    </Border>
                                    <!--<c1:C1ComboBox Grid.Row="0" Grid.Column="1" x:Name="cboProdId1"  AutoComplete="False" DisplayMemberPath="CBO_NAME" 
                                               SelectedValuePath="CBO_CODE" MinWidth="120" SelectedIndexChanged="OnCboProdId1SelectedIndexChanged"/>-->
                                </Grid>

                                <Grid Grid.Row="1" x:Name="grdStore">
                                    <c1:C1DataGrid x:Name="dgStore" 
                                                   Grid.Row="0" 
                                                   Grid.Column="0" 
                                                   IsReadOnly="True" 
                                                   HeadersVisibility="Column" 
                                                   FrozenColumnCount="1" 
                                                   AlternatingRowBackground="White"  
                                                   CanUserFilter="False" 
                                                   CanUserSort="False" 
                                                   SelectionMode="SingleCell" >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn Header="구분"      Binding="{Binding EQPTID}"      HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="구분"      Binding="{Binding EQPTNAME}"    HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn Header="용량"      Binding="{Binding RACK_MAX}"    HorizontalAlignment="Right" Format="#,##0" />
                                            <c1:DataGridNumericColumn Header="SKID 수"    Binding="{Binding RACK_CNT}"    HorizontalAlignment="Right" Format="#,##0" />
                                            <c1:DataGridNumericColumn Header="적재율(%)"    Binding="{Binding RACK_RATE}"  HorizontalAlignment="Right" Format="###,###,##0.##" />
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                </Grid>

                                <GridSplitter Grid.Row="2" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                <Grid Grid.Row="3" x:Name="grdProcessStockTitle">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='창고재고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    </Border>
                                    <!--<c1:C1ComboBox Grid.Row="0" Grid.Column="1" x:Name="cboProdId2"  AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" MinWidth="120" SelectedIndexChanged="OnCboProdId2SelectedIndexChanged"/>-->
                                    <CheckBox x:Name="chkHoldException" Grid.Row="0" Grid.Column="2" Content="{Binding ConverterParameter='HOLD 제외', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" Checked="chkHoldException_Checked" Unchecked="chkHoldException_Unchecked" Visibility="Collapsed"/>
                                </Grid>

                                <Grid Grid.Row="4" x:Name="grdProcessStock" >
                                    <c1:C1DataGrid x:Name="dgProjectStock"  
                                                   Grid.Row="0" 
                                                   Grid.Column="0"  
                                                   IsReadOnly="True" 
                                                   HeadersVisibility="None"
                                                   FrozenTopRowsCount="2"
                                                   AlternatingRowBackground="White" 
                                                   CanUserFilter="False" 
                                                   CanUserSort="False" 
                                                   SelectionMode="SingleRow"
                                                   MouseDoubleClick="dgProjectStock_MouseDoubleClick" 
                                                   LoadedCellPresenter="dgProjectStock_LoadedCellPresenter">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn Header = "[프로젝트명, 프로젝트명,프로젝트명]" Binding = "{Binding PRJT_NAME}"  HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn Header = "[양극, LOT 수, 가용]"        Binding = "{Binding C_LOT_CNT}"       HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[양극, LOT 수, HOLD]"        Binding = "{Binding C_HOLD_LOT_CNT}"  HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[양극, SKID 수, 가용]"       Binding = "{Binding C_SKID_CNT}"      HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[양극, SKID 수, HOLD]"       Binding = "{Binding C_HOLD_SKID_CNT}" HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[양극, 수량(m), 가용]"       Binding = "{Binding C_WIP_QTY}"       HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[양극, 수량(m), HOLD]"       Binding = "{Binding C_HOLD_WIP_QTY}"  HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[음극, LOT 수, 가용]"        Binding = "{Binding A_LOT_CNT}"       HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[음극, LOT 수, HOLD]"        Binding = "{Binding A_HOLD_LOT_CNT}"  HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[음극, SKID 수, 가용]"       Binding = "{Binding A_SKID_CNT}"      HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[음극, SKID 수, HOLD]"       Binding = "{Binding A_HOLD_SKID_CNT}" HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[음극, 수량(m), 가용]"       Binding = "{Binding A_WIP_QTY}"       HorizontalAlignment="Right"  Format="#,##0"/>
                                            <c1:DataGridNumericColumn Header = "[음극, 수량(m), HOLD]"       Binding = "{Binding A_HOLD_WIP_QTY}"  HorizontalAlignment="Right"  Format="#,##0"/>
                                        </c1:C1DataGrid.Columns>
                                        <c1:C1DataGrid.TopRows>
                                            <c1:DataGridColumnHeaderRow />
                                            <c1:DataGridColumnHeaderRow />
                                            <c1:DataGridColumnHeaderRow />
                                        </c1:C1DataGrid.TopRows>
                                    </c1:C1DataGrid>
                                </Grid>

                                <GridSplitter Grid.Row="5" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                <Grid Grid.Row="6" x:Name="grdWHOrderTitle">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter=출고순위, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    </Border>
                                    <!--<c1:C1ComboBox Grid.Row="0" Grid.Column="2" x:Name="cboPriority" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" MinWidth="100"  />-->
                                </Grid>

                                <Border Grid.Row="7" Background="#e8ebed" BorderThickness="0" BorderBrush="#8a8a8a"></Border>
                                <Grid Grid.Row="7" x:Name="grdInputRank">
                                    <c1:C1DataGrid x:Name="dgInputRank" Grid.Row="0" Grid.Column="0" 
                                                   IsReadOnly="False" HeadersVisibility="Column" 
                                                   FrozenColumnCount="3" 
                                                   AlternatingRowBackground="White"  
                                                   CanUserFilter="False" 
                                                   CanUserSort="False" 
                                                   SelectionMode="SingleCell" 
                                                   CommittedEdit="dgInputRank_CommittedEdit"
                                                   >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTemplateColumn x:Name="RANKCHK" Header = "선택" Width="50" HorizontalAlignment="Center">
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <CheckBox IsChecked="{Binding CHK, Mode=OneWay}" x:Name="rankCheck" HorizontalAlignment="Center" Checked="rankCheck_Checked" Unchecked="rankCheck_UnChecked"/>
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>
                                            <c1:DataGridTextColumn	Header = "순위"		 Binding = "{Binding SEQ}"    	        HorizontalAlignment="Right" IsReadOnly="True" />
                                            <c1:DataGridTextColumn	Header = "프로젝트"  Binding = "{Binding PRJT_NAME}"		HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn	Header = "극성"      Binding = "{Binding ELTR_TYPE_CODE}"	HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn	Header = "경과일수"	 Binding = "{Binding ELAPSE}"		    HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn	Header = "입고일시"	 Binding = "{Binding WH_RCV_DTTM}"		HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn	Header = "위치"	     Binding = "{Binding PSTN}"	            HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn	Header = "SKID ID"	 Binding = "{Binding MCS_CST_ID}"	    HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn	Header = "창고입고일시"	Binding = "{Binding WIPDTTM_ED}"	HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn	Header = "RACK"	     Binding = "{Binding RACK_ID}"	        HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                </Grid>
                                <Border Grid.Row="8" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                            </Grid>
                            <!--Splitter-->
                            <GridSplitter Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"  />
                            <Grid x:Name="HMIAera" Grid.Column="2" >
                                <Grid x:Name="RightArea">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="142"/>
                                        <RowDefinition Height="36"/>
                                        <RowDefinition Height="*"/>
                                        <!--<RowDefinition Height="32"/>-->
                                        <RowDefinition Height="8"/>
                                        <RowDefinition Height="137"/>
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}"></Border>
                                    <Grid Grid.Row="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="34"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid x:Name="PancakeInfoTitle" Grid.Row="0">
                                            <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="*"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                    <ToggleButton   Grid.Row="0"  Grid.Column="0" Margin="10,0,5,0" x:Name="btnExpandFrame" Style="{StaticResource btnLeftTopConerStyleHorizontal}"  IsChecked="true" Checked="btnExpandFrame_Checked" Unchecked="btnExpandFrame_Unchecked"/>
                                                    <TextBlock Grid.Row="0" Grid.Column="1" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter=출고대상 정보, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                                    <StackPanel Grid.Column="3" Orientation="Horizontal" Margin="0,0,4,0" >
                                                        <Button x:Name="btnManualIssue" Content="{Binding ConverterParameter=수동출고예약, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnManualIssue_Click"  />
                                                    </StackPanel>
                                                </Grid>
                                            </Border>
                                        </Grid>
                                        <Border Grid.Row="1" Background="#e8ebed" BorderThickness="0" BorderBrush="#8a8a8a"></Border>
                                        <Grid  Grid.Row="1" x:Name="PancakeInfo">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1268*"/>
                                                <ColumnDefinition Width="185*"/>
                                            </Grid.ColumnDefinitions>
                                            <c1:C1DataGrid x:Name="dgRackInfo" 
                                                           Grid.Row="0" Grid.Column="0" 
                                                           IsReadOnly="True" 
                                                           HeadersVisibility="Column" 
                                                           FrozenColumnCount="2" 
                                                           AlternatingRowBackground="White"  
                                                           CanUserFilter="False" 
                                                           CanUserSort="False" 
                                                           SelectionMode="SingleRow"
                                                           Grid.ColumnSpan="2"
                                                           HorizontalScrollBarVisibility="Hidden"
                                                           >
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn		Header = "RACK"		    Binding = "{Binding RACK_ID}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "SKID ID"	    Binding = "{Binding MCS_CST_ID}"  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "PRJT_NAME"	Binding = "{Binding PRJT_NAME}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "제품"		    Binding = "{Binding PRODID}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "입고일시"	    Binding = "{Binding WH_RCV_DTTM}" HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "LOT 수"	    Binding = "{Binding LOT_CNT}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridNumericColumn	Header = "수량"		    Binding = "{Binding WIPQTY}"	  HorizontalAlignment="Right" Format="#,##0" />
                                                    <c1:DataGridTextColumn		Header = "유효일자"		Binding = "{Binding VLD_DATE}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "경과일수"	    Binding = "{Binding PAST_DAY}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "HOLD여부"	    Binding = "{Binding WIPHOLD}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "PRODNAME"	    Binding = "{Binding PRODNAME}"	  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn		Header = "LOT ID"	    Binding = "{Binding LOTID}"		  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn		Header = "WO"		    Binding = "{Binding WOID}"		  HorizontalAlignment="Center" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header = "모델"		    Binding = "{Binding MODLID}"	  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn		Header = "RACK_STAT_CODE"	Binding = "{Binding RACK_STAT_CODE}"  HorizontalAlignment="Center" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn	    Header = "순위"		    Binding = "{Binding SEQ}"	HorizontalAlignment="Center" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn	    Header = "POSITION"	    Binding = "{Binding POSITION}"	HorizontalAlignment="Center" Visibility="Collapsed" />
                                                    
                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>
                                        </Grid>
                                    </Grid>

                                    <Border Grid.Row="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}"></Border>
                                    <Grid x:Name="StairSector" Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="13"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <Grid Grid.Column="1" >
                                        </Grid>

                                        <Grid Grid.Row="0" Grid.Column="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="7*"/>
                                            </Grid.ColumnDefinitions>

                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock Text="{Binding ConverterParameter='Stocker', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                            </StackPanel>
                                            <c1:C1ComboBox  Grid.Column="1" x:Name="cboStocker" DisplayMemberPath="EQPTNAME" SelectedValuePath="EQPTID"   Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" MinWidth="160" />
                                        </Grid>

                                        <Grid Grid.Row="0" Grid.Column="5">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="7*"/>
                                            </Grid.ColumnDefinitions>

                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock Text="{Binding ConverterParameter='프로젝트명', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                            </StackPanel>
                                            <!--<c1:C1ComboBox  Grid.Column="1" x:Name="cboStair" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"   MinWidth="160" SelectedValueChanged="cboStair_SelectedValueChanged"/>-->
                                            <TextBox    Grid.Row="0" Grid.Column="1" Style="{StaticResource SearchCondition_TextBoxStyle}" x:Name="txtProjectName" CharacterCasing="Upper" MinWidth="120" />
                                        </Grid>


                                        <Grid Grid.Row="0" Grid.Column="7">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="7*"/>
                                            </Grid.ColumnDefinitions>

                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock Text="{Binding ConverterParameter='PRDT_VER', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                            </StackPanel>
                                            <TextBox    Grid.Row="0" Grid.Column="1" Style="{StaticResource SearchCondition_TextBoxStyle}" x:Name="txtProductVersion" CharacterCasing="Upper" MinWidth="80" />
                                        </Grid>


