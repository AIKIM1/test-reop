<UserControl x:Class="LGC.GMES.MES.ASSY001.ASSY001_013"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">

        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="4"/>
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>

        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="PALLET 라벨 발행">
            <Grid Grid.Row="0">
                <Grid x:Name="_newMainGrid">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="8" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="8" />
                        <RowDefinition />
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="330" />
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="auto" />
                    </Grid.RowDefinitions>

                    <Grid x:Name="SeaArea" Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="50"></RowDefinition>
                        </Grid.RowDefinitions>

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="13" />
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="150"/>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="150" />
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="4"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="4"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="13" />
                        </Grid.ColumnDefinitions>

                        <!--백그라운드용-->
                        <Border     Grid.Row="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                        <TextBlock  Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='출고번호', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Row="0" Grid.Column="2" x:Name="txtRcvIssId"     Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtPallet_KeyDown"/>
                        <TextBlock  Grid.Row="0" Grid.Column="3" Text="{Binding ConverterParameter='PALLETID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Row="0" Grid.Column="4" x:Name="txtPallet"     Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtPallet_KeyDown" PreviewKeyDown="txtPalle_PreviewKeyDown" />
                        <Label x:Name="lblShipToName" Content="" Grid.Column="5"  Margin="0" VerticalAlignment="Center"/>
                        <Label x:Name="lblCustType"   Content="" Grid.Column="5"  Margin="0"  Visibility="Collapsed"/>
                        <Label x:Name="lblShipToID"   Content="" Grid.Column="5"  Margin="0"  Visibility="Collapsed"/>

                        <Button     Grid.Row="0" Grid.Column="7" x:Name="btnNote"      Content="{Binding ConverterParameter=Note,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnNote_Click" Visibility="Collapsed" />
                        <Button     Grid.Row="0" Grid.Column="9" x:Name="btnReset"      Content="{Binding ConverterParameter=초기화,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnReset_Click"/>
                        <Button x:Name="btnSearch"      Grid.Row="0" Grid.Column="11"   Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />

                    </Grid>

                    <Grid Grid.Row="3">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="34"/>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <!--백그라운드용-->
                        <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                        <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                        <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='PALLET목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                        <uc:UcBaseDataGrid Grid.Row="5" Grid.Column="1" x:Name="dgPallet" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="MultiRange"  CanUserRemoveRows="True"  CanUserAddRows="True" Margin="8" >
                            <c1:C1DataGrid.Resources>
                                <Style TargetType="{x:Type Button}" x:Key="grButtonStyle" BasedOn="{StaticResource ButtonIconBaseStyle}">
                                    <Setter Property="Background">
                                        <Setter.Value>
                                            <ImageBrush ImageSource="/LGC.GMES.MES.CMM001;component/Images/icon_trash.png" Stretch="Uniform"/>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </c1:C1DataGrid.Resources>
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTemplateColumn    Header="TRAY정보" EditOnSelection="True" Name="CHK" Width="Auto" HorizontalAlignment="Center" >
                                    <c1:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <RadioButton GroupName="dgPallet_Group" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgPallet_Choice" HorizontalAlignment="Center" Checked="dgPallet_Choice_Checked"/>
                                        </DataTemplate>
                                    </c1:DataGridTemplateColumn.CellTemplate>
                                </c1:DataGridTemplateColumn>
                                <c1:DataGridTextColumn        Header = "PALLETID"       Binding = "{Binding PALLETID}"           HorizontalAlignment="Center" />
                                <!--<c1:DataGridTextColumn        Header = "MODEL명"        Binding = "{Binding MODLID}"     HorizontalAlignment="Center" />-->
                                <c1:DataGridTextColumn        Header = "반제품ID"       Binding = "{Binding PRODID}"             HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn        Header = "반제품명"       Binding = "{Binding PRODID}"             HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn        Header = "제품ID"         Binding = "{Binding TOP_PRODID}"         HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn        Header = "수량"           Binding = "{Binding TOTAL_QTY}"          HorizontalAlignment="Center" Format="#,##0"/>
                                <c1:DataGridTextColumn        Header = "포장일자"       Binding = "{Binding PACKDATE}"           HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn        Header = "OCVDATE"        Binding = "{Binding OCV_DATE}"           HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn        Header = "출하예정"       Binding = "{Binding SHIPDATE}"           HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn        Header = "LINE"           Binding = "{Binding LINE_NO}"            HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn        Header = "TYPE"           Binding = "{Binding PACK_WRK_TYPE_CODE}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                <c1:DataGridTextColumn        Header = "비고"           Binding = "{Binding SHIPTO_NOTE}"        HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn        Header = "발행수"         Binding = "{Binding LABEL_PRT_COUNT}"    HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn        Header = "OCV2 경과일"    Binding = "{Binding OCV2_ELPS_DAYS}"     HorizontalAlignment="Center" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn        Header = "생산일"         Binding = "{Binding PACK_DATE}"          HorizontalAlignment="Center" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn        Header = "생산시간"       Binding = "{Binding PACK_TIME}"          HorizontalAlignment="Center" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn        Header = "생산일(년)"     Binding = "{Binding PACK_DAY_OF_YEAR}"   HorizontalAlignment="Center" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn        Header = "출고일시"       Binding = "{Binding INSDTTM}"            HorizontalAlignment="Center" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn        Header = "Note"           Binding = "{Binding SKID_NOTE}"          HorizontalAlignment="Center" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn        Header = "MODELID"        Binding = "{Binding MODELID}"            HorizontalAlignment="Center" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn        Header = "CELLID"         Binding = "{Binding CELLID}"             HorizontalAlignment="Center" Visibility="Collapsed"/>
                                <c1:DataGridTemplateColumn    Header="제거" Width="40">
                                    <c1:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Button Click="delete_Button_Click" Style="{StaticResource grButtonStyle}" />
                                        </DataTemplate>
                                    </c1:DataGridTemplateColumn.CellTemplate>
                                </c1:DataGridTemplateColumn>
                            </c1:C1DataGrid.Columns>
                        </uc:UcBaseDataGrid>
                    </Grid>
                    <Grid x:Name="SettingArea" Grid.Row="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>

                        <Grid.RowDefinitions>
                            <RowDefinition />
                        </Grid.RowDefinitions>

                        <Grid x:Name="TrayArea" Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>

                            <!--백그라운드용-->
                            <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                            <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                            <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='TRAY정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                            <uc:UcBaseDataGrid Grid.Row="1" Grid.Column="1" x:Name="dgTray" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleRow" Margin="8">
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridTextColumn        Header = "번호"           Binding = "{Binding SEQ_NO}"      HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn        Header = "TRAYID"         Binding = "{Binding TRAYID}"      HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn        Header = "수량"           Binding = "{Binding QTY}"         HorizontalAlignment="Center" Format="#,##0"/>
                                    <c1:DataGridTextColumn        Header = "비고"           Binding = "{Binding Binding05}"   HorizontalAlignment="Center" />
                                </c1:C1DataGrid.Columns>
                            </uc:UcBaseDataGrid>
                        </Grid>

                        <c1:C1TabControl Grid.Column="3">
                            <c1:C1TabItem x:Name="ctCMI" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='CMI'}" >
                                <Grid x:Name="grCMI">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>


                                    <Grid Grid.Row ="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="34"/>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="8"/>
                                        </Grid.RowDefinitions>

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="8" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="199*" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="199*" />
                                            <ColumnDefinition Width="8" />

                                        </Grid.ColumnDefinitions>
                                        <Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                        <Border Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                        <TextBlock Grid.Row="1" Grid.ColumnSpan="99"  Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='공통사항', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                                        <Border Grid.Row="3" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='FACTORY', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="3" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtPlantCMI" IsReadOnly="True" ToolTip="FACTORY"/>
                                        </Border>
                                        <Border Grid.Row="3" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='DOCK', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="3" Grid.Column="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtDockCMI" IsReadOnly="True" ToolTip="DOCK"/>
                                        </Border>

                                        <Border Grid.Row="4" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='MATERIALCODE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="4" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtMaterialCMI" IsReadOnly="True" ToolTip="GMMATERIAL"/>
                                        </Border>
                                        <Border Grid.Row="4" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='REFERENCE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="4" Grid.Column="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtReferenceCMI" IsReadOnly="True" ToolTip="REFERENCE"/>
                                        </Border>

                                        <Border Grid.Row="5" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='CONTAINER', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="5" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtContainerCMI" IsReadOnly="True" ToolTip="CONTAINERTYPE"/>
                                        </Border>
                                        <Border Grid.Row="5" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='GROSSWEIGHT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="5" Grid.Column="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtGrossWeightCMI" IsReadOnly="True" ToolTip="GROSSWEIGHT"/>
                                        </Border>

                                        <Border Grid.Row="6" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='WATTHOUR', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="6" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtWattHourCMI" IsReadOnly="True" ToolTip="WATTHOUR_RATE"/>
                                        </Border>
                                        <Border Grid.Row="6" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='QUANTITY', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="6" Grid.Column="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtQuantityCMI" IsReadOnly="True" ToolTip="QTY"/>
                                        </Border>

                                        <Border Grid.Row="7" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='PARTNO', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="7" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtPartNumberCMI" IsReadOnly="True" ToolTip="PARTNO"/>
                                        </Border>

                                        <Border Grid.Row="7" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='DUNECODE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="7" Grid.Column="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtDuneCodeCMI" IsReadOnly="True" ToolTip="DUNCODE"/>
                                        </Border>
                                        <TextBox Grid.Row="8" Grid.Column="1" x:Name="txtToAddr1CMI" IsReadOnly="True" ToolTip="TOADDR1" Visibility="Collapsed"/>
                                        <TextBox Grid.Row="8" Grid.Column="2" x:Name="txtToAddr2CMI" IsReadOnly="True" ToolTip="TOADDR2" Visibility="Collapsed"/>
                                        <TextBox Grid.Row="8" Grid.Column="3" x:Name="txtToAddr3CMI" IsReadOnly="True" ToolTip="TOADDR3" Visibility="Collapsed"/>
                                        <TextBox Grid.Row="8" Grid.Column="4" x:Name="txtToAddr4CMI" IsReadOnly="True" ToolTip="TOADDR4" Visibility="Collapsed"/>
                                    </Grid>
                                </Grid>
                            </c1:C1TabItem>
                            <c1:C1TabItem x:Name="ctF2" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='F2'}" >
                                <Grid x:Name="grF2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                    <Grid Grid.Row ="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="34"/>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="AUTO" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="8"/>
                                        </Grid.RowDefinitions>

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="8" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="199*" />
                                            <ColumnDefinition Width="8" />

                                        </Grid.ColumnDefinitions>
                                        <Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                        <Border Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                        <TextBlock Grid.Row="1" Grid.ColumnSpan="99"  Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='공통사항', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>


                                        <Border Grid.Row="4" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock  Text="{Binding ConverterParameter='출하전압', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="4" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <c1:C1ComboBox    x:Name="cboVoltageF2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" ToolTip="VOLT_RANGE"  IsEditable="True" />
                                        </Border>

                                        <Border Grid.Row="5" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='차종', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="5" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtCarTypeF2" IsReadOnly="True" ToolTip="CARTYPE"/>
                                        </Border>

                                    </Grid>
                                </Grid>
                            </c1:C1TabItem>
                            <c1:C1TabItem x:Name="ctHLGP" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='HLGP'}" >
                                <Grid x:Name="grHLGP">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>


                                    <Grid Grid.Row ="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="34"/>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="40" />
                                            <RowDefinition Height="8"/>
                                        </Grid.RowDefinitions>

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="8" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="199*" />
                                            <ColumnDefinition Width="8" />

                                        </Grid.ColumnDefinitions>
                                        <Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                        <Border Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                        <TextBlock Grid.Row="1" Grid.ColumnSpan="99"  Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='공통사항', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                                        <Border Grid.Row="3" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='용도구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="3" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <c1:C1ComboBox    x:Name="cboUseTypeHLGP" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                        </Border>

                                        <Border Grid.Row="4" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock  Text="{Binding ConverterParameter='출하전압', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="4" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <c1:C1ComboBox    x:Name="cboVoltageHLGP" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" ToolTip="VOLT_RANGE"  IsEditable="True"/>
                                        </Border>

                                        <Border Grid.Row="5" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='차종', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="5" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtCarTypeHLGP" IsReadOnly="True" ToolTip="CARTYPE"/>
                                        </Border>

                                        <Border Grid.Row="6" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                            <TextBlock   Text="{Binding ConverterParameter='품번', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        </Border>
                                        <Border Grid.Row="6" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                            <TextBox x:Name="txtPartCodeHLGP" IsReadOnly="True" ToolTip="PARTCODE"/>
                                        </Border>

