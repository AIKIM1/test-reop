<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.MTRL001.MTRL001_220"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:cmm001="clr-namespace:LGC.GMES.MES.CMM001;assembly=LGC.GMES.MES.CMM001"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1920"
             Loaded="UserControl_Loaded" >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.CMM001;component/PopupFindControlStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="원자재재공현황">
            <ctrlLib:LGCLayout.Content>
                <Grid>
                    <c1:C1TabControl x:Name="tabMain" Grid.Row="1" Grid.Column="0" >
                        <c1:C1TabItem x:Name="List" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='원자재 재공조회'}">
                            <Grid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="45"/>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="2*"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <!-- 98-->
                                    <RowDefinition Height="Auto"/>
                                    <!-- 3 -->
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>

                                <Grid x:Name="InitArea" Grid.Row="1">
                                    <Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Grid.RowSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                    <Grid Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="12"/>
                                            <ColumnDefinition Width="Auto" MinWidth="155"/>
                                            <ColumnDefinition Width="20"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="20"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="12"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="12"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="12"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="8" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="5"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="5"/>
                                        </Grid.RowDefinitions>
                                        <Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="16" Grid.RowSpan="4" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>


                                        <!-- 동 -->
                                        <Grid Grid.Row="1" Grid.Column="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="50"/>
                                                <ColumnDefinition Width="6*"/>
                                            </Grid.ColumnDefinitions>

                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                            </StackPanel>
                                            <c1:C1ComboBox   x:Name="cboArea"                Grid.Row="0" Grid.Column="1"   Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                                        </Grid>

                                        <!-- 자재코드 -->
                                        <Grid Grid.Row="1" Grid.Column="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="4*"/>
                                                <ColumnDefinition Width="6*"/>
                                            </Grid.ColumnDefinitions>
                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='자재코드', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                            </StackPanel>
                                            <cmm001:PopupFindControl Grid.Row="0" Grid.Column="1" x:Name="popSearchMtrl"  Visibility="Visible" MinWidth="200" MaxWidth="200" SelectedValuePath="MTRL_ID" DisplayMemberPath="MTRL_NM"  PopupDisplayMode="TextOnly" ValueChanged="popSearchMtrl_ValueChanged"/>

                                        </Grid>
                                        <!-- 조회 Btn -->
                                        <Grid Grid.Row="1" Grid.Column="11" Grid.ColumnSpan="4" HorizontalAlignment="Right" Margin="5">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <Button x:Name="btnSearch"  Grid.Column="1"   Content="{Binding ConverterParameter=조회,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                                        </Grid>
                                    </Grid>
                                </Grid>

                                <Grid x:Name="SearchArea" Grid.Row="3" >
                                    <Grid x:Name="grd">

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4.*"/>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="6.*"/>
                                        </Grid.ColumnDefinitions>

                                        <Grid x:Name="grdList" Grid.Column="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="34"/>
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>

                                            <Grid Grid.Row="0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                                <Grid Grid.Column="1" HorizontalAlignment="Right">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="30"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="15"/>
                                                        <ColumnDefinition Width="30"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="5"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="10"/>
                                                        <ColumnDefinition Width="10"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="1*"/>
                                                        <RowDefinition Height="Auto" />
                                                        <RowDefinition Height="1*" />
                                                    </Grid.RowDefinitions>
                                                </Grid>
                                            </Grid>

                                            <Grid   Grid.Row="1" Grid.Column="0" >
                                                <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                                <c1:C1DataGrid Grid.Row="1" x:Name="dgSummary" ClipboardPasteMode="None" IsReadOnly="True" HeadersVisibility="None" FrozenColumnCount="2" FrozenTopRowsCount="2" SelectionMode="MultiRange" Margin="8" MouseDoubleClick="dgSummary_MouseDoubleClick" LoadedCellPresenter="dgSummary_LoadedCellPresenter"   >
                                                    <c1:C1DataGrid.Columns >
                                                        <c1:DataGridTextColumn Header = "자재코드"      Binding = "{Binding MATERIAL_CD}"               HorizontalAlignment="Left"   IsReadOnly="True"  />
                                                        <c1:DataGridTextColumn Header = "자재명"        Binding = "{Binding MATERIAL_NM}"               HorizontalAlignment="Left"   IsReadOnly="True" />
                                                        <c1:DataGridTextColumn Header = "단위"          Binding = "{Binding MATERIAL_UNIT}"             HorizontalAlignment="Center" IsReadOnly="True" Width = "Auto"/>
                                                        <c1:DataGridTextColumn Header = "이동예정"      Binding = "{Binding MATERIAL_LOC_QTY}"          HorizontalAlignment="Right"  IsReadOnly="True" Width = "Auto" Format="###,###,##0.#####"/>
                                                        <c1:DataGridTextColumn Header = "자재창고"      Binding = "{Binding MATERIAL_LOC}"              HorizontalAlignment="Center" IsReadOnly="True" Width = "Auto" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header = "자동창고"      Binding = "{Binding STO_LOC_QTY}"               HorizontalAlignment="Right"  IsReadOnly="True" Width = "Auto" Format="###,###,##0.#####"/>
                                                        <c1:DataGridTextColumn Header = "자동창고코드"  Binding = "{Binding STO_LOC}"                   HorizontalAlignment="Center" IsReadOnly="True" Width = "Auto" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header = "생산대기"      Binding = "{Binding PROC_LOC_QTY}"              HorizontalAlignment="Right"  IsReadOnly="True" Width = "Auto" Format="###,###,##0.#####"/>
                                                        <c1:DataGridTextColumn Header = "생산창고"      Binding = "{Binding PROC_LOC}"                  HorizontalAlignment="Center" IsReadOnly="True" Width = "Auto" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header = "총수량"        Binding = "{Binding TOT_QTY}"                   HorizontalAlignment="Right"  IsReadOnly="True" Width = "Auto" Format="###,###,##0.#####"/>
                                                       
                                                    </c1:C1DataGrid.Columns>

                                                    <c1:C1DataGrid.TopRows>
                                                        <c1:DataGridColumnHeaderRow />
                                                    </c1:C1DataGrid.TopRows>

                                                </c1:C1DataGrid>
                                            </Grid>
                                        </Grid>

                                        <GridSplitter Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                        <Grid x:Name="grdListDetail" Grid.Column="2">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="34"/>
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>

                                            <Grid Grid.Row="0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>


                                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='상세', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                                            </Grid>

                                            <Grid   Grid.Row="1" Grid.Column="0" >
                                                <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                                <c1:C1DataGrid x:Name="dgDetail" 
                                                               ClipboardPasteMode="None" 
                                                               IsReadOnly="False" 
                                                               HeadersVisibility="Column" 
                                                               FrozenColumnCount="1" 
                                                               CanUserRemoveRows="True" 
                                                               CanUserAddRows="True" 
                                                               SelectionMode="MultiRange" 
                                                               Margin="8" 
                                                               MouseDoubleClick="dgDetail_MouseDoubleClick"
                                                               LoadedCellPresenter="dgDetail_LoadedCellPresenter">
                                                    <c1:C1DataGrid.Columns>
                                                        <c1:DataGridTextColumn Header = "Carrier ID"      Binding = "{Binding CARRIER_ID}"            HorizontalAlignment="Left"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "PLC_ID"         Binding = "{Binding RACK_ID}"                    HorizontalAlignment="Center"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "SEQ"             Binding = "{Binding SEQ}"                   HorizontalAlignment="Left"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "자재LOTID"       Binding = "{Binding MTRL_LOTID}"            HorizontalAlignment="Center" IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "자재코드"        Binding = "{Binding MATERIAL_CD}"           HorizontalAlignment="Center"  IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "자재명"          Binding = "{Binding MATERIAL_NM}"           HorizontalAlignment="Center"  IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "자재단위"        Binding = "{Binding MATERIAL_UNIT}"         HorizontalAlignment="Center"  IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "자재수량"        Binding = "{Binding MATERIAL_QTY}"          HorizontalAlignment="Right"  IsReadOnly="True" Width = "Auto" Format="###,###,##0.#####"/>
                                                        <c1:DataGridTextColumn Header = "IQC상태"         Binding = "{Binding IQC_STATE_NM}"          HorizontalAlignment="Center" IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "IQC상태"         Binding = "{Binding IQC_STATE}"             HorizontalAlignment="Center" IsReadOnly="True" Width = "Auto" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header = "유효기간"        Binding = "{Binding EXPIRY_DATE}"           HorizontalAlignment="Center"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "QMS HOLD"        Binding = "{Binding QMS_HOLD}"              HorizontalAlignment="Center"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "MES HOLD"        Binding = "{Binding HOLD_YN}"               HorizontalAlignment="Center"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "HOLD사유"        Binding = "{Binding HLOD_NOTE}"             HorizontalAlignment="Left"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "위험물여부"      Binding = "{Binding DGSU_YN}"               HorizontalAlignment="Center"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "요청서진행상태"  Binding = "{Binding STATUS_NM}"             HorizontalAlignment="Center"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "Pallet상태"      Binding = "{Binding TRANSACTION_CODE}"      HorizontalAlignment="Center"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "처리자"          Binding = "{Binding TRANSACTION_USER}"      HorizontalAlignment="Center"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "트랙잭션일시"    Binding = "{Binding LAST_TRANSACTION_DATE}" HorizontalAlignment="Center"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "현재 위치"   Binding = "{Binding MATERIAL_LOCATION_NM}"       HorizontalAlignment="Center"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "진행상태"        Binding = "{Binding STATUS_CODE}"           HorizontalAlignment="Left"   IsReadOnly="True" Width = "Auto" Visibility="Collapsed"/>
                                                    </c1:C1DataGrid.Columns>
                                                </c1:C1DataGrid>
                                            </Grid>
                                        </Grid>

                                    </Grid>
                                </Grid>

                                <Border Grid.Row="5" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <Grid Grid.Row="5" HorizontalAlignment="Right" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition Width="80"/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition Width="150"/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition Width="80"/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition Width="150"/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="5"/>
                                    </Grid.ColumnDefinitions>
                                </Grid>
                            </Grid>
                        </c1:C1TabItem>

                        <c1:C1TabItem x:Name="HISTORY" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='원자재 재공이력조회'}" Margin="0">
                            <Grid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="45"/>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="2*"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <!-- 98-->
                                    <RowDefinition Height="Auto"/>
                                    <!-- 3 -->
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>

                                <Grid x:Name="InitAreaT1" Grid.Row="1">
                                    <Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Grid.RowSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                    <Grid Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="12"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="30"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="40"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="30"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="30"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="12"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="12"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="8" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="5"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="5"/>
                                        </Grid.RowDefinitions>
                                        <Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="16" Grid.RowSpan="4" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                        <Grid x:Name="grdDate" Grid.Row="1" Grid.Column="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="10" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="20" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0"   Text="{Binding ConverterParameter='조회기간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                            <ctrlLib:LGCDatePicker x:Name="dtpStart" Grid.Column="2" DatepickerType="Date" Width="120" IsNullInitValue="True"  />
                                            <TextBlock  Grid.Column="3" Text="~" HorizontalAlignment="Center"  Style="{StaticResource Content_Search_LabelStyle}" />
                                            <ctrlLib:LGCDatePicker x:Name="dtpEnd" Grid.Column="4" DatepickerType="Date" Width="120" IsNullInitValue="True" />
                                        </Grid>
                                        
                                        <!--  -->
                                        <Grid Grid.Row="1" Grid.Column="3" >
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="4*"/>
                                                <ColumnDefinition Width="6*"/>
                                            </Grid.ColumnDefinitions>

                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='자재유형', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                            </StackPanel>
                                            <c1:C1ComboBox   x:Name="cboMtrlType" Grid.Row="0" Grid.Column="1"   Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" SelectedValueChanged="cboMtrlType_SelectedValueChanged"/>
                                        </Grid>

                                        <!-- 자재코드 -->
                                        <Grid Grid.Row="1" Grid.Column="5">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="4*"/>
                                                <ColumnDefinition Width="6*"/>
                                            </Grid.ColumnDefinitions>
                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='자재코드', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                            </StackPanel>
                                            <cmm001:PopupFindControl Grid.Row="0" Grid.Column="1" x:Name="popSearchMtrlT1"  Visibility="Visible" MinWidth="200" MaxWidth="200" SelectedValuePath="MTRL_ID" DisplayMemberPath="MTRL_NM"  PopupDisplayMode="TextOnly" ValueChanged="popSearchMtrl_ValueChanged"/>

                                        </Grid>


                                        <!-- Pallet -->
                                        <Grid Grid.Row="1" Grid.Column="7">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="4*"/>
                                                <ColumnDefinition Width="6*"/>
                                            </Grid.ColumnDefinitions>
                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='Pallet/Tray', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                            </StackPanel>
                                            <TextBox        Grid.Column="1" x:Name="txtPallet" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="150" CharacterCasing="Upper"  KeyDown="txtCellID_KeyDown" />

                                        </Grid>
                                        <!-- Pallet -->
                                        <Grid Grid.Row="1" Grid.Column="9">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="4*"/>
                                                <ColumnDefinition Width="8"/>
                                                <ColumnDefinition Width="6*"/>
                                            </Grid.ColumnDefinitions>
                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='투입취소포함여부', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                            </StackPanel>
                                            <CheckBox  Grid.Column="2" x:Name="ChkCancel" IsChecked="False" Width="19"  />

                                        </Grid>
                                        <!-- 조회 Btn -->
                                        <Grid Grid.Row="1" Grid.Column="13" Grid.ColumnSpan="4" HorizontalAlignment="Right" Margin="5">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <Button x:Name="btnSearchT1"  Grid.Column="1"   Content="{Binding ConverterParameter=조회,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchT1_Click" />
                                        </Grid>
                                    </Grid>
                                </Grid>

                                <Grid x:Name="SearchAreaT1" Grid.Row="3" >
                                    <Grid x:Name="Hist">

                                        <Grid.ColumnDefinitions>
                                             <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>

                                        <Grid x:Name="grdListHist" Grid.Column="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="34"/>
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>

                                            <Grid Grid.Row="0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>


                                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='상세', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                                            </Grid>

                                            <Grid   Grid.Row="1" Grid.Column="0" >
                                                <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                                <c1:C1DataGrid x:Name="dgHist" ClipboardPasteMode="None" IsReadOnly="False" HeadersVisibility="Column" CanUserRemoveRows="True" CanUserAddRows="True" SelectionMode="MultiRange" Margin="8"  FrozenColumnCount="5" LoadedCellPresenter="dgHist_LoadedCellPresenter"  >
                                                    <c1:C1DataGrid.Columns>
                                                        <c1:DataGridTextColumn Header = "Carrier ID"     Binding = "{Binding CARRIER_ID}"            HorizontalAlignment="Left"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "SEQ"            Binding = "{Binding SEQ}"                   HorizontalAlignment="Left"   IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "자재LOTID"      Binding = "{Binding MTRL_LOTID}"            HorizontalAlignment="Center" IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "자재코드"         Binding = "{Binding MATERIAL_CD}"           HorizontalAlignment="Center"  IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "자재명"         Binding = "{Binding MATERIAL_NM}"           HorizontalAlignment="Center"  IsReadOnly="True" Width = "Auto" />
                                                        <c1:DataGridTextColumn Header = "자재단위"       Binding = "{Binding MATERIAL_UNIT}"         HorizontalAlignment="Center"  IsReadOnly="True" Width = "Auto" />
