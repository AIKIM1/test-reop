                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                            <!--<TextBlock  Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />-->
                                            <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='WH_TYPE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                        </StackPanel>
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboStockerTypeT2"      DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStockerType_SelectedValueChanged" />
                                    </Grid>
                                    
                                    <Grid Grid.Row="1" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock  Style="{StaticResource SearchCondition_MandatoryMarkStyle}" Visibility="Collapsed" />
                                            <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='창고', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                        </StackPanel>
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboStockerT2"      DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"   />
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='제품명', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                        </StackPanel>
                                        <CMM001:PopupFindControl Grid.Row="0" Grid.Column="1" x:Name="popSearchMtrlT2"  Visibility="Visible" MinWidth="100" MaxWidth="300" SelectedValuePath="MTRL_ID" DisplayMemberPath="MTRL_NM"  PopupDisplayMode="TextOnly" ValueChanged="popSearchMtrl_ValueChanged"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="7">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='PalletID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                        </StackPanel>
                                        <TextBox Grid.Row="0" Grid.Column="1"     x:Name="txtPalletIDT2"   Style="{StaticResource SearchCondition_TextBoxStyle}"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="9">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*" />
                                            <ColumnDefinition Width="6*" />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='기준일자', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1">
                                            <ctrlLib:LGCDatePicker DatepickerType="Date" x:Name="dtpDate" />

                                        </StackPanel>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="13">
                                        <Button x:Name="btnSearchT2"  Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchT1_Click" />
                                    </Grid>


                                </Grid>

