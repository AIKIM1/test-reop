<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.MTRL001.MTRL001_214"  
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"                         
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:CMM001="clr-namespace:LGC.GMES.MES.CMM001;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024" Loaded="UserControl_Loaded" Initialized="UserControl_Initialized" >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.CMM001;component/PopupFindControlStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="자재현황" Grid.ColumnSpan="2">
            <ctrlLib:LGCLayout.Content>
                <Grid>
                    <c1:C1TabControl Grid.Row="1" Grid.Column="0" >
                        <c1:C1TabItem x:Name="tabT1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='자재현황'}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="64" />
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="*" />

                                </Grid.RowDefinitions>

                                <!-- Search Area-->
                                <Grid Grid.Row="0" Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="11" />
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="11" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="13"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Background="#EFF1F3" BorderThickness="1" BorderBrush="#9F9F9F"></Border>


                                    <Grid Grid.Row="1" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='조회 기준일', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                        </StackPanel>
                                        <ctrlLib:LGCDatePicker DatepickerType="Date" x:Name="dtpDateFrom" Grid.Row="0"  Grid.Column="1"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='WH_TYPE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                        </StackPanel>
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboStockerTypeT1"      DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStockerType_SelectedValueChanged" />
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock  Style="{StaticResource SearchCondition_MandatoryMarkStyle}" Visibility="Collapsed" />
                                            <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='창고', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                        </StackPanel>
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboStockerT1"      DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStocker_SelectedValueChanged"  />
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="7">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='제품명', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                        </StackPanel>
                                        <CMM001:PopupFindControl Grid.Row="0" Grid.Column="1" x:Name="popSearchMtrlT1"  Visibility="Visible" MinWidth="100" MaxWidth="300" SelectedValuePath="MTRL_ID" DisplayMemberPath="MTRL_NM"  PopupDisplayMode="TextOnly" ValueChanged="popSearchMtrl_ValueChanged"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="9">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                            <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='PalletID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                        </StackPanel>
                                        <TextBox Grid.Row="0" Grid.Column="1"     x:Name="txtPalletIDT1"   Style="{StaticResource SearchCondition_TextBoxStyle}"/>
                                    </Grid>
                                    
                                    <Grid Grid.Row="1" Grid.Column="13">
                                        <Button x:Name="btnSearchT1"  Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchT1_Click" />
                                    </Grid>

                                </Grid>

                                <Grid Grid.Row="2" Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Border    Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" /></TextBlock>
                                    <Border    Grid.Row="1" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />

                                    <c1:C1DataGrid Grid.Row="1" x:Name="dgListT1" HeadersVisibility="All" SelectionMode="SingleCell" Margin="8"  LoadedRowHeaderPresenter="dgListT1_LoadedRowHeaderPresenter">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn        Header = "창고"                       Binding = "{Binding EQUIPMENT_ID}"                 HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn        Header = "창고명"                     Binding = "{Binding EQUIPMENT_DESC}"               HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "RACKID"                     Binding = "{Binding RACK_ID}"                      HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "제품코드"                   Binding = "{Binding PRODUCT_SPECIFICATION_ID}"     HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn        Header = "PRODNAME"                   Binding = "{Binding PRODUCT_SPECIFICATION_DESC}"   HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "제품코드명"                 Binding = "{Binding MODLID}"                       HorizontalAlignment="Left"   IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "MODEL_NAME"                 Binding = "{Binding PRJT_NAME}"                    HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "P/TID"                      Binding = "{Binding PROCESSING_GROUP_ID}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "생산라인"                   Binding = "{Binding FACTORY_AREA_ID}"              HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn        Header = "생산라인"                   Binding = "{Binding FACTORY_AREA_DESC}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "재고수량"                   Binding = "{Binding LOT_QTY}"                      HorizontalAlignment="Right"  IsReadOnly="True" Format="#,##0"/>
                                            <c1:DataGridTextColumn        Header = "유효기간 초과"              Binding = "{Binding VLD_DAYS}"                     HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "장기재고"                   Binding = "{Binding LONG_STOCK}"                   HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "생산LOT"                    Binding = "{Binding ASSY_LOTID}"                   HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "재고상태"                   Binding = "{Binding STAT_CODE}"                    HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "ERP 저장위치"               Binding = "{Binding ERP_LOC}"                    HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "QMS HOLD"                   Binding = "{Binding HOLD_YN}"                      HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "OQC_PASS_FLAG"              Binding = "{Binding OQC_STATUS}"                   HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn        Header = "OQC_PASS_FLAG"              Binding = "{Binding OQC_STATUS_DESC}"              HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "NCR여부"                    Binding = "{Binding NCR_STATUS}"                   HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "P/T 발행일"                 Binding = "{Binding PT_DATE}"                      HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "SOC"                        Binding = "{Binding SOC}"                          HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn        Header = "활성화 작업유형"            Binding = "{Binding FORM_WRK_TYPE_CODE}"           HorizontalAlignment="Center" IsReadOnly="True" />
                                        </c1:C1DataGrid.Columns>

                                    </c1:C1DataGrid>
                                </Grid>


                            </Grid>
                        </c1:C1TabItem>


                        <c1:C1TabItem x:Name="tabT2" Visibility="Collapsed" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='자재현황(일별)'}">
