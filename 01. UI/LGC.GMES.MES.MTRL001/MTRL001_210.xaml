<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:cmm001="clr-namespace:LGC.GMES.MES.CMM001;assembly=LGC.GMES.MES.CMM001"
             x:Class="LGC.GMES.MES.MTRL001.MTRL001_210"
             mc:Ignorable="d" 
             d:DesignHeight="850" d:DesignWidth="1920" 
             Loaded="UserControl_Loaded" 
             Unloaded="UserControl_Unloaded" 
             >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.CMM001;component/PopupFindControlStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid x:Name="grdMain">
        <Grid.Resources>
            <DataTemplate x:Key="ItemTemplate">
                <WrapPanel>
                    <Image Width="138" Height="138" Stretch="Fill" Source="{Binding}" Margin="3,3,3,3" />
                </WrapPanel>
            </DataTemplate>
            <DataTemplate x:Key="ResultTemplate">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>
            <DataTemplate x:Key="ResultTemplateSum">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="4"  Text="{Binding ConverterParameter='TOTAL', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>
        </Grid.Resources>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="전극 원자재 STK 재공현황" >
            <ctrlLib:LGCLayout.Content>
                <Grid >
                    <Grid.RowDefinitions>
                        <!--TopSearch-->
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8"/>
                        <!--Contents-->
                        <RowDefinition Height="*"/>
                        <!--<RowDefinition Height="8"/>-->
                    </Grid.RowDefinitions>

                    <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                    <Grid x:Name="TopSearch" Grid.Row="0" Grid.ColumnSpan="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="8" />
                        </Grid.RowDefinitions>
                        <!-- 조회 버튼 영역-->
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="13"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="0.5*" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="13" />
                        </Grid.ColumnDefinitions>


                        <Grid Grid.Row="1" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock      Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            <c1:C1ComboBox  Grid.Column="1"  x:Name="cboArea" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboArea_SelectedValueChanged" />
                        </Grid>

                        <Grid Grid.Row="1" Grid.Column="3">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock  Text="{Binding ConverterParameter='WH_TYPE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            </StackPanel>
                            <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboStockerType"  DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStockerType_SelectedValueChanged" />
                        </Grid>

                        <Grid Grid.Row="1" Grid.Column="7" Visibility="Collapsed" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock  Text="{Binding ConverterParameter='극성', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            </StackPanel>
                            <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboElectrodeType" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboElectrodeType_SelectedValueChanged" />
                        </Grid>

                        <Grid Grid.Row="1" Grid.Column="5" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock  Text="{Binding ConverterParameter='창고', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                            </StackPanel>
                            <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboStocker"  DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStocker_SelectedValueChanged" />
                        </Grid>
                        <c1:C1ComboBox Grid.Row="1"  Grid.Column="9" x:Name="cboTimer" AutoComplete="False" DisplayMemberPath="NAME" SelectedValuePath="CODE" SelectedValueChanged="cboTimer_SelectedValueChanged"  Width="Auto" MinWidth="40"/>
                        <Button Grid.Row="1" Grid.Column="11" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                    </Grid>

                    <!--Contents-->
                    <Grid x:Name="Contents" Grid.Row="2">
                        <Grid Grid.Row="0"  x:Name="ContentsRow">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2.4*"/>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="7.6*" />
                            </Grid.ColumnDefinitions>

                            <!--Left Grid Area-->
                            <Grid Grid.Row="0" x:Name="LeftArea" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1.*" />
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="1.*" />
                                </Grid.RowDefinitions>

                                <Grid Grid.Row="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='창고 적재 현황', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    </Border>

                                    <Border Grid.Row="1" Background="#e8ebed" BorderThickness="0" BorderBrush="#8a8a8a"></Border>
                                    <Grid Grid.Row="1" x:Name="grdCapacitySummary">
                                        <c1:C1DataGrid x:Name="dgCapacitySummary" 
                                                   Grid.Row="0" 
                                                   Grid.Column="0" 
                                                   IsReadOnly="True" 
                                                   HeadersVisibility="Column" 
                                                   CanUserFilter="False" 
                                                   CanUserSort="False" 
                                                   SelectionMode="SingleCell" 
                                                   AlternatingRowBackground="White"  
                                                   LoadedCellPresenter="dgCapacitySummary_LoadedCellPresenter" 
                                                   UnloadedCellPresenter="dgCapacitySummary_UnloadedCellPresenter" 
                                                   MouseLeftButtonUp="dgCapacitySummary_MouseLeftButtonUp" 
                                                   MergingCells="dgCapacitySummary_MergingCells" 
                                                   >
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridTextColumn Header="창고"                Binding="{Binding EQPT_ID}"            HorizontalAlignment="Left" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn Header="창고"                Binding="{Binding EQPT_NM}"            HorizontalAlignment="Center" />
                                                <c1:DataGridNumericColumn Header="용량"             Binding="{Binding MAX_CAPA}"           HorizontalAlignment="Right" Format="#,##0" />
                                                <c1:DataGridNumericColumn Header="PLT QTY"          Binding="{Binding LOAD_REP_CSTID_QTY}" HorizontalAlignment="Right" Format="#,##0" />
                                                <c1:DataGridNumericColumn Header="실Carrier수"      Binding="{Binding FULL_QTY}"           HorizontalAlignment="Right" Format="#,##0" />
                                                <c1:DataGridNumericColumn Header="공Carrier수"      Binding="{Binding EMPTY_QTY}"          HorizontalAlignment="Right" Format="#,##0" />
                                                <c1:DataGridNumericColumn Header="비정상 Rack"      Binding="{Binding AB_RACK_QTY}"        HorizontalAlignment="Right" Format="#,##0" />
                                                <c1:DataGridNumericColumn Header="정보불일치수"     Binding="{Binding AB_CST_QTY}"         HorizontalAlignment="Right" Format="#,##0" />
                                                <c1:DataGridNumericColumn Header="금지단"           Binding="{Binding PROHIBIT_QTY}"       HorizontalAlignment="Right" Format="#,##0" />
                                                <c1:DataGridNumericColumn Header="적재율(%)"        Binding="{Binding LOAD_RATE}"          HorizontalAlignment="Right" Format="###,###,##0.##"/>
                                                <c1:DataGridTextColumn Header="극성"                Binding="{Binding ELTR_TYPE_CODE}"     HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            </c1:C1DataGrid.Columns>
                                        </c1:C1DataGrid>
                                    </Grid>
                                </Grid>
                                <GridSplitter Grid.Row="1" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                                <Grid Grid.Row="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Grid Grid.Row="0" x:Name="grdProductSummaryTitle">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="20"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="1.*"/>
                                        </Grid.ColumnDefinitions>
                                        <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                        <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='자재 창고 현황', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>



                                        <Grid Grid.Row="0" Grid.Column="4">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="6*"/>
                                                <ColumnDefinition Width="4*"/>
                                            </Grid.ColumnDefinitions>

                                        </Grid>
                                    </Grid>

                                    <Grid Grid.Row="1" x:Name="grdProductSummary" >
                                        <c1:C1DataGrid x:Name="dgProductSummary"
                                                       IsReadOnly="True" 
                                                       HeadersVisibility="None" 
                                                       AlternatingRowBackground="White" 
                                                       CanUserFilter="False" 
                                                       CanUserSort="False" 
                                                       SelectionMode="SingleCell" 
                                                       MouseLeftButtonUp="dgProductSummary_MouseLeftButtonUp" 
                                                       LoadedCellPresenter="dgProductSummary_LoadedCellPresenter"
                                                       UnloadedCellPresenter="dgProductSummary_UnloadedCellPresenter"
                                                    >
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridTextColumn Header="[자재그룹, 자재그룹]"        Binding="{Binding MTGRID}"               HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL" />
                                                <c1:DataGridTextColumn Header="[자재코드, 자재코드]"        Binding="{Binding MATERIAL_CD}"          HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn Header="[자재 SPEC, 자재 SPEC]"      Binding="{Binding MATERIAL_NAME}"        HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn Header="[등급, 등급]"                Binding="{Binding GRADE}"                HorizontalAlignment="Center" Visibility="Collapsed" />
                                                <c1:DataGridNumericColumn Header="[PLT QTY, PLT QTY]"       Binding="{Binding LOAD_REP_CSTID_QTY}"   HorizontalAlignment="Right"  Format="#,##0"  />
                                                <c1:DataGridNumericColumn Header="[자재 수, 가용]"          Binding="{Binding AVAIL_QTY_1}"          HorizontalAlignment="Right"  Format="#,##0"  />
                                                <c1:DataGridNumericColumn Header="[자재 수, HOLD]"          Binding="{Binding HOLD_QTY_1}"           HorizontalAlignment="Right"  Format="#,##0"  />
                                                <c1:DataGridNumericColumn Header="[자재 수량, 가용]"        Binding="{Binding AVAIL_QTY_2}"          HorizontalAlignment="Right"  Format="#,##0"  />
                                                <c1:DataGridNumericColumn Header="[자재 수량, HOLD]"        Binding="{Binding HOLD_QTY_2}"           HorizontalAlignment="Right"  Format="#,##0"  />
                                            </c1:C1DataGrid.Columns>
                                            <c1:C1DataGrid.TopRows>
                                                <c1:DataGridColumnHeaderRow />
                                                <c1:DataGridColumnHeaderRow />
                                            </c1:C1DataGrid.TopRows>
                                        </c1:C1DataGrid>
                                    </Grid>

                                </Grid>
                            </Grid>
                            <!--Splitter-->
                            <GridSplitter x:Name="Splitter" Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"  DragStarted="Splitter_DragStarted" DragCompleted="Splitter_DragCompleted"/>
                            <!--<Border Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"></Border>-->
                            <Grid x:Name="Right" Grid.Column="2" >
                                <c1:C1TabControl x:Name="C1TabControl" >
                                    <c1:C1TabItem x:Name="tabProduct" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='입고 자재'}" >
                                        <Grid >
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="7" />
                                                <RowDefinition Height="30" />
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>

                                            <Border Grid.Row="0" Grid.RowSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}"></Border>
                                            <Grid Grid.Row="1">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="10" />

                                                    <ColumnDefinition Width="20" />
                                                    <ColumnDefinition Width="3.5" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="8" />
                                                    <ColumnDefinition Width="20" />
                                                    <ColumnDefinition Width="3.5" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="8" />
                                                    <ColumnDefinition Width="20" />
                                                    <ColumnDefinition Width="3.5" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="8" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="8" />
                                                </Grid.ColumnDefinitions>
                                                <TextBlock  Grid.Column="1" Text="{Binding ConverterParameter='경과일수', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" FontSize="12" Margin="0,0,0,0" />

                                                <Border    Grid.Row="0" Grid.Column="3" Background="#F2CB61" BorderThickness="0.5" Style="{StaticResource BorderSubTitleStyle}" />
                                                <TextBlock Grid.Row="0" Grid.Column="5" Text="{Binding ConverterParameter='D+7이상', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                                <Border    Grid.Row="0" Grid.Column="7" Background="Orange" BorderThickness="0.5" Style="{StaticResource BorderSubTitleStyle}" />
                                                <TextBlock Grid.Row="0" Grid.Column="9" Text="{Binding ConverterParameter='D+15이상', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                                <Border    Grid.Row="0" Grid.Column="11" Background="Red" BorderThickness="0.5" Style="{StaticResource BorderSubTitleStyle}" />
                                                <TextBlock Grid.Row="0" Grid.Column="13" Text="{Binding ConverterParameter='D+30이상', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                                <Button x:Name="btnExcel"  Grid.Row="0" Grid.Column="15"  Content="{Binding ConverterParameter='EXCEL', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnExcel_Click" />
                                            </Grid>

                                            <c1:C1DataGrid x:Name="dgProduct" 
                                                           Grid.Row="2"
                                                           IsReadOnly="True" 
                                                           HeadersVisibility="Column" 
                                                           FrozenColumnCount="4" 
                                                           SelectionMode="SingleRow" 
                                                           Margin="8,8,8,0"
                                                           LoadedCellPresenter="dgProduct_LoadedCellPresenter" 
                                                           UnloadedCellPresenter="dgProduct_UnloadedCellPresenter" 
                                                           PreviewMouseLeftButtonDown="dgProduct_PreviewMouseLeftButtonDown" 
                                                           >
                                                <c1:C1DataGrid.Columns>

                                                    <c1:DataGridNumericColumn	Header = "순위"		                Binding = "{Binding ROWNUMBER}"                   HorizontalAlignment="Right" />
                                                    <c1:DataGridTextColumn      Header = "STOCKER"                  Binding = "{Binding EQPT_NM}"                     HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "Rack ID"                  Binding = "{Binding RACK_ID}"                     HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn      Header = "PLC_ID"                   Binding = "{Binding RACK_NM}"                     HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "입고일시"                 Binding = "{Binding RCK_DATE}"                    HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "경과일수"                 Binding = "{Binding PAST_DAY}"                    HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "대표 Carrier ID"          Binding = "{Binding REP_PROCESSING_GROUP_ID}"     HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL"/>
                                                    <c1:DataGridTextColumn      Header = "Carrier ID"               Binding = "{Binding MLOT_ID}"                     HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "PLT_ID"                   Binding = "{Binding PLLT_ID}"                     HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "PLT_SEQ"                  Binding = "{Binding PALLET_SERIAL_NO}"            HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "자재LOTID"                Binding = "{Binding SUPPLIER_LOTID}"              HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn		Header = "자재그룹"	                Binding = "{Binding MATERIAL_GROUP_CODE}"	      HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn		Header = "자재코드"	                Binding = "{Binding MATERIAL_CD}"                 HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn		Header = "자재명"	                Binding = "{Binding MATERIAL_NM}"                 HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "수량"                     Binding = "{Binding MATERIAL_QTY}"                HorizontalAlignment="Right" Format="#,###.000" />
                                                    <c1:DataGridTextColumn		Header = "단위"	                    Binding = "{Binding MATERIAL_UNIT}"               HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "IQC 상태"	                Binding = "{Binding IQC_STATE}"                   HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn		Header = "IQC 상태"	                Binding = "{Binding IQC_STATE_NM}"                HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "HOLD여부"	                Binding = "{Binding HOLD_YN}"                     HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn		Header = "HOLD사유"	                Binding = "{Binding HOLD_NOTE}"                   HorizontalAlignment="Left"/>
                                                    <c1:DataGridTextColumn		Header = "QMS HOLD"	                Binding = "{Binding QMS_HOLD}"                    HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn		Header = "QMS HOLD 사유"	        Binding = "{Binding QMS_HOLD_NOTE}"               HorizontalAlignment="Left"/>
                                                    <c1:DataGridTextColumn      Header = "유효일자"                 Binding = "{Binding EXPIRY_DATE}"                 HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "등급"                     Binding = "{Binding GRADE}"                       HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "자재 SPEC"                Binding = "{Binding LOAD_REP_PROCESSING_GROUP}"   HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn      Header = "위험물여부"               Binding = "{Binding DGSU_YN}"                     HorizontalAlignment="Center"/>

                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>
                                        </Grid>
                                    </c1:C1TabItem>

                                    <c1:C1TabItem x:Name="tabEmptyCarrier" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='공Carrier'}" >
                                        <Grid >

                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="0.3*"/>
                                                <ColumnDefinition Width="8"/>
                                                <ColumnDefinition Width="0.7*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Row="0" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}"></Border>
                                            <c1:C1DataGrid x:Name="dgEmptyCarrier" 
                                                           HeadersVisibility="Column" 
                                                           IsReadOnly="True"
                                                           SelectionMode="SingleCell" 
                                                           Margin="8,8,8,0"
                                                           Grid.Column="0"
                                            >
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn      Header = "극성"           Binding = "{Binding ELTR_TYPE_CODE}"    HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn      Header = "극성"           Binding = "{Binding ELTR_TYPE_NAME}"    HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn      Header = "EQPTID"         Binding = "{Binding EQPT_NM}"          HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL"/>
                                                    <c1:DataGridNumericColumn   Header = "Carrier 수"     Binding = "{Binding CST_CNT}"           HorizontalAlignment="Right" Format="#,##0" />
                                                    <c1:DataGridTextColumn      Header = "EQPTID"         Binding = "{Binding EQPT_ID}"            HorizontalAlignment="Center" Visibility="Collapsed" />
                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>

                                            <c1:C1DataGrid x:Name="dgCarrierList" 
                                                           HeadersVisibility="Column" 
                                                           IsReadOnly="True"
                                                           SelectionMode="SingleCell" 
                                                           Margin="8,8,8,0"
                                                           Grid.Column="2"
                                            >
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn      Header = "NO"                Binding = "{Binding ROWNUMBER}"     HorizontalAlignment="Right" />
                                                    <c1:DataGridTextColumn      Header = "STOCKER"           Binding = "{Binding EQPT_NM}"       HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn      Header = "Rack ID"           Binding = "{Binding RACK_ID}"       HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn      Header = "PLC_ID"            Binding = "{Binding RACK_NM}"       HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn      Header = "입고일시"          Binding = "{Binding RCK_DATE}"      HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn      Header = "대표 Carrier ID"   Binding = "{Binding REP_PROCESSING_GROUP_ID}"     HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL"/>
                                                    <c1:DataGridTextColumn      Header = "Carrier ID"        Binding = "{Binding CST_ID}"        HorizontalAlignment="Center" />

                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>

                                        </Grid>
                                    </c1:C1TabItem>

                                    <c1:C1TabItem x:Name="tabErrorCarrier" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='비정상 Rack'}" >
                                        <Grid >
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Border Grid.Row="0" Grid.RowSpan="2" Grid.Column="0" Style="{StaticResource Content_InputForm_ContentBorderStyle}"></Border>
