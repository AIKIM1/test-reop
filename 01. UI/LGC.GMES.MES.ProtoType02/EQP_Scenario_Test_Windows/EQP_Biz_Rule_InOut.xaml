<UserControl xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"  
             x:Class="LGC.GMES.MES.ProtoType02.EQP_Biz_Rule_InOut"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid Margin="5">
        
        <Grid.RowDefinitions>
            <RowDefinition Height="34"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="5"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
            <Button Style="{StaticResource Content_ModuleButtonExpandStyle}" Margin="0,0,10,0"/>
            <TextBlock Grid.Row="0" Grid.Column="0" x:Name="txtbSVC_ID" Text="{Binding ConverterParameter='SVC_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" ></TextBlock>
        </StackPanel>

        <Border    Grid.Row="2" Grid.Column="1" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>

        <Grid Grid.Row="2" Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="5"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="5"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="5"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="5"/>
            </Grid.ColumnDefinitions>
            <c1:C1TabControl Grid.Row="1" Grid.Column="1" x:Name="tabBizInData" >
                <c1:C1TabItem Header="TabItem" x:Name="titemIn01">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="5"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="5"/>
                        </Grid.ColumnDefinitions>
                        <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgInData01"  IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                    </Grid>
                </c1:C1TabItem>
                <c1:C1TabItem Header="TabItem" x:Name="titemIn02">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="5"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="5"/>
                        </Grid.ColumnDefinitions>
                        <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgInData02"  IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                    </Grid>
                </c1:C1TabItem>
                <c1:C1TabItem Header="TabItem" x:Name="titemIn03">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="5"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="5"/>
                        </Grid.ColumnDefinitions>
                        <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgInData03"  IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                    </Grid>
                </c1:C1TabItem>
                <c1:C1TabItem Header="TabItem" x:Name="titemIn04">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="5"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="5"/>
                        </Grid.ColumnDefinitions>
                        <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgInData04"  IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                    </Grid>
                </c1:C1TabItem>
                <c1:C1TabItem Header="TabItem" x:Name="titemIn05">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="5"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="5"/>
                        </Grid.ColumnDefinitions>
                        <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgInData05"  IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                    </Grid>
                </c1:C1TabItem>
                <c1:C1TabItem Header="TabItem" x:Name="titemIn06">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="5"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="5"/>
                        </Grid.ColumnDefinitions>
                        <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgInData06"  IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                    </Grid>
                </c1:C1TabItem>
            </c1:C1TabControl>
        </Grid>

        <Border    Grid.Row="5" Grid.Column="1" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>

        <Grid Grid.Row="4" Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="5"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="5"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="5"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="5"/>
            </Grid.ColumnDefinitions>
            
            <c1:C1TabControl Grid.Row="1" Grid.Column="1" x:Name="tabBizOutData" >
            <c1:C1TabItem Header="TabItem" x:Name="titemOut01">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="5"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgOutData01" AutoGenerateColumns="True" IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                </Grid>
            </c1:C1TabItem>
            <c1:C1TabItem Header="TabItem" x:Name="titemOut02">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="5"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgOutData02" AutoGenerateColumns="True"  IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                </Grid>
            </c1:C1TabItem>
            <c1:C1TabItem Header="TabItem" x:Name="titemOut03">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="5"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgOutData03" AutoGenerateColumns="True"  IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                </Grid>
            </c1:C1TabItem>
            <c1:C1TabItem Header="TabItem" x:Name="titemOut04">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="5"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgOutData04" AutoGenerateColumns="True"  IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                </Grid>
            </c1:C1TabItem>
            <c1:C1TabItem Header="TabItem" x:Name="titemOut05">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="5"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <c1:C1DataGrid   Grid.Row="1" Grid.Column="1" x:Name="dgOutData05" AutoGenerateColumns="True"  IsReadOnly="False" HeadersVisibility="All"  SelectionMode="SingleRow" />
                </Grid>
            </c1:C1TabItem>
        </c1:C1TabControl>

        </Grid>
    </Grid>
    
</UserControl>
