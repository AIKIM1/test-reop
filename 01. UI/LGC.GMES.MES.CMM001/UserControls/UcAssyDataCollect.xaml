<UserControl x:Class="LGC.GMES.MES.CMM001.UserControls.UcAssyDataCollect"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LGC.GMES.MES.CMM001.UserControls"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
            d:DesignHeight="300" d:DesignWidth="512">
         
            
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot">
            <Grid>
                <Border Grid.Row="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                <Grid>
                    <Grid.Resources>
                        <DataTemplate x:Key="ResultTemplate">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right" FontWeight="Bold" FontSize="13" />
                            </Grid>
                        </DataTemplate>
                    </Grid.Resources>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="34"/>
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="8"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="8"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="30"/>
                        <ColumnDefinition Width="8"/>
                    </Grid.ColumnDefinitions>

                    <c1:C1TabControl Name="TcDataCollect" Grid.Row="1" Grid.Column="1" Grid.RowSpan="3" Grid.ColumnSpan="2" SelectionChanged="TcDataCollect_SelectionChanged" >
                        <c1:C1TabItem x:Name="TabDefect" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='불량/LOSS/물품청구'}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="6"/>
                                    <RowDefinition Height="1*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Button Grid.Row="1" Grid.Column="3" x:Name="btnDefectSave" Content="{Binding ConverterParameter=저장, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDefectSave_Click" />
                                <c1:C1DataGrid Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="4" x:Name="dgDefect" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleCell" CommittedEdit="dgDefect_CommittedEdit" BeginningEdit="dgDefect_BeginningEdit"  FrozenBottomRowsCount="1" LoadedCellPresenter="dgDefect_LoadedCellPresenter">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn        Header = "활동ID"         Binding = "{Binding ACTID}"               HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn        Header = "활동명"         Binding = "{Binding ACTNAME}"             ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL" HorizontalAlignment="Center" IsReadOnly="True" Width="0.8*" />
                                        <c1:DataGridTextColumn        Header = "불량코드"       Binding = "{Binding RESNCODE}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "GRCODENAME"     Binding = "{Binding CLSS_NAME1}"          ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL" HorizontalAlignment="Center" IsReadOnly="True" Width="0.7*"/>
                                        <c1:DataGridTextColumn        Header = "활동사유명"     Binding = "{Binding RESNNAME}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "불량명"         Binding = "{Binding DFCT_CODE_DETL_NAME}" HorizontalAlignment="Left"   IsReadOnly="True" Width="1.0*"/>
                                        <c1:DataGridTextColumn        Header = "불량코드명"     Binding = "{Binding DFCT_CODE_NAME}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "불량상세명"     Binding = "{Binding DFCT_DETL_NAME}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "부품"           Binding = "{Binding DFCT_PART_NAME}"      HorizontalAlignment="Left"   IsReadOnly="True" Width="1.0*"/>
                                        <c1:DataGridTextColumn        Header = "불량코드"       Binding = "{Binding DFCT_CODE}"           HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "실적제외"       Binding = "{Binding RSLT_EXCL_FLAG}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" Width="0.6*"/>
                                        <c1:DataGridTextColumn        Header = "투입량관리"     Binding = "{Binding INPUT_QTY_APPLY_TYPE_CODE}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "투입량관리"     Binding = "{Binding INPUT_QTY_APPLY_TYPE_CODE_NAME}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" Width="0.8*"/>
                                        <c1:DataGridTextColumn        Header = "처리항목코드"   Binding = "{Binding PRCS_ITEM_CODE}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "불량상세코드"   Binding = "{Binding DFCT_DETL_CODE}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "불량부품코드"   Binding = "{Binding DFCT_PART_CODE}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "코스트센터코드" Binding = "{Binding COST_CNTR_ID}"        HorizontalAlignment="Left"   IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridNumericColumn     Header = "장비불량수량"   Binding = "{Binding EQP_DFCT_QTY}"        HorizontalAlignment="Right"  IsReadOnly="True" Format="#,##0" Width="0.6*">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridNumericColumn     Header = "수량"           Binding = "{Binding RESNQTY}"             HorizontalAlignment="Right"  Format="#,##0" EditOnSelection="True" Minimum="0" ShowButtons="False" Width="0.6*">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridTextColumn        Header = "코스트센터"     Binding = "{Binding COST_CNTR_NAME}"      HorizontalAlignment="Left"   IsReadOnly="True" Width="1.0*"/>
                                        <c1:DataGridTextColumn        Header = "와인더 특이사항 필수입력" Binding = "{Binding WINDER_REMARK_MANDATORY_WIPRESONCOLLECT}"        HorizontalAlignment="Left" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn        Header = "허용비율" Binding = "{Binding PERMIT_RATE}"        HorizontalAlignment="Left" IsReadOnly="True" Visibility="Collapsed" />
                                    </c1:C1DataGrid.Columns>
                                    <c1:C1DataGrid.BottomRows>
                                        <c1:DataGridSummaryRow RowStyle="{StaticResource SummaryRowStyle}"/>
                                    </c1:C1DataGrid.BottomRows>
                                </c1:C1DataGrid>
                            </Grid>
                        </c1:C1TabItem>

                        <c1:C1TabItem x:Name="TabReInput" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='재투입'}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="6"/>
                                    <RowDefinition Height="1*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Button Grid.Row="1" Grid.Column="3" x:Name="btnDefectReInputSave" Content="{Binding ConverterParameter=저장, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDefectReInputSave_Click" />
                                <c1:C1DataGrid Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="4" x:Name="dgDefectReInput" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleCell" CommittedEdit="dgDefectReInput_CommittedEdit" BeginningEdit="dgDefectReInput_BeginningEdit"  FrozenBottomRowsCount="1" LoadedCellPresenter="dgDefectReInput_LoadedCellPresenter">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn        Header = "활동ID"         Binding = "{Binding ACTID}"               HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn        Header = "활동명"         Binding = "{Binding ACTNAME}"             ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL" HorizontalAlignment="Center" IsReadOnly="True" Width="0.8*" />
                                        <c1:DataGridTextColumn        Header = "불량코드"       Binding = "{Binding RESNCODE}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "GRCODENAME"     Binding = "{Binding CLSS_NAME1}"          ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL" HorizontalAlignment="Center" IsReadOnly="True" Width="0.7*"/>
                                        <c1:DataGridTextColumn        Header = "활동사유명"     Binding = "{Binding RESNNAME}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "불량명"         Binding = "{Binding DFCT_CODE_DETL_NAME}" HorizontalAlignment="Left"   IsReadOnly="True" Width="1.0*"/>
                                        <c1:DataGridTextColumn        Header = "불량코드명"     Binding = "{Binding DFCT_CODE_NAME}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "불량상세명"     Binding = "{Binding DFCT_DETL_NAME}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "부품"           Binding = "{Binding DFCT_PART_NAME}"      HorizontalAlignment="Left"   IsReadOnly="True" Width="1.0*"/>
                                        <c1:DataGridTextColumn        Header = "불량코드"       Binding = "{Binding DFCT_CODE}"           HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "실적제외"       Binding = "{Binding RSLT_EXCL_FLAG}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" Width="0.6*"/>
                                        <c1:DataGridTextColumn        Header = "투입량관리"     Binding = "{Binding INPUT_QTY_APPLY_TYPE_CODE}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "투입량관리"     Binding = "{Binding INPUT_QTY_APPLY_TYPE_CODE_NAME}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" Width="0.8*"/>
                                        <c1:DataGridTextColumn        Header = "처리항목코드"   Binding = "{Binding PRCS_ITEM_CODE}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "불량상세코드"   Binding = "{Binding DFCT_DETL_CODE}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "불량부품코드"   Binding = "{Binding DFCT_PART_CODE}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "코스트센터코드" Binding = "{Binding COST_CNTR_ID}"        HorizontalAlignment="Left"   IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridNumericColumn     Header = "장비불량수량"   Binding = "{Binding EQP_DFCT_QTY}"        HorizontalAlignment="Right"  IsReadOnly="True" Format="#,##0" Width="0.6*" Visibility="Collapsed">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridNumericColumn     Header = "수량"           Binding = "{Binding RESNQTY}"             HorizontalAlignment="Right"  Format="#,##0" EditOnSelection="True" Minimum="0" ShowButtons="False" Width="0.6*">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridTextColumn        Header = "비고"     Binding = "{Binding RESNNOTE}"      HorizontalAlignment="Left" Width="1.0*"/>
                                    </c1:C1DataGrid.Columns>
                                    <c1:C1DataGrid.BottomRows>
                                        <c1:DataGridSummaryRow RowStyle="{StaticResource SummaryRowStyle}"/>
                                    </c1:C1DataGrid.BottomRows>
                                </c1:C1DataGrid>
                            </Grid>
                        </c1:C1TabItem>

                        <c1:C1TabItem x:Name="TabProdCellWinding" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='생산반제품'}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="39"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="35"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="4"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="4"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="4"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="4"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="4"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="4"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="4"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="4"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Orientation="Horizontal"  Grid.Row="1" Grid.Column="0" >
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="6" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="20" />
                                            <ColumnDefinition Width="4" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="8" />
                                            <ColumnDefinition Width="20" />
                                            <ColumnDefinition Width="4" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="8" />
                                            <ColumnDefinition Width="20" />
                                            <ColumnDefinition Width="4" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="8" />
                                            <ColumnDefinition Width="20" />
                                            <ColumnDefinition Width="4" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <Border    Grid.Row="1" Grid.Column="0" Background="#F8DAC0" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}" Height="20"/>
                                        <TextBlock Grid.Row="1" Grid.Column="2" Text="{Binding ConverterParameter='미확정', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                        <Border    Grid.Row="1" Grid.Column="4" Background="#E6F5FB" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}" Height="20"/>
                                        <TextBlock Grid.Row="1" Grid.Column="6" Text="{Binding ConverterParameter='확정', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                        <Border    Grid.Row="1" Grid.Column="8" Background="#E8F7C8" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}" Height="20"/>
                                        <TextBlock Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter='배출완료', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                        <Border    Grid.Row="1" Grid.Column="12" Background="#FFE400" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}" Height="20"/>
                                        <TextBlock Grid.Row="1" Grid.Column="14" Text="{Binding ConverterParameter='위치정보오류', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />
                                    </Grid>
                                </StackPanel>

                                <Button Grid.Row="1" Grid.Column="9"  x:Name="btnCellWindingCreate"   Content="{Binding ConverterParameter=생성,     Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnCellWindingCreate_Click" />
                                <Button Grid.Row="1" Grid.Column="11" x:Name="btnCellWindingConfirm" Content="{Binding ConverterParameter=확정, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnCellWindingConfirm_Click"  />
                                <Button Grid.Row="1" Grid.Column="13" x:Name="btnCellWindingConfirmCancel" Content="{Binding ConverterParameter=확정취소, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnCellWindingConfirmCancel_Click"  />
                                <Button Grid.Row="1" Grid.Column="15" x:Name="btnDischarge" Content="{Binding ConverterParameter=배출, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDischarge_Click" />
                                <Button Grid.Row="1" Grid.Column="17" x:Name="btnCellWindingDelete" Content="{Binding ConverterParameter=삭제, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnCellWindingDelete_Click" />

                                <c1:C1DataGrid Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="18" 
                                               x:Name="dgProdCellWinding" 
                                               IsReadOnly ="False"
                                               AlternatingRowBackground ="Transparent" 
                                               HeadersVisibility="Column" 
                                               SelectionMode="SingleCell" 
                                               FrozenBottomRowsCount="1" 
                                               FrozenColumnCount="3" 
                                               LoadedCellPresenter="dgProdCellWinding_LoadedCellPresenter" 
                                               UnloadedCellPresenter="dgProdCellWinding_UnloadedCellPresenter" 
                                                >
                                    <!--CurrentCellChanged="dgProdCellWinding_CurrentCellChanged"-->
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridCheckBoxColumn  Name = "CHK" Binding = "{Binding CHK, Mode=TwoWay}" HorizontalAlignment="Center" CanUserFilter="False" CanUserSort="False" CanUserResize="False" >
                                            <c1:DataGridCheckBoxColumn.Header>
                                                <StackPanel Orientation="Horizontal" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                                                    <CheckBox x:Name="chkHeaderAll" Margin="0" Width="19" Checked="chkHeaderAll_Checked" Unchecked="chkHeaderAll_Unchecked"></CheckBox>
                                                </StackPanel>
                                            </c1:DataGridCheckBoxColumn.Header>
                                        </c1:DataGridCheckBoxColumn>
                                        <c1:DataGridTextColumn        Header = "OUT_LOTID"   Binding = "{Binding OUT_LOTID}"        HorizontalAlignment="Center" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn        Header = "TRAY"        Binding = "{Binding TRAYID}"           HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridNumericColumn     Header = "수량"        Binding = "{Binding CELLQTY}"          HorizontalAlignment="Right" Format="#,##0" ShowButtons="False" HandleUpDownKeys="False" Minimum="0" >
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridTextColumn        Header = "단위"               Binding = "{Binding UNIT_CODE}"   HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTemplateColumn Header="위치" Name="btnModify" HorizontalAlignment="Center" IsReadOnly="True" >
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button x:Name="btnGridTraySearch" Content="조회" FontSize="11" Foreground="Black" Background="GhostWhite" Grid.Column="0" IsCancel="True" Click="btnGridTraySearch_Click"/>
                                                    <!--<StackPanel>
                                                    <TextBlock x:Name="txtBlock" Visibility="{Binding VisibilityText, Mode=TwoWay}"/>
                                                    <Button x:Name="btnGridTraySearch" Content="수정" Tag="U" Foreground="Red" FontWeight="Bold"  FontSize="11" Background="GhostWhite"  Visibility="{Binding VisibilityButton, Mode=TwoWay}" IsCancel="True" Click="btnGridTraySearch_Click"/>
                                                </StackPanel>-->
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>

                                        <!--<c1:DataGridTextColumn        Header = "특이"  Binding = "{Binding SPECIALYN}"   HorizontalAlignment="Center" />-->
                                        <c1:DataGridComboBoxColumn    Header = "특이"     Name="CBO_SPCL"  Binding = "{Binding SPECIALYN}"    CellContentStyle="{StaticResource Grid_CellContentStyle}"   EditOnSelection="True" DisplayMemberPath="NAME" SelectedValuePath="CODE" >
                                        </c1:DataGridComboBoxColumn>
                                        <c1:DataGridTextColumn        Header = "특이사항"                  Binding = "{Binding SPECIALDESC}"                HorizontalAlignment="Left" EditOnSelection="True" />
                                        <c1:DataGridComboBoxColumn    Header = "사유"                      Binding = "{Binding SPCL_RSNCODE}"               HorizontalAlignment="Left" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" EditOnSelection="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn        Header = "상태코드"                  Binding = "{Binding STAT_CODE}"        HorizontalAlignment="Center" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn        Header = "상태"                      Binding = "{Binding STAT_CODE_NAME}"   HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn        Header = "확정"                      Binding = "{Binding PROC_TRAY_CNFM_FLAG}"   HorizontalAlignment="Center" IsReadOnly="True" />
                                        <!--<c1:DataGridTextColumn        Header = "활성화인계"  Binding = "{Binding VITALYN}"   HorizontalAlignment="Center" />-->
                                        <!--<c1:DataGridTextColumn        Header = "불량"  Binding = "{Binding POOR_YN}"   HorizontalAlignment="Center" />-->
                                        <c1:DataGridTextColumn        Header = "제품ID"                    Binding = "{Binding PRODID}"                     HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn        Header = "제품명"                    Binding = "{Binding PRODNAME}"                   HorizontalAlignment="Left" IsReadOnly="True" />
                                        <c1:DataGridTextColumn        Header = "생성일시"                  Binding = "{Binding LOTDTTM_CR}"                 HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn        Header = "장비완료일시"              Binding = "{Binding LOTDTTM_ED }"                HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
