<c1:C1Window x:Class="LGC.GMES.MES.CMM001.Popup.CMM_POLYMER_FORM_CART_TAKEOVER"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
		     xmlns:System="clr-namespace:System;assembly=mscorlib"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"        
             ctrlLib:LGCPopupWindowExtension.ApplyStyle="True"
             Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='대차인계인수'}" 
             Tag="ProtoType01Windows01"  Height="850" Width="1300"  IsResizable="True" Loaded="C1Window_Loaded"  >

    <c1:C1Window.Resources>
        <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;Component/ControlStyles.xaml"/>
    </c1:C1Window.Resources>

    <Grid Name="grdTakeOver">
        <Grid.Resources>
            <DataTemplate x:Key="ResultTemplate">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="6"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right"></TextBlock>
                </Grid>
            </DataTemplate>

            <Style TargetType="{x:Type Button}" x:Key="grButtonStyle" BasedOn="{StaticResource ButtonIconBaseStyle}">
                <Setter Property="Background">
                    <Setter.Value>
                        <ImageBrush ImageSource="/LGC.GMES.MES.CMM001;component/Images/btn_i_print.png" Stretch="Uniform"/>
                    </Setter.Value>
                </Setter>
            </Style>
        </Grid.Resources>

        <c1:C1TabControl x:Name="ctbTakeOver">

            <!--대차인계-->
            <c1:C1TabItem x:Name="TakeOverOut" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='대차인계'}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="8" />
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8" />
                        <RowDefinition Height="2*"/>
                        <RowDefinition Height="8" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="8" />
                    </Grid.RowDefinitions>

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>

                    <Border Grid.Row="2" Grid.Column="0" Grid.RowSpan="99" BorderThickness="1,0,1,1" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                    <!--조회영역-->
                    <Grid Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="4" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="8" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <!--백그라운드용-->
                        <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                        <!--조회조건-->
                        <TextBlock       Grid.Row="1" Grid.Column="0"  Text="{Binding ConverterParameter='공정',          Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="1" Grid.Column="2"  Text="{Binding ConverterParameter='라인',          Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="1" Grid.Column="4"  Text="{Binding ConverterParameter='재공품질유형',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="1" Grid.Column="6"  Text="{Binding ConverterParameter='대차상태',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="1" Grid.Column="8"  Text="{Binding ConverterParameter='INBOX 상태',    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter='재공처리유형',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="3" Grid.Column="0"  Text="{Binding ConverterParameter='PJT',           Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="3" Grid.Column="2"  Text="{Binding ConverterParameter='제품',          Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="3" Grid.Column="4"  Text="{Binding ConverterParameter='대차 ID',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="3" Grid.Column="6"  Text="{Binding ConverterParameter='조립 Lot ID',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="3" Grid.Column="8"  Text="{Binding ConverterParameter='InBox ID',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                        <TextBox         x:Name="txtProcessOut"      Grid.Row="1" Grid.Column="1" MinWidth="120" Style="{StaticResource Content_InputForm_TextBoxStyle}" IsEnabled="False" />
                        <ctrlLib:MultiSelectionBox x:Name="cboEquipmentSegmentOut" Grid.Row="1" Grid.Column="3"  MinWidth="140"  DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" isAllUsed="False" Height="36" VerticalAlignment="Bottom"/>
                        <c1:C1ComboBox   x:Name="cboQltyTypeOut"     Grid.Row="1" Grid.Column="5"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                        <c1:C1ComboBox   x:Name="cboCtnrStatOut"     Grid.Row="1" Grid.Column="7"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                        <c1:C1ComboBox   x:Name="cboInboxStatOut"    Grid.Row="1" Grid.Column="9"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                        <c1:C1ComboBox   x:Name="cboWipPrcsTypeOut"  Grid.Row="1" Grid.Column="11" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>

                        <TextBox         x:Name="txtPjtNameOut"      Grid.Row="3" Grid.Column="1"  Style="{StaticResource Content_InputForm_TextBoxStyle}" KeyDown="txtPjtNameOut_KeyDown" />
                        <TextBox         x:Name="txtProdIDOut"       Grid.Row="3" Grid.Column="3"  Style="{StaticResource Content_InputForm_TextBoxStyle}" KeyDown="txtProdIDOut_KeyDown" />
                        <TextBox         x:Name="txtCtnrIDOut"       Grid.Row="3" Grid.Column="5"  Style="{StaticResource Content_InputForm_TextBoxStyle}" KeyDown="txtCtnrIDOut_KeyDown" />
                        <TextBox         x:Name="txtAssyLotIDOut"    Grid.Row="3" Grid.Column="7"  Style="{StaticResource Content_InputForm_TextBoxStyle}" KeyDown="txtAssyLotIDOut_KeyDown" />
                        <TextBox         x:Name="txtInboxIDOut"      Grid.Row="3" Grid.Column="9"  Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtInboxIDOut_KeyDown" />

                        <!--조회 버튼-->
                        <Grid Grid.Row="3" Grid.Column="11">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <Button          x:Name="btnSearchOut" Grid.Column="1" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchOut_Click" />
                        </Grid>

                    </Grid>

                    <!--조회목록-->
                    <Grid Grid.Row="3">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="34"/>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                        <c1:C1DataGrid x:Name="dgListOut" 
                                               Grid.Row="1" 
                                               HeadersVisibility="Column" 
                                               SelectionMode="SingleRow" 
                                               Margin="8,0,8,0" 
                                               FrozenColumnCount="3"
                                               FrozenBottomRowsCount="1" 
                                               LoadedColumnHeaderPresenter="dgListOut_LoadedColumnHeaderPresenter">
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn         Binding = "{Binding CTNR_ID2}"          HorizontalAlignment="Center" IsReadOnly="True"  Width="0" CanUserFilter="False" CanUserSort="False" CanUserResize="False" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTemplateColumn Name="CHK" Width="50" Header="선택" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" >
                                    <c1:DataGridTemplateColumn.CellTemplate >
                                        <DataTemplate>
                                            <RadioButton  GroupName="dgCartChoiceGroup"  x:Name="dgCartChoice" IsChecked="{Binding CHK, Mode=OneWay}"   VerticalAlignment="Center" HorizontalAlignment="Center" Checked="dgCartChoice_Checked" />
                                        </DataTemplate>
                                    </c1:DataGridTemplateColumn.CellTemplate>
                                </c1:DataGridTemplateColumn>
                                <c1:DataGridTextColumn      Header = "대차 ID"            Binding = "{Binding CTNR_ID}"              HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "PJT"                Binding = "{Binding PRJT_NAME}"            HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "TMP1"               Binding = "{Binding TMP1}"                 HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "제품"               Binding = "{Binding PRODID}"               HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "시장유형"           Binding = "{Binding MKT_TYPE_NAME}"        HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "TMP2"               Binding = "{Binding TMP2}"                 HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "재공품질유형"       Binding = "{Binding WIP_QLTY_TYPE_CODE}"   HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "재공품질유형"       Binding = "{Binding WIP_QLTY_TYPE_NAME}"   HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "대차상태"           Binding = "{Binding CTNR_STAT_CODE}"       HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "대차상태"           Binding = "{Binding CTNR_STAT_NAME}"       HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "재공처리유형"       Binding = "{Binding WIP_PRCS_TYPE_CODE}"   HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "재공처리유형"       Binding = "{Binding WIP_PRCS_TYPE_NAME}"   HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridNumericColumn   Header = "대차CELL수"         Binding = "{Binding CART_CELL_QTY}"        HorizontalAlignment="Right"  IsReadOnly="True" Format="###,##0" EditOnSelection="True" Minimum="0" ShowButtons="False" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "작업설비"           Binding = "{Binding CURR_EQPTID}"          HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "작업설비"           Binding = "{Binding EQPTNAME}"             HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "TMP3"               Binding = "{Binding TMP3}"                 HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "이동전공정"         Binding = "{Binding PRE_PROCNAME}"         HorizontalAlignment="Left"   IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "최근이동일시"       Binding = "{Binding ACTDTTM}"              HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "조립LOT"            Binding = "{Binding ASSY_LOTID}"           HorizontalAlignment="Center" IsReadOnly="True" />
                                <c1:DataGridTextColumn      Header = "작업유형"           Binding = "{Binding FORM_WRK_TYPE_NAME}"   HorizontalAlignment="Center" IsReadOnly="True" />
                                <c1:DataGridTextColumn      Header = "INBOX상태"          Binding = "{Binding INBOX_STAT_NAME}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                <c1:DataGridTextColumn      Header = "MOVE_ORD_ID"        Binding = "{Binding MOVE_ORD_ID}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "공정"               Binding = "{Binding PROCID}"               HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "라인"               Binding = "{Binding EQSGID}"               HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                <c1:DataGridNumericColumn   Header = "Inbox수"            Binding = "{Binding INBOX_QTY}"            HorizontalAlignment="Right"  IsReadOnly="True" Format="###,##0" EditOnSelection="True" Minimum="0" ShowButtons="False" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>
                                <c1:DataGridNumericColumn   Header = "CELL수"             Binding = "{Binding CELL_QTY}"             HorizontalAlignment="Right"  IsReadOnly="True" Format="###,##0" EditOnSelection="True" Minimum="0" ShowButtons="False">
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>
                                <c1:DataGridTextColumn      Header = "인수라인"           Binding = "{Binding TO_EQSGNAME}"          HorizontalAlignment="Left" IsReadOnly="True" />
                                <c1:DataGridTextColumn      Header = "인수공정"           Binding = "{Binding TO_PROCNAME}"          HorizontalAlignment="Left" IsReadOnly="True" />
                                <c1:DataGridTextColumn      Header = "AOMM등급"           Binding = "{Binding AOMM_GRD_CODE}"        HorizontalAlignment="Center" IsReadOnly="True" />
                            </c1:C1DataGrid.Columns>

                            <c1:C1DataGrid.BottomRows>
                                <c1:DataGridSummaryRow x:Name="summaryOut" RowStyle="{StaticResource SummaryRowStyle}"/>
                            </c1:C1DataGrid.BottomRows>

                        </c1:C1DataGrid>
                    </Grid>

                    <Grid Grid.Row ="5" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                        </Grid.ColumnDefinitions>

                        <Button x:Name="btnWipPrcsTypeChange"  Grid.Column="1"  Content="{Binding ConverterParameter=재공처리유형변경, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnWipPrcsTypeChange_Click"/>
                        <Button x:Name="btnCellInsert"         Grid.Column="3"  Content="{Binding ConverterParameter=Cell등록,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnCellInsert_Click"/>
                        <Button x:Name="btnDetailOut"          Grid.Column="5"  Content="{Binding ConverterParameter=대차상세,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnDetailOut_Click"/>
                        <Button x:Name="btnFCSMove"            Grid.Column="7"  Content="{Binding ConverterParameter=활성화이동, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnFCSMove_Click"/>
                        <Button x:Name="btnStoredCancel"       Grid.Column="9"  Content="{Binding ConverterParameter=보관취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnStoredCancel_Click"/>
                        <Button x:Name="btnTakeOverCancel"     Grid.Column="11" Content="{Binding ConverterParameter=인계취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnTakeOverCancel_Click"/>
                        <Button x:Name="btnTakeOverOut"        Grid.Column="13" Content="{Binding ConverterParameter=대차인계,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnTakeOverOut_Click"/>
                        <Button x:Name="btnCloseOut"           Grid.Column="15" Content="{Binding ConverterParameter=닫기,       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonStyle}" Click="btnCloseOut_Click" />
                    </Grid>

                </Grid>
            </c1:C1TabItem>

            <!--대차인수-->
            <c1:C1TabItem x:Name="TakeOverIn" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='대차인수'}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="8" />
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8" />
                        <RowDefinition Height="2*"/>
                        <RowDefinition Height="8" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="8" />
                    </Grid.RowDefinitions>

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>

                    <Border Grid.Row="2" Grid.Column="0" Grid.RowSpan="99" BorderThickness="1,0,1,1" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                    <!--조회영역-->
                    <Grid Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="4" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="8" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <!--백그라운드용-->
                        <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                        <!--조회조건-->
                        <TextBlock       Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='공정',          Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="1" Grid.Column="2" Text="{Binding ConverterParameter='라인',          Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='재공품질유형',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="1" Grid.Column="6" Text="{Binding ConverterParameter='PJT',           Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="1" Grid.Column="8" Text="{Binding ConverterParameter='제품',          Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="3" Grid.Column="0" Text="{Binding ConverterParameter='인계동',        Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="3" Grid.Column="2" Text="{Binding ConverterParameter='인계공정',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="3" Grid.Column="4" Text="{Binding ConverterParameter='인계라인',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="3" Grid.Column="6" Text="{Binding ConverterParameter='대차 ID',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBlock       Grid.Row="3" Grid.Column="8" Text="{Binding ConverterParameter='조립 Lot ID',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                        <TextBox                   x:Name="txtProcessIn"          Grid.Row="1" Grid.Column="1" MinWidth="120" Style="{StaticResource Content_InputForm_TextBoxStyle}" IsEnabled="False" />
                        <ctrlLib:MultiSelectionBox x:Name="cboEquipmentSegmentIn" Grid.Row="1" Grid.Column="3"  DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" isAllUsed="False" Height="36" VerticalAlignment="Bottom"/>
                        <c1:C1ComboBox             x:Name="cboQltyTypeIn"         Grid.Row="1" Grid.Column="5" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                        <TextBox                   x:Name="txtPjtNameIn"          Grid.Row="1" Grid.Column="7" MinWidth="120" Style="{StaticResource Content_InputForm_TextBoxStyle}" KeyDown="txtPjtNameIn_KeyDown" />
                        <TextBox                   x:Name="txtProdIDIn"           Grid.Row="1" Grid.Column="9" MinWidth="120" Style="{StaticResource Content_InputForm_TextBoxStyle}" KeyDown="txtProdIDIn_KeyDown" />
                        <c1:C1ComboBox             x:Name="cboTakeOverAreaIn"     Grid.Row="3" Grid.Column="1" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                        <c1:C1ComboBox             x:Name="cboTakeOverProcessIn"  Grid.Row="3" Grid.Column="3" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                        <c1:C1ComboBox             x:Name="cboTakeOverLineIn"     Grid.Row="3" Grid.Column="5" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                        <TextBox                   x:Name="txtCtnrIDIn"           Grid.Row="3" Grid.Column="7" Style="{StaticResource Content_InputForm_TextBoxStyle}" KeyDown="txtCtnrIDIn_KeyDown" />
                        <TextBox                   x:Name="txtAssyLotIDIn"        Grid.Row="3" Grid.Column="9" Style="{StaticResource Content_InputForm_TextBoxStyle}" KeyDown="txtAssyLotIDIn_KeyDown" />

                        <!--조회 버튼-->
                        <Button          x:Name="btnSearchIn"          Grid.Row="3" Grid.Column="11" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchIn_Click" />

                    </Grid>

                    <!--조회목록-->
                    <Grid Grid.Row="3">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="34"/>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                        <c1:C1DataGrid x:Name="dgListIn" 
                                               Grid.Row="1" 
                                               HeadersVisibility="Column" 
                                               SelectionMode="SingleCell" 
                                               Margin="8,0,8,0" 
                                               FrozenColumnCount="3"
                                               FrozenBottomRowsCount="1"
                                               LoadedColumnHeaderPresenter="dgListIn_LoadedColumnHeaderPresenter">
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn         Binding = "{Binding MOVE_ORD_ID}"          HorizontalAlignment="Center" IsReadOnly="True"  Width="0" CanUserFilter="False" CanUserSort="False" CanUserResize="False" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <!--<c1:DataGridTemplateColumn Name="CHK" Width="50" Header="선택" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" >
                                    <c1:DataGridTemplateColumn.CellTemplate >
                                        <DataTemplate>
                                            <RadioButton  GroupName="dgCartChoiceGroup"  x:Name="dgCartChoice" IsChecked="{Binding CHK, Mode=OneWay}"   VerticalAlignment="Center" HorizontalAlignment="Center" Checked="dgCartChoice_Checked" />
                                        </DataTemplate>
                                    </c1:DataGridTemplateColumn.CellTemplate>
                                </c1:DataGridTemplateColumn>-->
                                <c1:DataGridCheckBoxColumn  Name = "CHK" Binding = "{Binding CHK, Mode=TwoWay}" HorizontalAlignment="Center" CanUserFilter="False" CanUserSort="False" CanUserResize="False"  ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI">
                                    <c1:DataGridCheckBoxColumn.Header>
                                        <StackPanel Width="30" Margin="-5" Orientation="Horizontal" >
                                            <TextBlock x:Name="tbCheckHeaderAll" Padding="0" MouseLeftButtonDown="tbCheckHeaderAll_MouseLeftButtonDown" Text="" Width="40"/>
                                        </StackPanel>
                                    </c1:DataGridCheckBoxColumn.Header>
                                </c1:DataGridCheckBoxColumn>

                                <c1:DataGridTextColumn      Header = "대차 ID"            Binding = "{Binding CTNR_ID}"              HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "PJT"                Binding = "{Binding PRJT_NAME}"            HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "TMP1"               Binding = "{Binding TMP1}"                 HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "제품"               Binding = "{Binding PRODID}"               HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "시장유형"           Binding = "{Binding MKT_TYPE_NAME}"        HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "TMP2"               Binding = "{Binding TMP2}"                 HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "작업유형"           Binding = "{Binding FORM_WRK_TYPE_NAME}"   HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "재공품질유형"       Binding = "{Binding WIP_QLTY_TYPE_NAME}"   HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "인계동"             Binding = "{Binding FROM_AREANAME}"        HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "인계라인"           Binding = "{Binding FROM_EQSGNAME}"        HorizontalAlignment="Left"   IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "인계공정"           Binding = "{Binding FROM_PROCNAME}"        HorizontalAlignment="Left"   IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "인계자"             Binding = "{Binding MOVE_USERNAME}"        HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed" />
                                <c1:DataGridTextColumn      Header = "인계일시"           Binding = "{Binding MOVE_STRT_DTTM}"       HorizontalAlignment="Center" IsReadOnly="True" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                <c1:DataGridTextColumn      Header = "조립LOT"            Binding = "{Binding ASSY_LOTID}"           HorizontalAlignment="Center" IsReadOnly="True" />
                                <c1:DataGridNumericColumn   Header = "Inbox수"            Binding = "{Binding INBOX_QTY}"            HorizontalAlignment="Right"  IsReadOnly="True" Format="###,##0" EditOnSelection="True" Minimum="0" ShowButtons="False" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>
                                <c1:DataGridNumericColumn   Header = "CELL수"             Binding = "{Binding CELL_QTY}"             HorizontalAlignment="Right"  IsReadOnly="True" Format="###,##0" EditOnSelection="True" Minimum="0" ShowButtons="False">
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>
                                <c1:DataGridTextColumn      Header = "AOMM등급"            Binding = "{Binding AOMM_GRD_CODE}"        HorizontalAlignment="Center" IsReadOnly="True" />
                            </c1:C1DataGrid.Columns>

                            <c1:C1DataGrid.BottomRows>
                                <c1:DataGridSummaryRow x:Name="summaryIn" RowStyle="{StaticResource SummaryRowStyle}"/>
                            </c1:C1DataGrid.BottomRows>

                        </c1:C1DataGrid>
                    </Grid>

                    <Grid Grid.Row ="5" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                        </Grid.ColumnDefinitions>

                        <Button x:Name="btnDetailIn"      Grid.Column="1" Content="{Binding ConverterParameter=대차상세,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnDetailIn_Click"/>
                        <Button x:Name="btnTakeOverIn"    Grid.Column="3" Content="{Binding ConverterParameter=대차인수,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnTakeOverIn_Click"/>
                        <Button x:Name="btnCloseIn"       Grid.Column="5" Content="{Binding ConverterParameter=닫기,       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonStyle}" Click="btnCloseIn_Click" />
                    </Grid>

                </Grid>

            </c1:C1TabItem>

        </c1:C1TabControl>
        
        <ctrlLib:LoadingIndicator x:Name="loadingIndicator" Grid.Row="0" Grid.RowSpan="999" Grid.Column="0" Grid.ColumnSpan="999" />

    </Grid>

</c1:C1Window>
