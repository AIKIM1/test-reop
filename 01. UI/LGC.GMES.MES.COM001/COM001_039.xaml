<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_039"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="Shop간 이동_인수">
            <Grid>

                <c1:C1TabControl  Grid.Row="0" SelectionChanged="C1TabControl_SelectionChanged"  >
                    <c1:C1TabItem x:Name="Tab_Receive" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='FACTORY간이동(인수)'}" >

                        <Grid Grid.Row="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="45"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <Grid x:Name="SearchArea" Grid.Row="1" Grid.ColumnSpan="2">
                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid   Grid.Row="0" Grid.Column="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="300" />
                                        <ColumnDefinition Width="200"/>
                                        <ColumnDefinition Width="200"/>
                                        <ColumnDefinition Width="200"/>
                                        <ColumnDefinition Width="1.5*"/>
                                        <ColumnDefinition Width="13"/>
                                    </Grid.ColumnDefinitions>

                                    <Grid Grid.Row="0" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='인수라인', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right"  Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboFromEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='MoveOrderID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right"  Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBox       Grid.Row="0"  Grid.Column="1" x:Name="txtMoveOrderID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtMoveOrderID_KeyDown"/>

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='SKIDID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right"  Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBox       Grid.Row="0"  Grid.Column="1" x:Name="txtSKIDID1" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtSKIDID1_KeyDown" />

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="4">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='Carrier ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right"  Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBox       Grid.Row="0"  Grid.Column="1" x:Name="txtCARRIERID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtCARRIERID_KeyDown" />

                                    </Grid>
                                    
                                    <Grid Grid.Row="0" Grid.Column="5" HorizontalAlignment="Right">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <Button x:Name="btnConfrim"      Grid.Row="0" Grid.Column="1"   Content="{Binding ConverterParameter=인수처리,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnConfrim_Click" />
                                        <Button x:Name="btnSearch"       Grid.Row="0" Grid.Column="3"   Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                                        <Button x:Name="btnClear"        Grid.Row="0" Grid.Column="5"   Content="{Binding ConverterParameter=Clear,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnClear_Click" />

                                    </Grid>

                                </Grid>
                            </Grid>

                            <Grid x:Name="GArea" Grid.Row="3" Grid.ColumnSpan="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="6*" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="4*" />
                                </Grid.ColumnDefinitions>

                                <Grid Grid.Row="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <c1:C1DataGrid x:Name="dgMove_Master" Grid.Row="1" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleRow" Margin="8" BeganEdit="dgMove_Master_BeganEdit" LoadedColumnHeaderPresenter="dgMove_Master_LoadedColumnHeaderPresenter" LoadedRowPresenter="dgMove_Master_LoadedRowPresenter" ClipboardPasteMode="None" >
                                        <c1:C1DataGrid.Columns>

                                            <c1:DataGridCheckBoxColumn       Header="CHK"     Binding="{Binding CHK}" IsReadOnly="False" Width="Auto" CanUserSort="False"/>
                                            <c1:DataGridTextColumn Header = "이동ID"          Binding = "{Binding MOVE_ORD_ID}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header = "PRJT_NAME"       Binding = "{Binding PRJT_NAME}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header = "상태"            Binding = "{Binding MOVE_ORD_STAT_NAME}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header = "수량"            Binding = "{Binding MOVE_ORD_QTY}" HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header = "BOXID"           Binding = "{Binding BOXID}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header = "인계동"          Binding = "{Binding FROM_AREAID}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header = "인계라인"        Binding = "{Binding FROM_EQSGID}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header = "인계자"          Binding = "{Binding MOVE_USERID}" HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn Header = "인계시간"        Binding = "{Binding MOVE_STRT_DTTM}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header = "상태코드"        Binding = "{Binding MOVE_ORD_STAT_CODE}" HorizontalAlignment="Center" Visibility="Collapsed" IsReadOnly="True" />
                                            <c1:DataGridTextColumn Header = "인수동"          Binding = "{Binding TO_AREAID_CODE}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "인수라인"        Binding = "{Binding TO_EQSGID_CODE}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "SKIDID"          Binding = "{Binding PANCAKE_GR_ID}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible"/>
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                </Grid>

                                <Grid Grid.Row="0" Grid.Column="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='LOT목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <c1:C1DataGrid x:Name="dgMove_Detail" Grid.Row="1" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="SingleCell" Margin="8" LoadedRowPresenter ="dgMove_Detail_LoadedRowPresenter" ClipboardPasteMode="None">

                                        <c1:C1DataGrid.Columns>

                                            <c1:DataGridTextColumn Header = "LOTID"      Binding = "{Binding LOTID}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "PRODID"     Binding = "{Binding PRODID}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "수량(J/R)"       Binding = "{Binding WIPQTY}" HorizontalAlignment="Center" Format="###,###,##0.##" />
                                            <c1:DataGridTextColumn Header = "수량(PC)"       Binding = "{Binding WIPQTY2}" HorizontalAlignment="Center" Format="###,###,##0.##" />
                                            <c1:DataGridTextColumn Header = "상태"        Binding = "{Binding MOVE_STAT_CODE_DESC}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "MOVE_ORD_ID"       Binding = "{Binding MOVE_ORD_ID}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header = "상태"       Binding = "{Binding MOVE_STAT_CODE}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header = "시장유형"    Binding = "{Binding MKT_TYPE_CODE}"       HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "투입 구간 롤 LANE 방향"     Binding = "{Binding EM_SECTION_ROLL_LANE_DIRCTN}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "적재 중량1"     Binding = "{Binding LOAD_WEIGHT1}" HorizontalAlignment="Right" Format="#,##0.#" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "적재 중량2"     Binding = "{Binding LOAD_WEIGHT2}" HorizontalAlignment="Right" Format="#,##0.#" Visibility="Collapsed"/>
                                        </c1:C1DataGrid.Columns>

                                    </c1:C1DataGrid>
                                </Grid>

                            </Grid>
                        </Grid>


                    </c1:C1TabItem>

                    <c1:C1TabItem x:Name="Tab_MoveHist" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='인수이력'}" >

                        <Grid Grid.Row="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="45"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="1" Grid.ColumnSpan="2">
                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid   Grid.Row="0" Grid.Column="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="13"/>
                                    </Grid.ColumnDefinitions>

                                    <Grid Grid.Row="0" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="3*"/>
                                            <ColumnDefinition Width="7*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='기간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1">
                                            <ctrlLib:LGCDatePicker  x:Name="dtpDateFromHist"  DatepickerType="Date"/>
                                            <TextBlock  Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0"/>
                                            <ctrlLib:LGCDatePicker  x:Name="dtpDateToHist"  DatepickerType="Date"/>
                                        </StackPanel>

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='상태', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboStatHist" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                        <TextBox       Grid.Row="0"  Grid.Column="1" x:Name="txtProd_ID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100"
                                                   KeyDown="txtProd_ID_KeyDown"/>

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="4" HorizontalAlignment="Right">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <Button x:Name="btnSearchHist"      Grid.Row="0" Grid.Column="1"   Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchHist_Click" />

                                    </Grid>

                                </Grid>
                            </Grid>

                            <Grid Grid.Row="3" Grid.ColumnSpan="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="7*" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="3*" />
                                </Grid.ColumnDefinitions>

                                <Grid Grid.Row="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <c1:C1DataGrid x:Name="dgMoveHist_Master" Grid.Row="1" IsReadOnly="True" HeadersVisibility="None" SelectionMode="SingleRow" FrozenTopRowsCount="2" Margin="8" ClipboardPasteMode="None">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTemplateColumn  Header="[선택,선택]" EditOnSelection="True" Name="CHK" Width="Auto" HorizontalAlignment="Center" >
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <RadioButton GroupName="dgMoveHist_MasterGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgMoveHist_MasterChoice" HorizontalAlignment="Center" Checked="dgMoveHist_MasterChoice_Checked"  />
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>

                                            <c1:DataGridTextColumn Header = "[이동ID,이동ID]"        Binding = "{Binding MOVE_ORD_ID}" HorizontalAlignment="Left"/>
                                            <c1:DataGridTextColumn Header = "[상태,상태]"          Binding = "{Binding MOVE_ORD_STAT_NAME}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[PRJT_NAME,PRJT_NAME]"          Binding = "{Binding PRJT_NAME}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[제품코드,제품코드]"      Binding = "{Binding PRODID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[제품명,제품명]"      Binding = "{Binding PRODNAME}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[모델,모델]"      Binding = "{Binding MODLID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[LOT수량,LOT수량]"      Binding = "{Binding LOTQTY}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[인계,동]"        Binding = "{Binding FROM_AREAID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[인계,라인]"      Binding = "{Binding FROM_EQSGID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[인계,공정]"      Binding = "{Binding FROM_PROCID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[인계,수량(Roll)]"        Binding = "{Binding MOVE_ORD_QTY}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                            <c1:DataGridTextColumn Header = "[인계,수량(Lane)]"        Binding = "{Binding MOVE_ORD_QTY2}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                            <c1:DataGridTextColumn Header = "[인계,작업자]"        Binding = "{Binding MOVE_USERID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[인계,시간]"      Binding = "{Binding MOVE_STRT_DTTM }" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[인수,동]"        Binding = "{Binding TO_AREAID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[인수,라인]"      Binding = "{Binding TO_EQSGID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[인수,공정]"      Binding = "{Binding TO_PROCID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[인수,수량(Roll)]"        Binding = "{Binding MOVE_CMPL_QTY}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                            <c1:DataGridTextColumn Header = "[인수,수량(Lane)]"        Binding = "{Binding MOVE_CMPL_QTY2}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                            <c1:DataGridTextColumn Header = "[인수,작업자]"        Binding = "{Binding RCPT_USERID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[인수,시간]"      Binding = "{Binding MOVE_END_DTTM}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[비고,비고]"      Binding = "{Binding NOTE}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[ERP처리결과,ERP처리결과]"        Binding = "{Binding ERP_STAT_CODE}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[ERP처리시간,ERP처리시간]"        Binding = "{Binding ERP_INS}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "[ERP오류명,ERP오류명]"        Binding = "{Binding ERP_ERR_CODE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "[ERP오류메시지,ERP오류메시지]"        Binding = "{Binding ERP_ERR_CAUSE_CNTT}" HorizontalAlignment="Center"/>

                                        </c1:C1DataGrid.Columns>

                                        <c1:C1DataGrid.TopRows>
                                            <c1:DataGridColumnHeaderRow />
                                            <c1:DataGridColumnHeaderRow />
                                        </c1:C1DataGrid.TopRows>
                                    </c1:C1DataGrid>
                                </Grid>

                                <Grid Grid.Row="0" Grid.Column="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='LOT목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <c1:C1DataGrid x:Name="dgMoveHist_Detail" Grid.Row="1" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="SingleCell" Margin="8" ClipboardPasteMode="None">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn Header = "LOTID"       Binding = "{Binding LOTID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "전극버전"    Binding = "{Binding PROD_VER_CODE}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "Lane수"      Binding = "{Binding LANE_QTY}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "단위"        Binding = "{Binding UNIT_CODE}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "수량(Roll)"  Binding = "{Binding WIPQTY}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                            <c1:DataGridTextColumn Header = "수량(Lane)"  Binding = "{Binding WIPQTY2}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                            <c1:DataGridTextColumn Header = "유효기간"    Binding = "{Binding VLD_DATE}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "상태"        Binding = "{Binding MOVE_STAT_CODE_DESC}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "상태"        Binding = "{Binding MOVE_STAT_CODE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "MOVE_ORD_ID" Binding = "{Binding MOVE_ORD_ID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "시장유형"    Binding = "{Binding MKT_TYPE_CODE}"       HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "투입 구간 롤 LANE 방향"     Binding = "{Binding EM_SECTION_ROLL_LANE_DIRCTN}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "적재 중량1"     Binding = "{Binding LOAD_WEIGHT1}" HorizontalAlignment="Right" Format="#,##0.#" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "적재 중량2"     Binding = "{Binding LOAD_WEIGHT2}" HorizontalAlignment="Right" Format="#,##0.#" Visibility="Collapsed"/>
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                </Grid>

                            </Grid>
                        </Grid>


                    </c1:C1TabItem>

                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='인수상세이력'}" >

                        <Grid Grid.Row="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="45"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="1" Grid.ColumnSpan="2">
                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid   Grid.Row="0" Grid.Column="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="1.2*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
