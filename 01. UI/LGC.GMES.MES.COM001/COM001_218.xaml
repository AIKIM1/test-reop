<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_218"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="시생산 LOT 등록">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <c1:C1TabControl>
                    <!--폐기>시생산-->
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='폐기>시생산'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="1*" />
                                <RowDefinition Height="8" />
                                <RowDefinition Height="1*" />
                                <RowDefinition Height="8" />
                                <RowDefinition Height="51" />
                            </Grid.RowDefinitions>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>

                            <GridSplitter Grid.Row="4" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                            <!--조회영역-->
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <TextBlock  Grid.Row="0" Grid.Column="0"  Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="0" Grid.Column="2"  Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="0" Grid.Column="4"  Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                                <TextBlock  Grid.Row="1" Grid.Column="0"  Text="{Binding ConverterParameter='Lot ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="2"  Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="4"  Text="{Binding ConverterParameter='모델ID',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="6"  Text="{Binding ConverterParameter='프로젝트명', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="8"  Text="{Binding ConverterParameter='대차 ID',    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <TextBlock  Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter='Carrier ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="1"  x:Name="cboAreaScrapConv"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="3"  x:Name="cboEqsgScrapConv"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="5"  x:Name="cboProcScrapConv"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                <TextBox         Grid.Row="1" Grid.Column="1"  x:Name="txtLotidScrapConv" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140" />
                                <TextBox         Grid.Row="1" Grid.Column="3"  x:Name="txtProdScrapConv"  Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140" />
                                <TextBox         Grid.Row="1" Grid.Column="5"  x:Name="txtModlScrapConv"  Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140" />
                                <TextBox         Grid.Row="1" Grid.Column="7"  x:Name="txtPrjtScrapConv"  Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140" />
                                <TextBox         Grid.Row="1" Grid.Column="9"  x:Name="txtCtnridScrapConv"        Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140"/>
                                <TextBox         Grid.Row="1" Grid.Column="11" x:Name="txtCstidScrapConv" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140"/>

                                <!--조회 버튼-->
                                <Button     Grid.Row="0" Grid.Column="13" Grid.RowSpan="2" x:Name="btnSearchScrapConv" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand"  Click="btnSearchScrapConv_Click" />

                            </Grid>

                            <!--대상목록-->
                            <Grid Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                                <c1:C1DataGrid Grid.Row="1" 
                                               x:Name="dgListScrapConv" 
                                               IsReadOnly="False" 
                                               HeadersVisibility="Column" 
                                               SelectionMode="SingleCell"
                                               FrozenColumnCount="3" 
                                               Margin="8,8,8,8"
                                               LoadedColumnHeaderPresenter="dgListScrapConv_LoadedColumnHeaderPresenter">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTemplateColumn Name="CHK" Width="40">
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center"  Click="CheckBoxScrapConv_Click"/>
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>
                                        <c1:DataGridTextColumn      Header="라인ID"     Binding="{Binding EQSGID}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header="라인"       Binding="{Binding EQSGNAME}"    HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="LOTID"      Binding="{Binding LOTID}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="제품ID"     Binding="{Binding PRODID}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="PJT"        Binding="{Binding PRJT_NAME}"   HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="모델"       Binding="{Binding MODLID}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="제품명"     Binding="{Binding PRODNAME}"    HorizontalAlignment="Left"   IsReadOnly="True" />
                                        <c1:DataGridNumericColumn      Header="수량(Roll)" Binding="{Binding WIPQTY}"      HorizontalAlignment="Right" IsReadOnly="True" Format="###,##0"/>
                                        <c1:DataGridNumericColumn      Header="수량(Lane)" Binding="{Binding WIPQTY2}"     HorizontalAlignment="Right" IsReadOnly="True" Format="###,##0.00"/>
                                        <c1:DataGridTextColumn      Header="수량(Roll)" Binding="{Binding ACTQTY}"      HorizontalAlignment="Right" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header="수량(Roll)" Binding="{Binding ACTQTY2}"     HorizontalAlignment="Right" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header="단위"       Binding="{Binding UNIT_CODE}"   HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header="LANE_QTY"   Binding="{Binding LANE_QTY}"    HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header="LOTTYPE"    Binding="{Binding LOTTYPE}"     HorizontalAlignment="Center" IsReadOnly="True" Visibility="Hidden"/>
                                        <c1:DataGridTextColumn      Header="Carrier ID" Binding="{Binding CSTID}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>

                            </Grid>

                            <!--선택목록-->
                            <Grid Grid.Row="5" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="2*" />
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="1*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='선택목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <c1:C1DataGrid x:Name="dgSelectScrapConv" 
                                               Grid.Row="1" 
                                               IsReadOnly="False" 
                                               Width="Auto" 
                                               HeadersVisibility="Column" 
                                               FrozenColumnCount="2" 
                                               SelectionMode="SingleCell" 
                                               Margin="8,8,8,8" 
                                               LostFocus="dgSelectScrapConv_LostFocus" 
                                               CommittedEdit="dgSelectScrapConv_CommittedEdit"  
                                               LoadedCellPresenter="dgSelectScrapConv_LoadedCellPresenter" BeginningEdit="dgSelectScrapConv_BeginningEdit" >
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn      Header="라인ID"            Binding="{Binding EQSGID}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header="라인"              Binding="{Binding EQSGNAME}"    HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="LOTID"             Binding="{Binding LOTID}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="제품ID"            Binding="{Binding PRODID}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="PJT"               Binding="{Binding PRJT_NAME}"   HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="모델"              Binding="{Binding MODLID}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="제품명"            Binding="{Binding PRODNAME}"    HorizontalAlignment="Left"   IsReadOnly="True" />
                                        <c1:DataGridNumericColumn   Header="수량(Roll)"        Binding="{Binding WIPQTY}"      HorizontalAlignment="Right" IsReadOnly="True" Format="###,##0"/>
                                        <c1:DataGridNumericColumn   Header="수량(Lane)"        Binding="{Binding WIPQTY2}"     HorizontalAlignment="Right" IsReadOnly="True" Format="###,##0" />
                                        <c1:DataGridNumericColumn   Header="처리수량(Roll,EA)" Binding="{Binding ACTQTY}"      HorizontalAlignment="Right"  Format="###,##0.00" />
                                        <c1:DataGridNumericColumn   Header="처리수량(Lane)"    Binding="{Binding ACTQTY2}"     HorizontalAlignment="Right" IsReadOnly="True" Format="###,##0.00"/>
                                        <c1:DataGridTextColumn      Header="단위"              Binding="{Binding UNIT_CODE}"   HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="LOTTYPE"           Binding="{Binding LOTTYPE}"     HorizontalAlignment="Center" IsReadOnly="True" Visibility="Hidden"/>
                                        <c1:DataGridTextColumn      Header="LANE_QTY"          Binding="{Binding LANE_QTY}"    HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header="Carrier ID"        Binding="{Binding CSTID}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>

                                <Grid Grid.Row ="3">
                                    <Grid.RowDefinitions >
                                        <RowDefinition Height="8" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="8" />
                                    </Grid.RowDefinitions>

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="8" />
                                    </Grid.ColumnDefinitions>

                                    <!--백그라운드용-->
                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="4" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                    <TextBlock  Grid.Row="2" Grid.Column="0" Text="{Binding ConverterParameter='비고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                                    <TextBox                  Grid.Row="2" Grid.Column="1" x:Name="txtNoteScrapConv"             Style="{StaticResource SearchCondition_TextBoxStyle}" Height="Auto"  AcceptsReturn="True" Grid.ColumnSpan="11" />

                                </Grid>
                            </Grid>

                            <!--완료버튼-->
                            <Border Grid.Row="7" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"/>
                            <Grid Grid.Row="7">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <Button x:Name="btnSaveScrapConv"  Grid.Column="1" Content="{Binding ConverterParameter='등록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnSaveScrapConv_Click"/>

                            </Grid>
                        </Grid>
                    </c1:C1TabItem>

                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='시생산전환'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="51" />
                            </Grid.RowDefinitions>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>

                            <GridSplitter Grid.Row="4" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                            <!--조회영역-->
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                                <TextBlock  Grid.Row="1" Grid.Column="0"  Text="{Binding ConverterParameter='Lot ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="2"  Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="4"  Text="{Binding ConverterParameter='모델ID',     Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="6"  Text="{Binding ConverterParameter='프로젝트명', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <TextBlock  Grid.Row="1" Grid.Column="8"  Text="{Binding ConverterParameter='대차 ID',    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <TextBlock  Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter='Carrier ID',    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />

                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="1" x:Name="cboAreaRegister"          Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="3" x:Name="cboEqsgRegister"          Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="5" x:Name="cboProcRegister"          Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                <TextBox         Grid.Row="1" Grid.Column="1"  x:Name="txtLotidRegister"         Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140"  KeyDown="txtLotidRegister_KeyDown"/>
                                <TextBox         Grid.Row="1" Grid.Column="3"  x:Name="txtProdRegister"          Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140"/>
                                <TextBox         Grid.Row="1" Grid.Column="5"  x:Name="txtModlRegister"          Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140"/>
                                <TextBox         Grid.Row="1" Grid.Column="7"  x:Name="txtPrjtRegister"          Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140"/>
                                <TextBox         Grid.Row="1" Grid.Column="9"  x:Name="txtCtnridRegister"        Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140"/>
                                <TextBox         Grid.Row="1" Grid.Column="11" x:Name="txtCstidRegister"         Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140"  KeyDown="txtCstidRegister_KeyDown"/>

                                <!--조회 버튼-->
                                <Button     Grid.Row="0" Grid.Column="13" Grid.RowSpan="2" x:Name="btnSearchRegister" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchRegister_Click" />

                            </Grid>

                            <!--대상목록-->
                            <Grid Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <c1:C1DataGrid Grid.Row="1" x:Name="dgListRegister" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleCell" FrozenColumnCount="3" Margin="8,8,8,8" LoadedColumnHeaderPresenter="dgListRegister_LoadedColumnHeaderPresenter">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTemplateColumn Name="CHK" Width="40">
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center"  Click="CheckBoxRegister_Click"/>
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>
                                        <c1:DataGridTextColumn      Header="라인ID"     Binding="{Binding EQSGID}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header="라인"       Binding="{Binding EQSGNAME}"    HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="LOTID"      Binding="{Binding LOTID}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="제품ID"     Binding="{Binding PRODID}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="PJT"        Binding="{Binding PRJT_NAME}"   HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="모델"       Binding="{Binding MODLID}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="제품명"     Binding="{Binding PRODNAME}"    HorizontalAlignment="Left"   IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="수량(Roll)" Binding="{Binding WIPQTY}"      HorizontalAlignment="Right" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="수량(Lane)" Binding="{Binding WIPQTY2}"     HorizontalAlignment="Right" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header="수량(Roll)" Binding="{Binding ACTQTY}"      HorizontalAlignment="Right" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header="수량(Roll)" Binding="{Binding ACTQTY2}"     HorizontalAlignment="Right" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header="단위"       Binding="{Binding UNIT_CODE}"   HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header="LANE_QTY"   Binding="{Binding LANE_QTY}"    HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header="LOTTYPE"    Binding="{Binding LOTTYPE}"     HorizontalAlignment="Center" IsReadOnly="True" Visibility="Hidden"/>
                                        <c1:DataGridTextColumn      Header="Carrier ID" Binding="{Binding CSTID}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>

                            <!--선택목록-->
                            <Grid Grid.Row="5" >
