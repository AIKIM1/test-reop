<UserControl x:Class="LGC.GMES.MES.COM001.COM001_108"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024" Initialized="UserControl_Initialized" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="APD 데이터 조회">
            <ctrlLib:LGCLayout.Content>
                <Grid x:Name="MainContents">
                    <Grid.Resources>
                        <DataTemplate x:Key="ResultTemplate">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="{Binding}" HorizontalAlignment="Right"></TextBlock>
                            </Grid>
                        </DataTemplate>
                    </Grid.Resources>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="84" />
                        <RowDefinition Height="8" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>

                    <!--start search Area-->
                    <Grid x:Name="SearchArea" Grid.Row="0">
                        <Border Grid.Row="0" Grid.Column="0" Background="#EFF1F3" BorderThickness="1" BorderBrush="#9F9F9F"></Border>
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="13"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="10"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="10"/>
                                <ColumnDefinition Width="1.3*"/>
                                <ColumnDefinition Width="10"/>
                                <ColumnDefinition Width="2.7*"/>
                                <ColumnDefinition Width="20"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="13"/>
                            </Grid.ColumnDefinitions>

                            <Grid Grid.Row="0" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock  Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                </StackPanel>
                                <uc:UcBaseComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboEquipmentSegment"      DisplayMemberPath="" SelectedValuePath=""  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                            </Grid>
                            <Grid Grid.Row="0" Grid.Column="3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock  Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_Search_LabelStyle}"/>
                                     </StackPanel>
                                <uc:UcBaseComboBox   Grid.Row="0" Grid.Column="5" x:Name="cboProcess" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                            </Grid>
                            

                            <Grid Grid.Row="0" Grid.Column="5">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock  Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='설비', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                </StackPanel>
                                <uc:UcBaseComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboEquipment"      DisplayMemberPath="" SelectedValuePath=""  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectionCommitted="cboEquipment_SelectionCommitted"  />
                            </Grid>

                            <Grid Grid.Row="0" Grid.Column="7">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="7*"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock  Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='생성일시', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                </StackPanel>

                                <Grid Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                        <ctrlLib:LGCDatePicker Grid.Row="5"   Grid.Column="3" x:Name="dtpDateFrom"  DatepickerType="Date" SelectedDataTimeChanged="dtpDate_SelectedDataTimeChanged" />
                                        <c1:C1TimeEditor       Grid.Row="5"   Grid.Column="4" x:Name="tmedtFrom" Style="{StaticResource C1TimeEditorStyle}"  CustomFormat="HH:mm:ss" />
                                    </StackPanel>
                                    <TextBlock  Grid.Row="0" Grid.Column="1" Text="~" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                    <StackPanel Grid.Row="0" Grid.Column="2" Orientation="Horizontal" HorizontalAlignment="Right">
                                        <ctrlLib:LGCDatePicker Grid.Row="5"   Grid.Column="3" x:Name="dtpDateTo"  DatepickerType="Date" SelectedDataTimeChanged="dtpDate_SelectedDataTimeChanged" />
                                        <c1:C1TimeEditor       Grid.Row="5"   Grid.Column="4" x:Name="tmedtTo" Style="{StaticResource C1TimeEditorStyle}"  CustomFormat="HH:mm:ss" />
                                    </StackPanel>
                                </Grid>
                            </Grid>




                            <Grid Grid.Row="1" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock   x:Name="lblLOTTXT" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}" HorizontalAlignment="Right" Grid.ColumnSpan="2" Margin="0,13,0,12">
                                                <Run   Text="{Binding ConverterParameter='LOT ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                    </TextBlock>
                                </StackPanel>

                                

                                <Grid Grid.Column="3">
                                    <TextBox        Grid.Column="1" x:Name="txtLOTID" Style="{StaticResource SearchCondition_TextBoxStyle}"  CharacterCasing="Upper" MinWidth="100" KeyDown="txtLOTID_KeyDown" />
                                </Grid>
                            </Grid>

                            <Grid Grid.Row="1" Grid.Column="3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock   x:Name="lblCELLTXT" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}" HorizontalAlignment="Right">
                                                <Run   Text="{Binding ConverterParameter='CELLID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                    </TextBlock>
                                </StackPanel> 
                                <Grid Grid.Column="2">
                                    <TextBox        Grid.Column="1" x:Name="txtCELLID"  CharacterCasing="Upper" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtCELLID_KeyDown" PreviewKeyDown="txtCELLID_PreviewKeyDown" />
                                </Grid>
                            </Grid>

                            <Grid Grid.Row="1" Grid.Column="5">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock   x:Name="lblPKGLOTIDTXT" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}" HorizontalAlignment="Right">
                                                <Run   Text="{Binding ConverterParameter='PKG LOT ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                    </TextBlock>
                                </StackPanel>
                                <Grid Grid.Column="2">
                                    <TextBox        Grid.Column="1" x:Name="txtPKGLOTID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtPKGLOTID_KeyDown" />
                                </Grid>
                            </Grid>

                            <Grid Grid.Row="1" Grid.Column="9">
                                <uc:UcBaseButton x:Name="btnSearch"      Grid.Row="0" Grid.Column="0"   Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" PreviewMouseDown="btnSearch_PreviewMouseDown" />
                            </Grid>
                            <Grid Grid.Row="1" Grid.Column="7">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <uc:UcBaseButton x:Name="btnExcel"      Grid.Row="0" Grid.Column="5"   Content="{Binding ConverterParameter=Excel,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleButtonExcelExportStyle}" Cursor="Hand" Click="btnExcel_Click" />
                            </Grid>
                        </Grid>
                    </Grid>
                    <!--end search Area-->

                    <!--start First Content Area-->
                    <Grid x:Name="Content" Grid.Row="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="34"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Border Grid.Row="0" Grid.RowSpan="2" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}"/>
                        <Grid   Grid.Row="0" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="1*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="15"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="20" />
                                <ColumnDefinition Width="4" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="20" />
                                <ColumnDefinition Width="4" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="20" />
                                <ColumnDefinition Width="4" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>
                            <TextBlock      Grid.Row="0" Grid.RowSpan="3"  Grid.Column="1" Text="{Binding ConverterParameter='APD DATA', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />

                        </Grid>

                        <Grid   Grid.Row="1">

                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="8" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="8"/>
                            </Grid.ColumnDefinitions>

                            <Grid   Grid.Row="1" Grid.Column="1" >
                                <uc:UcBaseDataGrid Grid.Row="2" Grid.Column="1" x:Name="dgAPDDATA_PACKAGING" Visibility="Collapsed" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="4" SelectionMode="SingleRow" LoadedCellPresenter="dgAPDdata_LoadedCellPresenter" UnloadedCellPresenter="dgAPDdata_UnloadedCellPresenter" >
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn        Header = "NO."                Binding = "{Binding ROWNUM}"                    HorizontalAlignment="Center" IsReadOnly="True" Width="40" CanUserFilter="False"/>
                                        <c1:DataGridTextColumn        Header = "제품ID"             Binding = "{Binding PRODID}"                    HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "LOTID"              Binding = "{Binding LOTID}"                HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "CELLID"             Binding = "{Binding CELLID}"                    HorizontalAlignment="Center" />
                                        <c1:DataGridDateTimeColumn    Header = "측정시간"           Binding = "{Binding CLCT_DTTM}"                  HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "BEFORE_WEIGHT"      Binding = "{Binding BEFORE_WEIGHT}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "AFTER_WEIGHT"       Binding = "{Binding AFTER_WEIGHT}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "EL_FILLING"         Binding = "{Binding EL_FILLING}"                   HorizontalAlignment="Center" />
                                         

                                    </c1:C1DataGrid.Columns>
                                </uc:UcBaseDataGrid>
                            </Grid>

                            <Grid   Grid.Row="1" Grid.Column="1" >
                                <uc:UcBaseDataGrid Grid.Row="2" Grid.Column="1" x:Name="dgAPDDATA_VISION" Visibility="Visible" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="4" SelectionMode="MultiRange" LoadedCellPresenter="dgAPDdata_LoadedCellPresenter" UnloadedCellPresenter="dgAPDdata_UnloadedCellPresenter" >
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn        Header = "NO."                                Binding = "{Binding ROWNUM}"                    HorizontalAlignment="Center" IsReadOnly="True" Width="40" CanUserFilter="False" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "제품ID"                             Binding = "{Binding PRODID}"                    HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "LOTID"                              Binding = "{Binding LOTID}"                     HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "CELLID"                             Binding = "{Binding CELLID}"                    HorizontalAlignment="Center" />
                                        <c1:DataGridDateTimeColumn    Header = "측정시간"                           Binding = "{Binding CLCT_DTTM}"                  HorizontalAlignment="Center" /> 
                                        <c1:DataGridTextColumn        Header = "Cell Width_W1"                      Binding = "{Binding VALUE01}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W2"                      Binding = "{Binding VALUE02}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W3"                      Binding = "{Binding VALUE03}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W4"                      Binding = "{Binding VALUE04}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W5"                      Binding = "{Binding VALUE05}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W6"                      Binding = "{Binding VALUE06}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W7"                      Binding = "{Binding VALUE07}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Position(+)_L"                 Binding = "{Binding VALUE08}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Film Protrusion(+)_L"          Binding = "{Binding VALUE09}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Film Protrusion(+)_R"          Binding = "{Binding VALUE10}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Position_(+)"                  Binding = "{Binding VALUE11}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Position_(C)"                  Binding = "{Binding VALUE12}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Position_(-)"                  Binding = "{Binding VALUE13}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Length_R"                      Binding = "{Binding VALUE14}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Length_L"                      Binding = "{Binding VALUE15}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Position(-)_L"                 Binding = "{Binding VALUE16}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Film Protrusion(-)_L"          Binding = "{Binding VALUE17}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Film Protrusion(-)_R"          Binding = "{Binding VALUE18}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Corner Cutting Length(+)_R"         Binding = "{Binding VALUE19}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Corner Cutting Length(+)_L"         Binding = "{Binding VALUE20}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Corner Cutting Length(-)_R"         Binding = "{Binding VALUE21}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Corner Cutting Length(-)_L"         Binding = "{Binding VALUE22}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Position(+)_R"                 Binding = "{Binding VALUE23}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Position(-)_R"                 Binding = "{Binding VALUE24}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead length(+)_L"                   Binding = "{Binding VALUE25}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead length(-)_L"                   Binding = "{Binding VALUE26}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead length(+)_R"                   Binding = "{Binding VALUE27}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead length(-)_R"                   Binding = "{Binding VALUE28}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Bat ear (+)"                        Binding = "{Binding VALUE29}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Bat ear (-)"                        Binding = "{Binding VALUE30}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Film to Film(R)"                    Binding = "{Binding VALUE31}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Film to Film(L)"                    Binding = "{Binding VALUE32}"                   HorizontalAlignment="Center" /> 


                                    </c1:C1DataGrid.Columns>
                                </uc:UcBaseDataGrid>
                            </Grid>

                            <Grid   Grid.Row="1" Grid.Column="1" >
                                <uc:UcBaseDataGrid Grid.Row="2" Grid.Column="1" x:Name="dgAPDDATA_VISION_CNB" Visibility="Visible" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="4" SelectionMode="MultiRange" LoadedCellPresenter="dgAPDdata_LoadedCellPresenter" UnloadedCellPresenter="dgAPDdata_UnloadedCellPresenter" >
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn        Header = "NO."                                    Binding = "{Binding ROWNUM}"                    HorizontalAlignment="Center" IsReadOnly="True" Width="40" CanUserFilter="False" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "제품ID"                                 Binding = "{Binding PRODID}"                    HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "LOTID"                                  Binding = "{Binding LOTID}"                     HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "CELLID"                                 Binding = "{Binding CELLID}"                    HorizontalAlignment="Center" />
                                        <c1:DataGridDateTimeColumn    Header = "측정시간"                               Binding = "{Binding CLCT_DTTM}"                  HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W1"                          Binding = "{Binding VALUE01}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W2"                          Binding = "{Binding VALUE02}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W3"                          Binding = "{Binding VALUE03}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W4"                          Binding = "{Binding VALUE04}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Width_W5"                          Binding = "{Binding VALUE05}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Length_PKG"                        Binding = "{Binding VALUE06}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Length_DGS"                        Binding = "{Binding VALUE07}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Length_PouchToPouch_PKG"           Binding = "{Binding VALUE08}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Cell Length_PouchToPouch_DGS"           Binding = "{Binding VALUE09}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Position(+)_PKG"                   Binding = "{Binding VALUE10}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Position(+)_DGS"                   Binding = "{Binding VALUE11}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Position(-)_PKG"                   Binding = "{Binding VALUE12}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Position(-)_DGS"                   Binding = "{Binding VALUE13}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Film Protrusion(+)_PKG"            Binding = "{Binding VALUE14}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Film Protrusion(+)_DGS"            Binding = "{Binding VALUE15}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Film Protrusion(-)_PKG"            Binding = "{Binding VALUE16}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Film Protrusion(-)_DGS"            Binding = "{Binding VALUE17}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Length(+)_PKG"                     Binding = "{Binding VALUE18}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Length(+)_DGS"                     Binding = "{Binding VALUE19}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Length(-)_PKG"                     Binding = "{Binding VALUE20}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Length(-)_DGS"                     Binding = "{Binding VALUE21}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Width(+)"                          Binding = "{Binding VALUE22}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Lead Width(-)"                          Binding = "{Binding VALUE23}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Position(-)_PKG"                   Binding = "{Binding VALUE24}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Position(C)_PKG"                   Binding = "{Binding VALUE25}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Position(+)_PKG"                   Binding = "{Binding VALUE26}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Position(-)_DGS"                   Binding = "{Binding VALUE27}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Position(C)_DGS"                   Binding = "{Binding VALUE28}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Position(+)_DGS"                   Binding = "{Binding VALUE29}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Length(-)_PKG"                     Binding = "{Binding VALUE30}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Length(C)_PKG"                     Binding = "{Binding VALUE31}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Length(+)_PKG"                     Binding = "{Binding VALUE32}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Length(-)_DGS"                     Binding = "{Binding VALUE33}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Length(C)_DGS"                     Binding = "{Binding VALUE34}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Length(+)_DGS"                     Binding = "{Binding VALUE35}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Width(-)_PKG"                      Binding = "{Binding VALUE36}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Width(C)_PKG"                      Binding = "{Binding VALUE37}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Width(+)_PKG"                      Binding = "{Binding VALUE38}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Width(-)_DGS"                      Binding = "{Binding VALUE39}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Width(C)_DGS"                      Binding = "{Binding VALUE40}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "Tape Width(+)_DGS"                      Binding = "{Binding VALUE41}"                   HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "BCR_Print_Position_X"                   Binding = "{Binding VALUE42}"                   HorizontalAlignment="Center" />
                                    </c1:C1DataGrid.Columns>
