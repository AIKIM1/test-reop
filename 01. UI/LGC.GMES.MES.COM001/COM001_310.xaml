<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_310"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:cmm001="clr-namespace:LGC.GMES.MES.CMM001;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.CMM001;component/PopupFindControlStyle.xaml" />
                <!--<ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />-->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="Lot 홀딩/해제">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <c1:C1TabControl>
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='HOLD'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8" />
                                <RowDefinition />
                                <RowDefinition Height="8" />
                                <RowDefinition />
                                <RowDefinition Height="8" />
                                <RowDefinition Height="100" />
                            </Grid.RowDefinitions>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>

                            <!--조회영역-->
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="14" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='생산일', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  x:Name="txtElecType" Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='극성', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Visibility="Collapsed"/>
                                <TextBlock  Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='설비', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                
                                <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1" >
                                    <ctrlLib:LGCDatePicker  x:Name="ldpCaldateFrom"  DatepickerType="Date"/>
                                    <TextBlock  Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0"/>
                                    <ctrlLib:LGCDatePicker  x:Name="ldpCaldateTo"  DatepickerType="Date"/>
                                </StackPanel>
                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="3" x:Name="cboAreaHold"             Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="5" x:Name="cboEquipmentSegmentHold" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="7" x:Name="cboProcessHold"          Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" IsEnabled="False"/>
                                <c1:C1ComboBox   Grid.Row="1" Grid.Column="1" x:Name="cboEquipment"            Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                                
                                <!--조회 버튼-->
                                <Button     Grid.Row="0" Grid.Column="12" Grid.RowSpan="2" x:Name="btnSearchHold" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchHold_Click" />

                            </Grid>

                            <!--대상목록-->
                            <Grid Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                
                                <Grid Grid.Row="1">
                                    <Grid   Grid.Row="1" Grid.Column="1" >
                                        <!--<Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>-->
                                        <c1:C1DataGrid x:Name="dgListHold" Grid.Row="1" HeadersVisibility="None" FrozenColumnCount="2" SelectionMode="SingleRow" Margin="8,8,8,8" LoadedColumnHeaderPresenter="dgListHold_LoadedColumnHeaderPresenter" LoadedCellPresenter="dgListHold_LoadedCellPresenter">
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridTemplateColumn Name="CHK" Width="40" Header="[선택,선택]"  >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" Click="chkHold_Click" />
                                                        </DataTemplate>
                                                    </c1:DataGridTemplateColumn.CellTemplate>
                                                </c1:DataGridTemplateColumn>
                                                <c1:DataGridTextColumn      Header = "[설비,설비]"       Binding = "{Binding EQPTID}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[설비,설비]"       Binding = "{Binding EQPTNAME}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[완공일자,완공일자]"   Binding = "{Binding WIPDTTM_IN}"        HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[투입 LOT,투입 LOT]"   Binding = "{Binding PR_LOTID}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[CUTID,CUTID]"     Binding = "{Binding CUT_ID}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane수,Lane수]"  Binding = "{Binding LANE_QTY}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 1,LOTID]"  Binding = "{Binding LOTID_1}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 1,수량]"  Binding = "{Binding WIPQTY_1}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 1,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_1}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 2,LOTID]"  Binding = "{Binding LOTID_2}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 2,수량]"  Binding = "{Binding WIPQTY_2}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 2,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_2}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 3,LOTID]"  Binding = "{Binding LOTID_3}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 3,수량]"  Binding = "{Binding WIPQTY_3}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 3,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_3}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 4,LOTID]"  Binding = "{Binding LOTID_4}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 4,수량]"  Binding = "{Binding WIPQTY_4}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 4,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_4}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 5,LOTID]"  Binding = "{Binding LOTID_5}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 5,수량]"  Binding = "{Binding WIPQTY_5}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 5,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_5}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 6,LOTID]"  Binding = "{Binding LOTID_6}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 6,수량]"  Binding = "{Binding WIPQTY_6}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 6,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_6}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 7,LOTID]"  Binding = "{Binding LOTID_7}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 7,수량]"  Binding = "{Binding WIPQTY_7}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 7,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_7}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 8,LOTID]"  Binding = "{Binding LOTID_8}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 8,수량]"  Binding = "{Binding WIPQTY_8}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTextColumn      Header = "[Lane 8,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_8}"          HorizontalAlignment="Center" IsReadOnly="True" />

                                                <c1:DataGridTextColumn      Header = "[Lane 1,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_INT_1}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[Lane 2,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_INT_2}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[Lane 3,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_INT_3}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[Lane 4,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_INT_4}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[Lane 5,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_INT_5}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[Lane 6,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_INT_6}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[Lane 7,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_INT_7}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[Lane 8,판정]"  Binding = "{Binding QA_INSP_JUDG_VALUE_INT_8}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>

                                            </c1:C1DataGrid.Columns>
                                            
                                            <c1:C1DataGrid.TopRows>
                                                <c1:DataGridColumnHeaderRow />
                                                <c1:DataGridColumnHeaderRow />
                                            </c1:C1DataGrid.TopRows>
                                        </c1:C1DataGrid>
                                    </Grid>

                                </Grid>

                            </Grid>

