                                                <c1:DataGridTextColumn      Header = "[Lane 3,수량]"  Binding = "{Binding WIPQTY_3}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTemplateColumn Name="CHK_LOT_3" Width="40" Header = "[Lane 3,판정]"  >
