                                                <c1:DataGridTextColumn      Header = "[Lane 2,수량]"  Binding = "{Binding WIPQTY_2}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTemplateColumn Name="CHK_LOT_2" Width="40" Header = "[Lane 2,판정]"  >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <CheckBox IsChecked="{Binding QA_INSP_JUDG_VALUE_INT_2, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
