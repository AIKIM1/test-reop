                                                <c1:DataGridTextColumn      Header = "[Lane 1,수량]"  Binding = "{Binding WIPQTY_1}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTemplateColumn Name="CHK_LOT_1" Width="40" Header = "[Lane 1,판정]" >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <CheckBox IsChecked="{Binding QA_INSP_JUDG_VALUE_INT_1, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                        </DataTemplate>
                                                    </c1:DataGridTemplateColumn.CellTemplate>
                                                </c1:DataGridTemplateColumn>
