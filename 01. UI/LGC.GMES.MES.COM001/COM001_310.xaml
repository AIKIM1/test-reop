                                                <c1:DataGridTextColumn      Header = "[Lane 4,수량]"  Binding = "{Binding WIPQTY_4}"          HorizontalAlignment="Center" Format="###,###,##0.##" IsReadOnly="True" />
                                                <c1:DataGridTemplateColumn Name="CHK_LOT_4" Width="40" Header = "[Lane 4,판정]"  >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
