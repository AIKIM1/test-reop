<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_411"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:CMM001="clr-namespace:LGC.GMES.MES.CMM001;assembly=LGC.GMES.MES.CMM001"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="ERP 생산실적 이력조회" >
            <Grid>
                <Grid.Resources>
                    <DataTemplate x:Key="ResultTemplate">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <!--<ColumnDefinition Width="*"/>-->
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,0,0"  />
                        </Grid>
                    </DataTemplate>
                    <Style TargetType="{x:Type Button}" x:Key="grButtonStyle" BasedOn="{StaticResource ButtonIconBaseStyle}">
                        <Setter Property="Background">
                            <Setter.Value>
                                <ImageBrush ImageSource="/LGC.GMES.MES.CMM001;component/Images/btn_i_print.png" Stretch="Uniform"/>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Grid.Resources>
                <c1:C1TabControl x:Name="tbcProd">
                    <c1:C1TabItem x:Name="Prod" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='생산실적'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="347*" />
                                <RowDefinition Height="3" />
                                <RowDefinition Height="241*" />
                            </Grid.RowDefinitions>
                            <GridSplitter Grid.Row="3" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                            <!-- 조회 영역 시작 -->
                            <Grid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="4" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="8" />
                                </Grid.RowDefinitions>
                                <!-- 조회조건 시작-->
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="150" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="150" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <!-- 버튼 시작 -->
                                    <!--<ColumnDefinition Width="2"/>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="2"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="2"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="2"/>
                            <ColumnDefinition Width="Auto"/>-->
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건 text -->
                                <TextBlock Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='일자',         Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                                <TextBlock Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='Factory',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock Grid.Row="1" Grid.Column="6" Text="{Binding ConverterParameter='동',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock Grid.Row="1" Grid.Column="8" Text="{Binding ConverterParameter='처리유형',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter='처리결과',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                                <TextBlock Grid.Row="3" Grid.Column="4" Text="{Binding ConverterParameter='제품 ID',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock Grid.Row="3" Grid.Column="6" Text="{Binding ConverterParameter='Lot ID',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock Grid.Row="3" Grid.Column="8" Text="{Binding ConverterParameter=ERP_MNGT_LOTID, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock Grid.Row="3" Grid.Column="10" Text="{Binding ConverterParameter='조회건수',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                                <!-- 조회조건 1 LINE -->
                                <ctrlLib:LGCDatePicker  x:Name="dtpDateFrom" Grid.Row="1" Grid.Column="1"  DatepickerType="Date" SelectedDataTimeChanged="dtpDate_SelectedDataTimeChanged"/>
                                <TextBlock  Grid.Row="1" Grid.Column="2" Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0"/>
                                <ctrlLib:LGCDatePicker  x:Name="dtpDateTo" Grid.Row="1" Grid.Column="3" DatepickerType="Date" SelectedDataTimeChanged="dtpDate_SelectedDataTimeChanged"/>
                                <uc:UcBaseComboBox   Grid.Row="1" Grid.Column="5" x:Name="cboShop" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                <uc:UcBaseComboBox   Grid.Row="1" Grid.Column="7" x:Name="cboArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                                <uc:UcBaseComboBox   Grid.Row="1" Grid.Column="9" x:Name="cboRsltTypeCode" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                <uc:UcBaseComboBox   Grid.Row="1" Grid.Column="11" x:Name="cboErpStatusCode" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                                <!-- 조회조건 2 LINE -->
                                <uc:UcBaseRadioButton x:Name="rdoTrnfDate"   Grid.Row="3" Grid.Column="1" Style="{StaticResource SearchCondition_RadioButtonStyle}" Content="{Binding ConverterParameter='전송일자', Converter={StaticResource GlobalObjectDicConverter}}"  GroupName="searchDateType" IsChecked="True" />
                                <uc:UcBaseRadioButton x:Name="rdoPostDate"    Grid.Row="3" Grid.Column="3" Style="{StaticResource SearchCondition_RadioButtonStyle}" Content="{Binding ConverterParameter='전기일자', Converter={StaticResource GlobalObjectDicConverter}}"  GroupName="searchDateType" />
                                <TextBox x:Name="txtProdID" Grid.Row="3"  Grid.Column="5" Style="{StaticResource SearchCondition_TextBoxStyle}" />
                                <TextBox x:Name="txtLotID" Grid.Row="3"  Grid.Column="7" Style="{StaticResource SearchCondition_TextBoxStyle}" />
                                <TextBox x:Name="txtErpMngtLotID" Grid.Row="3"  Grid.Column="9" Style="{StaticResource SearchCondition_TextBoxStyle}" />
                                <c1:C1NumericBox x:Name="txtCount" Grid.Row="3"  Grid.Column="11" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="False" CornerRadius="0" Value="100" AllowNull="True" HandleUpDownKeys="False" Format="#,##0"/>
                                <!-- 조회조건 3 LINE -->
                                <uc:UcBaseCheckBox Grid.Row="5" Grid.Column="3"   x:Name="chkNonPrcsFlag" Content="{Binding ConverterParameter='미처리오류(1일이상)', Converter={StaticResource GlobalObjectDicConverter}}"  
                                        Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" IsChecked="False" />
                                <uc:UcBaseCheckBox Grid.Row="5" Grid.Column="4"  x:Name="chkExclCnclFlag" Content="{Binding ConverterParameter='실적취소제외', Converter={StaticResource GlobalObjectDicConverter}}"  
                                        Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" IsChecked="True"  />
                                <uc:UcBaseCheckBox Grid.Row="5" Grid.Column="5"   x:Name="chkActivityTrnfFlag" Content="{Binding ConverterParameter='ACT_SEND_EXCEPT', Converter={StaticResource GlobalObjectDicConverter}}"  
                                        Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" IsChecked="True" />

                                <!-- button -->
                                <Grid Grid.Row="5" Grid.Column="13"  HorizontalAlignment="Right">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="30" />
                                        <ColumnDefinition Width="auto" />
                                        <ColumnDefinition Width="4" />
                                        <ColumnDefinition Width="auto" />
                                        <ColumnDefinition Width="4" />
                                        <ColumnDefinition Width="auto" />
                                        <ColumnDefinition Width="4" />
                                        <ColumnDefinition Width="auto" />
                                        <ColumnDefinition Width="4" />
                                        <ColumnDefinition Width="auto" />
                                    </Grid.ColumnDefinitions>
                                    <uc:UcBaseButton x:Name="btnForceCancl"     Grid.Row="0" Grid.Column="1"   Content="{Binding ConverterParameter=강제취소,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnForceCancl_Click" Visibility="Collapsed" />
                                    <uc:UcBaseButton x:Name="btnResend"     Grid.Row="0" Grid.Column="3"   Content="{Binding ConverterParameter=재처리,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnResend_Click" Visibility="Collapsed" />
                                    <uc:UcBaseButton x:Name="btnSetUserConfig"     Grid.Row="0" Grid.Column="5"   Content="{Binding ConverterParameter=사용자확인설정,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnSetUserConfig_Click" Visibility="Collapsed" />
                                    <uc:UcBaseButton x:Name="btnUnSetUserConfig"     Grid.Row="0" Grid.Column="7"   Content="{Binding ConverterParameter=사용자확인해제,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnUnSetUserConfig_Click" Visibility="Collapsed" />
                                    <uc:UcBaseButton  x:Name="btnSearch"  Grid.Row="0"   Grid.Column="9"  Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click"   />

                                </Grid>



                            </Grid>


                            <!-- 조회 리스트 -->
                            <Grid Grid.Row="2" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="500*" />
                                    <ColumnDefinition Width="105*"/>
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="253*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="3" Grid.RowSpan="2"  Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                <Grid Grid.Row="0" Grid.Column="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="8" />
                                    </Grid.ColumnDefinitions>

                                    <TextBlock Grid.Column="0" Grid.ColumnSpan="2" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='ERP 생산실적', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                </Grid>
                                <GridSplitter Grid.Row="1" Grid.Column="2" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />


                                <uc:UcBaseDataGrid x:Name="dgErpHist" Grid.Row="1" Grid.Column="0"  IsReadOnly="False" HeadersVisibility="Column" SelectionMode="MultiRange"  FrozenColumnCount="5" FrozenBottomRowsCount="1" 
                                           LoadedColumnHeaderPresenter="dgErpHist_LoadedColumnHeaderPresenter" Grid.ColumnSpan="2"
                                       
                                           MouseDoubleClick="dgErpHist_MouseDoubleClick">
                                    <!--BeganEdit="dg_BeganEdit"-->
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTemplateColumn Header="선택" Name="선택" EditOnSelection="True" Width="60" >
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <RadioButton GroupName="dgProductLotChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgProductLotChoice" HorizontalAlignment="Center" Checked="dgProductLotChoice_Checked"  />
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>
                                        <c1:DataGridTextColumn      Header = "ERP_TRNF_SEQNO"                    Binding = "{Binding ERP_TRNF_SEQNO}"           IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "전기일"                   Binding = "{Binding POST_DATE}"                IsReadOnly="True" HorizontalAlignment="Center" Format="yyyy-MM-dd"/>
                                        <c1:DataGridTextColumn      Header = "TRNF_DATE"                 Binding = "{Binding TRNF_DATE}"                IsReadOnly="True" HorizontalAlignment="Center" Format="yyyy-MM-dd" />
                                        <c1:DataGridTextColumn      Header = "ERP_RSLT_TYPE_CODE"                   Binding = "{Binding ERP_RSLT_TYPE_CODE}"                   IsReadOnly="True" HorizontalAlignment="Left"/>
                                        <c1:DataGridTextColumn      Header = "처리유형"                   Binding = "{Binding ERP_RSLT_PRCS_TYPE}"                   IsReadOnly="True" HorizontalAlignment="Left"/>
                                        <c1:DataGridTextColumn      Header = "W/O"                      Binding = "{Binding WOID}"                     IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "PRODID"                   Binding = "{Binding PRODID}"                   IsReadOnly="True" HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn      Header = "LOTID"                    Binding = "{Binding LOTID}"                    IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "ERP_MNGT_LOTID"                    Binding = "{Binding ERP_MNGT_LOTID}"                    IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "MTRLID"                   Binding = "{Binding MTRLID}"                   IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "INPUT_ERP_MNGT_LOTID"                   Binding = "{Binding INPUT_ERP_MNGT_LOTID}"                   IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridNumericColumn   Header = "투입수량"    Binding = "{Binding INPUT_QTY}"        IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridNumericColumn   Header = "양품수량"    Binding = "{Binding GOOD_QTY}"        IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridNumericColumn   Header = "불량수량"    Binding = "{Binding DFCT_QTY}"        IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridNumericColumn   Header = "LOSS수량"    Binding = "{Binding LOSS_QTY}"        IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridNumericColumn   Header = "PRDT_REQ_QTY"    Binding = "{Binding PRDT_REQ_QTY}"        IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridNumericColumn   Header = "초과수량"    Binding = "{Binding ADD_QTY}"        IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridNumericColumn   Header = "차감수량"    Binding = "{Binding DDT_QTY}"        IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridTextColumn      Header = "단위"                     Binding = "{Binding UNIT_CODE}"                IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "MTRL_ADJ_FLAG"                 Binding = "{Binding MTRL_ADJ_FLAG}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "저장위치"                     Binding = "{Binding SLOC_ID}"                 IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "INPUT_SLOC_ID"                     Binding = "{Binding INPUT_SLOC_ID}"                 IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "전송일시"                 Binding = "{Binding TRNF_DTTM}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "ERP_EXEC_DTTM"                 Binding = "{Binding ERP_EXEC_DTTM}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "ERP_RSLT_TYPE_DETL"                 Binding = "{Binding ERP_RSLT_TYPE_DETL}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "취소여부"                 Binding = "{Binding CNCL_FLAG}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "ERP오류코드"              Binding = "{Binding ERP_ERR_CODE}"                 IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "ERP오류내용"              Binding = "{Binding ERP_ERR_CAUSE_CNTT}"       IsReadOnly="True" HorizontalAlignment="Left" />
                                        <c1:DataGridTextColumn      Header = "ERP_ERR_TYPE"              Binding = "{Binding ERP_ERR_TYPE}"                 IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridNumericColumn   Header = "LOT수량"    Binding = "{Binding LOT_CNT}"        IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridNumericColumn   Header = "DETL_CNT"    Binding = "{Binding DETL_CNT}"        IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                            <c1:DataGridAggregate.AggregateFunctions>
                                                <c1:DataGridAggregatesCollection>
                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                </c1:DataGridAggregatesCollection>
                                            </c1:DataGridAggregate.AggregateFunctions>
                                        </c1:DataGridNumericColumn>
                                        <c1:DataGridTextColumn      Header = "USER_CHK_FLAG"                 Binding = "{Binding USER_CHK_FLAG}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "FORCE_CNCL_ABLE_FLAG"                 Binding = "{Binding FORCE_CNCL_ABLE_FLAG}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "REPROCESS_ABLE_FLAG"                 Binding = "{Binding REPROCESS_ABLE_FLAG}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "USER_CHK_SET_ABLE_FLAG"                 Binding = "{Binding USER_CHK_SET_ABLE_FLAG}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "USER_CHK_REL_ABLE_FLAG"                 Binding = "{Binding USER_CHK_REL_ABLE_FLAG}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "TRGT_TBL_NAME"                     Binding = "{Binding TRGT_TBL_NAME}"                 IsReadOnly="True" HorizontalAlignment="Left" />
                                        <c1:DataGridTextColumn      Header = "대상Key1"                     Binding = "{Binding TRGT_KEY1}"                 IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "대상Key2"                     Binding = "{Binding TRGT_KEY2}"                 IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "대상Key3"                     Binding = "{Binding TRGT_KEY3}"                 IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "대상Key4"                     Binding = "{Binding TRGT_KEY4}"                 IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "대상Key5"                     Binding = "{Binding TRGT_KEY5}"                 IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "FACILITY_CODE"                     Binding = "{Binding FACILITY_CODE}"                 IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                    </c1:C1DataGrid.Columns>
                                    <c1:C1DataGrid.BottomRows >
                                        <c1:DataGridSummaryRow x:Name="summary" RowStyle="{StaticResource SummaryRowStyle}"/>
                                    </c1:C1DataGrid.BottomRows>
                                </uc:UcBaseDataGrid>
                                <Grid Grid.Row="0" Grid.Column="3" Grid.ColumnSpan="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="8" />
                                    </Grid.ColumnDefinitions>
