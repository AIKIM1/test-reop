<UserControl x:Class="LGC.GMES.MES.COM001.COM001_032_CRTCTL"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             x:Name="CrtImg"
             MouseLeftButtonDown="CrtImg_MouseLeftButtonDown" MouseLeftButtonUp="CrtImg_MouseLeftButtonUp" MouseMove="CrtImg_MouseMove"
             mc:Ignorable="d" Height="56" Width="80" >

    <Grid x:Name="grid1" HorizontalAlignment="Left" Height="56" VerticalAlignment="Top" Width="100">
        <Image x:Name="ImgCart" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="20" />
        <Label x:Name="lblCartType" Content="S" HorizontalAlignment="Left"  VerticalAlignment="Top" FontWeight="Bold" Background="{x:Null}" Height="25" Width="18" FontSize="11" Margin="3,-2,0,0"/>
        <Label x:Name="lblCartNo" Content="Cart No" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,19,0,0" Background="{x:Null}" Height="25" FontSize="11"/>
        <Label x:Name="lblCartID" Content="Cart ID" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Height="25" Margin="0,72,0,0" Visibility="Hidden"/>
    </Grid>


</UserControl>
