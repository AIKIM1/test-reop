                            <Button HorizontalAlignment="Center" Style="{DynamicResource Content_SearchButtonStyle}" Click="ButtonBase_OnClick" />
                            <Button HorizontalAlignment="Center" Style="{DynamicResource Content_SearchButtonStyle}" Click="ButtonBase_OnClick" />
                            <Button HorizontalAlignment="Center" Style="{DynamicResource Content_SearchButtonStyle}" Click="ButtonBase_OnClick" />
                        </StackPanel>
                    </Grid>
                </Grid>
