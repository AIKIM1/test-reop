                                                    <c1:DataGridTextColumn        Header = "PALLETID"     Binding = "{Binding BOXID}"   HorizontalAlignment="Left" IsReadOnly="True"  ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                                    <c1:DataGridTextColumn        Header = "LOTID"     Binding = "{Binding LOTID}"   HorizontalAlignment="Left" IsReadOnly="True"/>
                                                    <c1:DataGridTextColumn        Header = "제품코드"     Binding = "{Binding PRODID}"   HorizontalAlignment="Left" IsReadOnly="True"/>
                                                    <c1:DataGridTextColumn        Header = "제품명"     Binding = "{Binding PRODNAME}"   HorizontalAlignment="Left" IsReadOnly="True"/>
                                                    <c1:DataGridTextColumn       Header = "수량"       Binding = "{Binding MOVE_ORD_QTY}"         HorizontalAlignment="Left" IsReadOnly="True"/>
                                                    <c1:DataGridTextColumn       Header = "반품일자"       Binding = "{Binding MOVE_STRT_DTTM}"         HorizontalAlignment="Center" IsReadOnly="True"/>
