                                    <Button x:Name="btnReturnInfoSearch"  Grid.Column="9" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" HorizontalAlignment="Center" Click="btnReturnInfoSearch_Click"/>
                                </Grid>

                                <Grid Grid.Row="2" Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="31"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>

                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock  Grid.Row="0" Grid.Column="1"  Style="{StaticResource Content_ModuleTitleStyle}">
                                         <Run Text="{Binding ConverterParameter='반품이력조회', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                    </TextBlock>

                                    <Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="3" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <Grid Grid.Row="1">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="8"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="8"/>
                                        </Grid.ColumnDefinitions>

                                        <Grid Grid.Row="1" Grid.Column="1">
                                            <c1:C1DataGrid Grid.Row="0" x:Name="dgReturnInfo" HeadersVisibility="Column" SelectionMode="SingleCell" >

