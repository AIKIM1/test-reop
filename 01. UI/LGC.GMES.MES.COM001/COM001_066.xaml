                                        </StackPanel>

                                        <c1:C1ComboBox      Grid.Column="2" x:Name="cboReturnLocation"      DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" Margin="0"/>
