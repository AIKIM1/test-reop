                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>

                                            <ctrlLib:LGCDatePicker Grid.Column="0" x:Name="ldpDatePickerReturnFrom"  DatepickerType="Date" />
