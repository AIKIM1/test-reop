                                                                <Button  x:Name="btnLotDelete" Content="{Binding ConverterParameter=삭제,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnLotDelete_Click"/>
                                                            </DataTemplate>
                                                        </c1:DataGridTemplateColumn.CellTemplate>
                                                    </c1:DataGridTemplateColumn>

                                                </c1:C1DataGrid.Columns>


                                            </c1:C1DataGrid>

                                            

                                        </Grid>
                                        <Grid Grid.Row="2" Grid.Column="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
