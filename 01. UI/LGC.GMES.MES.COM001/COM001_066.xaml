                                                <Run Text="{Binding ConverterParameter='비고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                            </TextBlock>
                                            <RichTextBox  Grid.Column="2" x:Name="rtxReturnRemark" Margin="0" Grid.ColumnSpan="9"/>
                                        </Grid>
                                       

                                    </Grid>


                                </Grid>

                            </Grid>

                        </Grid>

