                                                    <c1:DataGridTextColumn        Header = "AREANAME"     Binding = "{Binding AREANAME}"   HorizontalAlignment="Left" IsReadOnly="True"/>
                                                    <c1:DataGridTextColumn        Header = "LOTID"     Binding = "{Binding LOTID}"   HorizontalAlignment="Left" IsReadOnly="True"/>
                                                    <c1:DataGridTextColumn        Header = "제품코드"     Binding = "{Binding PRODID}"   HorizontalAlignment="Left" IsReadOnly="True"/>
                                                    <c1:DataGridTextColumn        Header = "제품명"     Binding = "{Binding PRODNAME}"   HorizontalAlignment="Left" IsReadOnly="True"/>
                                                    <c1:DataGridTextColumn        Header = "FROM_FACTORYID"     Binding = "{Binding FROM_SHOPID}"   HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn        Header = "FROM_AREAID"     Binding = "{Binding FROM_AREAID}"   HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn       Header = "FROM_EQSGID"       Binding = "{Binding FROM_EQSGID}"         HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn       Header = "FROM_PROCID"       Binding = "{Binding FROM_PROCID}"         HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn       Header = "TO_FACTORYID"       Binding = "{Binding TO_SHOPID}"         HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn       Header = "TO_AREAID"       Binding = "{Binding TO_AREAID}"         HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn       Header = "TO_PROCID"       Binding = "{Binding TO_PROCID}"         HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn       Header = "TO_EQSGID"       Binding = "{Binding TO_EQSGID}"         HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn       Header = "TO_SLOC_ID"       Binding = "{Binding TO_SLOC_ID}"         HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn       Header = "MOVE_STRT_DTTM"       Binding = "{Binding MOVE_STRT_DTTM}"         HorizontalAlignment="Center" IsReadOnly="True"/>
                                                    <c1:DataGridTextColumn       Header = "MOVE_END_DTTM"       Binding = "{Binding MOVE_END_DTTM}"         HorizontalAlignment="Center" IsReadOnly="True"/>
                                                    <c1:DataGridTextColumn       Header = "수량"       Binding = "{Binding WIPQTY}"         HorizontalAlignment="Right" IsReadOnly="False"/>
                                                    <c1:DataGridTextColumn       Header = "HOLD"       Binding = "{Binding WIPHOLD}"         HorizontalAlignment="Right" IsReadOnly="False"/>
                                                    <c1:DataGridTextColumn       Header = "INSUSER"       Binding = "{Binding INSUSER}"         HorizontalAlignment="Center" IsReadOnly="True"/>
                                                    <!--<c1:DataGridTextColumn       Header = "비고"       Binding = "{Binding REMARK}"         HorizontalAlignment="Center" IsReadOnly="False"/>-->

                                                    <c1:DataGridTemplateColumn Name="Delete" Width="Auto" Header="삭제">
                                                        <c1:DataGridTemplateColumn.CellTemplate>
