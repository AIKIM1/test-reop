<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_125"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="111*"/>
            <ColumnDefinition Width="913*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="11*"/>
            <RowDefinition Height="757*"/>
        </Grid.RowDefinitions>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="전산재고" Grid.RowSpan="2" Grid.ColumnSpan="2">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <c1:C1TabControl x:Name="tabStckCnt">
                    <c1:C1TabItem x:Name="tabStckCntSnap" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='전산재고'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="51" />
                            </Grid.RowDefinitions>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <!--조회영역-->
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35"/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="18" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <!--ROW1-->
                                <TextBlock              Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="1" x:Name="cboAreaShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboAreaShot_SelectedValueChanged"/>
                                <TextBlock              Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='GROUP', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="3" x:Name="cboStckCntGrShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME1" SelectedValuePath="CBO_CODE" SelectedValueChanged="cboStckCntGrShot_SelectedValueChanged" />
                                <TextBlock              Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="5" x:Name="cboProcShot" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" SelectionChanged="cboProcShot_SelectionChanged"/>
                                <TextBlock              Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="7" x:Name="cboEqsgShot" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                                <TextBlock              Grid.Row="0" Grid.Column="8" Text="{Binding ConverterParameter='PRJT_NAME', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="0" Grid.Column="9" x:Name="txtPrjtNameShot" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" />
                                <TextBlock              Grid.Row="0" Grid.Column="10" Text="{Binding ConverterParameter='제품', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="0" Grid.Column="11" x:Name="txtProdShot" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" />
                               
                                <TextBlock              Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='기준월', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:LGCDatePicker  Grid.Row="1" Grid.Column="1" x:Name="ldpMonthShot"  DatepickerType="Month" SelectedDataTimeChanged="ldpMonthShot_SelectedDataTimeChanged" />
                                <TextBlock              Grid.Row="1" Grid.Column="2" Text="{Binding ConverterParameter='차수', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="1" Grid.Column="3" x:Name="cboStockSeqShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStockSeqShot_SelectedValueChanged"/>
                                <TextBlock              Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='극성', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Visibility="Collapsed"/>
                                <c1:C1ComboBox          Grid.Row="1" Grid.Column="5" x:Name="cboElecTypeShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" Visibility="Collapsed"/>
                                <TextBlock              Grid.Row="1" Grid.Column="6" Text="{Binding ConverterParameter='STCK_CNT_EXCL_FLAG', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="1" Grid.Column="7" x:Name="cboExclFlagShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                <TextBlock              Grid.Row="1" Grid.Column="8" Text="{Binding ConverterParameter='PalletID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="1" Grid.Column="9" x:Name="txtLotIdShot" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" PreviewKeyDown="txtLotIdShot_PreviewKeyDown" />
                                <Button     Grid.Row="1" Grid.Column="13" x:Name="btnDegreeClose"       Content="{Binding ConverterParameter=차수마감,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDegreeClose_Click" Height="29"/>
                                <Button     Grid.Row="1" Grid.Column="13" x:Name="btnDegreeCloseCancel" Content="{Binding ConverterParameter=마감취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDegreeCloseCancel_Click" Height="29" Visibility="Collapsed"/>
                                <Button     Grid.Row="1" Grid.Column="14" x:Name="btnDegreeAdd"         Content="{Binding ConverterParameter=차수추가,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDegreeAdd_Click" Height="29" Margin="4,0,0,0"/>
                                <!--조회 버튼-->
                                
                                <Button     Grid.Row="1" Grid.Column="15" x:Name="btnSearchShot" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchShot_Click" Margin="4,0,0,0"/>


                            </Grid>

                            <!--리스트-->
                            <Grid Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='리스트', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <TextBlock Grid.Column="3" x:Name="txtStckCntCmplFlagShot" Text="{Binding Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_Number_LabelStyle}"/>
                                </Grid>

                                <c1:C1DataGrid x:Name="dgListShot" Grid.Row="1" IsReadOnly="False" HeadersVisibility="Column" FrozenColumnCount="2" SelectionMode="SingleRow" Margin="8" LoadedColumnHeaderPresenter="dgListShot_LoadedColumnHeaderPresenter" >
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTemplateColumn Name="CHK" Width="40">
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center"  Click="chkHeader_SNAP_Click"/>
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "동"                         Binding = "{Binding AREANAME}"                 HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "공정"                       Binding = "{Binding PROCNAME}"                 HorizontalAlignment="Left"       IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "라인"                       Binding = "{Binding EQSGNAME}"                 HorizontalAlignment="Left"       IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "WH_ID"                      Binding = "{Binding WH_ID}"                    HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "RACK_ID"                    Binding = "{Binding RACK_ID}"                  HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "극성"                       Binding = "{Binding PRDT_CLSS_NAME}"           HorizontalAlignment="Center"     IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "PRJT_NAME"                   Binding = "{Binding PRJT_NAME}"                HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "제품ID"                      Binding = "{Binding PRODID}"                   HorizontalAlignment="Left"       IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "PROD_VER_CODE"               Binding = "{Binding PROD_VER_CODE}"            HorizontalAlignment="Center"     IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridNumericColumn   Width="Auto" Header = "Lane 수"                     Binding = "{Binding LANE_QTY}"                 HorizontalAlignment="Right"      IsReadOnly="True" Format="###,###,##0" Visibility="Collapsed"/>
                                        <c1:DataGridNumericColumn   Width="Auto" Header = "ROLLPRESS_COUNT"             Binding = "{Binding ROLLPRESS_SEQNO}"          HorizontalAlignment="Right"      IsReadOnly="True" Format="###,###,##0" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "LOTTYPE"                     Binding = "{Binding LOTYNAME}"                 HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "단위"                        Binding = "{Binding LOTUID}"                   HorizontalAlignment="Center"    IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "WIP_QLTY_TYPE_NAME"          Binding = "{Binding WIP_QLTY_TYPE_NAME}"       HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "PalletID"                       Binding = "{Binding LOTID}"                    HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "재공상태"                     Binding = "{Binding WIPSNAME}"                 HorizontalAlignment="Left"        IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "HOLD여부"                     Binding = "{Binding WIPHOLD}"                  HorizontalAlignment="Center"      IsReadOnly="True"/>
                                        <c1:DataGridNumericColumn   Width="Auto" Header = "수량"                         Binding = "{Binding WIP_QTY}"                  HorizontalAlignment="Right"      IsReadOnly="True" Format="###,###,##0"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "시점"                         Binding = "{Binding SNAP_DTTM}"                HorizontalAlignment="Center"     IsReadOnly="True"  Format="yyyy-MM-dd HH:mm:ss"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "SNAP_ASSY_LOT"                Binding = "{Binding SNAP_ASSY_LOT}"            HorizontalAlignment="Left"      IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_EXCL_FLAG"           Binding = "{Binding STCK_CNT_EXCL_FLAG}"      HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_EXCL_USER"           Binding = "{Binding STCK_CNT_EXCL_USER}"      HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_EXCL_DTTM"           Binding = "{Binding STCK_CNT_EXCL_DTTM}"       HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_EXCL_NOTE"           Binding = "{Binding STCK_CNT_EXCL_NOTE}"       HorizontalAlignment="Left"      IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "용량등급"           Binding = "{Binding CAPA_GRD_CODE}"       HorizontalAlignment="Left"      IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "불량사유"           Binding = "{Binding RESNNAME}"       HorizontalAlignment="Left"      IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "재고특이사항"           Binding = "{Binding WIP_NOTE}"       HorizontalAlignment="Left"      IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "생산특이사항"           Binding = "{Binding WIP_NOTE2}"       HorizontalAlignment="Left"      IsReadOnly="True"/>
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>

                            <!--제외버튼-->
                            <Border Grid.Row="5" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"/>
                            <Grid Grid.Row="7">
                                <Grid.RowDefinitions >
                                    <RowDefinition Height="4" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="4" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <TextBlock  Grid.Row="1" Grid.Column="0" x:Name="tblExcludeNote_SNAP" Text="{Binding ConverterParameter='비고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox    Grid.Row="1" Grid.Column="1" x:Name="txtExcludeNote_SNAP" Style="{StaticResource SearchCondition_TextBoxStyle}" Height="Auto"  AcceptsReturn="True" />
                                <Button     Grid.Row="1" Grid.Column="3" x:Name="btnExclude_SNAP"           Content="{Binding ConverterParameter='제외',        Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnExclude_SNAP_Click"/>
                                <Button     Grid.Row="1" Grid.Column="3" x:Name="btnExclude_SNAP_Cancel"    Content="{Binding ConverterParameter='제외취소',    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnExclude_SNAP_Cancel_Click" Visibility="Collapsed"/>
                                <Button     Grid.Row="1" Grid.Column="5" x:Name="btnRowReSet"               Content="{Binding ConverterParameter=선택재고변경,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnRowReSet_Click"/>
                            </Grid>
                        </Grid>
                    </c1:C1TabItem>
                    <c1:C1TabItem x:Name="tabStckCntRslt" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='실물재고조사'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="51"/>
                            </Grid.RowDefinitions>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>

                            <!--조회영역-->
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="1*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0"   Grid.RowSpan="2" Grid.ColumnSpan="18" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <!--ROW1-->
                                <TextBlock              Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="1" x:Name="cboAreaUpload" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboAreaUpload_SelectedValueChanged"/>
                                <TextBlock              Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='GROUP', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="3" x:Name="cboStckCntGrUpload" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME1" SelectedValuePath="CBO_CODE" SelectedValueChanged="cboStckCntGrUpload_SelectedValueChanged" />
                                <TextBlock              Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="5" x:Name="cboProcUpload" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  SelectionChanged="cboProcUpload_SelectionChanged"/>
                                <TextBlock              Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="7" x:Name="cboEqsgUpload" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                                <TextBlock              Grid.Row="0" Grid.Column="8" Text="{Binding ConverterParameter='PRJT_NAME', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="0" Grid.Column="9" x:Name="txtPrjtNameUpload" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" />
                                <TextBlock              Grid.Row="0" Grid.Column="10" Text="{Binding ConverterParameter='제품', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="0" Grid.Column="11" x:Name="txtProdUpload" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" />
                                <StackPanel Grid.Row="1" Grid.Column="10" Orientation="Horizontal" HorizontalAlignment="Right" Visibility="Collapsed">
                                    <RadioButton Name="rdoLot"  Margin="8,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=LOT}"         GroupName="RadioButton_Group" Style="{StaticResource SearchCondition_RadioButtonStyle}"  IsChecked="True" />
                                    <RadioButton Name="rdoBox"  Margin="8,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=BOX}"         GroupName="RadioButton_Group" Style="{StaticResource SearchCondition_RadioButtonStyle}"  IsChecked="False"/>
                                </StackPanel>

                                <TextBlock              Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='기준월', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <ctrlLib:LGCDatePicker  Grid.Row="1" Grid.Column="1" x:Name="ldpMonthUpload"  DatepickerType="Month" SelectedDataTimeChanged="ldpMonthUpload_SelectedDataTimeChanged"/>
                                <TextBlock              Grid.Row="1" Grid.Column="2" Text="{Binding ConverterParameter='차수', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox          Grid.Row="1" Grid.Column="3" x:Name="cboStockSeqUpload" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStockSeqUpload_SelectedValueChanged"/>
                                <TextBlock              Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='극성', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Visibility="Collapsed"/>
                                <c1:C1ComboBox          Grid.Row="1" Grid.Column="5" x:Name="cboElecTypeUpload" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" Visibility="Collapsed"/>
                                <TextBlock              Grid.Row="1" Grid.Column="6" Text="{Binding ConverterParameter='PalletID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="1" Grid.Column="7" x:Name="txtLotIdUpload" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" PreviewKeyDown="txtLotIdUpload_PreviewKeyDown" />

                                <Button                 Grid.Row="1" Grid.Column="13" x:Name="btnUploadFile"        Content="{Binding ConverterParameter=파일,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnUploadFile_Click" Height="29"/>
                                <Button                 Grid.Row="1" Grid.Column="14" x:Name="btnUploadSave"        Content="{Binding ConverterParameter=저장,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnUploadSave_Click" Height="29" Margin="4,0,0,0"/>
                                <!--조회 버튼-->
                                <Button     Grid.Row="1" Grid.Column="15"  x:Name="btnSearchStock" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchStock_Click" Margin="4,0,0,0"/>
                            </Grid>

                            <!--리스트-->
                            <Grid Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
