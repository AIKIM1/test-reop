<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_302"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:CMM001="clr-namespace:LGC.GMES.MES.CMM001;assembly=LGC.GMES.MES.CMM001"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="1024" Loaded="UserControl_Loaded" >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <Grid.Resources>
            <DataTemplate x:Key="ItemTemplate">
                <WrapPanel>
                    <Image Width="138" Height="138" Stretch="Fill" Source="{Binding}" Margin="3,3,3,3" />
                </WrapPanel>
            </DataTemplate>
            <DataTemplate x:Key="ResultTemplate">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <!--<ColumnDefinition Width="*"/>-->
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>

        </Grid.Resources>

        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="공정별 생산실적 조회" Grid.ColumnSpan="2">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="8" />
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>

                <!--백그라운드용-->
                <Border     Grid.Row="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                <!-- 조회영역 -->
                <Grid Grid.Row="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="4" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="2" />
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="2"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="4"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="8" />
                        <ColumnDefinition Width="1.5*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="0.5*" />
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="8" />
                    </Grid.ColumnDefinitions>

                    <!--조회조건-->
                    <!--1 row-->
                    <Grid Grid.Row="1" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                        Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <uc:UcBaseComboBox  x:Name="cboArea"  Grid.Column="1" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>
                    <Grid Grid.Row="1" Grid.Column="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                                    Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <uc:UcBaseComboBox  x:Name="cboEquipmentSegment"  Grid.Column="1" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>
                    <Grid Grid.Row="1" Grid.Column="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                           Grid.Column="0" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <uc:UcBaseComboBox  x:Name="cboProcess"  Grid.Column="1" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>
                    <Grid Grid.Row="1" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                             Grid.Column="0" Text="{Binding ConverterParameter='설비', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <uc:UcBaseComboBox  x:Name="cboEquipment"  Grid.Column="1" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>
                    <!--3 row-->
                    <Grid Grid.Row="3" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Column="0" Text="{Binding ConverterParameter='작업일', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <StackPanel Grid.Column="1" Orientation="Horizontal">
                            <ctrlLib:LGCDatePicker x:Name="dtpDateFrom"   DatepickerType="Date"   />
                            <TextBlock                                    Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment='Center' />
                            <ctrlLib:LGCDatePicker x:Name="dtpDateTo"     DatepickerType="Date"   />
                        </StackPanel>
                    </Grid>
                    <Grid Grid.Row="3" Grid.Column="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                             Grid.Column="0" Text="{Binding ConverterParameter='공급LOT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox       x:Name="txtPRLOTID"     Grid.Column="1"  Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtLotId_KeyDown" />
                    </Grid>
                    <Grid Grid.Row="3" Grid.Column="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                             Grid.Column="0" Text="{Binding ConverterParameter='경로유형', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <uc:UcBaseComboBox x:Name="cboFlowType"    Grid.Column="1"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>
                    <Grid Grid.Row="3" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                             Grid.Column="0" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox       x:Name="txtLotId"       Grid.Column="1"  Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtLotId_KeyDown" />
                    </Grid>
                    <Grid Grid.Row="3" Grid.Column="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                             Grid.Column="0" Text="{Binding ConverterParameter='극성', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <uc:UcBaseComboBox x:Name="cboElecType"    Grid.Column="1"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>
                    <!--5 row-->
                    <Grid Grid.Row="5" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                                      Grid.Column="0" Text="{Binding ConverterParameter='모델', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <CMM001:AutoCompleteTextBox x:Name="txtModlId"  Grid.Column="1" Threshold="2" DelayTime="100" Height="30" Width="180"  GotFocus="txtModlId_GotFocus" HorizontalAlignment="Left" VerticalAlignment="Stretch"/>
                    </Grid>
                    <Grid Grid.Row="5" Grid.Column="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                                      Grid.Column="0" Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox       x:Name="txtProdId"               Grid.Column="1"  Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtLotId_KeyDown" />
                    </Grid>
                    <Grid Grid.Row="5" Grid.Column="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                                      Grid.Column="0" Text="{Binding ConverterParameter='프로젝트명', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox       x:Name="txtPrjtName"             Grid.Column="1"  Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtLotId_KeyDown" />
                    </Grid>
                    <Grid Grid.Row="5" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock                              Grid.Column="0" Text="{Binding ConverterParameter='생산구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <uc:UcBaseComboBox  x:Name="cboProductDiv"  Grid.Column="1" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>
                    <Grid Grid.Row="5" Grid.Column="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <uc:UcBaseCheckBox      x:Name="chkProc"                 Grid.Column="1"  Content="{Binding ConverterParameter='진행중인Lot 포함', Converter={StaticResource GlobalObjectDicConverter}}"  
                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" HorizontalAlignment="Right" />
                        <uc:UcBaseCheckBox      x:Name="chkModel"                Grid.Column="3"   Content="{Binding ConverterParameter='모델합산', Converter={StaticResource GlobalObjectDicConverter}}"  
                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" HorizontalAlignment="Left" Checked="chkModel_Checked" Unchecked="chkModel_Unchecked" Margin="10,0,0,0" />
                    </Grid>

                    <!--조회 버튼-->
                    <uc:UcBaseButton     Grid.Row="5" Grid.Column="7" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                </Grid>
                <!--조회조건 End-->

                <Grid Grid.Row="2">
                    <Border Grid.Column="0"  Style="{StaticResource Content_InputForm_GroupBorderStyle}" Margin="0,3,0,-3"/>

                    <c1:C1TabControl x:Name="tbcList" Grid.Column="0" Margin="3" SelectionChanged="tbcList_SelectionChanged">
                        <!--LOT 리스트-->
                        <c1:C1TabItem x:Name="cTabLot" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='LOT'}" >
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <!--<Border Grid.Row="1" Grid.Column="0"  Style="{StaticResource Content_InputForm_TitleBorderStyle}"/>
                                <Border Grid.Row="1" Grid.Column="1" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_TitleBorderStyle}"/>-->
                                <GridSplitter Grid.Row="1" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                <Grid Grid.Row="0" Grid.Column="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <!--백그라운드용-->
                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                    <TextBlock Grid.Row="0" Grid.ColumnSpan="4" Style="{StaticResource Content_ModuleTitleStyle}" Margin="15,9,0,8"><Run Text="{Binding ConverterParameter='LOT리스트', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <uc:UcBaseDataGrid x:Name="dgLotList" 
                                                   Grid.Row="1" 
                                                   IsReadOnly="False" 
                                                   HeadersVisibility="None"
                                                   FrozenTopRowsCount="3"
                                                   FrozenColumnCount="4"
                                                   FrozenBottomRowsCount="2"
                                                   SelectionMode="MultiRange" 
                                                   LoadedCellPresenter="dgLotList_LoadedCellPresenter">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTemplateColumn  Header="선택" EditOnSelection="True" Name="CHK" Width="60" >
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <RadioButton GroupName="dgProductLotChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgProductLotChoice" HorizontalAlignment="Center" Checked="dgProductLotChoice_Checked" />
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>
                                            <c1:DataGridTextColumn      Header = "투입 Lot"                    Binding = "{Binding PR_LOTID}"                   IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "COATING LINE"                Binding = "{Binding COATING_LINE}"               IsReadOnly="True" HorizontalAlignment="Left" Visibility="Collapsed" />
                                            <c1:DataGridNumericColumn   Header = "불량태그수"                  Binding = "{Binding DFCT_TAG_QTY}"               IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn   Header = "설비수집TAG수"               Binding = "{Binding EQPT_DFCT_TAG_QTY}"          IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "LOTID"                       Binding = "{Binding LOTID}"                      IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "WIPSEQ"                      Binding = "{Binding WIPSEQ}"                     IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTemplateColumn  Header = "이력" Width="Auto" Name="HIST_LIST"  >
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Button x:Name="btnHist" Style="{DynamicResource Content_SearchButtonStyle}" VerticalAlignment="Center" Click="btnHist_Click"  />
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>
                                            <c1:DataGridTextColumn      Header = "Carrier ID"                  Binding = "{Binding CSTID}"                      IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "제품"                        Binding = "{Binding PRODID}"                     IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "제품명"                      Binding = "{Binding PRODNAME}"                   IsReadOnly="True" HorizontalAlignment="Left" />
                                            <c1:DataGridTextColumn      Header = "모델"                        Binding = "{Binding MODLID}"                     IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "PJT"                         Binding = "{Binding PRJT_NAME}"                  IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "버전"                        Binding = "{Binding ASSY_PROD_VER_CODE}"         IsReadOnly="True" HorizontalAlignment="Center" />
                                            <!-- 2024.11.29 이동주 E20240904-000991 CP Version 표시 추가 -->
                                            <c1:DataGridTextColumn      Header = "CP_VERSION"                  Binding = "{Binding CP_VER_CODE}"                IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "LOT 유형"                    Binding = "{Binding LOTYNAME}"                   IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "단위"                        Binding = "{Binding UNIT_CODE}"                  IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "REWINDER"                    Binding = "{Binding EQPT_END_PSTN_ID}"           IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "극성"                        Binding = "{Binding WRK_TYPE}"                   IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "구분"                        Binding = "{Binding PRDT_CLSS_CODE}"             IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn   Header = "[투입,투입량,투입량]"        Binding = "{Binding PRE_PROC_INPUT_QTY}"         IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[투입,소진량,소진량]"        Binding = "{Binding EQPT_INPUT_QTY}"             IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[투입,LOSS(L),WIP_QTY_DIFF]" Binding = "{Binding PRE_PROC_LOSS_QTY}"          IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[투입,LOSS(L),고정]"         Binding = "{Binding FIX_LOSS_QTY}"               IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[투입,LOSS(L),자공정]"       Binding = "{Binding CURR_PROC_LOSS_QTY}"         IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[투입,LOSS(L),JAMMING]"      Binding = "{Binding JAMMING_COUNT}"              IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[투입,LOSS(L),파단총횟수]"   Binding = "{Binding OCCR_COUNT}"                 IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[투입,잔량,잔량]"            Binding = "{Binding RMN_QTY}"                    IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[완성,생산량,생산량]"        Binding = "{Binding INPUT_QTY}"                  IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[완성,양품량,양품량]"        Binding = "{Binding WIPQTY_ED}"                  IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[완성,불량량,불량량]"        Binding = "{Binding CNFM_DFCT_QTY}"              IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[완성,LOSS량,LOSS량]"        Binding = "{Binding CNFM_LOSS_QTY}"              IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[완성,물품청구,물품청구]"    Binding = "{Binding CNFM_PRDT_REQ_QTY}"          IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />

                                            <c1:DataGridTextColumn      Header = "[투입 TAB COUNT,투입 TAB COUNT,투입 TAB COUNT]"     Binding = "{Binding INPUT_TAB_COUNT}"            IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0"  Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "[배출 Tab Count,배출 Tab Count,배출 Tab Count]"     Binding = "{Binding END_TAB_COUNT}"              IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0"  Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "[고정LOSS,고정LOSS,고정LOSS]"                       Binding = "{Binding FIX_LOSS_QTY_VD}"            IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0"  Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "[파단횟수,타발 전,타발 전]"                         Binding = "{Binding PRE_PUNCH_CUTOFF_COUNT}"     IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "[파단횟수,타발 후,타발 후]"                         Binding = "{Binding AFTER_PUNCH_CUTOFF_COUNT}"   IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0" />
                                            <c1:DataGridNumericColumn   Header = "재투입"                                             Binding = "{Binding RE_INPUT_QTY}"               IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0"/>
                                            <c1:DataGridNumericColumn   Header = "REWINDER_TAB_COUNT"                                 Binding = "{Binding REWINDER_TAB_COUNT}"         IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0"/>
                                            <c1:DataGridNumericColumn   Header = "VISION_COUNT_QTY"                                   Binding = "{Binding VISION_COUNT_QTY}"           IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0"/>
                                            <c1:DataGridTextColumn      Header = "설비"                                               Binding = "{Binding EQPTNAME}"                   IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "STORAGE_PERIOD_TIME"                                Binding = "{Binding STORAGE_PERIOD_TIME}"        IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "작업조"                      Binding = "{Binding SHFT_NAME}"                  IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "작업자"                      Binding = "{Binding WRK_USER_NAME}"              IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "작업일"                      Binding = "{Binding CALDATE}"                    IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "시작유형"                    Binding = "{Binding IFMODE_ST}"                  IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "완료유형"                    Binding = "{Binding IFMODE_ED}"                  IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "시작시간"                    Binding = "{Binding STARTDTTM}"                  IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "장비완료일시"                Binding = "{Binding EQPT_END_DTTM}"              IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "실적확정일시"                Binding = "{Binding ENDDTTM}"                    IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "가동시간(분)"                Binding = "{Binding RUN_MINUTE}"                 IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "경로유형"                    Binding = "{Binding FLOW_NAME}"                  IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "작업방식"                    Binding = "{Binding RWK_TYPE_NAME}"              IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "작업유형"                    Binding = "{Binding IRREGL_PROD_LOT_TYPE_NAME}"  IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "COATING LINE"                Binding = "{Binding COATING_LINE_NOTCHING}"      IsReadOnly="True" HorizontalAlignment="Left" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "W/O"                         Binding = "{Binding WOID}"                       IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "W/O상세"                     Binding = "{Binding WO_DETL_ID}"                 IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "AREAID"                      Binding = "{Binding AREAID}"                     IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "공정"                        Binding = "{Binding PROCID}"                     IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
