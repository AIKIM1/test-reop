<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012" x:Class="LGC.GMES.MES.COM001.COM001_015"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d"
             d:DesignHeight="768" d:DesignWidth="1024"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="11*" />
            <RowDefinition Height="757*" />
        </Grid.RowDefinitions>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="설비 LOSS 현황" Grid.RowSpan="2">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition />
                </Grid.RowDefinitions>
                <c1:C1TabControl>
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='설비 LOSS 현황'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="4" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="8" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="13" />
                                    <ColumnDefinition Width="*" MinWidth="350" />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="30" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="13" />
                                </Grid.ColumnDefinitions>
                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="5" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                                <!--조회조건-->
                                <!--ROW1-->
                                <Grid Grid.Row="1" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="3*" />
                                        <ColumnDefinition Width="6*" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <uc:UcBaseComboBox Grid.Row="0" Grid.Column="1" x:Name="cboArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                </Grid>
                                <Grid Grid.Row="1" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="3*" />
                                        <ColumnDefinition Width="6*" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <uc:UcBaseComboBox Grid.Row="0" Grid.Column="1" x:Name="cboEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                </Grid>
                                <Grid Grid.Row="1" Grid.Column="5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="3*" />
                                        <ColumnDefinition Width="6*" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <uc:UcBaseComboBox Grid.Row="0" Grid.Column="1" x:Name="cboProcess" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                </Grid>
                                <Grid Grid.Row="1" Grid.Column="7">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="3*" />
                                        <ColumnDefinition Width="6*" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='설비', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <uc:UcBaseComboBox Grid.Row="0" Grid.Column="1" x:Name="cboEquipment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                </Grid>
                                <Grid Grid.Row="1" Grid.Column="11">
                                    <uc:UcBaseButton Grid.Row="0" Grid.Column="11" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                                </Grid>
                                <Grid Grid.Row="3" Grid.Column="1" x:Name="grdFromToDate">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="3*" />
                                        <ColumnDefinition Width="6*" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='기간', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <StackPanel Orientation="Horizontal" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3">
                                        <ctrlLib:LGCDatePicker Grid.Row="1" Grid.Column="1" x:Name="ldpDateFrom" DatepickerType="Date" />
                                        <TextBlock Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0" />
                                        <ctrlLib:LGCDatePicker Grid.Row="1" Grid.Column="1" x:Name="ldpDateTo" DatepickerType="Date" />
                                    </StackPanel>
                                </Grid>
                                <Grid Grid.Row="3" Grid.Column="1" x:Name="grdWorkDate" Visibility="Collapsed">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="3*" />
                                        <ColumnDefinition Width="6*" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column="0" Text="{Binding ConverterParameter='날짜', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <ctrlLib:LGCDatePicker Grid.Column="1" x:Name="ldpDateWorkDate" DatepickerType="Date" />
                                </Grid>
                                <Grid Grid.Row="3" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="3*" />
                                        <ColumnDefinition Width="6*" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column="0" Text="{Binding ConverterParameter='작업조', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <uc:UcBaseComboBox Grid.Column="1" x:Name="cboShift" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboShift_SelectedValueChanged" />
                                </Grid>
                                <Grid Grid.Row="3" Grid.Column="9" Grid.ColumnSpan="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <!-- TEST/개발 -->
                                    <!--<Grid Grid.Row="0" Grid.Column="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock              Grid.Column="0" Text="{Binding ConverterParameter='TEST_DEV', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <uc:UcBaseCheckBox               Grid.Column="1" x:Name="chkTestDev" />
                                    </Grid>-->
                                    <!-- TEST/개발 -->
                                    <uc:UcBaseButton Grid.Row="0" Grid.Column="1" x:Name="btnTestLossHist" Content="{Binding ConverterParameter='LOSS_HIST_IN_TESTLOSS', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnTestLossHist_Click" VerticalAlignment="Bottom" />
                                </Grid>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Border Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                <Border BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                            </Grid>
                            <Grid Grid.Row="2">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="700" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>
                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                <Border BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                <Grid Grid.Row="0" Grid.Column="0">
                                    <Border Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                    <Border BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                    <TextBlock Style="{StaticResource Content_ModuleTitleStyle}" MouseLeftButtonDown="TextBlock_MouseLeftButtonDown">
                                        <Run Text="{Binding ConverterParameter='설비LOSS현황', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" />
                                    </TextBlock>
                                </Grid>
                                <Grid Grid.Row="1" Grid.Column="0">
                                    <Border Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                    <Border BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                    <uc:UcBaseDataGrid x:Name="dgLossList" Grid.Row="1" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="2" LoadedCellPresenter="dgLossList_LoadedCellPresenter" MouseDoubleClick="dgLossList_MouseDoubleClick" Margin="8">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn     Header="설비명"         Binding="{Binding EQPTNAME}"       HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="HORIZONTALHIERARCHI" />
                                            <c1:DataGridNumericColumn  Header="생산가동율"     Binding="{Binding PRODOPERRATE}"   HorizontalAlignment="Right"  Format="#,##0.00" ctrlLib:DataGridMergeExtension.MergeMode="HORIZONTALHIERARCHI" Visibility="Collapsed" />
                                            <c1:DataGridNumericColumn  Header="가동율"         Binding="{Binding RUNRATE}"        HorizontalAlignment="Right"  Format="#,##0.00" ctrlLib:DataGridMergeExtension.MergeMode="HORIZONTALHIERARCHI" Visibility="Collapsed" />
                                            <c1:DataGridNumericColumn  Header="시간가동율"     Binding="{Binding TIME_RUNRATE}"   HorizontalAlignment="Right"  Format="#,##0.00" ctrlLib:DataGridMergeExtension.MergeMode="HORIZONTALHIERARCHI" />
                                            <c1:DataGridTextColumn     Header="LEVEL1"         Binding="{Binding UPPR_LOSS_NAME}" HorizontalAlignment="Center" ctrlLib:DataGridMergeExtension.MergeMode="HORIZONTALHIERARCHI" />
                                            <c1:DataGridTextColumn     Header="LEVEL2"         Binding="{Binding LOSS_NAME}"      HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn  Header="건수"           Binding="{Binding LOSSCNT}"        HorizontalAlignment="Right"  Format="###,###,###" />
                                            <c1:DataGridNumericColumn  Header="발생시간(분)"   Binding="{Binding LOSSMINUTE}"     HorizontalAlignment="Right"  Format="###,###,###" />
                                            <c1:DataGridNumericColumn  Header="발생시간(초)"   Binding="{Binding LOSS_SEC}"       HorizontalAlignment="Right"  Format="###,###,###" />
                                            <c1:DataGridNumericColumn  Header="LOSSRATE"       Binding="{Binding LOSSRATE}"       HorizontalAlignment="Center" Format="#,##0.0" />
                                            <c1:DataGridTextColumn     Header="EQPTID"         Binding="{Binding EQPTID}"         HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn     Header="LOSS_CODE"      Binding="{Binding LOSS_CODE}"      HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn     Header="UPPR_LOSS_CODE" Binding="{Binding UPPR_LOSS_CODE}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn     Header="SHFT_WRK_DATE"  Binding="{Binding SHFT_WRK_DATE}"  HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridDateTimeColumn Header="SHFT_STRTTIME"  Binding="{Binding SHFT_STRTTIME}"  HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridDateTimeColumn Header="SHFT_ENDTIME"   Binding="{Binding SHFT_ENDTIME}"   HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn     Header="SHFT_SEARCH"    Binding="{Binding SHFT_SEARCH}"    HorizontalAlignment="Center" Visibility="Collapsed" />
                                        </c1:C1DataGrid.Columns>
                                    </uc:UcBaseDataGrid>
                                </Grid>

                                <GridSplitter Grid.Column="1" Grid.Row="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                <Grid Grid.Row="0" Grid.Column="2" Grid.RowSpan="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1*" />
                                        <RowDefinition Height="8" />
                                        <RowDefinition Height="1*" />
                                    </Grid.RowDefinitions>
                                    <c1:C1TabControl Grid.Row="0">
                                        <c1:C1TabItem x:Name="tabLossDetail" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='LOSS별상세내역'}">
                                            <Grid>
                                                <Border Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                                <Border BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                                <uc:UcBaseDataGrid x:Name="dgLossDetail" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="2" SelectionMode="SingleRow" MouseDoubleClick="dgLossDetail_MouseDoubleClick" Margin="8">
                                                    <c1:C1DataGrid.Columns>
                                                        <c1:DataGridTextColumn Header="PJT"                     Binding="{Binding PRJT_NAME}"               HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="LOTID"                   Binding="{Binding LOTID}"                   HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="작업일자"                Binding="{Binding WRK_DATE1}"               HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="시작시간"                Binding="{Binding START_TIME}"              HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="종료시간"                Binding="{Binding END_TIME}"                HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridNumericColumn Header="총시간(초)"              Binding="{Binding LOSS_SEC}"                HorizontalAlignment="Center" Visibility="Visible"  Format="###,##0" />
                                                        <c1:DataGridTextColumn Header="LOSS명"                  Binding="{Binding LOSS_NAME}"               HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="원인설비"                Binding="{Binding CAUSE_EQPTNAME}"          HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="부동내용"                Binding="{Binding LOSS_DETL_NAME}"          HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="TROUBLE명"               Binding="{Binding TRBL_NAME}"               HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="TRBL_KWD"                Binding="{Binding TRBL_KWD}"                HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="비고"                    Binding="{Binding LOSS_NOTE}"               HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="현상"                    Binding="{Binding SYMP_NAME}"               HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="원인"                    Binding="{Binding CAUSE_NAME}"              HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="조치"                    Binding="{Binding REPAIR_NAME}"             HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="작업자"                  Binding="{Binding WRK_USERID}"              HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn Header="TROUBLE_CAUSE_EQPTID"    Binding="{Binding UNIT_TRBL_EQPTID}"        HorizontalAlignment="Center" Visibility="Collapsed" />
                                                        <c1:DataGridTextColumn Header="TROUBLE_CAUSE_EQPTNAME"  Binding="{Binding TROUBLE_CAUSE_EQPTNAME}"  HorizontalAlignment="Center" Visibility="Collapsed" />
                                                        <c1:DataGridTextColumn Header="TROUBLE_CAUSE_ALARMCODE" Binding="{Binding UNIT_TRBL_CODE}"          HorizontalAlignment="Center" Visibility="Collapsed" />
                                                        <c1:DataGridTextColumn Header="TROUBLE_CAUSE_ALARMNAME" Binding="{Binding TROUBLE_CAUSE_ALARMNAME}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                                        <c1:DataGridTextColumn Header="TROUBLE_CAUSE_EQPT_STAT" Binding="{Binding TROUBLE_CAUSE_EQPT_STAT}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                                        <c1:DataGridTextColumn Header="EQPTID"                  Binding="{Binding EQPTID}"                  HorizontalAlignment="Center" Visibility="Collapsed" />
                                                        <c1:DataGridTextColumn Header="EIOSTAT"                 Binding="{Binding EIOSTAT}"                 HorizontalAlignment="Center" Visibility="Collapsed" />
                                                        <c1:DataGridTextColumn Header="EQGRID"                  Binding="{Binding EQGRID}"                  HorizontalAlignment="Center" Visibility="Collapsed" />
                                                        <c1:DataGridTextColumn Header="EQSGID"                  Binding="{Binding EQSGID}"                  HorizontalAlignment="Center" Visibility="Collapsed" />
                                                    </c1:C1DataGrid.Columns>
                                                </uc:UcBaseDataGrid>
                                            </Grid>
                                        </c1:C1TabItem>
                                        <c1:C1TabItem x:Name="tabLossRate" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='항목별비율'}">
                                            <Grid>
                                                <Border Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                                <Border BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                                <uc:UcBaseDataGrid x:Name="dgLossRate" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="SingleRow" Margin="8">
                                                    <c1:C1DataGrid.Columns>
                                                        <c1:DataGridTextColumn     Header="원인설비"      Binding="{Binding CAUSE_EQPTNAME}"                         HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn     Header="비율"          Binding="{Binding OCCUR_EQPTID_PROPORTION_BY_LOSS_RATE}"   HorizontalAlignment="Right"  Visibility="Visible" />
                                                        <c1:DataGridNumericColumn  Header="발생시간(초)"  Binding="{Binding OCCUR_EQPTID_LOSS_SEC}"                  HorizontalAlignment="Right"  Format="###,###,###" Visibility="Visible" />
                                                        <c1:DataGridTextColumn     Header="부동내용"      Binding="{Binding LOSS_DETL_NAME}"                         HorizontalAlignment="Center" Visibility="Visible" />
                                                        <c1:DataGridTextColumn     Header="비율"          Binding="{Binding LOSS_DETL_CODE_PROPORTION_BY_LOSS_RATE}" HorizontalAlignment="Right"  Visibility="Visible" />
                                                        <c1:DataGridNumericColumn  Header="발생시간(초)"  Binding="{Binding LOSS_DETL_CODE_LOSS_SEC}"                HorizontalAlignment="Right"  Format="###,###,###" Visibility="Visible" />
                                                    </c1:C1DataGrid.Columns>
                                                </uc:UcBaseDataGrid>
                                            </Grid>
                                        </c1:C1TabItem>
                                    </c1:C1TabControl>

                                    <GridSplitter Grid.Row="1" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                    <!-- 설비별 특이사항 -->
                                    <Grid Grid.Row="2">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="34" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid Grid.Row="0">
                                            <Border Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                            <Border BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                            <TextBlock Style="{StaticResource Content_ModuleTitleStyle}">
                                                <Run Text="{Binding ConverterParameter='설비별 특이사항', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" />
                                            </TextBlock>
                                        </Grid>
                                        <Grid Grid.Row="1" Grid.Column="0">
                                            <Border Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                            <Border BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                            <uc:UcBaseDataGrid x:Name="dgEQPTDetail" Grid.Row="1" Grid.Column="2" IsReadOnly="False" HeadersVisibility="Column" FrozenColumnCount="2" SelectionMode="SingleRow" Margin="8">
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn Header="EQPTID"    Binding="{Binding EQPTID}"    HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn Header="WRK_DATE"  Binding="{Binding WRK_DATE}"  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn Header="SHFT_ID"   Binding="{Binding SHIFT_ID}"  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn Header="EQPT_NOTE" Binding="{Binding EQPT_NOTE}" HorizontalAlignment="Center" />
                                                </c1:C1DataGrid.Columns>
                                            </uc:UcBaseDataGrid>
                                        </Grid>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>
                    </c1:C1TabItem>

                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='일자별 Loss 현황'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="90" />
                                <RowDefinition Height="8" />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <!--조회영역-->
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="45" />
                                    <RowDefinition Height="45" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="31*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" /> <!--오화백 2023 02 20 -->
                                    <ColumnDefinition Width="122*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>
                                <!--백그라운드용 오화백 2023 02 20 -->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="14" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                                <!--조회조건-->
                                <!--ROW1-->
                                <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <uc:UcBaseComboBox Grid.Row="0" Grid.Column="1" x:Name="cboAreaDaily" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                <TextBlock Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <uc:UcBaseComboBox Grid.Row="0" Grid.Column="3" x:Name="cboEquipmentSegmentDaily" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                <TextBlock Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <uc:UcBaseComboBox Grid.Row="0" Grid.Column="5" x:Name="cboProcessDaily" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                <TextBlock Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='설비', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <uc:UcBaseComboBox Grid.Row="0" Grid.Column="7" x:Name="cboEquipmentDaily" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                <!--Machine 설비 오화백 2023 02 20-->
                                <Grid Grid.Row="0" Grid.Column="8" x:Name="Machine" Visibility="Collapsed">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="2*" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock          Grid.Column="0" Text="{Binding ConverterParameter='Machine', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
