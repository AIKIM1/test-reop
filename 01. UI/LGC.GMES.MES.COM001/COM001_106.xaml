                                    <c1:DataGridTextColumn  Header="[등록일시,등록일시]"       Binding = "{Binding REG_DTTM}"                    HorizontalAlignment="Center" IsReadOnly="True" />
                                    <c1:DataGridTextColumn  Header="[취소자,취소자]"           Binding = "{Binding CNCL_USER_NAME}"              HorizontalAlignment="Center" IsReadOnly="True" />
                                    <c1:DataGridTextColumn  Header="[취소일시,취소일시]"       Binding = "{Binding CNCL_DTTM}"                   HorizontalAlignment="Center" IsReadOnly="True" />
                                    <c1:DataGridTextColumn  Header="[ERP,ERP 전송 상태]"        Binding = "{Binding ERP_STAT_CODE}"               HorizontalAlignment="Center" IsReadOnly="True" />
                                    <c1:DataGridTextColumn  Header="[ERP,ERP 전송 번호]"       Binding = "{Binding ERP_TRNF_SEQNO}"              HorizontalAlignment="Center" IsReadOnly="True" />
                                    <c1:DataGridTextColumn  Header="[ERP,ERP 처리 결과]"       Binding = "{Binding ERP_ERR_CODE}"                HorizontalAlignment="Center" IsReadOnly="True" />
                                    <c1:DataGridTextColumn  Header="[ERP,ERP 오류 내용]"       Binding = "{Binding ERP_ERR_CAUSE_CNTT}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                </c1:C1DataGrid.Columns>
                                <c1:C1DataGrid.TopRows>
                                    <c1:DataGridColumnHeaderRow />
                                    <c1:DataGridColumnHeaderRow />
                                </c1:C1DataGrid.TopRows>
                            </c1:C1DataGrid>
                        </Grid>

                        <!--등록취소버튼-->
                        <Border Grid.Row="5" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                        <Grid Grid.Row="5">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="8"/>
                            </Grid.ColumnDefinitions>

                            <Button x:Name="btnCancelFCS"  Grid.Column="1" Content="{Binding ConverterParameter='등록취소', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnCancelFCS_Click"/>

                        </Grid>
                        <!--비용등록-->
                        <Grid Grid.Row="7" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>

                            <!--백그라운드용-->
                            <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                            <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                            <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='비용등록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                            <Grid Grid.Row ="1">
                                <Grid.RowDefinitions >
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="60"/>
                                    <RowDefinition Height="8" />
                                </Grid.RowDefinitions>

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="5" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                <TextBlock  Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='FACTORY', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="3" Text="{Binding ConverterParameter='TRAY ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="6" Text="{Binding ConverterParameter='제품', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="9" Text="{Binding ConverterParameter='수량', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                                <TextBlock  Grid.Row="2" Grid.Column="0" Text="{Binding ConverterParameter='저장위치', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="2" Grid.Column="3" Text="{Binding ConverterParameter='비용창고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="2" Grid.Column="6" Text="{Binding ConverterParameter='BizWF 처리 부서', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="2" Grid.Column="9" Text="{Binding ConverterParameter='BizWF 처리 담당자', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="2" Grid.Column="12" Text="{Binding ConverterParameter='BizWF 처리 예정일', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                                <c1:C1ComboBox   Grid.Row="1" Grid.Column="1"  x:Name="cboShop"        Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                <TextBox         Grid.Row="1" Grid.Column="4"  x:Name="txtRegTrayFCS"  Style="{StaticResource SearchCondition_TextBoxStyle}" CharacterCasing="Upper"  />
                                <TextBox         Grid.Row="1" Grid.Column="7"  x:Name="txtRegProdFCS"  Style="{StaticResource SearchCondition_TextBoxStyle}" CharacterCasing="Upper"  />
                                <c1:C1NumericBox Grid.Row="1" Grid.Column="10" x:Name="txtQty"         Style="{StaticResource C1NumericBoxStyle}" Visibility="{Binding TEXTVISIBLE}" ShowButtons="False" CornerRadius="0" AllowNull="True" HandleUpDownKeys="False" Format="#,##0.##" />

                                <c1:C1ComboBox   Grid.Row="2" Grid.Column="1" x:Name="cboStockLocation"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                <c1:C1ComboBox   Grid.Row="2" Grid.Column="4" x:Name="cboCostFCS"        Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                <Grid  Grid.Row="2" Grid.Column="7">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <TextBox Grid.Row="0" Grid.Column="0" x:Name="txtDeptFCS"  Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtDept_KeyDown" IsReadOnly="True"/>
                                    <Button  Grid.Row="0" Grid.Column="1" x:Name="btnDeptFCS"    Content="{Binding ConverterParameter=Title17,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_SearchButtonStyle}" Click="btnDept_Click" HorizontalAlignment="Left" Visibility="Collapsed"/>
                                </Grid>

                                <Grid  Grid.Row="2" Grid.Column="10">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <TextBox Grid.Row="0" Grid.Column="0" x:Name="txtPrscUserFCS"  Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtPrscUserFCS_KeyDown" LostFocus="txtPrscUserFCS_LostFocus"/>
                                    <Button  Grid.Row="0" Grid.Column="1" x:Name="btnPrscUserFCS"    Content="{Binding ConverterParameter=Title17,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_SearchButtonStyle}" Click="btnPrscUserFCS_Click"  HorizontalAlignment="Left"/>
                                </Grid>

                                <ctrlLib:LGCDatePicker    Grid.Row="2" Grid.Column="13" x:Name="ldpCmplDateRegisterFCS"  DatepickerType="Date"/>

                                <TextBlock  Grid.Row="3" Grid.Column="0" Text="{Binding ConverterParameter='비고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox    Grid.Row="3" Grid.Column="1" x:Name="txtFCSNote"          Style="{StaticResource SearchCondition_TextBoxStyle}" AcceptsReturn="True" Grid.ColumnSpan="13" Height="Auto"  />
                            </Grid>

                        </Grid>

                        <!--등록버튼-->
                        <Border Grid.Row="9" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                        <Grid Grid.Row="9">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="8"/>
                            </Grid.ColumnDefinitions>

                            <Button x:Name="btnSaveRegisterFCS"  Grid.Column="1" Content="{Binding ConverterParameter='등록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnSaveRegisterFCS_Click"/>

                        </Grid>
                    </Grid>
                </c1:C1TabItem>
                </c1:C1TabControl>
            </Grid>
        </ctrlLib:LGCLayout>
        <ctrlLib:LoadingIndicator x:Name="loadingIndicator" Grid.Row="999" Grid.Column="999" />
    </Grid>
</UserControl>
