                                                                    <TextBox  x:Name="txtVal01" Text="{Binding CLCTVAL01, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource Content_InputForm_TextBoxStyle}" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0" TextChanged="CLCTVAL_TextChanged"/>
                                                                    <ComboBox x:Name="cbVal01" Text="{Binding CLCTVAL01}" Visibility="{Binding COMBOVISIBLE}" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" Loaded="cbVal_Loaded">
                                                                        <ComboBoxItem >OK</ComboBoxItem>
                                                                        <ComboBoxItem >NG</ComboBoxItem>
                                                                    </ComboBox>
                                                                </StackPanel>
                                                            </DataTemplate>
                                                        </c1:DataGridTemplateColumn.CellTemplate>
                                                    </c1:DataGridTemplateColumn>
                                                    <c1:DataGridTemplateColumn    Header = "2"   EditOnSelection="True" Name="CLCTVAL02"  Width="70" HorizontalAlignment="Center" CanUserFilter="False" CanUserSort="False" CanUserResize="False" >
                                                        <c1:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <StackPanel>
                                                                    <!--<c1:C1NumericBox x:Name="txtVal02" Value="{Binding CLCTVAL02, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="False" CornerRadius="0" AllowNull="True" Format="#,##0.###" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0"/>-->
                                                                    <TextBox  x:Name="txtVal02" Text="{Binding CLCTVAL02, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource Content_InputForm_TextBoxStyle}"  KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0" TextChanged="CLCTVAL_TextChanged"/>
                                                                    <ComboBox x:Name="cbVal02" Text="{Binding CLCTVAL02}" Visibility="{Binding COMBOVISIBLE}" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" Loaded="cbVal_Loaded">
                                                                        <ComboBoxItem >OK</ComboBoxItem>
                                                                        <ComboBoxItem >NG</ComboBoxItem>
                                                                    </ComboBox>
                                                                </StackPanel>
                                                            </DataTemplate>
                                                        </c1:DataGridTemplateColumn.CellTemplate>
                                                    </c1:DataGridTemplateColumn>
                                                    <c1:DataGridTemplateColumn    Header = "3"   EditOnSelection="True" Name="CLCTVAL03"  Width="70" HorizontalAlignment="Center" CanUserFilter="False" CanUserSort="False" CanUserResize="False" >
                                                        <c1:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <StackPanel>
                                                                    <!--<c1:C1NumericBox x:Name="txtVal03" Value="{Binding CLCTVAL03, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="False" CornerRadius="0" AllowNull="True" Format="#,##0.###" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0"/>-->
                                                                    <TextBox  x:Name="txtVal03" Text="{Binding CLCTVAL03, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource Content_InputForm_TextBoxStyle}"  KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0" TextChanged="CLCTVAL_TextChanged"/>
                                                                    <ComboBox x:Name="cbVal03" Text="{Binding CLCTVAL03}" Visibility="{Binding COMBOVISIBLE}" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" Loaded="cbVal_Loaded">
                                                                        <ComboBoxItem >OK</ComboBoxItem>
                                                                        <ComboBoxItem >NG</ComboBoxItem>
                                                                    </ComboBox>
                                                                </StackPanel>
                                                            </DataTemplate>
                                                        </c1:DataGridTemplateColumn.CellTemplate>
                                                    </c1:DataGridTemplateColumn>
                                                    <c1:DataGridTemplateColumn    Header = "4"   EditOnSelection="True" Name="CLCTVAL04"  Width="70" HorizontalAlignment="Center" CanUserFilter="False" CanUserSort="False" CanUserResize="False" >
                                                        <c1:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <StackPanel>
                                                                    <!--<c1:C1NumericBox x:Name="txtVal04" Value="{Binding CLCTVAL04, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="False" CornerRadius="0" AllowNull="True" Format="#,##0.###" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0"/>-->
                                                                    <TextBox  x:Name="txtVal04" Text="{Binding CLCTVAL04, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource Content_InputForm_TextBoxStyle}"  KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0" TextChanged="CLCTVAL_TextChanged"/>
                                                                    <ComboBox x:Name="cbVal04" Text="{Binding CLCTVAL04}" Visibility="{Binding COMBOVISIBLE}" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" Loaded="cbVal_Loaded">
                                                                        <ComboBoxItem >OK</ComboBoxItem>
                                                                        <ComboBoxItem >NG</ComboBoxItem>
                                                                    </ComboBox>
                                                                </StackPanel>
                                                            </DataTemplate>
                                                        </c1:DataGridTemplateColumn.CellTemplate>
                                                    </c1:DataGridTemplateColumn>
                                                    <c1:DataGridTemplateColumn    Header = "5"   EditOnSelection="True" Name="CLCTVAL05"  Width="70" HorizontalAlignment="Center" CanUserFilter="False" CanUserSort="False" CanUserResize="False" >
                                                        <c1:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <StackPanel>
                                                                    <!--<c1:C1NumericBox x:Name="txtVal05" Value="{Binding CLCTVAL05, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="False" CornerRadius="0" AllowNull="True" Format="#,##0.###" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0"/>-->
                                                                    <TextBox  x:Name="txtVal05" Text="{Binding CLCTVAL05, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource Content_InputForm_TextBoxStyle}"  KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0" TextChanged="CLCTVAL_TextChanged"/>
                                                                    <ComboBox x:Name="cbVal05" Text="{Binding CLCTVAL05}" Visibility="{Binding COMBOVISIBLE}" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" Loaded="cbVal_Loaded">
                                                                        <ComboBoxItem >OK</ComboBoxItem>
                                                                        <ComboBoxItem >NG</ComboBoxItem>
                                                                    </ComboBox>
                                                                </StackPanel>
                                                            </DataTemplate>
                                                        </c1:DataGridTemplateColumn.CellTemplate>
                                                    </c1:DataGridTemplateColumn>
                                                    <c1:DataGridTemplateColumn    Header = "6"   EditOnSelection="True" Name="CLCTVAL06"  Width="70" HorizontalAlignment="Center" CanUserFilter="False" CanUserSort="False" CanUserResize="False" >
                                                        <c1:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <StackPanel>
                                                                    <!--<c1:C1NumericBox x:Name="txtVal06" Value="{Binding CLCTVAL06, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="False" CornerRadius="0" AllowNull="True" Format="#,##0.###" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0"/>-->
                                                                    <TextBox  x:Name="txtVal06" Text="{Binding CLCTVAL06, Mode=TwoWay}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource Content_InputForm_TextBoxStyle}"  KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" GotFocus="CLCTVAL_GotFocus" LostFocus="CLCTVAL_LostFocus" BorderThickness="0" TextChanged="CLCTVAL_TextChanged"/>
                                                                    <ComboBox x:Name="cbVal06" Text="{Binding CLCTVAL06}" Visibility="{Binding COMBOVISIBLE}" KeyDown="CLCTVAL_KeyDown" PreviewKeyDown="CLCTVAL_PreviewKeyDown" Loaded="cbVal_Loaded">
                                                                        <ComboBoxItem >OK</ComboBoxItem>
                                                                        <ComboBoxItem >NG</ComboBoxItem>
                                                                    </ComboBox>
                                                                </StackPanel>
                                                            </DataTemplate>
                                                        </c1:DataGridTemplateColumn.CellTemplate>
                                                    </c1:DataGridTemplateColumn>
                                                    <c1:DataGridTemplateColumn    Header = "7"   EditOnSelection="True" Name="CLCTVAL07"  Width="70" HorizontalAlignment="Center" CanUserFilter="False" CanUserSort="False" CanUserResize="False" >
                                                        <c1:DataGridTemplateColumn.CellTemplate>
