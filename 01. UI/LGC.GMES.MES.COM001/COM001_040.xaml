<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_040"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="Shop간이동 인계">
            <Grid>

                <c1:C1TabControl  Grid.Row="0" >
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='FACTORY간이동(인계)'}" >
                        <Grid Grid.Row="0">

                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="98"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Grid x:Name="InfoArea" Grid.Row="1">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="13"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="13" />
                                </Grid.ColumnDefinitions>

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="14"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="14"/>
                                </Grid.RowDefinitions>

                                <Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Grid.RowSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <TextBlock  Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='인계처', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center"/>

                                <Grid Grid.Row="1" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Column="0" Text="{Binding ConverterParameter='FACTORY', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Column="1" x:Name="cboShopFrom" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="4">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Column="1" x:Name="cboAreaFrom" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Column="0" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox        Grid.Column="1" x:Name="txtLotID_Out" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtLotID_Out_KeyDown" PreviewKeyDown="txtLotID_Out_PreviewKeyDown " />

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="6">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Column="0" Text="{Binding ConverterParameter='SKIDID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox        Grid.Column="1" x:Name="txtSKIDID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtSKIDID_KeyDown" />

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="7">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Column="0" x:Name="tbCarrierId" Text="{Binding ConverterParameter='CARRIER ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox        Grid.Column="1" x:Name="txtCARRIERID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtCARRIERID_KeyDown" />

                                </Grid>


                                <TextBlock      Grid.Row="3" Grid.Column="1" Text="{Binding ConverterParameter='인수처', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center"/>

                                <Grid Grid.Row="3" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='FACTORY', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" x:Name="cboShop" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="3" Grid.Column="4">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" x:Name="cboArea_Plant" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="3" Grid.Column="5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" x:Name="cboEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="3" Grid.Column="6" HorizontalAlignment="Right">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                    </Grid.ColumnDefinitions>
                                    <CheckBox   Grid.Row="0"  Grid.Column="1" Content="{Binding ConverterParameter=포장카드발행, Converter={StaticResource GlobalObjectDicConverter}}"    
                                                Style="{StaticResource SearchCondition_CheckBoxStyle}" x:Name="chkPrint" IsChecked="False" Margin="15,0" Width ="Auto"  FontWeight="Bold" />
                                </Grid>


                                <Grid Grid.Row="3" Grid.Column="7" HorizontalAlignment="Right">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <Button Grid.Row="0" Grid.Column="1"  x:Name="btnRefresh"    Content="{Binding ConverterParameter=초기화, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Cursor="Hand" Click="btnRefresh_Click" />
                                    <Button Grid.Row="0" Grid.Column="3"  x:Name="btnOut" Content="{Binding ConverterParameter=인계처리, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnOut_Click" />
                                </Grid>


                            </Grid>

                            <Grid x:Name="OutGridArea" Grid.Row="4" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회결과', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                                <Grid   Grid.Row="1" Grid.Column="1" >
                                    <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <c1:C1DataGrid x:Name="dgMoveList" IsReadOnly="True" HeadersVisibility="Column" CanUserRemoveRows="True" CanUserAddRows="True" SelectionMode="SingleRow" Margin="8" ClipboardPasteMode="None">

                                        <c1:C1DataGrid.Resources>
                                            <DataTemplate x:Key="ResultTemplate">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock Grid.Column="1" Text="{Binding}" Style="{StaticResource Content_InputForm_TextBlockStyle}" HorizontalAlignment="Center"></TextBlock>
                                                </Grid>
                                            </DataTemplate>
                                        </c1:C1DataGrid.Resources>

                                        <c1:C1DataGrid.Columns>

                                            <c1:DataGridTextColumn Header = "LOTID"              Binding = "{Binding LOTID}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "PRJT_NAME"          Binding = "{Binding PROJECTNAME}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "제품코드"           Binding = "{Binding PRODID}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "공정"               Binding = "{Binding PROCNAME}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "극성"               Binding = "{Binding ELECNAME}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "전극버전"           Binding = "{Binding PROD_VER_CODE}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "수량(J/R)"          Binding = "{Binding WIPQTY}" HorizontalAlignment="Center" Format="###,###,##0.##" />
                                            <c1:DataGridTextColumn Header = "수량(PC)"           Binding = "{Binding WIPQTY2}" HorizontalAlignment="Center" Format="###,###,##0.##" />
                                            <c1:DataGridTextColumn Header = "단위"               Binding = "{Binding UNIT_CODE}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "라인"               Binding = "{Binding EQSGNAME}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "상태"               Binding = "{Binding WIPSTAT}" HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "공정"               Binding = "{Binding PROCID}" HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "설비"               Binding = "{Binding EQSGID}" HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "시장유형"           Binding = "{Binding MKT_TYPE_CODE}" HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "시장유형"           Binding = "{Binding MKT_TYPE_NAME}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "LotType"            Binding = "{Binding LOTTYPE}" HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "LotType"            Binding = "{Binding LOTTYPE_NAME}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "SpecialFlag"        Binding = "{Binding SPCL_FLAG}" HorizontalAlignment="Center"   Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "SpecialLine"        Binding = "{Binding RSV_EQSGID_LIST}" HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "PILOT_PROD_DIVS_CODE"     Binding = "{Binding PILOT_PROD_DIVS_CODE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "투입 구간 롤 LANE 방향"     Binding = "{Binding EM_SECTION_ROLL_LANE_DIRCTN}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "적재 중량1"     Binding = "{Binding LOAD_WEIGHT1}" HorizontalAlignment="Right" Format="#,##0.#" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "적재 중량2"     Binding = "{Binding LOAD_WEIGHT2}" HorizontalAlignment="Right" Format="#,##0.#" Visibility="Collapsed"/>
                                            <c1:DataGridTemplateColumn Name="Delete" Width="Auto" Header="삭제" >
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Button  x:Name="btnDelete" Width="Auto" Content="{Binding ConverterParameter=삭제, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                                 Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDelete_Click"/>
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>

                                        </c1:C1DataGrid.Columns>

                                        <c1:C1DataGrid.BottomRows>
                                            <c1:DataGridSummaryRow x:Name="summary" RowStyle="{StaticResource SummaryRowStyle}" />
                                        </c1:C1DataGrid.BottomRows>

                                    </c1:C1DataGrid>
                                </Grid>
                            </Grid>
                        </Grid>
                    </c1:C1TabItem>

                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='인계이력조회'}" >
                        <Grid x:Name="TitleArea" Grid.Row="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="98"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>


                            <Grid x:Name="SearchArea" Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="14"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="14"/>
                                </Grid.RowDefinitions>

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="13" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="13" />
                                </Grid.ColumnDefinitions>

                                <Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Grid.RowSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <TextBlock      Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='인수처', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center"/>

                                <Grid Grid.Row="1" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='FACTORY', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboFromShop" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>


                                <Grid Grid.Row="1" Grid.Column="4">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboFromArea_Plant" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>


                                <Grid Grid.Row="1" Grid.Column="5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right"  Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboFromEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>


                                <Grid Grid.Row="1" Grid.Column="6">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='상태', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboStat" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="7">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                    <TextBox       Grid.Row="0"  Grid.Column="1" x:Name="txtProd_ID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100"
                                                   KeyDown="txtProd_ID_KeyDown"/>

                                </Grid>

                                <TextBlock      Grid.Row="3" Grid.Column="1" Text="{Binding ConverterParameter='인계처', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center"/>

                                <Grid Grid.Row="3" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='FACTORY', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboFromShop2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="3" Grid.Column="4" Grid.ColumnSpan="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="16*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='기간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1">
                                        <ctrlLib:LGCDatePicker  x:Name="dtpDateFrom"  DatepickerType="Date"/>
                                        <TextBlock  Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0"/>
                                        <ctrlLib:LGCDatePicker  x:Name="dtpDateTo"  DatepickerType="Date"/>
                                    </StackPanel>

                                </Grid>

                                <!-- [E20231101-000795] MES UI improvement from pain point -->
                                <Grid Grid.Row="3" Grid.Column="5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboFromArea_Plant2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <!-- [E20231101-000795] MES UI improvement from pain point -->
                                <Grid Grid.Row="3" Grid.Column="6">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right"  Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboFromEquipmentSegment2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>


                                <Grid Grid.Row="3" Grid.Column="7" Grid.ColumnSpan="2" HorizontalAlignment="Right">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>

                                    <Button x:Name="btnReprint"     Grid.Row="0" Grid.Column="1"   Content="{Binding ConverterParameter=재발행,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnReprint_Click"/>
                                    <Button x:Name="btnCancel"      Grid.Row="0" Grid.Column="3"   Content="{Binding ConverterParameter=인계취소,  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnCancel_Click" />
                                    <Button x:Name="btnSearch"      Grid.Row="0" Grid.Column="5"   Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click"/>

                                </Grid>


                            </Grid>

                            <Grid x:Name="GridArea" Grid.Row="3">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="7*" />
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="3*" />
                                </Grid.ColumnDefinitions>

                                <Grid Grid.Row="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <Grid   Grid.Row="1" Grid.Column="1" >
                                        <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                        <c1:C1DataGrid x:Name="dgMove_Master" IsReadOnly="True" HeadersVisibility="None" FrozenColumnCount="2" FrozenTopRowsCount="2"  SelectionMode="SingleRow" Margin="8" ClipboardPasteMode="None">
                                            <c1:C1DataGrid.Columns>
                                                <!--<c1:DataGridTemplateColumn Name="CHK" Width="50" Header="선택" >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" Click="CheckBox_Click" />
                                                        </DataTemplate>
                                                    </c1:DataGridTemplateColumn.CellTemplate>
                                                </c1:DataGridTemplateColumn>-->
                                                <c1:DataGridTemplateColumn  Header="[선택,선택]" EditOnSelection="True" Name="CHK" Width="Auto" HorizontalAlignment="Center" >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <RadioButton GroupName="dgMove_MasterChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgMove_MasterChoice" HorizontalAlignment="Center" Checked="dgMove_MasterChoice_Checked"  />
                                                        </DataTemplate>
                                                    </c1:DataGridTemplateColumn.CellTemplate>
                                                </c1:DataGridTemplateColumn>

                                                <c1:DataGridTextColumn Header = "[이동ID,이동ID]"        Binding = "{Binding MOVE_ORD_ID}" HorizontalAlignment="Left"/>
