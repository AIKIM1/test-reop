<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_203"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="활성화 대차 재구성">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <c1:C1TabControl>
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='대차 재구성'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="10" />
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="8" />
                            </Grid.RowDefinitions>
                            <!--조회영역-->
                            <Grid Grid.Row="1" Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="9" />
                                    <RowDefinition Height="33" />
                                    <RowDefinition Height="9" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="15" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="30" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="30" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="30" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="5" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="13" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Grid.ColumnSpan="21" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                </Border>

                                <!--조회조건-->
                                <!--ROW1-->
                                <Grid Grid.Row="1" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*" />
                                        <ColumnDefinition Width="6*" />
                                    </Grid.ColumnDefinitions>
                                    <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <c1:C1ComboBox   Grid.Row="0" Grid.Column="1" x:Name="cboArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" IsEnabled="False"/>
                                </Grid>

                                <!--재구성완료, 초기화-->
                                <Grid Grid.Row="1" Grid.Column="9" HorizontalAlignment="Right" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="2"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <Button Grid.Row="0" Grid.Column="0" x:Name="btnComplate" Click="btnComplate_Click"  Content="{Binding ConverterParameter='재구성완료', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  Style="{DynamicResource Content_MainButtonSpecialStyle}"   Cursor="Hand" Margin="0,2" Height="Auto"  />
                                </Grid>
                                <Grid Grid.Row="1" Grid.Column="11" HorizontalAlignment="Right" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="2"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <Button Grid.Row="0" Grid.Column="0" x:Name="btnReSet"  Content="{Binding ConverterParameter='초기화', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  Style="{DynamicResource Content_MainButtonSpecialStyle}" VerticalAlignment="Center"   Cursor="Hand" Click="btnReSet_Click" />
                                </Grid>
                            </Grid>

                            <Grid Grid.Row="3" Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="31"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="1"/>
                                </Grid.ColumnDefinitions>

                                <Border Grid.Row="1" Grid.Column="1" Grid.RowSpan="7" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>

                                <Grid Grid.Row="0" Grid.Column="1" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="31"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="AUTO"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="5"/>
                                    </Grid.ColumnDefinitions>

                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="7" Grid.RowSpan="2"  Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}" Text="{Binding ConverterParameter='이동대차', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" />
                                    <StackPanel Grid.Row="0" Grid.Column="2" Orientation="Horizontal">
                                        <TextBlock   Style="{StaticResource SearchCondition_LabelStyle}" Text="{Binding ConverterParameter='대차ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                        <TextBox     x:Name="txtMove"        Style="{StaticResource Content_InputForm_TextBoxStyle}" Width="150" CharacterCasing="Upper" KeyDown="txtMove_KeyDown"/>
                                    </StackPanel>
                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="8" />
                                        <RowDefinition Height="0.21*" />
                                        <RowDefinition Height="8" />
                                        <RowDefinition Height="30" />
                                        <RowDefinition Height="0.5*" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="1.*" />
                                        <RowDefinition Height="8" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition Width="80"/>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition Width="80"/>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition Width="80"/>
                                        <ColumnDefinition Width="8"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" BorderThickness="1" Grid.RowSpan="99" Grid.ColumnSpan="99" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <Border Grid.Row="1" BorderThickness="1" Grid.RowSpan="99" Grid.ColumnSpan="99" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <Border Grid.Row="4" BorderThickness="1,0,1,1" Grid.ColumnSpan="99" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <Border Grid.Row="6" BorderThickness="1,0,1,1" Grid.ColumnSpan="99" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <c1:C1DataGrid Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="7" x:Name="dgCurrentList" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleRow" LoadedCellPresenter="dgCurrentList_LoadedCellPresenter" >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn      Header = "대차ID"         Binding = "{Binding CTNR_ID}"            HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "PJT명"          Binding = "{Binding PJT}"                HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "제품"           Binding = "{Binding PRODID}"             HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "시장유형"       Binding = "{Binding MKT_TYPE_NAME}"      HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "품질유형"       Binding = "{Binding WIP_QLTY_TYPE_NAME}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridNumericColumn   Header = "Inbox 수량"     Binding = "{Binding INBOX_QTY}"          HorizontalAlignment="Right"  Format="###,###,##0.##" IsReadOnly="True" />
                                            <c1:DataGridNumericColumn   Header = "Cell 수량"      Binding = "{Binding WIPQTY}"             HorizontalAlignment="Right"  Format="###,###,##0.##" IsReadOnly="True" />
                                            <c1:DataGridTextColumn      Header = "PROCID"         Binding = "{Binding PROCID}"             HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header = "AREAID"         Binding = "{Binding AREAID}"             HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header = "시장유형코드"   Binding = "{Binding MKT_TYPE_CODE}"      HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header = "품질유형코드"   Binding = "{Binding WIP_QLTY_TYPE_CODE}" HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed" />
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>

                                    <TextBlock  Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="7" Text="{Binding ConverterParameter='조립LOT정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                    <c1:C1DataGrid Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="7" x:Name="dgLotID_RT" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleRow" MouseLeftButtonUp="dgLotID_RT_MouseLeftButtonUp">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridCheckBoxColumn  Header = "선택"           Binding = "{Binding CHK}"       HorizontalAlignment="Center" Width="40" />
                                            <c1:DataGridTextColumn      Header = "조립LOT"        Binding = "{Binding LOTID_RT}"  HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "작업구분"       Binding = "{Binding FORM_WRK_TYPE_NAME}"  HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridNumericColumn   Header = "Inbox 수량"     Binding = "{Binding INBOX_QTY}" HorizontalAlignment="Right"  Format="###,###,##0.##" IsReadOnly="True" />
                                            <c1:DataGridNumericColumn   Header = "불량그룹LOT수"  Binding = "{Binding INBOX_QTY_DEF}" HorizontalAlignment="Right"  Format="###,###,##0.##" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "Cell 수량"      Binding = "{Binding WIPQTY}"    HorizontalAlignment="Right"  Format="###,###,##0.##" IsReadOnly="True" />
                                            <c1:DataGridTextColumn      Header = "대차ID"         Binding = "{Binding CTNR_ID}"   HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "INPUT_DATA"     Binding = "{Binding INPUT_DATA}" HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "PJT명"          Binding = "{Binding PRJT_NAME}" HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "제품"           Binding = "{Binding PRODID}"    HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "작업구분코드"   Binding = "{Binding FORM_WRK_TYPE_CODE}"  HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                    <TextBlock  Grid.Row="5" Grid.Column="1" Text="{Binding ConverterParameter='INBOX', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                    <Button x:Name="btnMerge" Grid.Row="5" Grid.Column="3" Content="{Binding ConverterParameter=병합, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnMerge_Click"  />
                                    <Button x:Name="btnSplit" Grid.Row="5" Grid.Column="5" Content="{Binding ConverterParameter=분할, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnSplit_Click" />
                                    <Button x:Name="btnPrint" Grid.Row="5" Grid.Column="7" Content="{Binding ConverterParameter=발행, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnPrint_Click" />
                                    <c1:C1DataGrid Grid.Row="6" Grid.ColumnSpan="8" x:Name="dgInbox" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleRow"  LoadedColumnHeaderPresenter="dgInbox_LoadedColumnHeaderPresenter" Margin="6,2,2,6" Grid.RowSpan="2"  >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTemplateColumn Name="CHK" Width="35">
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>
                                            <c1:DataGridTextColumn      Header = "조립LOT"        Binding = "{Binding LOTID_RT}"      HorizontalAlignment="Center"  IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "InboxID"        Binding = "{Binding INBOX_ID}"      HorizontalAlignment="Center"  IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "불량그룹LOT"    Binding = "{Binding INBOX_ID_DEF}"  HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "Inbox유형"      Binding = "{Binding INBOX_TYPE_NAME}"  HorizontalAlignment="Center"  IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "불량그룹명"     Binding = "{Binding DFCT_RSN_GR_NAME}" HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "등급"           Binding = "{Binding CAPA_GRD_CODE}" HorizontalAlignment="Center"  IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "Cell수량"       Binding = "{Binding WIPQTY}"        HorizontalAlignment="Right"  IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "대차ID"         Binding = "{Binding CTNR_ID}"       HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "INPUT_DATA"     Binding = "{Binding INPUT_DATA}"    HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn   Header = "Inbox 수량"     Binding = "{Binding INBOX_QTY}"     HorizontalAlignment="Right"  Format="###,###,##0.##" IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridNumericColumn   Header = "불량Inbox 수량" Binding = "{Binding INBOX_QTY_DEF}"     HorizontalAlignment="Right"  Format="###,###,##0.##" IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header = "MODLID"         Binding = "{Binding MODLID}"        HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "MKT_TYPE_NAME"  Binding = "{Binding MKT_TYPE_NAME}" HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "CALDATE"        Binding = "{Binding CALDATE}"       HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header = "SHFT_NAME"      Binding = "{Binding SHFT_NAME}"     HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header = "EQPTSHORTNAME"  Binding = "{Binding EQPTSHORTNAME}"             HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "VISL_INSP_USERNAME"  Binding = "{Binding VISL_INSP_USERNAME}"   HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header = "PROCNAME"            Binding = "{Binding PROCNAME}"             HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "PJT명"          Binding = "{Binding PRJT_NAME}"                 HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "제품"           Binding = "{Binding PRODID}"                    HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "Inbox유형코드"  Binding = "{Binding INBOX_TYPE_CODE}"           HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "불량그룹코드"   Binding = "{Binding DFCT_RSN_GR_ID}"            HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "작업구분코드"   Binding = "{Binding FORM_WRK_TYPE_CODE}"        HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "작업구분"       Binding = "{Binding FORM_WRK_TYPE_NAME}"        HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "PROCID"         Binding = "{Binding PROCID}"                    HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "RESNGR_ABBR_CODE"         Binding = "{Binding RESNGR_ABBR_CODE}"                    HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "AOMM등급"                 Binding = "{Binding AOMM_GRD_CODE}"                       HorizontalAlignment="Center"  IsReadOnly="True" />
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                </Grid>
                                <Grid Grid.Row="0" Grid.Column="2" Grid.RowSpan="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="8"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="20"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="8"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Button Grid.Row="5" x:Name="btnRight" Style="{DynamicResource Content_SubTaskButtonNext02Style}" HorizontalAlignment="Right" Margin="3,0,3,0" Click="btnRight_Click"/>
                                </Grid>
                                <Grid Grid.Row="0" Grid.Column="3" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="31" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="AUTO"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="5"/>
                                    </Grid.ColumnDefinitions>

                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="5"  Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}" Text="{Binding ConverterParameter='생성대차', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" />

                                    <Grid Grid.Row="0" Grid.Column="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="AUTO"/>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="AUTO"/>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="AUTO"/>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="AUTO"/>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="AUTO"/>
                                            <ColumnDefinition Width="8"/>
                                        </Grid.ColumnDefinitions>

                                        <CheckBox   Grid.Row="0"  Grid.Column="1"  x:Name="chkNew" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='신규'}"    Style="{StaticResource SearchCondition_CheckBoxStyle}" Click="chkNew_Click" />
                                        <TextBlock  Grid.Row="0"  Grid.Column="3" Style="{StaticResource SearchCondition_LabelStyle}" Text="{Binding ConverterParameter='대차ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                        <TextBox    Grid.Row="0"  Grid.Column="5" x:Name="txtTaget" Style="{StaticResource Content_InputForm_TextBoxStyle}" Width="100" CharacterCasing="Upper" KeyDown="txtTaget_KeyDown"/>
                                        <CheckBox   Grid.Row="0"  Grid.Column="7" x:Name="chkOrder" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='특수'}"    Style="{StaticResource SearchCondition_CheckBoxStyle}" Click="chkOrder_Click" />
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="9"   x:Name="cboOrder" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" IsEnabled="False" />


                                    </Grid>


                                </Grid>

                                <Border Grid.Row="1" Grid.Column="3" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                <Grid Grid.Row="1" Grid.Column="3">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="8" />
                                        <RowDefinition Height="0.21*" />
                                        <RowDefinition Height="8" />
                                        <RowDefinition Height="30" />
                                        <RowDefinition Height="0.5*" />
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="1.*" />
                                        <RowDefinition Height="8" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="8"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" BorderThickness="1" Grid.RowSpan="99" Grid.ColumnSpan="99" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <Border Grid.Row="1" BorderThickness="1" Grid.RowSpan="99" Grid.ColumnSpan="99" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <Border Grid.Row="4" BorderThickness="1,0,1,1" Grid.ColumnSpan="99" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <Border Grid.Row="6" BorderThickness="1,0,1,1" Grid.ColumnSpan="99" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                    <c1:C1DataGrid Grid.Row="1" Grid.Column="1" x:Name="dgCurrentList_Taget" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleRow" LoadedCellPresenter="dgCurrentList_Taget_LoadedCellPresenter" >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn      Header = "대차ID"         Binding = "{Binding CTNR_ID}"            HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "PJT명"          Binding = "{Binding PJT}"                HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "제품"           Binding = "{Binding PRODID}"             HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "시장유형"       Binding = "{Binding MKT_TYPE_NAME}"      HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn      Header = "품질유형"       Binding = "{Binding WIP_QLTY_TYPE_NAME}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridNumericColumn   Header = "Inbox 수량"     Binding = "{Binding INBOX_QTY}"          HorizontalAlignment="Right"  IsReadOnly="True" Format="###,###,##0.##"  />
                                            <c1:DataGridNumericColumn   Header = "Cell 수량"      Binding = "{Binding WIPQTY}"             HorizontalAlignment="Right"  IsReadOnly="True" Format="###,###,##0.##"  />
                                            <c1:DataGridTextColumn      Header = "PROCID"         Binding = "{Binding PROCID}"             HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "시장유형코드"   Binding = "{Binding MKT_TYPE_CODE}"      HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header = "품질유형코드"   Binding = "{Binding WIP_QLTY_TYPE_CODE}" HorizontalAlignment="Center"  IsReadOnly="True" Visibility="Collapsed" />
