<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_027"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="창고이동_창고 출고">
            <Grid>
                <c1:C1TabControl  Grid.Row="0" >
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='동간이동(인계)'}">
                        <Grid Grid.Row="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="58"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="55"/>
                            </Grid.RowDefinitions>

                            <Grid x:Name="InfoArea" Grid.Row="1">
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13" />
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="260"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="13" />
                                    </Grid.ColumnDefinitions>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="14"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="14"/>
                                    </Grid.RowDefinitions>

                                    <Grid Grid.Row="1" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock    Grid.Row="1"  Grid.Column="0" Text="{Binding ConverterParameter='인계동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBlock    Grid.Row="1"  Grid.Column="1" x:Name="txtFromArea" Style="{StaticResource Content_InputForm_LabelStyle}" />

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="2">
                                        <CheckBox     x:Name="chkMultiInput"       Content="{Binding ConverterParameter='Multi-Input', Converter={StaticResource GlobalObjectDicConverter}}"  
                                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" HorizontalAlignment="Left" IsChecked="False" Checked ="chkMultiInput_Checked" Unchecked="chkMultiInput_Unchecked" />
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="4">
                                        <CheckBox     x:Name="chkSkidID"           Content="{Binding ConverterParameter='SKIDID', Converter={StaticResource GlobalObjectDicConverter}}"  
                                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" HorizontalAlignment="Right" IsChecked="False" Checked ="chkSkidID_Checked" Unchecked="chkSkidID_Unchecked"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="6">
                                        <CheckBox     x:Name="chkRFID"             Content="{Binding ConverterParameter='Carrier ID', Converter={StaticResource GlobalObjectDicConverter}}"  
                                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" HorizontalAlignment="Right" IsChecked="False" Checked="chkRFID_Checked" Unchecked="chkRFID_Unchecked"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="8">
                                        <CheckBox     x:Name="chkBoxID"             Content="{Binding ConverterParameter='BOX ID', Converter={StaticResource GlobalObjectDicConverter}}"  
                                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" HorizontalAlignment="Right" IsChecked="False" Checked="chkBoxID_Checked" Unchecked="chkBoxID_Unchecked"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="9">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4.5*"/>
                                            <ColumnDefinition Width="5.5*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0" Grid.Column="0" x:Name="tbLotID_Out" Text="{Binding ConverterParameter='Lot ID (팔레트 ID)', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBox        Grid.Row="0" Grid.Column="1" x:Name="txtLotID_Out" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtLotID_Out_KeyDown" PreviewKeyDown="txtLotID_OUT_PreviewKeyDown" />
                                    </Grid>
                                    
                                    <Grid Grid.Row="1" Grid.Column="10">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='인수동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" x:Name="cboMoveToArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="11">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" x:Name="cboEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="12">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='이동유형', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" x:Name="cboTransType" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="13" HorizontalAlignment="Right" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="30"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <Button Grid.Row="0" Grid.Column="1"  x:Name="btnRefresh"   Content="{Binding ConverterParameter=초기화,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Cursor="Hand" Click="btnRefresh_Click" />
                                        <Button Grid.Row="0" Grid.Column="3"  x:Name="btnOut"       Content="{Binding ConverterParameter=인계처리,  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnOut_Click" />
                                    </Grid>
                                </Grid>
                            </Grid>

                            <Grid x:Name="OutGridArea" Grid.Row="3" >
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                            </Grid>

                            <Grid x:Name="OutArea" Grid.Row="4">
                                <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                <c1:C1DataGrid x:Name="dgMoveList" IsReadOnly="True" HeadersVisibility="Column" CanUserRemoveRows="True" CanUserAddRows="True" SelectionMode="SingleRow" Margin="8"
                                               LoadedCellPresenter="dgMoveList_LoadedCellPresenter" >
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTemplateColumn Name="Delete" Width="Auto" Header="삭제">
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button  x:Name="btnDelete" Width="Auto" Content="{Binding ConverterParameter=삭제, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                                 Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDelete_Click"/>
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>

                                        <c1:DataGridTextColumn Header = "LOTID"              Binding = "{Binding LOTID}"                    HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "SKIDID"             Binding = "{Binding CSTID}"                    HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "BOXID"              Binding = "{Binding BOXID}"                    HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "PRJT_NAME"          Binding = "{Binding PROJECTNAME}"              HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "제품코드"           Binding = "{Binding PRODID}"                   HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "제품명"             Binding = "{Binding PRODNAME}"                 HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "품질검사결과"       Binding = "{Binding QMS_JUDG_VALUE}"           HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "품질검사결과(FACTORY)"  Binding = "{Binding QMS_MOVE_SHOP_VALUE}"      HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "모델"               Binding = "{Binding MODLID}"                   HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "공정"               Binding = "{Binding PROCNAME}"                 HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "공정"               Binding = "{Binding PROCID}"                   HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "라인"               Binding = "{Binding EQSGNAME}"                 HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "라인"               Binding = "{Binding EQSGID}"                   HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "전극버전"           Binding = "{Binding PROD_VER_CODE}"            HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "Lane수"             Binding = "{Binding LANE_QTY}"                 HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "단위"               Binding = "{Binding UNIT_CODE}"                HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "수량(Roll)"         Binding = "{Binding WIPQTY}"                   HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                        <c1:DataGridTextColumn Header = "수량(Lane)"         Binding = "{Binding WIPQTY2}"                  HorizontalAlignment="Center" Format="###,###,##0.##" />
                                        <c1:DataGridTextColumn Header = "재작업대상"         Binding = "{Binding RWK_TRGT_FLAG}"            HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "상태"               Binding = "{Binding WIPSTAT}"                  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "HOLD"               Binding = "{Binding WIPHOLD}"                  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "유효기간"           Binding = "{Binding VLD_DATE}"                 HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "PJT"                Binding = "{Binding PROJECTNAME}"              HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "극성"               Binding = "{Binding ELECNAME}"                 HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "재공품질유형코드"   Binding = "{Binding CURR_WIP_QLTY_TYPE_CODE}"  HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "시장유형"           Binding = "{Binding MKT_TYPE_CODE}"            HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "시장유형"           Binding = "{Binding MKT_TYPE_NAME}"            HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "LotType"            Binding = "{Binding LOTTYPE}"                  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "LotType"            Binding = "{Binding LOTTYPE_NAME}"             HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn Header = "SpecialFlag"        Binding = "{Binding SPCL_FLAG}"                HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "SpecialLine"        Binding = "{Binding RSV_EQSGID_LIST}"          HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "PILOT_PROD_DIVS_CODE"     Binding = "{Binding PILOT_PROD_DIVS_CODE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>
                            


                            <Grid x:Name="NoteArea" Grid.Row="6" >
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid Grid.Row="0">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13" />
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="13" />
                                    </Grid.ColumnDefinitions>

                                    <TextBlock    Grid.Row="0" Grid.Column="1"  Text="{Binding ConverterParameter='NOTE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox      Grid.Row="0"  Grid.Column="2" Grid.ColumnSpan="2" x:Name="txtRemark" Style="{StaticResource SearchCondition_TextBoxStyle}" AcceptsReturn="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" Height="42" MinLines="2" MaxLines="2"/>

                                    <CheckBox    Grid.Row="0"  Grid.Column="4" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=포장카드발행 }"    
                                                Style="{StaticResource SearchCondition_CheckBoxStyle}" x:Name="chkPrint" IsChecked="False" Margin="30,0" FontWeight="Bold" />

                                </Grid>
                            </Grid>

                        </Grid>
                    </c1:C1TabItem>

                    
                    
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='동간이동(인계) - SKID_ID'}" x:Name="Cut" >

                        <Grid Grid.Row="0">

                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="58"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="55"/>
                            </Grid.RowDefinitions>

                            <Grid x:Name="InfoArea3" Grid.Row="1">

                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13" />
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="13" />
                                    </Grid.ColumnDefinitions>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="14"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="14"/>
                                    </Grid.RowDefinitions>

                                    <Grid Grid.Row="1" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock    Grid.Row="1"  Grid.Column="0" Text="{Binding ConverterParameter='인계동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBlock    Grid.Row="1"  Grid.Column="1" x:Name="txtFromArea3" Style="{StaticResource Content_InputForm_LabelStyle}" />

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='SKIDID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBox        Grid.Row="0" Grid.Column="1" x:Name="txtCutID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtCutID_KeyDown" />

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='인수동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" x:Name="cboMoveToArea3" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="4">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" x:Name="cboEquipmentSegment3" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='이동유형', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" x:Name="cboTransType2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="6" HorizontalAlignment="Right" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="30"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <Button Grid.Row="0" Grid.Column="1"  x:Name="btnRefresh3"    Content="{Binding ConverterParameter=초기화,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Cursor="Hand" Click="btnRefresh3_Click" />

                                        <Button Grid.Row="0" Grid.Column="3"  x:Name="btnOut3" Content="{Binding ConverterParameter=인계처리,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnOut3_Click" />
                                    </Grid>

                                </Grid>

                              </Grid>


                            <Grid x:Name="OutGridArea3" Grid.Row="3" >

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>


                                <Grid   Grid.Row="1" Grid.Column="1" >
                                    <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <c1:C1DataGrid x:Name="dgCutidList" IsReadOnly="True" HeadersVisibility="Column" CanUserRemoveRows="True" CanUserAddRows="True" SelectionMode="SingleRow" Margin="8"
                                                   LoadedCellPresenter="dgMoveList_LoadedCellPresenter">
                                        <c1:C1DataGrid.Columns>

                                            <c1:DataGridTextColumn Header = "SKIDID"      Binding = "{Binding CSTID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "LOTID"       Binding = "{Binding LOTID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "PRJT_NAME"   Binding = "{Binding PROJECTNAME}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "제품코드"    Binding = "{Binding PRODID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "제품명"      Binding = "{Binding PRODNAME}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "품질검사결과"            Binding = "{Binding QMS_JUDG_VALUE}"           HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "품질검사결과(FACTORY)"     Binding = "{Binding QMS_MOVE_SHOP_VALUE}"           HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "모델"        Binding = "{Binding MODLID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "공정"        Binding = "{Binding PROCNAME}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "공정"        Binding = "{Binding PROCID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "라인"        Binding = "{Binding EQSGNAME}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "라인"        Binding = "{Binding EQSGID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "전극버전"    Binding = "{Binding PROD_VER_CODE}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "Lane수"      Binding = "{Binding LANE_QTY}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "단위"        Binding = "{Binding UNIT_CODE}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "수량(Roll)"  Binding = "{Binding WIPQTY}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                            <c1:DataGridTextColumn Header = "수량(Lane)"  Binding = "{Binding WIPQTY2}" HorizontalAlignment="Center" Format="###,###,##0.##" />
                                            <c1:DataGridTextColumn Header = "재작업대상"  Binding = "{Binding RWK_TRGT_FLAG}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "상태"        Binding = "{Binding WIPSTAT}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "시장유형"      Binding = "{Binding LOTTYPE}"           HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "LotType"       Binding = "{Binding LOTTYPE_NAME}"      HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "SpecialFlag"   Binding = "{Binding SPCL_FLAG}"         HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "SpecialLine"   Binding = "{Binding RSV_EQSGID_LIST}"   HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "PILOT_PROD_DIVS_CODE"     Binding = "{Binding PILOT_PROD_DIVS_CODE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            
                                            <c1:DataGridTemplateColumn Name="Delete" Width="Auto" Header="삭제">
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Button  x:Name="btnDelete3" Width="Auto" Content="{Binding ConverterParameter=삭제, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                                 Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDelete3_Click"/>
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>

                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                </Grid>

                            </Grid>


                            <Grid x:Name="NoteArea3" Grid.Row="5" >
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid Grid.Row="0">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13" />
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
