<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_028"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="창고이동_창고 입고">
            <Grid>

                <c1:C1TabControl  Grid.Row="0" >
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='동간이동(인수)'}" >

                        <Grid Grid.Row="0">

                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="45"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="55"/>
                            </Grid.RowDefinitions>

                            <Grid x:Name="SearchArea" Grid.Row="1" Grid.ColumnSpan="2">
                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="12" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid   Grid.Row="0" Grid.Column="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="13"/>
                                    </Grid.ColumnDefinitions>

                                    <Grid Grid.Row="0" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='인계동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboMoveToArea2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='Carrier ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBox       Grid.Row="0"  Grid.Column="1" x:Name="txtCarrierID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtCarrierID_KeyDown"/>

                                    </Grid>


                                    <Grid Grid.Row="0" Grid.Column="4" HorizontalAlignment="Right">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <StackPanel x:Name="stRack"  Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right" Visibility="Collapsed">
                                            <TextBlock     x:Name="tbIWHID"  Text="{Binding ConverterParameter='위치', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                            <c1:C1ComboBox x:Name="cboWHID"        SelectedValuePath="CBO_CODE" DisplayMemberPath="CBO_NAME" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" IsEditable="False" SelectedValueChanged="cboWHID_SelectedValueChanged" />
                                            <TextBlock x:Name="tbIRackID"  Text="{Binding ConverterParameter='RACKID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                            <TextBox   x:Name="txtRackID"  Style="{StaticResource TextBoxBaseStyle_PDA}" CharacterCasing="Upper" KeyDown="txtRackID_KeyDown"  Width="100" Margin="0,0,8,0"/>
                                        </StackPanel>

                                        <Button Grid.Row="0" Grid.Column="1"  x:Name="btnInitialize" Content="{Binding ConverterParameter=초기화,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnInitialize_Click" />
                                        <Button Grid.Row="0" Grid.Column="3" x:Name="btnConfrim"   Content="{Binding ConverterParameter=인수처리,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnConfrim_Click"/>
                                        <Button Grid.Row="0" Grid.Column="5" x:Name="btnSearch"   Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click"/>

                                    </Grid>

                                </Grid>
                            </Grid>

                            <Grid x:Name="GArea" Grid.Row="3" Grid.Column="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="6*" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="4*" />
                                </Grid.ColumnDefinitions>

                                <GridSplitter Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />


                                <Grid x:Name="GridArea1" Grid.Row="2" Grid.Column="0" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <Grid Grid.Row="1">
                                        <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                        <c1:C1DataGrid x:Name="dgMove_Master" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="3"  SelectionMode="SingleRow" Margin="8" 
                                                       LoadedCellPresenter="dgMove_Master_LoadedCellPresenter">
                                            <c1:C1DataGrid.Columns>
                                                <!--<c1:DataGridTemplateColumn Name="CHK" Width="50" Header="선택" >
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" Click="CheckBox_Click" />
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>-->

                                                <c1:DataGridTemplateColumn  Header="선택" EditOnSelection="True" Name="CHK" Width="Auto" HorizontalAlignment="Center" >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <RadioButton GroupName="dgMove_MasterGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgMove_MasterChoice" HorizontalAlignment="Center" Checked="dgMove_MasterChoice_Checked"  />
                                                        </DataTemplate>
                                                    </c1:DataGridTemplateColumn.CellTemplate>
                                                </c1:DataGridTemplateColumn>
                                                <c1:DataGridTextColumn Header = "이동유형"    Binding = "{Binding MOVE_MTHD_CODE}"     HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "이동ID"      Binding = "{Binding MOVE_ORD_ID}"        HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "PRJT_NAME"   Binding = "{Binding PRJT_NAME}"          HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "제품코드"    Binding = "{Binding PRODID}"             HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "제품명"      Binding = "{Binding PRODNAME}"           HorizontalAlignment="Center"/>                                                
                                                <c1:DataGridTextColumn Header = "상태"        Binding = "{Binding MOVE_ORD_STAT_NAME}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "전체수량"    Binding = "{Binding MOVE_ORD_QTY}"       HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                <c1:DataGridTextColumn Header = "단위"        Binding = "{Binding UNIT_CODE}"          HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "인계동"      Binding = "{Binding FROM_AREAID}"        HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "인계라인"    Binding = "{Binding FROM_EQSGID}"        HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "인계자"      Binding = "{Binding MOVE_USERID}"        HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "인계시간"    Binding = "{Binding MOVE_STRT_DTTM }"    HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "인수동"      Binding = "{Binding TO_AREAID}"          HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "인수라인"    Binding = "{Binding TO_EQSGID}"          HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "상태코드"    Binding = "{Binding MOVE_ORD_STAT_CODE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn Header = "FLAG"        Binding = "{Binding FLAG}"               HorizontalAlignment="Center" Visibility="Collapsed"/>

                                            </c1:C1DataGrid.Columns>
                                            
                                            
                                            
                                        </c1:C1DataGrid>
                                    </Grid>

                                </Grid>

                                <Grid x:Name="GridArea3" Grid.Row="2" Grid.Column="2" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Border Grid.Row="0" Grid.Column="2" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="2" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='LOT목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <Grid Grid.Row="1" Grid.Column="2">
                                        <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                        <c1:C1DataGrid x:Name="dgMove_Detail" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="MultiRange" Grid.ColumnSpan="2" Margin="8"
                                                       LoadedCellPresenter="dgMove_Detail_LoadedCellPresenter">
                                            <c1:C1DataGrid.Columns>
                                                <!--<c1:DataGridTemplateColumn Name="CHK" Width="Auto" Header="입고선택" >
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>-->

                                                <c1:DataGridTextColumn Header = "NO"          Binding = "{Binding ROW_NUMBER}"          HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "LOTID"       Binding = "{Binding LOTID}"               HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "BOXID"       Binding = "{Binding CSTID}"               HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "전극버전"    Binding = "{Binding PROD_VER_CODE}"       HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "Lane수"      Binding = "{Binding LANE_QTY}"            HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "단위"        Binding = "{Binding UNIT_CODE}"           HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "수량(Roll)"  Binding = "{Binding WIPQTY}"              HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                <c1:DataGridTextColumn Header = "수량(Lane)"  Binding = "{Binding WIPQTY2}"             HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                <c1:DataGridTextColumn Header = "유효기간"    Binding = "{Binding VLD_DATE}"            HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "품질검사결과" Binding = "{Binding QMS_JUDG_VALUE_NAME}"     HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "품질검사결과" Binding = "{Binding QMS_JUDG_VALUE}"     HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn Header = "상태"        Binding = "{Binding MOVE_STAT_CODE_DESC}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "재작업대상"  Binding = "{Binding RWK_TRGT_FLAG}"       HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "상태"        Binding = "{Binding MOVE_STAT_CODE}"      HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn Header = "MOVE_ORD_ID" Binding = "{Binding MOVE_ORD_ID}"         HorizontalAlignment="Center" Visibility="Collapsed"/>

                                            </c1:C1DataGrid.Columns>
                                        </c1:C1DataGrid>
                                        <c1:C1DataGrid x:Name="dgMove_Detail_Hidden" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="MultiRange" Margin="8"
                                            LoadedCellPresenter="dgMove_Detail_LoadedCellPresenter" Visibility="Collapsed">
                                            <c1:C1DataGrid.Columns>
                                                <!--<c1:DataGridTemplateColumn Name="CHK" Width="Auto" Header="입고선택" >
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>-->

                                                <c1:DataGridTextColumn Header = "NO"          Binding = "{Binding ROW_NUMBER}"          HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "LOTID"       Binding = "{Binding LOTID}"               HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "BOXID"       Binding = "{Binding CSTID}"               HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "전극버전"    Binding = "{Binding PROD_VER_CODE}"       HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "Lane수"      Binding = "{Binding LANE_QTY}"            HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "단위"        Binding = "{Binding UNIT_CODE}"           HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "수량(Roll)"  Binding = "{Binding WIPQTY}"              HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                <c1:DataGridTextColumn Header = "수량(Lane)"  Binding = "{Binding WIPQTY2}"             HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                <c1:DataGridTextColumn Header = "유효기간"    Binding = "{Binding VLD_DATE}"            HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "품질검사결과" Binding = "{Binding QMS_JUDG_VALUE_NAME}"     HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "품질검사결과" Binding = "{Binding QMS_JUDG_VALUE}"     HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn Header = "상태"        Binding = "{Binding MOVE_STAT_CODE_DESC}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "재작업대상"  Binding = "{Binding RWK_TRGT_FLAG}"       HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "상태"        Binding = "{Binding MOVE_STAT_CODE}"      HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn Header = "MOVE_ORD_ID" Binding = "{Binding MOVE_ORD_ID}"         HorizontalAlignment="Center" Visibility="Collapsed"/>

                                            </c1:C1DataGrid.Columns>
                                        </c1:C1DataGrid>
                                    </Grid>

                                </Grid>

                            </Grid>

                            <Grid x:Name="NoteArea" Grid.Row="5" >
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid Grid.Row="0">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13" />
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="13" />
                                    </Grid.ColumnDefinitions>

                                    <TextBlock    Grid.Row="0" Grid.Column="1"  Text="{Binding ConverterParameter='NOTE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <!--<RichTextBox  Grid.Row="0" Grid.Column="2"  x:Name="rtxRemark"  />-->
                                    <TextBox      Grid.Row="0"  Grid.Column="2" Grid.ColumnSpan="2" x:Name="txtRemark" Style="{StaticResource SearchCondition_TextBoxStyle}" AcceptsReturn="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" Height="42" MinLines="2" MaxLines="2"/>

                                </Grid>


                            </Grid>

                        </Grid>


                    </c1:C1TabItem>

                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='인수이력조회'}" Margin="0" >

                        <Grid x:Name="TitleArea2" Grid.Row="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="98"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <Grid x:Name="SearchArea2" Grid.Row="1">


                                <Border Grid.Row="0" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                                <Grid   Grid.Row="0" Grid.Column="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13" />
                                        <ColumnDefinition Width="1.2*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="13" />
                                    </Grid.ColumnDefinitions>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="14"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="10"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="14"/>
                                    </Grid.RowDefinitions>

                                    <Grid Grid.Row="1" Grid.Column="1" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='기간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <Grid Grid.Column="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="AUTO"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>

                                            <ctrlLib:LGCDatePicker  Grid.Column="0" x:Name="dtpDateFrom2" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center" Margin="0" TabIndex="1" />
                                            <TextBlock              Grid.Column="1" Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center" Margin="6,0"/>
                                            <ctrlLib:LGCDatePicker  Grid.Column="2" x:Name="dtpDateTo2" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center" Margin="0" TabIndex="2"/>
                                        </Grid>

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='인계동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboMoveToArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right"  Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboMoveToEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                    </Grid>


                                    <Grid Grid.Row="1" Grid.Column="4">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='상태', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboStat" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='이동유형', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="1" Grid.Column="1" x:Name="cboTransType" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                    </Grid>

                                    <!--<Grid Grid.Row="3" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='프로젝트', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <TextBox        Grid.Row="0" Grid.Column="1" x:Name="txtPRJT_NAME" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtPRJT_NAME_KeyDown" />

                                    </Grid>-->

                                    <Grid Grid.Row="3" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                        <TextBox       Grid.Row="0"  Grid.Column="1" x:Name="txtProd_ID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100"
                                                   KeyDown="txtProd_ID_KeyDown"/>

                                    </Grid>

                                    <Grid Grid.Row="3" Grid.Column="5" HorizontalAlignment="Right">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <Button         Grid.Row="0"  Grid.Column="1"  x:Name="btnSearch2"  Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch2_Click"/>

                                    </Grid>
                                    
                                </Grid>

                            </Grid>

                            <Grid x:Name="GridArea" Grid.Row="3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="7*" />
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="3*" />
                                </Grid.ColumnDefinitions>

                                <GridSplitter Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                <Grid x:Name="HistArea" Grid.Row="3">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>


                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <Grid Grid.Row="1">

