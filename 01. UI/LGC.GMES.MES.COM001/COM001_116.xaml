                                <Button x:Name="btnSaveHistory"  Grid.Column="1" Content="{Binding ConverterParameter='완료취소', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnSaveHistory_Click"/>

                            </Grid>
