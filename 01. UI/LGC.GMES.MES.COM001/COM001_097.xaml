<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.COM001.COM001_097"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="공정별 생산실적 수정 System" Grid.ColumnSpan="2">
            <Grid>
                <Grid.Resources>
                    <DataTemplate x:Key="ResultTemplate">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="1" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,0,0"  />
                        </Grid>
                    </DataTemplate>
                    <Style TargetType="{x:Type Button}" x:Key="grButtonStyle" BasedOn="{StaticResource ButtonIconBaseStyle}">
                        <Setter Property="Background">
                            <Setter.Value>
                                <ImageBrush ImageSource="/LGC.GMES.MES.CMM001;component/Images/btn_i_print.png" Stretch="Uniform"/>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Grid.Resources>
                <Grid.RowDefinitions>
                    <RowDefinition Height="110"/>
                    <RowDefinition Height="3" />
                    <RowDefinition/>
                    <RowDefinition Height="8" />
                    <RowDefinition />
                </Grid.RowDefinitions>
             

                <GridSplitter Grid.Row="3" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                <!--조회영역-->
                <Grid Grid.Column="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="35" />
                        <RowDefinition Height="35" />
                        <RowDefinition Height="35" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="13" />
                        <ColumnDefinition Width="190" />
                        <ColumnDefinition Width="300" />
                        <ColumnDefinition Width="250" />
                        <ColumnDefinition Width="280" />
                        <ColumnDefinition Width="190" />
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="AUTO"/>
                        <ColumnDefinition Width="13" />
                    </Grid.ColumnDefinitions>

                    <!--백그라운드용-->
                    <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                    <!--조회조건-->
                    <!--ROW1-->
                    <Grid Grid.Row="0" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="3*"/>
                            <ColumnDefinition Width="7*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <c1:C1ComboBox    Grid.Column="1" x:Name="cboArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>

                    <Grid Grid.Row="0" Grid.Column="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <c1:C1ComboBox   Grid.Row="0" Grid.Column="1" x:Name="cboProcess" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedItemChanged="cboProcess_SelectedItemChanged" />
                    </Grid>
                    
                    <Grid Grid.Row="0" Grid.Column="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="4*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <!--<c1:C1ComboBox   Grid.Row="0" Grid.Column="1" x:Name="cboEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedItemChanged="cboEquipmentSegment_SelectedItemChanged"/>-->
                        <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="1" x:Name="cboEquipmentSegment" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" isAllUsed="False" SelectionChanged="cboEquipmentSegment_SelectionChanged"/>
                    </Grid>
               

                    <Grid Grid.Row="0" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='설비', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <c1:C1ComboBox   Grid.Row="0" Grid.Column="1" x:Name="cboEquipment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>

                    <!--ROW2-->
                    <Grid Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="3*"/>
                            <ColumnDefinition Width="7*"/>
                        </Grid.ColumnDefinitions>

                        <TextBlock  Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='작업일', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <StackPanel Orientation="Horizontal"  Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3">
                            <CheckBox  x:Name="chk"  IsChecked="False" Checked="chk_Checked" Unchecked="chk_Unchecked"/>
                            <ctrlLib:LGCDatePicker DatepickerType="Date" x:Name="dtpDateFrom" SelectedDataTimeChanged="dtpDate_SelectedDataTimeChanged" />
                            <c1:C1ComboBox  x:Name="cboTimeFrom" Height="21" MinWidth="40" Width="Auto" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Visibility="Collapsed" />
                            <TextBlock  Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0" />
                            <ctrlLib:LGCDatePicker DatepickerType="Date" x:Name="dtpDateTo" SelectedDataTimeChanged="dtpDate_SelectedDataTimeChanged" />
                            <c1:C1ComboBox x:Name="cboTimeTo" Height="21" MinWidth="40" Width="Auto" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Visibility="Collapsed"/>
                        </StackPanel>
                    </Grid>
                    <Grid Grid.Row="1" Grid.Column="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='작업구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <c1:C1ComboBox   Grid.Row="0" Grid.Column="1" x:Name="cboJob" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>
                    <Grid Grid.Row="1" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='작업조', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <c1:C1ComboBox   Grid.Row="0" Grid.Column="1" x:Name="cboShift" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>
                    <!--ROW3-->
                    <Grid Grid.Row="2" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="4*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='모델', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Row="0" Grid.Column="1"  x:Name="txtModel"        Style="{StaticResource SearchCondition_TextBoxStyle}"  />
                    </Grid>
                    <Grid Grid.Row="2" Grid.Column="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="4*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='업체', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Row="0" Grid.Column="1"  x:Name="txtCompany"        Style="{StaticResource SearchCondition_TextBoxStyle}"  />
                    </Grid>
                    <Grid Grid.Row="2" Grid.Column="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='조립LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Row="0" Grid.Column="1"  x:Name="txtLotId_RT"        Style="{StaticResource SearchCondition_TextBoxStyle}"  />
                    </Grid>
                    <Grid Grid.Row="2" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='생산LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Row="0" Grid.Column="1"  x:Name="txtLotId" CharacterCasing="Upper" Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtLotId_KeyDown" />
                    </Grid>
                    <Grid Grid.Row="2" Grid.Column="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='대차ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Row="0" Grid.Column="1"  x:Name="txtCtnrID"        Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtCtnrID_KeyDown" />
                    </Grid>
                    <!--조회 버튼-->
                    <Grid Grid.Row="1" Grid.Column="7">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>

                        <Button     Grid.Row="2" Grid.Column="10" x:Name="btnSearchHold" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                    </Grid>
                </Grid>

                <!--LOT 리스트-->
                <Grid Grid.Row="2" Grid.Column="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="34"/>
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="8" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="8" />
                        
                    </Grid.ColumnDefinitions>

                    <!--백그라운드용-->
                    <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" Grid.ColumnSpan="4" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                    <TextBlock Grid.Row="0" Grid.ColumnSpan="4" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='LOT리스트', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                    <Grid Grid.Row="0" Grid.Column="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5*"/>
                            <ColumnDefinition Width="5*"/>
                            <ColumnDefinition Width="170"/>
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="170"/>
                            <ColumnDefinition Width="8" />

                        </Grid.ColumnDefinitions>
                        <CheckBox x:Name="chkProc" Grid.Column="2"   Content="{Binding ConverterParameter='진행중인Lot 포함', Converter={StaticResource GlobalObjectDicConverter}}"  
                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" IsChecked="False" HorizontalAlignment="Right" />
                        <CheckBox x:Name="chkZero" Grid.Column="4"  Content="{Binding ConverterParameter='실적수량 0 제외', Converter={StaticResource GlobalObjectDicConverter}}"  
                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" IsChecked="False" HorizontalAlignment="Right" />
                    </Grid>
                  <c1:C1DataGrid x:Name="dgLotList" Grid.Row="1" Grid.ColumnSpan="4" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="3"   FrozenBottomRowsCount="1" SelectionMode="MultiRange" Margin="8,8,8,8">
                        <c1:C1DataGrid.Columns>
                            <c1:DataGridTemplateColumn  Header="선택" EditOnSelection="True" Name="CHK" Width="60" >
                                <c1:DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <RadioButton GroupName="dgProductLotChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgProductLotChoice" HorizontalAlignment="Center" Checked="dgProductLotChoice_Checked" />
                                    </DataTemplate>
                                </c1:DataGridTemplateColumn.CellTemplate>
                            </c1:DataGridTemplateColumn>
                            <c1:DataGridTextColumn      Header = "작업일자"                Binding = "{Binding CALDATE}"              IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "생산LOTID"               Binding = "{Binding LOTID}"                IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "PJT명"                   Binding = "{Binding PRJT_NAME}"            IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "WIPSEQ"                  Binding = "{Binding WIPSEQ}"               IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "제품"                    Binding = "{Binding PRODID}"               IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "제품명"                  Binding = "{Binding PRODNAME}"             IsReadOnly="True" HorizontalAlignment="Left" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "설비"                    Binding = "{Binding EQPTID}"               IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                            <c1:DataGridTextColumn      Header = "설비명"                  Binding = "{Binding EQPTNAME}"             IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "조립LOT"                 Binding = "{Binding LOTID_RT}"             IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "조립 LOT 생산 라인명"    Binding = "{Binding PKG_EQSGNAME}"         IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                            <c1:DataGridTextColumn      Header = "시장유형"                Binding = "{Binding MKT_TYPE_NAME}"        IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "MKT_TYPE_CODE"           Binding = "{Binding MKT_TYPE_CODE}"        IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "LOT유형코드"             Binding = "{Binding LOTTYPE}"              IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                            <c1:DataGridTextColumn      Header = "LOT유형"                 Binding = "{Binding LOTYNAME}"             IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "SOC"                     Binding = "{Binding SOC_VALUE}"            IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "작업구분코드"            Binding = "{Binding FORM_WRK_TYPE_CODE}"   IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                            <c1:DataGridTextColumn      Header = "작업구분"                Binding = "{Binding FORM_WRK_TYPE_NAME}"   IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "QMS HOLD코드"            Binding = "{Binding HOLD_FLAG}"            IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                            <c1:DataGridTextColumn      Header = "QMS HOLD 여부"           Binding = "{Binding HOLD_FLAG_NAME}"       IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "GROUP"                   Binding = "{Binding WND_GR_CODE}"          IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                            <c1:DataGridNumericColumn                Header = "투입량"     Binding = "{Binding INPUT_QTY}"             HorizontalAlignment="Right"  Format="###,###,##0.##" EditOnSelection="True" HandleUpDownKeys="False" ShowButtons="False" Minimum="0" Visibility="Collapsed">
                                <c1:DataGridAggregate.AggregateFunctions>
                                    <c1:DataGridAggregatesCollection>
                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                    </c1:DataGridAggregatesCollection>
                                </c1:DataGridAggregate.AggregateFunctions>
                            </c1:DataGridNumericColumn>
                            <c1:DataGridNumericColumn                Header = "생산량"           Binding = "{Binding PRODUCT_QTY}"             HorizontalAlignment="Right"  Format="###,###,##0.##" EditOnSelection="True" HandleUpDownKeys="False" ShowButtons="False" Minimum="0">
                                <c1:DataGridAggregate.AggregateFunctions>
                                    <c1:DataGridAggregatesCollection>
                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                    </c1:DataGridAggregatesCollection>
                                </c1:DataGridAggregate.AggregateFunctions>
                            </c1:DataGridNumericColumn>
                            <c1:DataGridNumericColumn                Header = "양품량"           Binding = "{Binding GOOD_QTY}"             HorizontalAlignment="Right"  Format="###,###,##0.##" EditOnSelection="True" HandleUpDownKeys="False" ShowButtons="False" Minimum="0">
                                <c1:DataGridAggregate.AggregateFunctions>
                                    <c1:DataGridAggregatesCollection>
                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                    </c1:DataGridAggregatesCollection>
                                </c1:DataGridAggregate.AggregateFunctions>
                            </c1:DataGridNumericColumn>
                            <c1:DataGridNumericColumn                Header = "불량량"           Binding = "{Binding DEFECT_QTY}"             HorizontalAlignment="Right"  Format="###,###,##0.##" EditOnSelection="True" HandleUpDownKeys="False" ShowButtons="False" Minimum="0">
                                <c1:DataGridAggregate.AggregateFunctions>
                                    <c1:DataGridAggregatesCollection>
                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                    </c1:DataGridAggregatesCollection>
                                </c1:DataGridAggregate.AggregateFunctions>
                            </c1:DataGridNumericColumn>
                            <c1:DataGridNumericColumn                Header = "LOSS량"           Binding = "{Binding CNFM_LOSS_QTY}"             HorizontalAlignment="Right"  Format="###,###,##0.##" EditOnSelection="True" HandleUpDownKeys="False" ShowButtons="False" Minimum="0" Visibility="Collapsed">
                                <c1:DataGridAggregate.AggregateFunctions>
                                    <c1:DataGridAggregatesCollection>
                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                    </c1:DataGridAggregatesCollection>
                                </c1:DataGridAggregate.AggregateFunctions>
                            </c1:DataGridNumericColumn>
                            <c1:DataGridNumericColumn                Header = "물품청구"           Binding = "{Binding CNFM_PRDT_REQ_QTY}"             HorizontalAlignment="Right"  Format="###,###,##0.##" EditOnSelection="True" HandleUpDownKeys="False" ShowButtons="False" Minimum="0" Visibility="Collapsed">
                                <c1:DataGridAggregate.AggregateFunctions>
                                    <c1:DataGridAggregatesCollection>
                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                    </c1:DataGridAggregatesCollection>
                                </c1:DataGridAggregate.AggregateFunctions>
                            </c1:DataGridNumericColumn>
                            <c1:DataGridNumericColumn                Header = "LOSS 조정 수량"    Binding = "{Binding CNFM_LOSS_ETC}"             HorizontalAlignment="Right"  Format="###,###,##0.##" EditOnSelection="True" HandleUpDownKeys="False" ShowButtons="False" Minimum="0" Visibility="Collapsed">
                                <c1:DataGridAggregate.AggregateFunctions>
                                    <c1:DataGridAggregatesCollection>
                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                    </c1:DataGridAggregatesCollection>
                                </c1:DataGridAggregate.AggregateFunctions>
                            </c1:DataGridNumericColumn>
                            <c1:DataGridTextColumn      Header = "업체코드"                Binding = "{Binding WRK_SUPPLIERID}"       IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "업체명"                  Binding = "{Binding WRK_SUPPLIERNAME}"     IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "작업조"                  Binding = "{Binding SHFT_NAME}"            IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "작업조코드"              Binding = "{Binding SHIFT}"                IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "작업자"                  Binding = "{Binding WRK_USER_NAME}"        IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "작업ID"                  Binding = "{Binding WRK_USERID}"           IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "작업시작일시"            Binding = "{Binding WIPDTTM_ST}"           IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "작업종료일시"            Binding = "{Binding WIPDTTM_ED}"           IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "LOTDTTM_CR"              Binding = "{Binding LOTDTTM_CR}"           IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "공정"                    Binding = "{Binding PROCID}"               IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "라인"                    Binding = "{Binding EQSGID}"               IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "동"                      Binding = "{Binding AREAID}"               IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "WIP_NOTE"                Binding = "{Binding WIP_NOTE}"             IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "CLSS3_CODE"              Binding = "{Binding CLSS3_CODE}"           IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "WRK_USERID"              Binding = "{Binding WRK_USERID}"           IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "WRK_USER_NAME"           Binding = "{Binding WRK_USER_NAME}"        IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "PROCNAME"                Binding = "{Binding PROCNAME}"             IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "MODLID"                  Binding = "{Binding MODLID}"               IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "EQPTSHORTNAME"           Binding = "{Binding EQPTSHORTNAME}"        IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "CTNR_TYPE_CODE"          Binding = "{Binding CTNR_TYPE_CODE}"       IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "LINE_GROUP_CODE"         Binding = "{Binding LINE_GROUP_CODE}"      IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "MIX_LOT_FLAG"            Binding = "{Binding MIX_LOT_FLAG}"         IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn      Header = "MIX_LOT_GNRT_PROCID"     Binding = "{Binding MIX_LOT_GNRT_PROCID}"  IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
