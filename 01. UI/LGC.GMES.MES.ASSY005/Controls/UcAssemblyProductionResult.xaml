    <UserControl x:Class="LGC.GMES.MES.ASSY005.Controls.UcAssemblyProductionResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LGC.GMES.MES.ASSY005.Controls"             
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="900" 
             Initialized="UserControl_Initialized"
             Unloaded="UserControl_Unloaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot">
            <Grid >
                <Grid.Resources>
                    <DataTemplate x:Key="ResultTemplate">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="6"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right"></TextBlock>
                        </Grid>
                    </DataTemplate>
                </Grid.Resources>

                <!--start -->
                <Grid x:Name="grdPublic">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8" />
                        <RowDefinition Height="1.3*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="8" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="8" />
                    </Grid.ColumnDefinitions>

                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Background="#e8ebed" BorderThickness="1" BorderBrush="#8a8a8a"></Border>
                    <GridSplitter Grid.Row="1" Grid.Column="0" ResizeDirection="Rows" Grid.ColumnSpan="3" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                    <Grid Grid.Row="0" Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="34"/>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="90"/>
                            <RowDefinition Height="8"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <!-- Row 0 -->
                        <!--<TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_ModuleTitleStyle}" ><Run Text="{Binding ConverterParameter='생산실적', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>-->
                        <!--<Grid Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="5" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                        </Grid>-->

                        <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="4">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}" ><Run Text="{Binding ConverterParameter='생산실적', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                            <Grid Grid.Row="0" Grid.Column="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>
                                <Rectangle x:Name="recEquipment" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Margin="0,5,0,5"/>
                                <TextBlock x:Name="txtEquipment" Grid.Row="1" Grid.Column="1" Style="{StaticResource SearchCondition_LabelStyle}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" />
                            </Grid>
                        </Grid>

                        <!-- Border -->
                        <Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="4" Grid.ColumnSpan="99" Background="#ffffffff" />

                        <!-- Row 2 -->
                        <Grid x:Name="grdLotInfo" Grid.Row="2" Grid.Column="2" Visibility="Collapsed">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="13" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="13" />
                            </Grid.ColumnDefinitions>

                            <!-- TextBlock -->
                            <TextBlock                                Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold" />
                            <TextBox    x:Name="txtLotId"             Grid.Row="0" Grid.Column="2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" FontWeight="Bold" Margin="8,0,8,0"/>
                            <TextBlock                                Grid.Row="0" Grid.Column="3" Text="{Binding ConverterParameter='Carrier ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}"  Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>

                            <TextBox    x:Name="txtCarrierId"         Grid.Row="0" Grid.Column="4" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" FontWeight="Bold" Foreground="Blue" Margin="8,0,8,0" Cursor="Hand"  MouseDoubleClick="txtCarrierId_MouseDoubleClick" />

                            <TextBlock                                Grid.Row="0" Grid.Column="5" Text="{Binding ConverterParameter='작업일자', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <ctrlLib:LGCDatePicker Grid.Row="0" Grid.Column="6" x:Name="dtpCaldate" DatepickerType="Date" />

                            <!--<c1:C1NumericBox x:Name="txtWorkTime"     Grid.Row="0" Grid.Column="6" ShowButtons="False" Format="F0" IsEnabled="False" FontWeight="Bold" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>-->

                            <TextBlock                                Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="txtProdId"            Grid.Row="2" Grid.Column="2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>

                            <TextBlock                                Grid.Row="2" Grid.Column="3" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="txtProjectName"       Grid.Row="2" Grid.Column="4" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>
                            <TextBlock                                Grid.Row="2" Grid.Column="5" Text="{Binding ConverterParameter='LOT상태', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="txtLotStatus"         Grid.Row="2" Grid.Column="6" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>

                            <TextBlock                                Grid.Row="4" Grid.Column="1" Text="{Binding ConverterParameter='시작시간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="txtStartTime"         Grid.Row="4" Grid.Column="2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>

                            <TextBlock                                Grid.Row="4" Grid.Column="3" Text="{Binding ConverterParameter='장비완료시간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="txtEndTime"           Grid.Row="4" Grid.Column="4" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>

                            <TextBlock                                Grid.Row="4" Grid.Column="5" Text="{Binding ConverterParameter='가동시간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <Grid Grid.Row="4" Grid.Column="6" Margin="6,0,8,0" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <TextBox Grid.Column="0" x:Name="txtWorkMinute"        Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" HorizontalContentAlignment="Right"/>
                                <TextBlock Grid.Column="1"  Text="{Binding ConverterParameter='분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" HorizontalAlignment="Left" VerticalAlignment="Center" FontWeight="SemiBold"/>
                            </Grid>

                            <TextBlock                                Grid.Row="6" Grid.Column="1" Text="{Binding ConverterParameter='생산 수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="txtInputQty"     Grid.Row="6" Grid.Column="2" ShowButtons="False" Format="#,##0" IsEnabled="False" FontWeight="Bold" Margin="8,0,8,0" />
                            <TextBlock                                Grid.Row="6" Grid.Column="3" Text="{Binding ConverterParameter='양품수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="txtGoodQty"      Grid.Row="6" Grid.Column="4" ShowButtons="False" Format="#,##0" IsEnabled="False" FontWeight="Bold" Margin="8,0,8,0" />

                            <TextBlock   x:Name="tbParentQtyInfo"     Grid.Row="6" Grid.Column="5" Text="{Binding ConverterParameter='잔량(M/EA)', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold" Visibility="Collapsed" />
                            <Grid Grid.Row="6" Grid.Column="6" x:Name="grdParentQty" Visibility="Collapsed">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="4"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <TextBox Grid.Column="0" x:Name="txtParent2_M"       Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" Visibility="Collapsed"/>
                                <TextBox Grid.Column="9" x:Name="txtParent2"         Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" Visibility="Collapsed"/>
                            </Grid>
                        </Grid>

                        <!-- Row 2 -->
                        <Grid x:Name="grdLotInfoCommon" Grid.Row="2" Grid.Column="2" Visibility="Visible">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="13" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="13" />
                            </Grid.ColumnDefinitions>

                            <TextBlock                                Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold" />
                            <TextBox  x:Name="textLotId"            Grid.Row="0" Grid.Column="2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" FontWeight="Bold" Margin="8,0,8,0"/>
                            <TextBlock                              Grid.Row="0" Grid.Column="3" Text="{Binding ConverterParameter='작업시간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox  x:Name="textWorkTime"         Grid.Row="0" Grid.Column="4" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>
                            <TextBlock                              Grid.Row="0" Grid.Column="5" Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="textProdId"         Grid.Row="0" Grid.Column="6" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>

                            <TextBlock                              Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="textProjectName"    Grid.Row="2" Grid.Column="2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>
                            <TextBlock                              Grid.Row="2" Grid.Column="3" Text="{Binding ConverterParameter='생산 수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="textInputQty"  Grid.Row="2" Grid.Column="4" ShowButtons="False" Format="#,##0" IsEnabled="False" FontWeight="Bold" Margin="8,0,8,0" />
                            <TextBlock                              Grid.Row="2" Grid.Column="5" Text="{Binding ConverterParameter='양품수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="textGoodQty"   Grid.Row="2" Grid.Column="6" ShowButtons="False" Format="#,##0" IsEnabled="False" FontWeight="Bold" Margin="8,0,8,0" />

                            <!--<TextBlock                              Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold" />
                            <TextBox  x:Name="textLotId"            Grid.Row="0" Grid.Column="2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" FontWeight="Bold" Margin="8,0,8,0"/>
                            <TextBlock                              Grid.Row="0" Grid.Column="3" Text="{Binding ConverterParameter='작업시간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox  x:Name="textWorkTime"         Grid.Row="0" Grid.Column="4" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>
                            <TextBlock                              Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="textProdId"         Grid.Row="2" Grid.Column="2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>
                            <TextBlock                              Grid.Row="2" Grid.Column="3" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="textProjectName"    Grid.Row="2" Grid.Column="4" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>
                            <TextBlock                              Grid.Row="4" Grid.Column="1" Text="{Binding ConverterParameter='생산 수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="textInputQty"  Grid.Row="4" Grid.Column="2" ShowButtons="False" Format="#,##0" IsEnabled="False" FontWeight="Bold" Margin="8,0,8,0" />
                            <TextBlock                              Grid.Row="4" Grid.Column="3" Text="{Binding ConverterParameter='양품수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="textGoodQty"   Grid.Row="4" Grid.Column="4" ShowButtons="False" Format="#,##0" IsEnabled="False" FontWeight="Bold" Margin="8,0,8,0" />-->

                        </Grid>

                        <!-- Row 4 -->
                        <Grid Grid.Row="4" Grid.Column="2">
                            <c1:C1DataGrid x:Name="dgDetail" 
                                           IsReadOnly="True" 
                                           AlternatingRowBackground="White"
                                           HeadersVisibility="Column"
                                           SelectionMode="SingleRow">
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridTextColumn    Header="항목"        Binding="{Binding ITEM}"                    HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn    Header="합계"        Binding="{Binding DFCT_SUM}"                HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn    Header="불량"        Binding="{Binding DTL_DEFECT_LOT}"          HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn    Header="LOSS"        Binding="{Binding DTL_LOSS_LOT}"            HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn    Header="물품청구"    Binding="{Binding DTL_CHARGE_PROD_LOT}"     HorizontalAlignment="Center"/>
                                </c1:C1DataGrid.Columns>
                            </c1:C1DataGrid>
                        </Grid>
                    </Grid>

                    <Grid x:Name="grdDataCollect" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>

                        <Border Grid.Row="0" Grid.Column="0" Background="#e8ebed" BorderThickness="1" BorderBrush="#8a8a8a"></Border>
                        <Border Grid.Row="0" Grid.Column="2" Background="#e8ebed" BorderThickness="1" BorderBrush="#8a8a8a"></Border>

                        <GridSplitter Grid.Row="0" Grid.Column="1" Grid.RowSpan="99" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                        <Grid x:Name="grdDataCollectLeft" Grid.Row="1" Grid.Column="0">
                            <c1:C1TabControl Name="tcDataCollectLeft" Grid.Row="1" >
                                <!--불량/Loss/물품청구 NND 공정진척 용도-->
                                <c1:C1TabItem x:Name="tabDefectNotching" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='불량/LOSS/물품청구'}" >
                                    <Grid>
                                        <Grid.Resources>
                                            <DataTemplate x:Key="ResultTemplate">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*"/>
                                                        <ColumnDefinition Width="6"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right"></TextBlock>
                                                </Grid>
                                            </DataTemplate>
                                        </Grid.Resources>

                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="3"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="5"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <Button     Grid.Row="1" Grid.Column="2" x:Name="btnSecCutOff"        Content="{Binding ConverterParameter='파단관리', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnSecCutOff_Click"/>
                                        <Button     Grid.Row="1" Grid.Column="4" x:Name="btnSaveFaulty"       Content="{Binding ConverterParameter='저장',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnSaveFaulty_Click" />
                                        <c1:C1DataGrid Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="4" x:Name="dgFaulty" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleCell" FrozenBottomRowsCount="1" CommittedEdit="dgFaulty_CommittedEdit" BeginningEdit="dgFaulty_BeginningEdit" LoadedCellPresenter="dgFaulty_LoadedCellPresenter" UnloadedCellPresenter="dgFaulty_UnloadedCellPresenter" >
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridTextColumn       Header = "활동ID"                   Binding = "{Binding ACTID}"                         HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                <c1:DataGridTextColumn       Header = "활동명"                   Binding = "{Binding ACTNAME}"                       ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL" HorizontalAlignment="Center" IsReadOnly="True" />
                                                <c1:DataGridTextColumn       Header = "불량코드"                 Binding = "{Binding RESNCODE}"                      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn       Header = "불량명"                   Binding = "{Binding RESNNAME}"                      HorizontalAlignment="Left" IsReadOnly="True" />
                                                <c1:DataGridTextColumn       Header = "실적반영여부"             Binding = "{Binding RSLT_EXCL_FLAG}"                HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn       Header = "처리항목코드"             Binding = "{Binding PRCS_ITEM_CODE}"                HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn       Header = "코스트센터코드"           Binding = "{Binding COST_CNTR_ID}"                  HorizontalAlignment="Left" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn       Header = "코스트센터"               Binding = "{Binding COST_CNTR_NAME}"                HorizontalAlignment="Left" IsReadOnly="True"/>
                                                <c1:DataGridTextColumn       Header = "DFCT_QTY_CHG_BLOCK_FLAG"  Binding = "{Binding DFCT_QTY_CHG_BLOCK_FLAG}"       HorizontalAlignment="Left" IsReadOnly="True"  Visibility="Collapsed"/>
                                                <c1:DataGridTemplateColumn   Header="CELL등록" Width="Auto" Visibility="Collapsed" >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <Button Grid.Column="2"  x:Name="btnDfctCell" Content="{Binding ConverterParameter=Title17,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_SearchButtonStyle}" VerticalAlignment="Top" Click="btnDfctCell_Click"  />
                                                        </DataTemplate>
                                                    </c1:DataGridTemplateColumn.CellTemplate>
                                                </c1:DataGridTemplateColumn>
                                            </c1:C1DataGrid.Columns>

                                            <c1:C1DataGrid.BottomRows>
                                                <c1:DataGridSummaryRow RowStyle="{StaticResource SummaryRowStyle}"/>
                                            </c1:C1DataGrid.BottomRows>
                                        </c1:C1DataGrid>
                                    </Grid>
                                </c1:C1TabItem>

                                <!--불량/Loss/물품청구-->
                                <c1:C1TabItem x:Name="tabDefect" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='불량/Loss/물품청구'}" >
                                    <Grid>
                                        <Grid.Resources>
                                            <DataTemplate x:Key="ResultTemplate">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*"/>
                                                        <ColumnDefinition Width="6"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right"></TextBlock>
                                                </Grid>
                                            </DataTemplate>
                                        </Grid.Resources>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="3"/>
