<UserControl x:Class="LGC.GMES.MES.ELEC001.ELEC001_031"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
      xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
      mc:Ignorable="d" 
      d:DesignHeight="768" d:DesignWidth="1024" Initialized="UserControl_Initialized" Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded" >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="thickcheck">
        <ctrlLib:LGCLayout Tag="두께측정 데이터" Grid.ColumnSpan="2" >
            <Grid>
                <Grid.Resources>
                    <DataTemplate x:Key ="ResultTemplate">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="1" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                        </Grid>
                    </DataTemplate>
                </Grid.Resources>
                <Grid.RowDefinitions>
                    <RowDefinition Height="50"/>
                    <!--조회 조건 첫번째 높이-->
                    <RowDefinition Height="8" />
                    <RowDefinition/>
                    <!--<RowDefinition Height="8" />
                    <RowDefinition />-->
                </Grid.RowDefinitions>

                <!-- 조회 영역 -->
                <Grid Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <!--조회조건 가로 칸수 하나더 추가하면-->
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="13" />
                        <!-- 조회 조건 레이아웃 열 칸수-->
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="90" />
                        <!--조회버튼-->
                        <ColumnDefinition Width="13" />
                    </Grid.ColumnDefinitions>
                    <!--백그라운드용-->
                    <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                    <!-- 조회조건 배경색-->

                    <!--조회조건-->
                    <!--ROW1-->
                    <Grid Grid.Row="0" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <c1:C1ComboBox    Grid.Column="1" x:Name="cboArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>


                    <Grid Grid.Row="0" Grid.Column="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <c1:C1ComboBox   Grid.Row="1" Grid.Column="1" x:Name="cboEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedIndexChanged="cboEquipmentSegment_SelectedIndexChanged"  />

                    </Grid>

                    <Grid Grid.Row="0" Grid.Column="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <c1:C1ComboBox   Grid.Row="0" Grid.Column="1" x:Name="cboProcess" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    </Grid>


                    <Grid Grid.Row="0" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='설비', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <ctrlLib:MultiSelectionBox   Grid.Row="0" Grid.Column="1" x:Name="cboEquipment" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                        <!--<ctrlLib:MultiSelectionBox                    Grid.Row="1" Grid.Column="5" x:Name="cboProcess" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />-->
                    </Grid>


                    <Grid Grid.Row="0" Grid.Column="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" Text="" />
                        <TextBlock Grid.Column="1" Text="{Binding ConverterParameter='시간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" />
                        <TextBlock Grid.Column="2" Text="" />
                        <c1:C1NumericBox Grid.Column="3" x:Name="txtCycle" ShowButtons="True" Minimum="5" Maximum="600" Value="60"  Format="F0" Margin="0,1" Height="23"/>
                        <Button Grid.Column="4" x:Name="btnCycle" Content="{Binding ConverterParameter=설정, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Cursor="Hand" Click="btnCycle_Click"  />
                    </Grid>
                    <Grid Grid.Row="0" Grid.Column="6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" Text="" />
                        <Button   Grid.Column="1"  x:Name="btnSearchHold" Content="{Binding ConverterParameter=조회, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                    </Grid>
                </Grid>

                <ScrollViewer HorizontalScrollBarVisibility="Visible" Grid.Row="2" Grid.Column="0" VerticalScrollBarVisibility="Hidden">
                    <!--조회 조건 아래 부분-->
                    <Grid Grid.Row="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1"/>
                            <RowDefinition Height="80"/>
                            <RowDefinition Height="6"/>
                            <RowDefinition Height="AUTO"/>
                            <RowDefinition Height="6"/>
                            <RowDefinition Height="40"/>
                            <RowDefinition Height="6"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="6"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="6"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="6"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="6"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="6"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="6"/>
                        </Grid.ColumnDefinitions>
                        <!--1-->
                        <GridSplitter x:Name="dgSplitter1" Grid.Row="3" Grid.Column="1" ResizeDirection="Columns"  ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                        <GridSplitter x:Name="dgSplitter2" Grid.Row="3" Grid.Column="3" ResizeDirection="Columns"  ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                        <GridSplitter x:Name="dgSplitter3" Grid.Row="3" Grid.Column="5" ResizeDirection="Columns"  ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                        <GridSplitter x:Name="dgSplitter4" Grid.Row="3" Grid.Column="7" ResizeDirection="Columns"  ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="Collapsed" />
                        <GridSplitter x:Name="dgSplitter5" Grid.Row="3" Grid.Column="9" ResizeDirection="Columns"  ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="Collapsed" />
                        <GridSplitter x:Name="dgSplitter6" Grid.Row="3" Grid.Column="11" ResizeDirection="Columns"  ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="Collapsed" />
                        <Grid x:Name="dgSearch1" Grid.Row="1" Grid.Column="0"  >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1" />
                                <ColumnDefinition Width="AUTO" />
                                <ColumnDefinition Width="125" />
                                <ColumnDefinition Width="1" />
                                <ColumnDefinition Width="AUTO" />
                                <ColumnDefinition Width="10" />
                                <ColumnDefinition Width="145" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                            <!--드래그 그리드 사이즈 조정-->
                            
                            <TextBlock  Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleTitleStyle}"  />
                            <TextBox Grid.Row="0" Grid.Column="2" x:Name="Lotid1" Style="{StaticResource Content_InputForm_ThickTextBoxStyle}" IsReadOnly="True"/>

                            <TextBlock  Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleTitleStyle}"/>
                            <TextBox Grid.Row="1" Grid.Column="2" x:Name="Pjt1" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" IsReadOnly="True"/>

                            <TextBlock  Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='설비', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleTitleStyle}"/>
                            <TextBox Grid.Row="1" Grid.Column="6" x:Name="Polarity1" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" IsReadOnly="True"/>
                            <TextBox Grid.Row="0" Grid.Column="7" x:Name="Cbo_code1" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" IsReadOnly="True" Visibility="Collapsed"/>
                            <TextBox Grid.Row="0" Grid.Column="8" x:Name="Wipseq1" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" IsReadOnly="True" Visibility="Collapsed"/>

                        </Grid>


                        <c1:C1DataGrid x:Name="dgQuality1" Grid.Row="3" Grid.Column="0" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="MultiRange" CanUserSort="False" CanUserFilter="False" Margin="0,0,0,0" LoadedCellPresenter="LoadedQualityInfoCellPresenter" FontSize="10" >
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn Header="항목" Binding="{Binding CLCTITEM}" HorizontalAlignment="Left" IsReadOnly="True" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="항목1" Binding="{Binding CLSS_NAME1}" HorizontalAlignment="Left" IsReadOnly="True" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateCount />
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn Header="항목2" Binding="{Binding CLSS_NAME2}" HorizontalAlignment="Left" IsReadOnly="True" />
                                <c1:DataGridTextColumn Header="항목3" Binding="{Binding CLSS_NAME3}" HorizontalAlignment="Left" IsReadOnly="True" />
                                <c1:DataGridTextColumn Header="LSL" Binding="{Binding LSL}" HorizontalAlignment="Center" IsReadOnly="True" />
                                <c1:DataGridTextColumn Header="USL" Binding="{Binding USL}" HorizontalAlignment="Center" IsReadOnly="True" />
                                <c1:DataGridTextColumn Header="CLCTSEQ" Binding="{Binding CLCTSEQ}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="INSP_VALUE_TYPE_CODE" Binding="{Binding INSP_VALUE_TYPE_CODE}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                <c1:DataGridTemplateColumn Header="측정값" EditOnSelection="True" Name="CLCTVAL01" MinWidth="45">
                                    <c1:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel>
                                                <c1:C1NumericBox Value="{Binding CLCTVAL01, Mode=TwoWay, Converter={StaticResource NumericBoxConverter}}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource C1NumericBoxStyle}" HorizontalAlignment="Stretch" Format="F2" AllowNull="True" HandleUpDownKeys="False" ShowButtons="False" Background="Transparent" BorderThickness="0" />
                                                <ComboBox  Text="{Binding CLCTVAL01}" Visibility="{Binding COMBOVISIBLE}" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" >
                                                    <ComboBoxItem>OK</ComboBoxItem>
                                                    <ComboBoxItem>NG</ComboBoxItem>
                                                </ComboBox>
                                            </StackPanel>
                                        </DataTemplate>
                                    </c1:DataGridTemplateColumn.CellTemplate>
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateAvg ResultTemplate="{StaticResource ResultTemplate}" />
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTemplateColumn>
                                <c1:DataGridTextColumn Header="단위" Binding="{Binding CLCTUNIT}" HorizontalAlignment="Left" IsReadOnly="True"/>
                                <c1:DataGridTextColumn Header="필수체크항목" Binding="{Binding MAND_INSP_ITEM_FLAG}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="세부항목" Binding="{Binding INSP_ITEM_ID}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="세부항목명" Binding="{Binding INSP_ITEM_NAME}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="USL상한치" Binding="{Binding USL_LIMIT}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="LSL하한치" Binding="{Binding LSL_LIMIT}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                            </c1:C1DataGrid.Columns>
                            <!--<c1:C1DataGrid.BottomRows>
                                <c1:DataGridSummaryRow x:Name="defectAvry1" RowStyle="{StaticResource SummaryRowStyle}" DetailsVisibility="Visible" />
                            </c1:C1DataGrid.BottomRows>-->
                        </c1:C1DataGrid>
                        
                        <!--저장버튼 그리드-->
                        <Grid x:Name="dgBottom1" Grid.Row="5" Grid.Column="0" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="13" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="80" />
                                <ColumnDefinition Width="13" />
                            </Grid.ColumnDefinitions>

                            <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                            <TextBlock Grid.Row="0" Grid.Column="1" Text="" />
                            <Button Grid.Row="0" Grid.Column="2" x:Name="btnSaveQuality1" Content="{Binding ConverterParameter=저장, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Cursor="Hand" Click="btnSaveQuality1_Click" Margin="0,8,0,0" VerticalAlignment="Top" />
                        </Grid>
                        <!--저장버튼 그리드-->



                        <!--2-->
                        <Grid x:Name="dgSearch2" Grid.Row="1" Grid.Column="2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1" />
                                <ColumnDefinition Width="AUTO" />
                                <ColumnDefinition Width="125" />
                                <ColumnDefinition Width="1" />
                                <ColumnDefinition Width="AUTO" />
                                <ColumnDefinition Width="10" />
                                <ColumnDefinition Width="145" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Border     Grid.Row="0" Grid.Column="1" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                            <TextBlock  Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleTitleStyle}"/>
                            <TextBox Grid.Row="0" Grid.Column="2" x:Name="Lotid2" Style="{StaticResource Content_InputForm_ThickTextBoxStyle}" IsReadOnly="True"/>

                            <TextBlock  Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleTitleStyle}"/>
                            <TextBox Grid.Row="1" Grid.Column="2" x:Name="Pjt2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" IsReadOnly="True"/>

                            <TextBlock  Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='설비', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleTitleStyle}"/>
                            <TextBox Grid.Row="1" Grid.Column="6" x:Name="Polarity2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" IsReadOnly="True"/>
                            <TextBox Grid.Row="0" Grid.Column="7" x:Name="Cbo_code2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" IsReadOnly="True" Visibility="Collapsed"/>
                            <TextBox Grid.Row="0" Grid.Column="8" x:Name="Wipseq2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" IsReadOnly="True" Visibility="Collapsed"/>
                        </Grid>
                        <c1:C1DataGrid x:Name="dgQuality2" Grid.Row="3" Grid.Column="2" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="MultiRange" CanUserSort="False" CanUserFilter="False" Margin="0,0,0,0" LoadedCellPresenter="LoadedQualityInfoCellPresenter" FontSize="10" >
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn Header="항목" Binding="{Binding CLCTITEM}" HorizontalAlignment="Left" IsReadOnly="True" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="항목1" Binding="{Binding CLSS_NAME1}" HorizontalAlignment="Left" IsReadOnly="True" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateCount />
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn Header="항목2" Binding="{Binding CLSS_NAME2}" HorizontalAlignment="Left" IsReadOnly="True" />
                                <c1:DataGridTextColumn Header="항목3" Binding="{Binding CLSS_NAME3}" HorizontalAlignment="Left" IsReadOnly="True" />
                                <c1:DataGridTextColumn Header="LSL" Binding="{Binding LSL}" HorizontalAlignment="Center" IsReadOnly="True" />
                                <c1:DataGridTextColumn Header="USL" Binding="{Binding USL}" HorizontalAlignment="Center" IsReadOnly="True" />
                                <c1:DataGridTextColumn Header="CLCTSEQ" Binding="{Binding CLCTSEQ}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="INSP_VALUE_TYPE_CODE" Binding="{Binding INSP_VALUE_TYPE_CODE}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                <c1:DataGridTemplateColumn Header="측정값" EditOnSelection="True" Name="CLCTVAL01" MinWidth="45">
                                    <c1:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel>
                                                <c1:C1NumericBox Value="{Binding CLCTVAL01, Mode=TwoWay, Converter={StaticResource NumericBoxConverter}}" Visibility="{Binding TEXTVISIBLE}" Style="{StaticResource C1NumericBoxStyle}" HorizontalAlignment="Stretch" Format="F2" AllowNull="True" HandleUpDownKeys="False" ShowButtons="False" Background="Transparent" BorderThickness="0" />
                                                <ComboBox  Text="{Binding CLCTVAL01}" Visibility="{Binding COMBOVISIBLE}" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" >
                                                    <ComboBoxItem>OK</ComboBoxItem>
                                                    <ComboBoxItem>NG</ComboBoxItem>
                                                </ComboBox>
                                            </StackPanel>
                                        </DataTemplate>
                                    </c1:DataGridTemplateColumn.CellTemplate>
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateAvg ResultTemplate="{StaticResource ResultTemplate}" />
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTemplateColumn>
                                <c1:DataGridTextColumn Header="단위" Binding="{Binding CLCTUNIT}" HorizontalAlignment="Left" IsReadOnly="True"/>
                                <c1:DataGridTextColumn Header="필수체크항목" Binding="{Binding MAND_INSP_ITEM_FLAG}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="세부항목" Binding="{Binding INSP_ITEM_ID}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="세부항목명" Binding="{Binding INSP_ITEM_NAME}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="USL상한치" Binding="{Binding USL_LIMIT}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                <c1:DataGridTextColumn Header="LSL하한치" Binding="{Binding LSL_LIMIT}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                            </c1:C1DataGrid.Columns>
                            <!--<c1:C1DataGrid.BottomRows>
                                <c1:DataGridSummaryRow x:Name="tdefectAvry" RowStyle="{StaticResource SummaryRowStyle}" DetailsVisibility="Visible" />
                            </c1:C1DataGrid.BottomRows>-->
                        </c1:C1DataGrid>
                        <!--저장버튼 그리드-->
                        <Grid x:Name="dgBottom2" Grid.Row="5" Grid.Column="2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="13" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="80" />
                                <ColumnDefinition Width="13" />
                            </Grid.ColumnDefinitions>

                            <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                            <TextBlock Grid.Row="0" Grid.Column="1" Text="" />
                            <Button Grid.Row="0" Grid.Column="2" x:Name="btnSaveQuality2" Content="{Binding ConverterParameter=저장, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Cursor="Hand" Click="btnSaveQuality2_Click" />
                        </Grid>
                        <!--저장버튼 그리드 2 -->

                        <!--3-->
                        <Grid x:Name="dgSearch3" Grid.Row="1" Grid.Column="4">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1" />
                                <ColumnDefinition Width="AUTO" />
                                <ColumnDefinition Width="125" />
                                <ColumnDefinition Width="1" />
                                <ColumnDefinition Width="AUTO" />
                                <ColumnDefinition Width="10" />
                                <ColumnDefinition Width="145" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Border     Grid.Row="0" Grid.Column="1" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                            <TextBlock  Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleTitleStyle}"/>
                            <TextBox Grid.Row="0" Grid.Column="2" x:Name="Lotid3" Style="{StaticResource Content_InputForm_ThickTextBoxStyle}" IsReadOnly="True"/>

                            <TextBlock  Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleTitleStyle}"/>
