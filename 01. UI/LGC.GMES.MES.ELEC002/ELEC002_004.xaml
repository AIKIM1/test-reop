<UserControl x:Class="LGC.GMES.MES.ELEC002.ELEC002_004"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>    
    <Grid Background="#FFDDDDDD" x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="슬리터 공정진척 (CWA용 자동확정UI)">
            <!--#region Main Grid -->
            <Grid x:Name="MainGrid">
                <Grid.RowDefinitions>
                    <RowDefinition Height="51" />
                    <RowDefinition Height="5" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <!--#region Button Area -->
                <Grid x:Name="grdCommand" Grid.Row="0" Grid.Column="0" Margin="0,-47,0,0" VerticalAlignment="Top">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="34" />
                    </Grid.ColumnDefinitions>
                    <TextBox Grid.Column="1" Name="txtEndLotId" Width="100" Visibility="Collapsed" />
                    <c1:C1DropDownButton Grid.Row="0" Grid.Column="2" x:Name="btnExtra" Style="{StaticResource C1DropDownButtonStyle}" VerticalAlignment="Bottom" Height="30">
                        <c1:C1DropDownButton.Header>
                            <TextBlock x:Name="selection" Text="{Binding ConverterParameter='추가기능', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" TextAlignment="Center" VerticalAlignment="Center"/>
                        </c1:C1DropDownButton.Header>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="4" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />                                
                                <RowDefinition Height="4" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="9"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                            <!--추가기능버튼-->
                            <Button Grid.Row="2" Grid.Column="1" x:Name="btnEqptIssue" Content="{Binding ConverterParameter='설비특이사항', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Margin="0,3" Height="34"  />
                            <Button Grid.Row="3" Grid.Column="1" x:Name="btnEqptCond" Content="{Binding ConverterParameter='작업조건등록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Margin="0,3" Height="34" />
                            <Button Grid.Row="4" Grid.Column="1" x:Name="btnSamplingProdT1" Content="{Binding ConverterParameter='샘플링 제품등록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Visibility="Collapsed" Margin="0,3" Height="34" />
                            <Button Grid.Row="5" Grid.Column="1" x:Name="btnSkidTypeSettingByPort" Content="{Binding ConverterParameter='Port별 Skid Type 설정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Visibility="Collapsed" Margin="0,3" Height="34" />
                            <Button Grid.Row="1" Grid.Column="1" x:Name="btnManualWorkMode" Content="{Binding ConverterParameter='수작업모드', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Margin="0,3" Height="34" />
                        </Grid>
                    </c1:C1DropDownButton>
                    <Button Grid.Row="0" Grid.Column="3" x:Name="btnStart" Content="{Binding ConverterParameter='작업시작', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl01Style}" Margin="5,0,0,0" />
                    <Button Grid.Row="0" Grid.Column="4" x:Name="btnCancel" Content="{Binding ConverterParameter='시작취소', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl03Style}" Margin="5,0,0,0" />
                    <Button Grid.Row="0" Grid.Column="5" x:Name="btnEqptEnd" Content="{Binding ConverterParameter='장비완료', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl04Style}" Margin="5,0,0,0" />
                    <Button Grid.Row="0" Grid.Column="6" x:Name="btnEqptEndCancel" Content="{Binding ConverterParameter='장비완료취소', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl04Style}" Margin="5,0,0,0" />
                    <Button Grid.Row="0" Grid.Column="7" x:Name="btnEndCancel" Content="{Binding ConverterParameter='실적확정취소', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl04Style}" Margin="5,0,0,0" />
                    <Button Grid.Row="0" Grid.Column="8" x:Name="btnDispatch" Content="{Binding ConverterParameter='공정이동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl02Style}" Margin="5,0,0,0" />
                    <Button Grid.Row="0" Grid.Column="9" x:Name="btnBarcodeLabel" Content="{Binding ConverterParameter='바코드발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControlBarCodeStyle}" Margin="5,0,0,0" Visibility="Collapsed" />
                    <Button Grid.Row="0" Grid.Column="10" x:Name="btnPrintLabel" Content="{Binding ConverterParameter='발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControlBarCodeStyle}" Margin="5,0,0,0" />
                    <Button Grid.Row="0" Grid.Column="11" x:Name="btnHistoryCard" Content="{Binding ConverterParameter='이력카드', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl02Style}" Margin="5,0,0,0" Visibility="Collapsed" />
                </Grid>
                <!--#endregion-->
                <!--#region Search Area -->
                <Grid x:Name="grdSearch" Grid.Row="0" Grid.Column="0">
                    <Grid Grid.Row="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="51" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                        <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="13"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="13"/>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Row="0" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>
                                <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                </StackPanel>
                                <c1:C1ComboBox Grid.Row="0" Grid.Column="1" x:Name="cboEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="" SelectedValuePath="" />
                                <StackPanel Grid.Row="0" Grid.Column="2" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='설비', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                </StackPanel>
                                <c1:C1ComboBox Grid.Row="0" Grid.Column="3" x:Name="cboEquipment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="" SelectedValuePath="" />

                                <StackPanel Grid.Row="0" Grid.Column="4" Name="spSingleCoater" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='TOP/BACK', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                </StackPanel>
                                <c1:C1ComboBox Grid.Row="0" Grid.Column="5" x:Name="cboCoatType" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="" SelectedValuePath="" />

                                <Grid Grid.Row="0" Grid.Column="5" x:Name="gridWorkHalfSlittingSide" Visibility="Collapsed">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="3*"/>
                                        <ColumnDefinition Width="7*"/>
                                    </Grid.ColumnDefinitions>

                                    <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                        <TextBlock Text="{Binding ConverterParameter='무지부', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                    </StackPanel>
                                    <TextBox  Grid.Row="0" Grid.Column="1" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" Margin="5,0,5,0" x:Name="txtWorkHalfSlittingSide" CharacterCasing="Upper" MinWidth="120"/>
                                </Grid>

                                <StackPanel Grid.Row="2" Grid.Column="5" Orientation="Horizontal" HorizontalAlignment="Left" >
                                    <RadioButton Grid.Row="0" Grid.Column="5" Style="{StaticResource SearchCondition_RadioButtonStyle}" Content="{Binding ConverterParameter='TOP', Converter={StaticResource GlobalObjectDicConverter}}"  Name="rdoTop" GroupName="cboCoatType2" Margin="3,0,0,0"  Visibility="Collapsed" />
                                    <RadioButton Grid.Row="0" Grid.Column="5" Style="{StaticResource SearchCondition_RadioButtonStyle}" Content="{Binding ConverterParameter='BACK', Converter={StaticResource GlobalObjectDicConverter}}"  Name="rdoBack" GroupName="cboCoatType2" Margin="5,0,0,0"  Visibility="Collapsed" />
                                </StackPanel>

                                <StackPanel Grid.Row="0" Grid.Column="8" Orientation="Horizontal">
                                    <CheckBox x:Name="chkWait" Style="{StaticResource SearchCondition_CheckBoxStyle}" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=대기}" Tag="WAIT"/>
                                    <Border Width="15"/>
                                    <CheckBox x:Name="chkRun" Style="{StaticResource SearchCondition_CheckBoxStyle}" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=진행}" IsChecked="True" Tag="PROC"/>
                                    <Border Width="15"/>
                                    <CheckBox x:Name="chkEqpEnd" Style="{StaticResource SearchCondition_CheckBoxStyle}" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=설비완공}" IsChecked="True" Tag="EQPT_END"/>
                                    <Border Width="15"/>
                                    <CheckBox x:Name="chkConfirm" Style="{StaticResource SearchCondition_CheckBoxStyle}" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=완공}" IsChecked="True" Tag="END" />
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Row="0" Grid.Column="3">
                                <Button x:Name="btnSearch" Grid.Row="0" Grid.Column="0" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" />
                            </Grid>
                        </Grid>
                    </Grid>
                </Grid>
                <!--#endregion-->
                <!--#region Contents Area -->
                <Grid x:Name="Content" Grid.Row="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="2*"/>
                        <RowDefinition Height="8"/>
                        <RowDefinition x:Name="yPosition" Height="3*"/>
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="8"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*" />
                        <ColumnDefinition Width="8" />
                        <ColumnDefinition Width="1*" />
                    </Grid.ColumnDefinitions>
                    <!--start WorkOrder-->
                    <Grid x:Name="grdWorkOrder" Grid.Row="1" Grid.Column="0">
                        <!--<local:UC_WORKORDER_CWA />-->
                    </Grid>
                    <!--end WorkOrder-->
                    <!--start Product Lot-->
                    <Grid x:Name="grdProdLot" Grid.Row="1" Grid.Column="2">
                        <Grid Grid.Row="1" Grid.Column="3">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="6"/>
                                <RowDefinition/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="3*"/>
                                <ColumnDefinition Width="5"/>
                            </Grid.ColumnDefinitions>
                            <!-- 1row-->
                            <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="6" Grid.ColumnSpan="5" Style="{StaticResource BorderSubTitleStyle}" BorderThickness="1" />
                            <TextBlock Grid.Row="0" Grid.Column="1" Style="{StaticResource Content_ModuleTitleStyle}">
                                <Run Text="{Binding ConverterParameter='Product Lot', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                            </TextBlock>
                            <Grid Grid.Column="3" HorizontalAlignment="Right">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <CheckBox Grid.Column="0" x:Name="chkWoProduct" Style="{StaticResource SearchCondition_CheckBoxStyle}" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=W/O선택제품}" HorizontalAlignment="Right" />
                                <c1:C1ComboBox Grid.Column="2" x:Name="cboColor" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                <Button   Grid.Column="4" x:Name="btnCancelDelete" Content="{Binding ConverterParameter='CANCEL_DELETE_LOT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}"  Margin="2"/>

                            </Grid>
                            <!--2row-->
                            <Border Grid.Row="1" Grid.RowSpan="3" Grid.ColumnSpan="6" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                            <!--3row-->
                            <Grid Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="3" >
                                <!--<StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch">-->
                                <Grid>                                    
                                    <c1:C1DataGrid Grid.Column="1" x:Name="dgProductLot" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleRow" HorizontalScrollBarVisibility="Auto" CanUserSort="False" CanUserFilter="True">
                                        <c1:C1DataGrid.Columns>
                                            <!--<c1:DataGridCheckBoxColumn Name="CHK" Binding="{Binding Path=CHK, NotifyOnSourceUpdated=False, NotifyOnTargetUpdated=True, Mode=TwoWay}" Width="Auto" />-->
                                            <c1:DataGridTextColumn IsReadOnly="True" Width="0.1" />
                                            <c1:DataGridCheckBoxColumn Name="CHK" Width="Auto" Binding="{Binding CHK}" HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn Header="LOTID" Binding="{Binding LOTID}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="OUT_CARRIERID" Binding="{Binding OUT_CSTID}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="CUT" Binding="{Binding CUT}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="PJT" Binding="{Binding PRJT_NAME}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="코터PJT" Binding="{Binding COATER_PRJT_NAME}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <!--<c1:DataGridTextColumn Header="버전" Binding="{Binding COATERVER}" HorizontalAlignment="Center" IsReadOnly="True" />-->
                                            <c1:DataGridTextColumn Header="버전" Binding="{Binding PROD_VER_CODE}" HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn Header="CUTID" Binding="{Binding CUT_ID}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="투입 LOT" Binding="{Binding LOTID_PR}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="CarrierID" Binding="{Binding CSTID}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn Header="투입량" Binding="{Binding INPUTQTY}" HorizontalAlignment="Right" Format="#,###.00" ShowButtons="False" IsReadOnly="True"/>
                                            <c1:DataGridNumericColumn Header="압연차수" Binding="{Binding PRESSCOUNT}" HorizontalAlignment="Center" ShowButtons="False" IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridNumericColumn Header="재공량" Binding="{Binding WIPQTY}" HorizontalAlignment="Right" Format="#,###.00" ShowButtons="False" IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridNumericColumn Header="생산량" Binding="{Binding OUTPUTQTY}" HorizontalAlignment="Right" Format="#,###.00" ShowButtons="False" IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="상태" Binding="{Binding WIPSTAT_NAME}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="단위" Binding="{Binding UNIT_CODE}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="제품ID" Binding="{Binding PRODID}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="제품명" Binding="{Binding PRODNAME}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header="제품SPEC" Binding="{Binding PRODDESC}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="모델" Binding="{Binding MODELID}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="극성" Binding="{Binding ELEC_TYPE}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="HOLD" Binding="{Binding WIPHOLD}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="시작시간" Binding="{Binding WIPDTTM_ST}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="완료시간" Binding="{Binding WIPDTTM_ED}" HorizontalAlignment="Center" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn Header="작업지시" Binding="{Binding WOID}" HorizontalAlignment="Center" IsReadOnly="True" />
                                            <!--<c1:DataGridTextColumn Header="조건조정량" Binding="{Binding CTRLQTY}" HorizontalAlignment="Center" />-->
                                            <!-- Hidden Column -->
                                            <c1:DataGridTextColumn Header="대LOT" Binding="{Binding LOTID_LARGE}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn Header="투입량" Binding="{Binding INPUTQTY}" HorizontalAlignment="Right" Format="#,###.00" ShowButtons="False" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn Header="조건조정량" Binding="{Binding CTRLQTY}" HorizontalAlignment="Right" Format="#,###.00" ShowButtons="False" IsReadOnly="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header="설비" Binding="{Binding EQPTID}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="LOT상태" Binding="{Binding WIPSTAT}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="작업조" Binding="{Binding SHIFT}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="작업자" Binding="{Binding USERID_ED}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="특이사항" Binding="{Binding REMARK}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="작업일자" Binding="{Binding WORKDATE}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="믹서버전" Binding="{Binding MIXER_VER}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn Header="FINALCUT" Binding="{Binding FINAL_CUT_FLAG}" HorizontalAlignment="Center" Visibility="Collapsed" />
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                </Grid>
                                <!--</StackPanel>-->
                            </Grid>
                            <!--end Product Lot-->
                        </Grid>
                    </Grid>
                    <GridSplitter Grid.Row="2" Grid.Column="0" ResizeDirection="Rows" Grid.ColumnSpan="3" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                    <!--<GridSplitter Grid.Row="2" Grid.Column="2" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />-->
                    <GridSplitter Grid.Row="1" Grid.Column="1" ResizeDirection="Columns" Grid.RowSpan="3" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                    <!--<GridSplitter Grid.Row="3" Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />-->
                    <!--<GridSplitter Grid.Row="4" Grid.Column="0" ResizeDirection="Rows" Grid.ColumnSpan="3" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />-->
                    <!--end Product Lot-->
                    <!--start detail-->
                    <Grid x:Name="grdResult" Grid.Row="3" Grid.Column="0">
                        <Border Grid.Row="3" Grid.Column="1" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                        <Grid Grid.Row="3" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="31"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="165"/>
                                <RowDefinition Height="6"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="6"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="6"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="6"/>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Name="grdTopSummary">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <!--<ColumnDefinition Width="*"/>-->
                                    <!--<ColumnDefinition Width="*"/>-->
                                </Grid.ColumnDefinitions>
                                <Border Grid.Row="0" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}"/>
                                <Grid x:Name="grdResultTitle" Grid.Column="0">
                                    <Grid.ColumnDefinitions>
                                        <!--<ColumnDefinition Width="100"/>-->
                                        <ColumnDefinition Width="*" />
                                        <!-- <ColumnDefinition Width="8"/> -->
                                        <!-- <ColumnDefinition Width="*"/> -->
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}" Text="{Binding ConverterParameter='생산실적', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" />
                                </Grid>
                            </Grid>
                            <!--1row-->
                            <Border Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left" >
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Height="Auto" >
                                    <Button x:Name="btnSaveRegDefectLane" Content="{Binding ConverterParameter=전수불량Lane등록, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Cursor="Hand" Width="Auto"  Visibility="Collapsed"/>
                                </StackPanel>
                            </Border>
                            <Border Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right" Visibility="Visible">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Height="Auto">
