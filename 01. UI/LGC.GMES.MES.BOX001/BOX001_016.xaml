<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.BOX001.BOX001_016"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024"
             Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded"  >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <Storyboard x:Key="storyBoard" RepeatBehavior="Forever" AutoReverse="True">
                <ColorAnimation  Storyboard.TargetName="btnQuality" Storyboard.TargetProperty = "(Canvas.Background).(LinearGradientBrush.GradientStops)[0].(GradientStop.Color)" From="#FFFFFFFF" To="Orange" Duration="0:0:1"/>
                <ColorAnimation  Storyboard.TargetName="btnQuality" Storyboard.TargetProperty = "(Canvas.Background).(LinearGradientBrush.GradientStops)[1].(GradientStop.Color)" From="#FFEFEFEF" To="Orange" Duration="0:0:1"/>
            </Storyboard>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="전극포장및출고">
            <Grid>

                <c1:C1TabControl  Grid.Row="0" Grid.Column="1">
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='포장출고'}" >
                        <Grid>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="45"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="0.6*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="1.4*"/>
                            </Grid.RowDefinitions>

                            <Grid x:Name="SearchArea" Grid.Row="1">
                                <Border Grid.Row="0" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                                <Grid   Grid.Row="0" Grid.Column="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="13"/>
                                    </Grid.ColumnDefinitions>

                                    <Grid Grid.Row="0" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="3*"/>
                                            <ColumnDefinition Width="7*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='제품구분', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" Visibility="Collapsed" />

                                        <uc:UcBaseComboBox Grid.Row="0"  Grid.Column="1"  x:Name="cboType" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" Visibility="Collapsed" SelectedValueChanged="cboType_SelectedValueChanged"/>

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='출고처', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                        <uc:UcBaseComboBox Grid.Row="0"  Grid.Column="1"  x:Name="cboTransLoc2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='SKIDID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                        <TextBox Grid.Row="0"  Grid.Column="1" x:Name="txtLotID" CharacterCasing="Upper" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="8,3"  MinWidth="100" 
                                             KeyDown="txtLotID_KeyDown" />

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="4">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='COMMENT', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                        <TextBox Grid.Row="0"  Grid.Column="1" x:Name="txtComment" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="8,3"  MinWidth="100" />

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                        <TextBox Grid.Row="0"  Grid.Column="1" x:Name="txtLotIDScan" Style="{StaticResource SearchCondition_TextBoxStyle}" Margin="8,3"  MinWidth="100" 
                                             KeyDown="txtLotIDScan_KeyDown" />

                                    </Grid>

                                    <Grid Grid.Row="0" Grid.Column="6" HorizontalAlignment="Right">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <StackPanel x:Name="stQuality" Grid.Row="0" Grid.Column="0" Orientation="Horizontal" Visibility="Collapsed" >
                                            <uc:UcBaseCheckBox x:Name="chkQuality" Style="{StaticResource SearchCondition_CheckBoxStyle}" Padding="5,0,0,0" Visibility="Collapsed" />
                                            <Button x:Name="btnQuality" Content="{Binding ConverterParameter=미검사/불합격 조회,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Cursor="Hand" FontSize="12" FontFamily="/LGC.GMES.MES.ControlsLibrary;Component/#맑은 고딕" Foreground="#FF333333" FontWeight="Bold" Padding="12,-1,12,0" BorderBrush="#FFAAAAAA" BorderThickness="1" Click="btnQuality_Click"  >
                                                <Button.Background>
                                                    <LinearGradientBrush EndPoint="0.504,1.5" StartPoint="0.504,0.03">
                                                        <GradientStop Color="#FFFFFFFF" Offset="0"/>
                                                        <GradientStop Color="#FFEFEFEF" Offset="0.5"/>
                                                    </LinearGradientBrush>
                                                </Button.Background>
                                            </Button>
                                        </StackPanel>

                                        <uc:UcBaseButton Grid.Row="0" Grid.Column="2"  x:Name="btnRefresh" Content="{Binding ConverterParameter=초기화,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Cursor="Hand" Click="btnRefresh_Click" />
                                        <uc:UcBaseButton Grid.Row="0" Grid.Column="4"  x:Name="btnPackOut" Content="{Binding ConverterParameter=포장구성,  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnPackOut_Click" />

                                    </Grid>


                                </Grid>

                            </Grid>

                            <Grid x:Name="InfoArea" Grid.Row="3">
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회결과', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>


                            </Grid>

                            <Grid x:Name="GridArea" Grid.Row="4">
                                <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                <c1:C1DataGrid x:Name="dgOut" IsReadOnly="True" HeadersVisibility="Column" Margin="8" CanUserRemoveRows="True" CanUserAddRows="True"
                                               SelectionMode="SingleRow" Grid.ColumnSpan="2">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn        Header = "SKIDID"         Binding = "{Binding LOTID}"           HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "PRODID"         Binding = "{Binding PRODID}"          HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "모델"           Binding = "{Binding MODLID}"          HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "PJT"            Binding = "{Binding PROJECTNAME}"     HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "극성"           Binding = "{Binding PRDT_CLSS_NAME}"  HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "C/ROLL"         Binding = "{Binding M_WIPQTY}"        HorizontalAlignment="Center" Format="###,###,##0.##" />
                                        <c1:DataGridTextColumn        Header = "S/ROLL"         Binding = "{Binding CELL_WIPQTY}"     HorizontalAlignment="Center" Format="###,###,##0.##" />
                                        <c1:DataGridTextColumn        Header = "수량(Lane)"     Binding = "{Binding LANE_QTY}"        HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "PROCID"         Binding = "{Binding PROCID}"          HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn        Header = "시장유형코드"   Binding = "{Binding MKT_TYPE_CODE}"   HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn        Header = "전극버전"       Binding = "{Binding PROD_VER_CODE}"   HorizontalAlignment="Center" Visibility="Hidden"/>

                                        <c1:DataGridTemplateColumn Width="Auto" Header="삭제" x:Name="btnDelete">
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button  x:Name="btnDelete" Width="Auto" Content="{Binding ConverterParameter=삭제, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                                 Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDelete_Click"/>
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>

                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>


                            <Grid x:Name="dgSub" Grid.Row="6" Grid.Column="0" >


                            </Grid>



                        </Grid>

                    </c1:C1TabItem>

                    <c1:C1TabItem x:Name="TabOUT_Hist" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='출고이력조회'}" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="98"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Grid x:Name="SearchArea2" Grid.Row="1">
                                <Border Grid.Row="0" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                                <Grid   Grid.Row="0" Grid.Column="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="1.2*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="13"/>
                                    </Grid.ColumnDefinitions>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="14"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="10"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="10"/>
                                        <RowDefinition Height="14"/>
                                    </Grid.RowDefinitions>

                                    <Grid Grid.Row="1" Grid.Column="1" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='기간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <Grid Grid.Column="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="AUTO"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>

                                            <ctrlLib:LGCDatePicker  Grid.Column="0" x:Name="dtpDateFrom" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center" Margin="0" TabIndex="1" />
                                            <TextBlock              Grid.Column="1" Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center" Margin="6,0"/>
                                            <ctrlLib:LGCDatePicker  Grid.Column="2" x:Name="dtpDateTo" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center" Margin="0" TabIndex="2"/>
                                        </Grid>

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='출고처', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                        <uc:UcBaseComboBox Grid.Row="0"  Grid.Column="1"  x:Name="cboTransLoc" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='출고상태', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                        <uc:UcBaseComboBox Grid.Row="0"  Grid.Column="1"  x:Name="cboStatus2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="4">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='BOXID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                        <TextBox       Grid.Row="0"  Grid.Column="1" x:Name="txtBoxid" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100"
                                                   KeyDown="txtBoxid_KeyDown"/>

                                    </Grid>

                                    <Grid Grid.Row="3" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                            Style="{StaticResource Content_InputForm_LabelStyle}" />

                                        <TextBox       Grid.Row="0"  Grid.Column="1" x:Name="txtProd_ID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100"
                                                   KeyDown="txtProd_ID_KeyDown"/>

                                    </Grid>



                                    <Grid Grid.Row="3" Grid.Column="3" Grid.ColumnSpan="2" HorizontalAlignment="Right">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <uc:UcBaseButton Grid.Row="0" Grid.Column="0"  x:Name="btnReprint" Content="{Binding ConverterParameter=재발행,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" 
                                            Cursor="Hand" Click="btnReprint_Click" />

                                        <uc:UcBaseButton Grid.Row="0" Grid.Column="2"  x:Name="btnCancel"  Content="{Binding ConverterParameter=출고취소,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" 
                                            Cursor="Hand" Click="btnCancel_Click" />

                                        <uc:UcBaseButton Grid.Row="0" Grid.Column="4"  x:Name="btnSearch2" Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" 
                                            Cursor="Hand" Click="btnSearch2_Click"  />

                                    </Grid>



                                </Grid>

                            </Grid>


                            <Grid x:Name="GridArea2" Grid.Row="3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="6*" />
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="4*" />
                                </Grid.ColumnDefinitions>

                                <GridSplitter Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                <Grid Grid.Row="3">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='출고정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <Grid Grid.Row="1">

                                        <Grid   Grid.Row="1" Grid.Column="1" >
                                            <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                            <c1:C1DataGrid x:Name="dgOutHIst" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="SingleRow" Grid.ColumnSpan="2" Margin="8">
                                                <c1:C1DataGrid.Columns>

                                                    <c1:DataGridTemplateColumn  Header="선택" EditOnSelection="True" Name="CHK" Width="Auto" HorizontalAlignment="Center" >
                                                        <c1:DataGridTemplateColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <RadioButton GroupName="dgOutHIstChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgOutHIstChoice" HorizontalAlignment="Center" Checked="dgOutHIstChoice_Checked"  />
                                                            </DataTemplate>
                                                        </c1:DataGridTemplateColumn.CellTemplate>
                                                    </c1:DataGridTemplateColumn>

                                                    <c1:DataGridTextColumn        Header = "BOXID"      Binding = "{Binding OUTER_BOXID}"   HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "제품코드"    Binding = "{Binding PRODID}" HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn        Header = "출고처"      Binding = "{Binding SHIPTO_NAME}"   HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "출고상태"      Binding = "{Binding BOX_RCV_ISS_STAT_CODE_DESC}"   HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "출고일"      Binding = "{Binding INSDTTM}"   HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "출고수량"      Binding = "{Binding TOTAL_QTY}"   HorizontalAlignment="Center" Format="###,###,##0.##" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn        Header = "출고수량"      Binding = "{Binding TOTAL_QTY2}"   HorizontalAlignment="Center" Format="###,###,##0.##" />
                                                    <c1:DataGridTextColumn        Header = "실제수량"      Binding = "{Binding REAL_TOTAL_QTY2}"   HorizontalAlignment="Center" Format="###,###,##0.##" />
                                                    <c1:DataGridTextColumn        Header = "차이수량"      Binding = "{Binding DIFF_QTY2}"   HorizontalAlignment="Center" Format="###,###,##0.##" />
                                                    <c1:DataGridTextColumn        Header = "출고상태"      Binding = "{Binding BOX_RCV_ISS_STAT_CODE}"   HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn        Header = "출고NO"      Binding = "{Binding RCV_ISS_ID}"   HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn        Header = "OWMS_BOX_TYPE"      Binding = "{Binding OWMS_BOX_TYPE_CODE}"   HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn        Header = "BOX_TYPE_NAME"      Binding = "{Binding BOX_TYPE_NAME}"   HorizontalAlignment="Center" />

                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>
                                        </Grid>

                                    </Grid>

                                </Grid>

                                <Grid Grid.Row="3" Grid.Column="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Grid.Resources>
                                        <DataTemplate x:Key="ResultTemplate">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <TextBlock Grid.Column="1" Text="{Binding}" HorizontalAlignment="Right"></TextBlock>
                                            </Grid>
                                        </DataTemplate>
                                    </Grid.Resources>

                                    <Border Grid.Row="0" Grid.Column="2" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="2" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='상세정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <Grid Grid.Row="1" Grid.Column="2">

                                        <Grid   Grid.Row="1" Grid.Column="1" >
                                            <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                            <c1:C1DataGrid x:Name="dgOutDetail" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="SingleCell" Grid.ColumnSpan="2" Margin="8" FrozenBottomRowsCount="1">
                                                <c1:C1DataGrid.Columns>

                                                    <c1:DataGridTextColumn Header = "BOXID"       Binding = "{Binding BOXID}" HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn Header = "SKIDID"       Binding = "{Binding CSTID}" HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn Header = "CUTID"       Binding = "{Binding CUT_ID}" HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn Header = "LOTID"       Binding = "{Binding LOTID}" HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn Header = "PJT"       Binding = "{Binding PROJECTNAME}" HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn Header = "극성"       Binding = "{Binding PRDT_CLSS_NAME}" HorizontalAlignment="Center"/>
                                                    <!--<c1:DataGridTextColumn Header = "C/ROLL"        Binding = "{Binding WIPQTY}" HorizontalAlignment="Center" Format="###,###,##0.##"/>-->

                                                    <c1:DataGridNumericColumn     Header = "C/ROLL"              Binding = "{Binding WIPQTY}"       HorizontalAlignment="Right"  Format="###,###,##0.##" EditOnSelection="True" Minimum="0" ShowButtons="False">
                                                        <c1:DataGridAggregate.AggregateFunctions>
                                                            <c1:DataGridAggregatesCollection>
                                                                <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                            </c1:DataGridAggregatesCollection>
                                                        </c1:DataGridAggregate.AggregateFunctions>
                                                    </c1:DataGridNumericColumn>

                                                    <!--<c1:DataGridTextColumn Header = "S/ROLL"        Binding = "{Binding WIPQTY2}" HorizontalAlignment="Center" Format="###,###,##0.##"/>-->

                                                    <c1:DataGridNumericColumn     Header = "S/ROLL"              Binding = "{Binding WIPQTY2}"       HorizontalAlignment="Right"  Format="###,###,##0.##" EditOnSelection="True" Minimum="0" ShowButtons="False">
                                                        <c1:DataGridAggregate.AggregateFunctions>
                                                            <c1:DataGridAggregatesCollection>
                                                                <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                            </c1:DataGridAggregatesCollection>
                                                        </c1:DataGridAggregate.AggregateFunctions>
                                                    </c1:DataGridNumericColumn>
                                                    <c1:DataGridTextColumn Header = "유효일자"       Binding = "{Binding VLD_DATE}" HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn Header = "OUTER_BOXID"       Binding = "{Binding OUTER_BOXID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn Header = "EQSG_NAME"         Binding = "{Binding EQSG_NAME}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                </c1:C1DataGrid.Columns>

