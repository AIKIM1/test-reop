<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.BOX001.BOX001_226"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="850" d:DesignWidth="1920"
             Loaded="UserControl_Loaded" 
             Unloaded="UserControl_Unloaded" 
             >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid  x:Name="grdMain">
        <Grid.Resources>
            <DataTemplate x:Key="ResultTemplate">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>
        </Grid.Resources>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="전극 자동창고 모니터링" >
            <ctrlLib:LGCLayout.Content>
                <Grid >
                    <Grid.RowDefinitions>
                        <!--TopSearch-->
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8"/>
                        <!--Contents-->
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="4"/>
                        <!--Bottom-->
                        <!--<RowDefinition Height="42"/>-->
                    </Grid.RowDefinitions>

                    <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                    <Grid x:Name="TopSearch" Grid.Row="0" Grid.ColumnSpan="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="8" />
                        </Grid.RowDefinitions>
                        <!-- 조회 버튼 영역-->
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="18"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>

                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>

                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>

                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="Auto"/>

                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="4"/>
                        </Grid.ColumnDefinitions>

                        <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Stocker', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                        <uc:UcBaseComboBox  Grid.Row="1" Grid.Column="3"  x:Name="cboStocker" DisplayMemberPath="EQPTNAME" SelectedValuePath="EQPTID"   Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" MinWidth="160" Margin="3,0,3,3" Height="23" VerticalAlignment="Bottom" />

                        <TextBlock Grid.Row="1" Grid.Column="5" Text="{Binding ConverterParameter='TBOXID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox Grid.Row="1" Grid.Column="7" x:Name="txtTBoxId"  Style="{StaticResource Content_InputForm_TextBoxStyle}" MinWidth="120"/>

                        <uc:UcBaseButton Grid.Row="1" Grid.Column="11" x:Name="btnPancakeWarehousingHistory" Content="{Binding ConverterParameter='전극 자동창고 입출고 이력 조회',    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonStyle}" Cursor="Hand" Click="btnPancakeWarehousingHistory_Click" VerticalAlignment="Bottom" />

                        <uc:UcBaseButton x:Name="btnSKIDSearch" Grid.Row="1" Grid.Column="17"  Content="{Binding ConverterParameter='SKID 현황조회', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnSKIDSearch_Click" Visibility="Hidden" />
                        <uc:UcBaseButton x:Name="btnSkidInfo" Grid.Row="1" Grid.Column="23"  Content="{Binding ConverterParameter='SKID조회', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnSkidInfo_Click" Visibility="Collapsed" />
                        <TextBlock Grid.Row="1"  Grid.Column="26" Text="{Binding ConverterParameter='자동조회', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" />
                        <uc:UcBaseComboBox Grid.Row="1"  Grid.Column="28" x:Name="cboAutoSearch" Margin="4,3,0,3" AutoComplete="False" DisplayMemberPath="NAME" SelectedValuePath="CODE" SelectedValueChanged="cboAutoSearch_SelectedValueChanged"  Width="Auto" MinWidth="40"/>
                        <uc:UcBaseButton Grid.Row="1" Grid.Column="30" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" PreviewMouseDown="btnSearch_PreviewMouseDown"  />
                    </Grid>
                    <!--Contents-->
                    <Grid x:Name="Contents" Grid.Row="2">
                        <Grid Grid.Row="0"  x:Name="ContentsRow">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="600*"/>
                                <ColumnDefinition Width="4" />
                                <ColumnDefinition Width="1000*" />
                            </Grid.ColumnDefinitions>
                            <!--Left Grid Area-->
                            <Grid Grid.Row="0" x:Name="Left">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="32" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="4" />
                                </Grid.RowDefinitions>


                                <Grid Grid.Row="0" x:Name="grdWHOrderTitle">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                        <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter=출고순위, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    </Border>
                                    <!--<uc:UcBaseComboBox Grid.Row="0" Grid.Column="2" x:Name="cboPriority" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" MinWidth="100"  />-->
                                </Grid>

                                <Border Grid.Row="1" Background="#e8ebed" BorderThickness="0" BorderBrush="#8a8a8a"></Border>
                                <Grid Grid.Row="1" x:Name="grdInputRank">
                                    <c1:C1DataGrid x:Name="dgInputRank" Grid.Row="0" Grid.Column="0" IsReadOnly="False" HeadersVisibility="Column" FrozenBottomRowsCount="1" FrozenColumnCount="3"  SelectionMode="SingleRow" LoadedCellPresenter="dgInputRank_LoadedCellPresenter" >
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTemplateColumn Header = "선택" Width="50" HorizontalAlignment="Center">
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" x:Name="rankCheck" HorizontalAlignment="Center"  Click="rankCheck_Click"/>
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>
                                            <c1:DataGridTextColumn		Header = "코터설비"	 	    Binding = "{Binding COAT_EQPT}"	  	    HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn      Header = "TBOXID"	 	    Binding = "{Binding TBOXID}"	    	HorizontalAlignment="Center" IsReadOnly="True" >
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateDistinct />
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridTextColumn>

                                            <c1:DataGridTextColumn		Header = "제품"		        Binding = "{Binding PRODID}"	  		HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn		Header = "프로젝트"  		Binding = "{Binding PRJT_NAME}"			HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn		Header = "극성"      		Binding = "{Binding ELTR_TYPE_CODE}"	HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn		Header = "유효일자"		    Binding = "{Binding VLD_DATE}"	  		HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridNumericColumn	Header = "수량"		        Binding = "{Binding WIPQTY}"	  		HorizontalAlignment="Right"  IsReadOnly="True" Format="#,##0"  >
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>
                                            <c1:DataGridTextColumn		Header = "HOLD여부"	        Binding = "{Binding WIPHOLD}"	  		HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn		Header = "시장유형"	 	    Binding = "{Binding MKT_TYPE_NAME}"		HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn		Header = "위치"	     	    Binding = "{Binding PSTN}"	            HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn		Header = "사유"	 	        Binding = "{Binding NOTE}"		        HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn		Header = "버전"	 	        Binding = "{Binding PROD_VER_CODE}"		HorizontalAlignment="Center" IsReadOnly="True" />
                                            <c1:DataGridTextColumn		Header = "창고입고일시"		Binding = "{Binding WIPDTTM_ED}"		HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn		Header = "RACK"	     	    Binding = "{Binding RACK_ID}"	        HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        </c1:C1DataGrid.Columns>
                                        <c1:C1DataGrid.BottomRows >
                                            <c1:DataGridSummaryRow RowStyle="{StaticResource SummaryRowStyle}"/>
                                        </c1:C1DataGrid.BottomRows>
                                    </c1:C1DataGrid>
                                </Grid>
                                <Border Grid.Row="2" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                            </Grid>
                            <!--Splitter-->
                            <GridSplitter Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"  />
                            <Grid x:Name="HMIAera" Grid.Column="2" >
                                <Grid x:Name="RightArea">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="*"/>
                                        <!--<RowDefinition Height="32"/>-->
                                        <RowDefinition Height="8"/>
                                        <RowDefinition Height="137"/>
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}"></Border>
                                    <Grid Grid.Row="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="34"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid x:Name="PancakeInfoTitle" Grid.Row="0">
                                            <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="*"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="100"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                    <ToggleButton   Grid.Row="0"  Grid.Column="0" Margin="10,0,5,0" x:Name="btnExpandFrame" Style="{StaticResource btnLeftTopConerStyleHorizontal}"  IsChecked="true" Checked="btnExpandFrame_Checked" Unchecked="btnExpandFrame_Unchecked"/>
                                                    <uc:UcBaseComboBox  Grid.Column="3" x:Name="cboColor" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DropDownHeight="400" MinWidth="150" />
                                                    <StackPanel Grid.Column="5" Orientation="Horizontal" Margin="0,0,4,0" >
                                                        <uc:UcBaseButton x:Name="btnManualIssue" Content="{Binding ConverterParameter=수동출고예약, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnManualIssue_Click"  />
                                                    </StackPanel>
                                                </Grid>
                                            </Border>
                                        </Grid>
                                        <Border Grid.Row="1" Background="#e8ebed" BorderThickness="0" BorderBrush="#8a8a8a"></Border>
                                        <Grid  Grid.Row="1" x:Name="PancakeInfo">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="1268*"/>
                                                <ColumnDefinition Width="185*"/>
                                            </Grid.ColumnDefinitions>
                                            <c1:C1DataGrid x:Name="dgRackInfo" 
                                                           Grid.Row="0" Grid.Column="0" 
                                                           IsReadOnly="True" 
                                                           HeadersVisibility="Column" 
                                                           FrozenColumnCount="2" 
                                                           AlternatingRowBackground="White"  
                                                           CanUserFilter="True" 
                                                           CanUserSort="True" 
                                                           SelectionMode="SingleRow"
                                                           Grid.ColumnSpan="2"
                                                           HorizontalScrollBarVisibility="Hidden"
                                                           >
                                                <c1:C1DataGrid.Columns>
                                                    <c1:DataGridTextColumn		Header = "RACK"		    Binding = "{Binding RACK_ID}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "SKID ID"	    Binding = "{Binding MCS_CST_ID}"  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "PRJT_NAME"	Binding = "{Binding PRJT_NAME}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "제품"		    Binding = "{Binding PRODID}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "입고일시"	    Binding = "{Binding WH_RCV_DTTM}" HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "LOT 수"	    Binding = "{Binding LOT_CNT}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridNumericColumn	Header = "수량"		    Binding = "{Binding WIPQTY}"	  HorizontalAlignment="Right" Format="#,##0" />
                                                    <c1:DataGridTextColumn		Header = "유효일자"		Binding = "{Binding VLD_DATE}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "경과일수"	    Binding = "{Binding PAST_DAY}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "HOLD여부"	    Binding = "{Binding WIPHOLD}"	  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn		Header = "PRODNAME"	    Binding = "{Binding PRODNAME}"	  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn		Header = "LOT ID"	    Binding = "{Binding LOTID}"		  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn		Header = "WO"		    Binding = "{Binding WOID}"		  HorizontalAlignment="Center" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn		Header = "모델"		    Binding = "{Binding MODLID}"	  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn		Header = "RACK_STAT_CODE"	Binding = "{Binding RACK_STAT_CODE}"  HorizontalAlignment="Center" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn	    Header = "순위"		    Binding = "{Binding SEQ}"	HorizontalAlignment="Center" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn	    Header = "POSITION"	    Binding = "{Binding POSITION}"	HorizontalAlignment="Center" Visibility="Collapsed" />
                                                    <c1:DataGridTextColumn	    Header = "EQPTID"	    Binding = "{Binding EQPTID}"	HorizontalAlignment="Center" Visibility="Collapsed" />

                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>
                                        </Grid>
                                    </Grid>

                                    <Border Grid.Row="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}"></Border>
                                    <Grid x:Name="StairSector" Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="13"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <Grid Grid.Column="1" >
                                        </Grid>

                                        <Grid Grid.Row="0" Grid.Column="11">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="7*"/>
                                            </Grid.ColumnDefinitions>
                                            
                                        </Grid>
                                        <Grid Grid.Row="0" Grid.Column="15">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="7*"/>
                                            </Grid.ColumnDefinitions>

                                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                                <TextBlock Text="{Binding ConverterParameter='QMS', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" Visibility="Hidden" />
                                            </StackPanel>
                                            <uc:UcBaseComboBox  Grid.Column="1" x:Name="cboQMS" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" MinWidth="160" Visibility="Hidden" />
                                        </Grid>

                                        <!--<StackPanel Grid.Column="13" Orientation="Horizontal" Margin="0,0,4,0" >
                                            <uc:UcBaseButton x:Name="btnSaveRackInfo" Content="{Binding ConverterParameter='RACK정보 수정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnSaveRackInfo_Click" Visibility="Collapsed" />
                                        </StackPanel>-->

                                    </Grid>

                                    <Border x:Name="HMIRackBorder"  Grid.Row="2" Background="White" BorderBrush="Black" />
                                    <ScrollViewer x:Name="HMIRack" Grid.Row="1" ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Auto" Margin="0,34,0,2" Grid.RowSpan="2">
                                        <Grid x:Name="stairs" VerticalAlignment="Top" Width="Auto">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="10"/>
                                            </Grid.RowDefinitions>

                                            <!--열연단 중 연에 해당하는 컬럼 세팅-->
                                            <ScrollViewer x:Name="scrollViewer1" VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Hidden"  Grid.Row="2"  Grid.Column="0" >
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="*" />
                                                    </Grid.RowDefinitions>

                                                    <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                        <TextBlock Text="{Binding ConverterParameter=1열, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                                    </StackPanel>

                                                    <Grid Grid.Row="1" Grid.Column="0" x:Name="grdColumn1" />
                                                    <Grid Grid.Row="0" Grid.Column="1" x:Name="grdStair1" />
                                                    <Grid Grid.Row="1" Grid.Column="1" x:Name="grdRackstair1" />
                                                </Grid>
                                            </ScrollViewer>

                                            <ScrollViewer x:Name="scrollViewer2" VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Hidden" Grid.Row="6"  Grid.Column="0" >
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="*" />
                                                    </Grid.RowDefinitions>

                                                    <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                        <TextBlock Text="{Binding ConverterParameter=2열, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                                    </StackPanel>

                                                    <Grid Grid.Row="1" Grid.Column="0" x:Name="grdColumn2" />
                                                    <Grid Grid.Row="0" Grid.Column="1" x:Name="grdStair2" />
                                                    <Grid Grid.Row="1" Grid.Column="1" x:Name="grdRackstair2" />
                                                </Grid>
                                            </ScrollViewer>

                                            <ScrollViewer x:Name="scrollViewer3" VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Hidden" Grid.Row="10"  Grid.Column="0" >
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="*" />
                                                    </Grid.RowDefinitions>

                                                    <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                        <TextBlock Text="{Binding ConverterParameter=3열, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                                    </StackPanel>

                                                    <Grid Grid.Row="1" Grid.Column="0" x:Name="grdColumn3" />
                                                    <Grid Grid.Row="0" Grid.Column="1" x:Name="grdStair3" />
                                                    <Grid Grid.Row="1" Grid.Column="1" x:Name="grdRackstair3" />
                                                </Grid>
                                            </ScrollViewer>

                                            <ScrollViewer x:Name="scrollViewer4" VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Hidden" Grid.Row="14"  Grid.Column="0" >
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="*" />
                                                    </Grid.RowDefinitions>

                                                    <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                        <TextBlock Text="{Binding ConverterParameter=4열, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_LabelStyle}" />
                                                    </StackPanel>

                                                    <Grid Grid.Row="1" Grid.Column="0" x:Name="grdColumn4" />
