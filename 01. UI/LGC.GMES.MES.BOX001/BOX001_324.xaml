                                                <c1:DataGridTextColumn      Header = "PALLETID"        Binding = "{Binding PALLETID}"            HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn      Header = "교체일"          Binding = "{Binding ACTDTTM}"           HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn      Header = "교체사유"        Binding = "{Binding REMARK}"           HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn      Header = "작업자"        Binding = "{Binding USERNAME}"           HorizontalAlignment="Center" />
