                                        <uc:UcBaseButton         Grid.Row="0"  Grid.Column="21"  x:Name="btnSearch1" Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" PreviewMouseDown="btnSearch_PreviewMouseDown" Visibility="Collapsed" />
                                    </Grid>

                                <Grid Grid.Row="1" Grid.Column="2">
                                    <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
