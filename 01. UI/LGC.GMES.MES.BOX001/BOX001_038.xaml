<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.BOX001.BOX001_038"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <Grid.Resources>
            <DataTemplate x:Key="ItemTemplate">
                <WrapPanel>
                    <Image Width="138" Height="138" Stretch="Fill" Source="{Binding}" Margin="3,3,3,3" />
                </WrapPanel>
            </DataTemplate>
            <DataTemplate x:Key="ResultTemplate">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="1" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>
        </Grid.Resources>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="물류라벨발행(수동)">
            <ctrlLib:LGCLayout.Content>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="4"/>
                        <RowDefinition Height="35"/>
                    </Grid.RowDefinitions>
                    <c1:C1TabControl Grid.Row="0">
                        <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='OUTBOX'}">
                            <Grid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="51"/>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="55*"/>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="45*"/>
                                </Grid.RowDefinitions>
                                <Grid x:Name="SearchArea" Grid.Row="0">
                                    <Border Grid.Row="0" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                                    <Grid Grid.Row="0" Grid.Column="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="13"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="2*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="30"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="13"/>
                                        </Grid.ColumnDefinitions>
                                        <uc:UcBaseButton  x:Name="btnMergeSplit"   Grid.Column="1" Content="{Binding ConverterParameter='Outbox 분할/병합', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnMergeSplit_Click" />
                                        <uc:UcBaseButton x:Name="btnRePrint"   Grid.Column="3" Content="{Binding ConverterParameter='재발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnRePrint_Click"/>
                                        <Grid Grid.Column="5">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="2*"/>
                                                <ColumnDefinition Width="3*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock      Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='In Pallet', Converter={StaticResource GlobalObjectDicConverter},  Mode=oneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                            <TextBox        Grid.Row="0" Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" x:Name="txtPalletId" KeyDown="txtPalletId_KeyDown"/>
                                        </Grid>
                                        <uc:UcBaseButton Grid.Row="1" Grid.Column="7" x:Name="btnSearch" Content="{Binding ConverterParameter='SEARCH', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                                    </Grid>
                                </Grid>
                                <!--Search Area End-->
                                <Grid x:Name="ContentTop" Grid.Row="2">
                                    <Grid Grid.Row="0" Grid.Column="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="5*"/>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>
                                        <Border Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                        <Grid Grid.Column="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="34"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>
                                            <Border Grid.Row="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                            <Grid   Grid.Row="0" >
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="15"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="4"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="4"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="13"/>
                                                </Grid.ColumnDefinitions>
                                                <TextBlock      Grid.Row="0"  Grid.Column="1" Text="{Binding ConverterParameter='List', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                                <uc:UcBaseButton x:Name="btnPrint"   Grid.Column="3" Content="{Binding ConverterParameter='발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource  Content_MainButtonNoMinWidthSpecialStyle}" Click="btnPrint_Click" />
                                                <uc:UcBaseButton x:Name="btnPrintAll"   Grid.Column="5" Content="{Binding ConverterParameter='일괄발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource  Content_MainButtonNoMinWidthSpecialStyle}" Click="btnPrintAll_Click" />
                                                <uc:UcBaseButton x:Name="btnInbox"   Grid.Column="7" Content="{Binding ConverterParameter='Inbox발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource  Content_MainButtonNoMinWidthSpecialStyle}" Click="btnInbox_Click" />

                                            </Grid> 
                                            <Grid Grid.Row="2">
                                                <c1:C1DataGrid Grid.Row="1" Grid.Column="1" Margin="8" x:Name="dgPrintList" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="SingleRow"  MouseLeftButtonUp="dgPrintList_MouseLeftButtonUp" FrozenBottomRowsCount="1" FrozenColumnCount="1">
                                                    <c1:C1DataGrid.Columns>
                                                        <c1:DataGridTemplateColumn  EditOnSelection="True" Name="CHK" Width="Auto" CanUserFilter="False" CanUserSort="False" CanUserResize="False"  >
                                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <RadioButton GroupName="dgPrintListChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgPrintListChoice" HorizontalAlignment="Center" Checked="dgPrintListChoice_Checked" />
                                                                </DataTemplate>
                                                            </c1:DataGridTemplateColumn.CellTemplate>
                                                        </c1:DataGridTemplateColumn>
                                                        <c1:DataGridTextColumn Header="동"     Binding = "{Binding AREAID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="동"     Binding = "{Binding AREANAME}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="제품코드"     Binding = "{Binding PRODID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="InPallet ID"     Binding = "{Binding BOXID}" HorizontalAlignment="Center"/>
                                                        <c1:DataGridTextColumn Header="LOT ID"     Binding = "{Binding LOTID}" HorizontalAlignment="Center"/>
                                                        <c1:DataGridTextColumn Header="출하가능여부"     Binding = "{Binding QA_RESULT}" HorizontalAlignment="Center"/>
                                                        <c1:DataGridTextColumn Header="등급"     Binding = "{Binding PRDT_GRD_CODE}" HorizontalAlignment="Center"/>
                                                        <c1:DataGridNumericColumn Header="등급수량"     Binding = "{Binding TOTAL_CELL_QTY}" HorizontalAlignment="Right" Format="###,###,##0.##"/>
                                                        <!--<c1:DataGridAggregate.AggregateFunctions>
                                                                <c1:DataGridAggregatesCollection>
                                                                    <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                                </c1:DataGridAggregatesCollection>
                                                            </c1:DataGridAggregate.AggregateFunctions>
                                                        </c1:DataGridNumericColumn>-->
                                                        <c1:DataGridNumericColumn Header="완료수량"     Binding = "{Binding END_CELL_QTY}" HorizontalAlignment="Right" Format="###,###,##0.##"/>
                                                        <c1:DataGridNumericColumn Header="BOX수량"     Binding = "{Binding TOTAL_BOX_QTY}" HorizontalAlignment="Right" Format="###,###,##0.##"/>
                                                        <c1:DataGridNumericColumn Header="완료BOX"     Binding = "{Binding END_BOX_QTY}" HorizontalAlignment="Right" Format="###,###,##0.##"/>
                                                        <c1:DataGridNumericColumn Header="잔량 BOX"     Binding = "{Binding REST_BOX_QTY}" HorizontalAlignment="Right" Format="###,###,##0.##"/>
                                                        <c1:DataGridNumericColumn Header="잔여수량"     Binding = "{Binding REST_CELL_QTY}" HorizontalAlignment="Right" Format="###,###,##0.##"/>
                                                        <c1:DataGridNumericColumn Header="잔량"     Binding = "{Binding LAST_QTY}" HorizontalAlignment="Right" Format="###,###,##0.##"/>
                                                        <c1:DataGridTextColumn Header="Project"     Binding = "{Binding PROJECT}" HorizontalAlignment="Center"/>
                                                        <c1:DataGridDateTimeColumn Header="작업일"     Binding = "{Binding  WORKDTTM}" HorizontalAlignment="Center" Format="yyyy-MM-dd"/>

                                                        <!--Hidden Columns-->
                                                        <c1:DataGridTextColumn Header="OUTBOX_CELL_QTY"     Binding = "{Binding OUTBOX_CELL_QTY}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="BOX_WEIGHT"     Binding = "{Binding BOX_WEIGHT}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="CELL_NET_WEIGHT"     Binding = "{Binding CELL_NET_WEIGHT}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="CAPA"     Binding = "{Binding CAPA}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="SOC"     Binding = "{Binding SOC}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="VLTG_VALUE"     Binding = "{Binding VLTG_VALUE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="BOX_NET_WEIGHT"     Binding = "{Binding BOX_NET_WEIGHT}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="JOB_COUNT"     Binding = "{Binding JOB_COUNT}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="FOOTPRINT"     Binding = "{Binding FOOTPRINT}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="PRINT_MODEL_NAME"     Binding = "{Binding PRINT_MODEL_NAME}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                        <c1:DataGridTextColumn Header="MODEL_NAME"     Binding = "{Binding MODEL_NAME}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    </c1:C1DataGrid.Columns>
                                                    <c1:C1DataGrid.BottomRows>
                                                        <c1:DataGridSummaryRow x:Name="summary_inPallet" RowStyle="{StaticResource SummaryRowStyle}"/>
                                                    </c1:C1DataGrid.BottomRows>
                                                </c1:C1DataGrid>
                                            </Grid>
                                        </Grid>
                                        <GridSplitter Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                                        <Grid Grid.Row="0" Grid.Column="2">
                                            <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Background="#FAFAFA" BorderThickness="1" BorderBrush="#B3B3B3"></Border>
                                            <Border Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>

                                            <Grid Grid.Row="0" Grid.Column="0">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="34"/>
                                                    <RowDefinition Height="2*"/>
                                                    <RowDefinition Height="34"/>
                                                    <RowDefinition Height="*"/>
                                                </Grid.RowDefinitions>
                                                <Border Grid.Row="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                                <Grid   Grid.Row="0" >

                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="15"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock      Grid.Row="0"  Grid.Column="1" Text="{Binding ConverterParameter='작업설정', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                                </Grid>
                                                <Grid Grid.Row="1">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="8"/>
                                                        <ColumnDefinition Width="*"/>
                                                        <ColumnDefinition Width="8"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="8"/>
                                                        <RowDefinition Height="*"/>
                                                        <RowDefinition Height="8"/>
                                                    </Grid.RowDefinitions>
                                                    <Grid Grid.Row="1" Grid.Column="1">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="2"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="13"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="13"/>
                                                        </Grid.ColumnDefinitions>

                                                        <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderBrush="#c8294b" ></Border>

                                                        <!--<TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='포장 설비', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>-->
                                                        <TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='SOC', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                                        <TextBlock Grid.Row="3" Grid.Column="1" Text="{Binding ConverterParameter='출하처', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Visibility="Collapsed"/>
                                                        <TextBlock Grid.Row="7"  x:Name="blckCustomer" Grid.Column="1" Text="{Binding ConverterParameter='고객사', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                                        <TextBlock Grid.Row="4" Grid.Column="1" Text="{Binding ConverterParameter='운송방법', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Visibility="Collapsed"/>
                                                        <TextBlock Grid.Row="6" Grid.Column="1" Text="{Binding ConverterParameter='인쇄수량', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                                        <StackPanel Grid.Row="5" Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right">
                                                            <uc:UcBaseCheckBox x:Name="chkAdjustQty" IsChecked="False" Checked="chkAdjustQty_Checked" Unchecked="chkAdjustQty_Unchecked" />
                                                            <TextBlock  Text="{Binding ConverterParameter='수량조정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                                        </StackPanel>
                                                        <!--<uc:UcBaseComboBox Grid.Row="1" Grid.Column="2" x:Name="cboBoxEqpt" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />-->
                                                        <uc:UcBaseComboBox Grid.Row="2" Grid.Column="2" x:Name="cboSOC" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboSOC_SelectedValueChanged"/>
                                                        <uc:UcBaseComboBox Grid.Row="3" Grid.Column="2" x:Name="cboVender" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboVender_SelectedValueChanged" Visibility="Collapsed"/>
                                                        <uc:UcBaseComboBox Grid.Row="4" Grid.Column="2" x:Name="cboShippingMethod" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"  SelectedValueChanged="cboShippingMethod_SelectedValueChanged" Visibility="Collapsed"/>
                                                        <c1:C1NumericBox       Grid.Row="5" Grid.Column="2" Style="{StaticResource C1NumericBoxStyle}" x:Name="txtAdjustQty"  Minimum="0"  IsEnabled="False" GotMouseCapture="txtAdjustQty_GotMouseCapture"/>
                                                        <c1:C1NumericBox       Grid.Row="6" Grid.Column="2" Style="{StaticResource C1NumericBoxStyle}" x:Name="txtPrintQty"  Minimum="1" Value="1" GotMouseCapture="txtPrintQty_GotMouseCapture"/>
                                                        <uc:UcBaseComboBox Grid.Row="7" Grid.Column="2" x:Name="cboMcsCustomer" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" Width="Auto"/>

                                                        <Grid Grid.Row="8" Grid.ColumnSpan="4">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="*"/>
                                                                <ColumnDefinition Width="Auto"/>
                                                                <ColumnDefinition Width="4"/>
                                                                <ColumnDefinition Width="Auto"/>
                                                                <ColumnDefinition Width="4"/>
                                                                <ColumnDefinition Width="Auto"/>
                                                                <ColumnDefinition Width="*"/>
                                                            </Grid.ColumnDefinitions>
                                                        </Grid>
                                                    </Grid>
                                                </Grid>

                                                <Border Grid.Row="2" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                                <Grid   Grid.Row="2" >

                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="15"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock      Grid.Row="0"  Grid.Column="1" Text="{Binding ConverterParameter='TTI INBOX LIST', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                                </Grid>
                                                <Grid Grid.Row="3">
                                                    <Border Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                                    <c1:C1DataGrid Grid.Row="3" Grid.Column="0" Margin="8" x:Name="dgTTIInBoxList" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="SingleRow" >
                                                        <c1:C1DataGrid.Columns>
                                                            <c1:DataGridTextColumn Header="LABEL CODE"     Binding = "{Binding PRT_ITEM}" HorizontalAlignment="Center"/>
                                                            <c1:DataGridTextColumn Header="COUNT"     Binding = "{Binding PRT_CNT}" HorizontalAlignment="Center"/>
                                                         </c1:C1DataGrid.Columns>
                                                    </c1:C1DataGrid>
                                                </Grid>
                                            </Grid>
                                        </Grid>
                                    </Grid>
                                </Grid>
                                <GridSplitter Grid.Row="3" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                                <Grid x:Name="ContentBot" Grid.Row="4">
                                    <Grid Grid.Row="0" Grid.Column="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="5*"/>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="2*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid Grid.Column="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="34"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>

                                            <Grid Grid.Row="0">
                                                <Border Grid.Row="0" BorderThickness="1" Grid.ColumnSpan="99" Style="{StaticResource BorderSubTitleStyle}"/>
                                                <TextBlock Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='LABEL INFO', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                            </Grid>

                                            <Grid x:Name="LabelPreview" Grid.Row="2">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="8"/>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="8"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="8"/>
                                                    <RowDefinition Height="*"/>
                                                    <RowDefinition Height="8"/>
                                                </Grid.RowDefinitions>
                                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Grid.RowSpan="99" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                                <Border Grid.Row="1" Grid.Column="1" Style="{StaticResource BorderSubTitleStyle}" BorderThickness="1"/>
                                                <Grid x:Name="labelInfoArea" Grid.Row="1" Grid.Column="1">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="3*"/>
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>
                                                    <Grid Grid.Row="0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="8"/>
                                                            <ColumnDefinition Width="0.7*"/>
                                                            <ColumnDefinition Width="0.7*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="8"/>
                                                        </Grid.ColumnDefinitions>
                                                        <Grid Grid.Row="0" Grid.Column="1">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="4"/>
                                                                <RowDefinition Height="*"/>
                                                                <RowDefinition Height="*"/>
                                                                <RowDefinition Height="*"/>
                                                                <RowDefinition Height="*"/>
                                                                <RowDefinition Height="*"/>
                                                                <RowDefinition Height="*"/>
                                                            </Grid.RowDefinitions>
                                                            <Grid Grid.Row="1">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="70"/>
                                                                    <ColumnDefinition Width="*"/>
                                                                </Grid.ColumnDefinitions>
                                                                <Border Grid.Column="0"  Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                                                    <TextBlock Text="Model" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                                                                </Border>
                                                                <Border Grid.Column="1"  Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                                                    <TextBlock   x:Name="txtInfo01" VerticalAlignment="Center" FontSize="16" HorizontalAlignment="Left" Margin="5,0,0,0"/>
                                                                </Border>
                                                            </Grid>
                                                            <Grid Grid.Row="2">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="70"/>
                                                                    <ColumnDefinition Width="*"/>
                                                                </Grid.ColumnDefinitions>
                                                                <Border Grid.Column="0"  Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                                                    <TextBlock Text="Grade" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                                                                </Border>
                                                                <Border Grid.Column="1"  Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                                                    <TextBlock   x:Name="txtInfo02" VerticalAlignment="Center" FontSize="16" HorizontalAlignment="Left" Margin="5,0,0,0"/>
                                                                </Border>
                                                            </Grid>
                                                            <Grid Grid.Row="3">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="70"/>
                                                                    <ColumnDefinition Width="*"/>
                                                                </Grid.ColumnDefinitions>
                                                                <Border Grid.Column="0"  Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                                                    <TextBlock  Text="Quantity" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                                                                </Border>
                                                                <Border Grid.Column="1"  Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                                                    <TextBlock   x:Name="txtInfo03" VerticalAlignment="Center" FontSize="16" HorizontalAlignment="Left" Margin="5,0,0,0"/>
                                                                </Border>
                                                            </Grid>
                                                            <Grid Grid.Row="4">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="70"/>
                                                                    <ColumnDefinition Width="*"/>
                                                                </Grid.ColumnDefinitions>
                                                                <Border Grid.Column="0"  Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                                                    <TextBlock  Text="Weight" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                                                                </Border>
                                                                <Border Grid.Column="1"  Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                                                    <TextBlock   x:Name="txtInfo04" VerticalAlignment="Center" FontSize="16" HorizontalAlignment="Left" Margin="5,0,0,0"/>
                                                                </Border>


                                                            </Grid>
                                                            <Grid Grid.Row="5">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="70"/>
                                                                    <ColumnDefinition Width="*"/>
                                                                </Grid.ColumnDefinitions>
                                                                <Border Grid.Column="0"  Style="{StaticResource Content_InputForm_LabelBorderStyle}">
