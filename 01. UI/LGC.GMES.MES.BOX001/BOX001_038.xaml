                                            <c1:DataGridTextColumn Header="BOXID"     Binding = "{Binding BOXID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridDateTimeColumn Header="PRODDATE"     Binding = "{Binding  PRODDATE}" HorizontalAlignment="Center" Format="YYYY-MM-DD" Visibility="Collapsed"/>
                                            <c1:DataGridDateTimeColumn Header="PRODWEEK"     Binding = "{Binding  PRODWEEK}" HorizontalAlignment="Center" Format="WW" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn Header="CELL_NET_WEIGHT"     Binding = "{Binding CELL_NET_WEIGHT}" HorizontalAlignment="Right" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn Header="CAPA"     Binding = "{Binding CAPA}" HorizontalAlignment="Right" Visibility="Collapsed"/>
                                            <c1:DataGridNumericColumn Header="SOC"     Binding = "{Binding SOC}" HorizontalAlignment="Right" Visibility="Collapsed"/>
