<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.BOX001.BOX001_022"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1280"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="전극입고">
            <Grid>

                <c1:C1TabControl  Grid.Row="0">
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='자재입고'}" Margin="0" >

                        <Grid Grid.Row="0">

                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <!--<RowDefinition Height="20"/>-->
                                <RowDefinition Height="80"/>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="55"/>
                            </Grid.RowDefinitions>
                            <Border Grid.Row="1" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                            <Border Grid.Row="2" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                            <!--<Grid x:Name="InfoRdo" Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="5"/>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Row="1" Grid.Column="1" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>

                                    <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1">
                                        <uc:UcBaseRadioButton Grid.Row="0"  Grid.Column="0" Margin="10,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=RollPancake}"   
                                             GroupName="RadioButton_Type" Name="rdoRoll" Style="{StaticResource SearchCondition_RadioButtonStyle}" IsChecked="True"  Click="rdoRoll_Click" Visibility="Collapsed"/>

                                        <uc:UcBaseRadioButton Grid.Row="0"  Grid.Column="1" Margin="10,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=PANCAKE}"   
                                             GroupName="RadioButton_Type" Name="rdoPancake" Style="{StaticResource SearchCondition_RadioButtonStyle}" Click="rdoPancake_Click" Visibility="Collapsed"/>

                                        <uc:UcBaseRadioButton Grid.Row="0"  Grid.Column="2" Margin="10,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=NOCHEDPANCAKE}"   
                                             GroupName="RadioButton_Type" Name="rdoNochedPancake" Style="{StaticResource SearchCondition_RadioButtonStyle}" Click="rdoNochedPancake_Click" Visibility="Collapsed"/>
                                    </StackPanel>
                                </Grid>

                            </Grid-->
                            <Grid x:Name="InfoArea" Grid.Row="1">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="4"/>
                                    <!-- <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="5"/>-->
                                </Grid.ColumnDefinitions>

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="50" />
                                    <RowDefinition Height="14" />
                                </Grid.RowDefinitions>

                                <Grid Grid.Row="0" Grid.Column="1" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>

                                    <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1">
                                        <uc:UcBaseRadioButton Grid.Row="0"  Grid.Column="0" Margin="10,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=RollPancake}"   
                                             GroupName="RadioButton_Type" Name="rdoRoll" FontSize="16" Style="{StaticResource SearchCondition_RadioButtonStyle}" IsChecked="True"  Click="rdoRoll_Click" Visibility="Collapsed"/>

                                        <uc:UcBaseRadioButton Grid.Row="0"  Grid.Column="1" Margin="10,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=PANCAKE}"   
                                             GroupName="RadioButton_Type" Name="rdoPancake" FontSize="16" Style="{StaticResource SearchCondition_RadioButtonStyle}" Click="rdoPancake_Click" Visibility="Collapsed" />

                                        <uc:UcBaseRadioButton Grid.Row="0"  Grid.Column="2" Margin="10,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=NOCHEDPANCAKE}"   
                                             GroupName="RadioButton_Type" Name="rdoNochedPancake" FontSize="16" Style="{StaticResource SearchCondition_RadioButtonStyle}" Click="rdoNochedPancake_Click" Visibility="Collapsed"/>
                                    </StackPanel>
                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock   Grid.Row="0" Grid.Column="0"    Text="{Binding ConverterParameter='Pallet ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                                 Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox Grid.Row="1" Grid.Column="1"  x:Name="txtPalletId" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtPalletId_KeyDown" /> <!--PreviewKeyDown="txtPalletId_PreviewKeyDown"-->
                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock   Grid.Row="0" Grid.Column="0"    Text="{Binding ConverterParameter='SKIDID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                                 Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox Grid.Row="0" Grid.Column="1"  x:Name="txtSkidId" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtSkidId_KeyDown" />
                                    <!--PreviewKeyDown="txtPalletId_PreviewKeyDown"-->
                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock   Grid.Row="0" Grid.Column="0"    Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                        Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox Grid.Row="0" Grid.Column="1"  x:Name="txtLotid" CharacterCasing="Upper" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtLotid_KeyDown" PreviewKeyDown="txtLotid_PreviewKeyDown" />
                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="4">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='입고동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <uc:UcBaseComboBox  Grid.Row="0" Grid.Column="1" x:Name="cboArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <uc:UcBaseComboBox  Grid.Row="0" Grid.Column="1" x:Name="cboEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="6">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0" Grid.Column="0" x:Name="tbProcess" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <uc:UcBaseComboBox  Grid.Row="0" Grid.Column="1" x:Name="cboProcid" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="7" HorizontalAlignment="Right" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>

                                    <uc:UcBaseButton Grid.Row="0" Grid.Column="1"  x:Name="btnReceive" Content="{Binding ConverterParameter=입고,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnReceive_Click" />
                                </Grid>
                            </Grid>
                            <Grid x:Name="TitleArea" Grid.Row="2">
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회결과', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                            </Grid>
                            <Grid x:Name="OutArea" Grid.Row="3">
                                <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                <c1:C1DataGrid x:Name="dgReceive" IsReadOnly="True" HeadersVisibility="Column" Margin="8" CanUserRemoveRows="True" CanUserAddRows="True" SelectionMode="MultiRange" Grid.ColumnSpan="2">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn      Header = "PLLT_ID"                       Binding = "{Binding PLLT_ID}"                      HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "LOTID"                         Binding = "{Binding LOTID}"                        HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "PR_LOTID"                      Binding = "{Binding PR_LOTID}"                     HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "IWMS_RCV_ISS_ID"               Binding = "{Binding IWMS_RCV_ISS_ID}"              HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "PJT"                           Binding = "{Binding PRJT_NAME}"                    HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "버전"                          Binding = "{Binding PROD_VER_CODE}"                HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "모델"                          Binding = "{Binding MODLID}"                       HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "제품코드"                      Binding = "{Binding PRODID}"                       HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "제품명"                        Binding = "{Binding PRODNAME}"                     HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "유효기간"                      Binding = "{Binding VLD_DATE}"                     HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "LOTID_RT"                      Binding = "{Binding LOTID_RT}"                     HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "패턴수"                        Binding = "{Binding LANE_PTN_QTY}"                 HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "시장유형"                      Binding = "{Binding MKT_TYPE_CODE}"                HorizontalAlignment="Center" />
                                        <c1:DataGridNumericColumn   Header = "수량"                          Binding = "{Binding WIPQTY}"                       HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                        <c1:DataGridNumericColumn   Header = "수량(Lane)"                    Binding = "{Binding WIPQTY2}"                      HorizontalAlignment="Center" Visibility="Collapsed" Format="###,###,##0.##" />
                                        <c1:DataGridTextColumn      Header = "FACTORYID"                        Binding = "{Binding SHOPID}"                       HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "AREAID"                        Binding = "{Binding AREAID}"                       HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "ROUTID"                        Binding = "{Binding ROUTID}"                       HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "FLOWID"                        Binding = "{Binding FLOWID}"                       HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn      Header = "LOTTYPE"                       Binding = "{Binding LOTTYPE}"                      HorizontalAlignment="Center" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "CSTID"                         Binding = "{Binding CSTID}"                        HorizontalAlignment="Center" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "VD_QA_INSP_TRGT_FLAG"          Binding = "{Binding VD_QA_INSP_TRGT_FLAG}"         HorizontalAlignment="Center" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "VD샘플링대상"                  Binding = "{Binding VD_QA_INSP_SMPLG_TRGT_FLAG}"   HorizontalAlignment="Center" />
                                        <c1:DataGridTemplateColumn Name="Delete" Width="Auto" Header="삭제">
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button  x:Name="btnDelete" Width="Auto" Content="{Binding ConverterParameter=삭제, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDelete_Click"/>
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>

                            <Grid x:Name="NoteArea" Grid.Row="4" >
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                                <Grid Grid.Row="0">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13" />
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="13" />
                                    </Grid.ColumnDefinitions>

                                    <TextBlock    Grid.Row="0" Grid.Column="1"  Text="{Binding ConverterParameter='NOTE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox      Grid.Row="0"  Grid.Column="2" Grid.ColumnSpan="2" x:Name="txtRemark" Style="{StaticResource SearchCondition_TextBoxStyle}" AcceptsReturn="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" Height="42" MinLines="2" MaxLines="2"/>

                                </Grid>


                            </Grid>

                        </Grid>

                    </c1:C1TabItem>


                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='입고이력조회'}" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="54"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Border Grid.Row="1" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                            <Grid   Grid.Row="1" Grid.Column="0" >

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="13"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="1.3*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="13"/>
                                </Grid.ColumnDefinitions>

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="14" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="14" />
                                </Grid.RowDefinitions>

                                <Grid Grid.Row="1" Grid.Column="1" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>

                                    <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="0">
                                        <uc:UcBaseRadioButton Grid.Row="0"  Grid.Column="0" Margin="10,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=ROLL}"   
                                             GroupName="RadioButton_Type2" Name="rdoRoll2" Style="{StaticResource SearchCondition_RadioButtonStyle}" IsChecked="True" Visibility="Collapsed"/>

                                        <uc:UcBaseRadioButton Grid.Row="0"  Grid.Column="1" Margin="10,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=PANCAKE}"   
                                             GroupName="RadioButton_Type2" Name="rodPancake2" Style="{StaticResource SearchCondition_RadioButtonStyle}" IsChecked="True" Visibility="Collapsed"/>

                                        <uc:UcBaseRadioButton Grid.Row="0"  Grid.Column="2" Margin="10,0,5,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=NOCHEDPANCAKE}"   
                                             GroupName="RadioButton_Type2" Name="rdoNochedPancake2" Style="{StaticResource SearchCondition_RadioButtonStyle}"  IsChecked="True" Visibility="Collapsed"/>
                                    </StackPanel>

                                </Grid>


                                <Grid Grid.Row="1" Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='기간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                                    Style="{StaticResource Content_InputForm_LabelStyle}" />

                                    <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1">
                                        <ctrlLib:LGCDatePicker  x:Name="dtpDateFrom"  DatepickerType="Date"/>
                                        <TextBlock  Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0"/>
                                        <ctrlLib:LGCDatePicker  x:Name="dtpDateTo"  DatepickerType="Date"/>
                                    </StackPanel>

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='입고동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <uc:UcBaseComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboArea2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="4">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='Pancake상태', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <uc:UcBaseComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboPancakeStat" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox Grid.Row="0" Grid.Column="1"  x:Name="txtSearchLotid" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtSearchLotid_KeyDown"/>

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="6" HorizontalAlignment="Right" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>

                                    <uc:UcBaseButton     Grid.Row="0" Grid.Column="1" x:Name="btnExcel"    Content="{Binding ConverterParameter=EXCEL,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                    Style="{StaticResource Content_ModuleButtonExcelExportStyle}" Cursor="Hand" Click="btnExcel_Click" Height="29" />

                                    <uc:UcBaseButton     Grid.Row="0" Grid.Column="3" x:Name="btnSearch"   Content="{Binding ConverterParameter=SEARCH,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                    Style="{StaticResource SearchButtonStyle}" Click="btnSearch_Click"/>

                                </Grid>

                            </Grid>

                            <Grid x:Name="GridArea" Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <Grid x:Name="TitleArea2" Grid.Row="0">
                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회결과', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                                </Grid>

                                <Grid   Grid.Row="1" Grid.Column="1" >
                                    <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <c1:C1DataGrid x:Name="dgReceive_Hist" IsReadOnly="True" HeadersVisibility="Column" Margin="8" CanUserRemoveRows="True" CanUserAddRows="True"
                                               SelectionMode="MultiRange" Grid.ColumnSpan="2">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn Header = "LOTID"         Binding = "{Binding LOTID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTemplateColumn Header="바코드" Name="BARCODE" HorizontalAlignment="Center" Width="Auto" IsReadOnly="True">
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Grid>
                                                            <Button x:Name="btnBarcode" Content="Print" Style="{DynamicResource Content_MainButtonSpecialStyle}" HorizontalAlignment="Center" Margin="3,2,3,2" IsCancel="True" Click="btnBarcode_Click" />
                                                        </Grid>
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>

                                            <c1:DataGridTextColumn Header = "IWMS_RCV_ID"         Binding = "{Binding IWMS_RCV_ID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "PANCAKE_STAT_CODE"         Binding = "{Binding PANCAKE_STAT_CODE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "Pancake상태"         Binding = "{Binding PANCAKE_STAT_NAME}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "제품코드"      Binding = "{Binding PRODID}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "제품명"      Binding = "{Binding PRODNAME}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "생산일"      Binding = "{Binding PROD_DATE}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "시장유형"      Binding = "{Binding MKT_TYPE_CODE}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "수량"          Binding = "{Binding WIPQTY}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                            <c1:DataGridTextColumn Header = "수량2"         Binding = "{Binding WIPQTY2}" HorizontalAlignment="Center" Format="###,###,##0.##" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn Header = "유효기간"      Binding = "{Binding VLD_DATE}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "버전"         Binding = "{Binding PROD_VER_CODE}" HorizontalAlignment="Center"/>
                                            <c1:DataGridTextColumn Header = "입고일자"     Binding = "{Binding INSDTTM}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "인계공장"     Binding = "{Binding FROM_SHOPID}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "입고동"       Binding = "{Binding RECEIVE_AREA}" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn Header = "PLLT_ID"         Binding = "{Binding PLLT_ID}" HorizontalAlignment="Center" />
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>

                                </Grid>
                            </Grid>

