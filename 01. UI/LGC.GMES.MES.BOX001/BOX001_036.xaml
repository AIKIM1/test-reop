                                                    <c1:DataGridTextColumn        Header = "LOTID"              Binding = "{Binding LOTID}"                        HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "PJT"                Binding = "{Binding PROJECTNAME}"                  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "극성"               Binding = "{Binding PRDT_CLSS_NAME}"               HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "C/ROLL"             Binding = "{Binding WIPQTY}"                       HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                    <c1:DataGridTextColumn        Header = "S/ROLL"             Binding = "{Binding WIPQTY2}"                      HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                    <c1:DataGridTextColumn        Header = "변환수량(Roll)"     Binding = "{Binding CONV_QTY}"                     HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                    <c1:DataGridTextColumn        Header = "변환수량(Lane)"     Binding = "{Binding CONV_QTY2}"                    HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                    <c1:DataGridTextColumn        Header = "차이수량(Roll)"     Binding = "{Binding DIFF_QTY}"                     HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                    <c1:DataGridTextColumn        Header = "차이수량(Lane)"     Binding = "{Binding DIFF_QTY2}"                    HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                    <c1:DataGridTextColumn        Header = "제품코드"           Binding = "{Binding PRODID}"                       HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn        Header = "모델ID"             Binding = "{Binding MODLID}"                       HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn        Header = "LANE 수"            Binding = "{Binding LANE_QTY}"                     HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn        Header = "단위"               Binding = "{Binding UNIT_CODE}"                    HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn        Header = "유효기간"           Binding = "{Binding VLD_DATE}"                     HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn        Header = "전극버전"           Binding = "{Binding PROD_VER_CODE}"                HorizontalAlignment="Center"/>
                                                    <c1:DataGridTextColumn        Header = "출고처"             Binding = "{Binding SHIPTO_NAME}"                  HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "출고상태"           Binding = "{Binding BOX_RCV_ISS_STAT_CODE_DESC}"   HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "GMES 출고일"        Binding = "{Binding INSDTTM}"                      HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "물류이관/출고일자"  Binding = "{Binding RCV_DTTM}"                     HorizontalAlignment="Center" />
                                                    <c1:DataGridTextColumn        Header = "출고상태"           Binding = "{Binding BOX_RCV_ISS_STAT_CODE}"        HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn        Header = "출고NO"             Binding = "{Binding RCV_ISS_ID}"                   HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                    <c1:DataGridTextColumn        Header = "전기일"             Binding = "{Binding INSDTTM1}"                     HorizontalAlignment="Center"/>

                                                </c1:C1DataGrid.Columns>
                                            </c1:C1DataGrid>
                                        </Grid>

                                    </Grid>

                                </Grid>

                            </Grid>
                        </Grid>

                    </c1:C1TabItem>
                    <!-- [E20240624-000248] [MES팀] MES 시스템 라벨 발행 기능 추가 요청의 건 상세이력탭 End -->

                </c1:C1TabControl>
            </Grid>
        </ctrlLib:LGCLayout>

        <ctrlLib:LoadingIndicator x:Name="loadingIndicator" Grid.Row="999" Grid.Column="999" />
    </Grid>
</UserControl>
