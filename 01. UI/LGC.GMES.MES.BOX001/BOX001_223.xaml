<UserControl  xmlns:UserControls="clr-namespace:LGC.GMES.MES.CMM001.UserControls;assembly=LGC.GMES.MES.CMM001"
    xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.BOX001.BOX001_223"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="900" d:DesignWidth="1200"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="2차 포장 구성">
            <ctrlLib:LGCLayout.Content>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="51"/>
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="4*"/>
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="6*"/>
                        <RowDefinition Height="4"/>
                        <RowDefinition Height="35"/>
                    </Grid.RowDefinitions>

                    <Grid x:Name="ButtonArea" Margin="0,-48,0,0">
                        <Grid VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="34"/>
                            </Grid.ColumnDefinitions>
                            <uc:UcBaseButton x:Name="btnRun"        Grid.Row="0" Grid.Column="3" Content="{Binding ConverterParameter='작업시작',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainControl01Style}" VerticalAlignment="Top" Click="btnRun_Click" />
                            <uc:UcBaseButton x:Name="btnRunCancel"       Grid.Row="0" Grid.Column="5" Content="{Binding ConverterParameter='작업취소',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainControl03Style}" Click="btnRunCancel_Click" />
                            <uc:UcBaseButton x:Name="btnConfirm"     Grid.Row="0" Grid.Column="7" Content="{Binding ConverterParameter='실적확정',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainControl04Style}" Click="btnConfirm_Click" />
                            <uc:UcBaseButton x:Name="btnConfirmCancel"       Grid.Row="0" Grid.Column="9" Content="{Binding ConverterParameter='실적취소',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainControl03Style}" Click="btnConfirmCancel_Click"/>
                            <uc:UcBaseButton x:Name="btnShip"     Grid.Row="0" Grid.Column="11" Content="{Binding ConverterParameter='출고',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainControl04Style}" Click="btnShip_Click" />
                            <uc:UcBaseButton x:Name="btnCancelShip"       Grid.Row="0" Grid.Column="13" Content="{Binding ConverterParameter='출고취소',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainControl03Style}" Click="btnCancelShip_Click"   />
                            <uc:UcBaseButton x:Name="btnPrint"          Grid.Row="0" Grid.Column="15" Content="{Binding ConverterParameter='Packing List 발행',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainControl05Style}" Click="btnPrint_Click"/>
                            <uc:UcBaseButton x:Name="btnGogoroPrint"          Grid.Row="0" Grid.Column="17" Content="{Binding ConverterParameter='Gogoro향 Tag 발행',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainControl05Style}" Click="btnGogoroPrint_Click"/>
                        </Grid>
                    </Grid>

                    <Grid x:Name="SearchArea" Grid.Row="0">
                        <Border Grid.Row="0" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                        <Grid Grid.Row="0" Grid.Column="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="13"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="13"/>
                            </Grid.ColumnDefinitions>

                            <Grid Grid.Row="0" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="3*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock      Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter},  Mode=oneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <uc:UcBaseComboBox  Grid.Row="0" Grid.Column="1" x:Name="cboLine" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="" SelectedValuePath=""/>
                            </Grid>

                            <Grid Grid.Row="0" Grid.Column="1" Visibility="Collapsed" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="3*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock      Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter},  Mode=oneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <uc:UcBaseComboBox  Grid.Row="0" Grid.Column="1" x:Name="cboArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="" SelectedValuePath=""/>
                            </Grid>

                            <Grid Grid.Row="0" Grid.Column="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="3*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock      Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='PalletID', Converter={StaticResource GlobalObjectDicConverter},  Mode=oneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox Grid.Row="0" Grid.Column="1"  x:Name="txtPalletID" Style="{StaticResource SearchCondition_TextBoxStyle}"   CharacterCasing="Upper" GotFocus="text_GotFocus" KeyDown="txtPalletID_KeyDown"/>
                            </Grid>


                            <Grid Grid.Row="0" Grid.Column="3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="3*"/>
                                </Grid.ColumnDefinitions>
                            </Grid>
                            <uc:UcBaseButton Grid.Row="1" Grid.Column="6" x:Name="btnSearch" Content="{Binding ConverterParameter='SEARCH', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                        </Grid>
                    </Grid>
                    <!--Search Area End-->

                    <Grid x:Name="ContentTop" Grid.Row="2">
                        <Border Grid.Row="0" Grid.Column="0" Background="#ffffffff" BorderThickness="1" BorderBrush="#ffbebebe"></Border>
                        <Grid Grid.Row="0" Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <Border     Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                            <Grid Grid.Row="0" Grid.Column="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="15"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="15" />
                                </Grid.ColumnDefinitions>

                                <TextBlock  Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='작업PALLET', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}"/>

                                <Grid Grid.Column="3" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1*" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="1*" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="20" />
                                        <ColumnDefinition Width="4" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="20" />
                                        <ColumnDefinition Width="4" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="20" />
                                        <ColumnDefinition Width="4" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="20" />
                                        <ColumnDefinition Width="4" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>

                                    <Border    Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Name="lblCreated" Tag="CREATED" Background="#FFFF96" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}" />
                                    <uc:UcBaseCheckBox  Grid.Row="1" Grid.Column="0"  x:Name="chkCreated" IsChecked="True" Checked="chkSearch_Checked" Unchecked="chkSearch_Unchecked"/>
                                    <TextBlock Cursor="Hand" MouseUp="lblCreated_MouseUp" Grid.Row="1" Grid.Column="2" Margin="0,0,5,0" Text="{Binding ConverterParameter='포장대기', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />

                                    <Border    Grid.Row="1" Grid.Column="4" Grid.ColumnSpan="3" Name="lblPacking" Tag="PACKING" Background="#E8F7C8" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <uc:UcBaseCheckBox  Grid.Row="1" Grid.Column="4"  x:Name="chkPacking" IsChecked="True" Checked="chkSearch_Checked" Unchecked="chkSearch_Unchecked"/>
                                    <TextBlock Cursor="Hand" MouseUp="lblPacking_MouseUp" Grid.Row="1" Grid.Column="6" Margin="0,0,5,0" Text="{Binding ConverterParameter='포장중', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />

                                    <Border    Grid.Row="1" Grid.Column="8"  Grid.ColumnSpan="3" Name="lblPacked" Tag="PACKED"  Background="#E6F5FB" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <uc:UcBaseCheckBox  Grid.Row="1" Grid.Column="8"  x:Name="chkPacked" IsChecked="True" Checked="chkSearch_Checked" Unchecked="chkSearch_Unchecked"/>
                                    <TextBlock Cursor="Hand" MouseUp="lblPacked_MouseUp" Grid.Row="1" Grid.Column="10" Margin="0,0,5,0" Text="{Binding ConverterParameter='포장완료', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />

                                    <Border    Grid.Row="1" Grid.Column="12"  Grid.ColumnSpan="3" Name="lblShipping" Tag="SHIPPING"  Background="#FFF6E6FB" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <uc:UcBaseCheckBox  Grid.Row="1" Grid.Column="12"  x:Name="chkShipping" IsChecked="True" Checked="chkSearch_Checked" Unchecked="chkSearch_Unchecked"/>
                                    <TextBlock Cursor="Hand" MouseUp="lblShipping_MouseUp" Grid.Row="1" Grid.Column="14" Margin="0,0,5,0" Text="{Binding ConverterParameter='출고요청', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />

                                    <!--<Border    Grid.Row="1" Grid.Column="12" Name="lblCancel" Tag="CANCEL_SHIP" Background="#F8DAC0" BorderThickness="1" Style="{StaticResource BorderSubTitleStyle}"/>-->
                                    <!--<TextBlock Grid.Row="1" Grid.Column="14" Text="{Binding ConverterParameter='출고취소', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource SearchCondition_LabelStyle}" />-->
                                </Grid>
                            </Grid>

                            <Grid Grid.Row="1" Grid.Column="0">
                                <c1:C1DataGrid x:Name="dgPalletList" Grid.Row="0" IsReadOnly="True" HeadersVisibility="Column" RowHeight="Auto" SelectionMode="MultiRange" FrozenBottomRowsCount="1" LoadedCellPresenter="dgPalletList_LoadedCellPresenter" >
                                    <c1:C1DataGrid.Resources>
                                        <DataTemplate x:Key="ResultTemplate">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <TextBlock Grid.Column="1" Text="{Binding}" Style="{StaticResource Content_InputForm_TextBlockStyle}" HorizontalAlignment="Center"></TextBlock>
                                            </Grid>
                                        </DataTemplate>
                                    </c1:C1DataGrid.Resources>
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTemplateColumn   EditOnSelection="True" Name="CHK" Width="Auto" HorizontalAlignment="Center" >
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <RadioButton GroupName="dgPalletListChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgPalletListChoice" HorizontalAlignment="Center" Checked="dgPalletListChoice_Checked"/>
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>
                                        <c1:DataGridTextColumn          Header="PALLETID"       Binding = "{Binding BOXID}" IsReadOnly="True" HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn          Header="PROCID"        Binding = "{Binding PROCID}"  IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn          Header="포장구분"        Binding = "{Binding PROCNAME}"  IsReadOnly="True" HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn          Header="PROJECT"        Binding = "{Binding PROJECT}"  IsReadOnly="True" HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn          Header="제품코드"       Binding = "{Binding PRODID}"  IsReadOnly="True" HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn          Header="제품코드"       Binding = "{Binding PRODNAME}"  IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn          Header="조립LOTID"     Binding = "{Binding PKG_LOTID}"  IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn          Header="SOC"     Binding = "{Binding SOC_VALUE}"  IsReadOnly="True" HorizontalAlignment="Center"  Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn          Header="포장상태"     Binding = "{Binding BOXSTAT}"  IsReadOnly="True" HorizontalAlignment="Center"   Visibility="Collapsed" />
                                        <c1:DataGridTextColumn          Header="포장상태"     Binding = "{Binding BOXSTAT_NAME}"  IsReadOnly="True" HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn          Header="출고상태"     Binding = "{Binding RCV_ISS_STAT_CODE}"  IsReadOnly="True" HorizontalAlignment="Center"   Visibility="Collapsed" />
                                        <c1:DataGridTextColumn          Header="출고상태"     Binding = "{Binding RCV_ISS_STAT_NAME}"  IsReadOnly="True" HorizontalAlignment="Center"/>
                                        <c1:DataGridNumericColumn       Header="투입수량"     Binding = "{Binding INPUT_QTY}"  IsReadOnly="True"  Format="#,##0" HorizontalAlignment="Center"/>
                                        <c1:DataGridNumericColumn       Header="포장수량"     Binding = "{Binding TOTAL_QTY}"  IsReadOnly="True"  Format="#,##0" HorizontalAlignment="Center"/>
                                        <c1:DataGridNumericColumn       Header="OUTBOX수량"     Binding = "{Binding BOXQTY}"  IsReadOnly="True"  Format="#,##0" HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn          Header="수출/내수"     Binding = "{Binding EXP_DOM_TYPE_NAME}" IsReadOnly="True"  HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn          Header="수출/내수"     Binding = "{Binding EXP_DOM_TYPE_CODE}" IsReadOnly="True"  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn          Header="출하처"     Binding = "{Binding SHIPTO_NAME}" IsReadOnly="True"  HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn          Header="출하처"     Binding = "{Binding SHIPTO_ID}" IsReadOnly="True"  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridDateTimeColumn      Header="시작일시"     Binding = "{Binding  START_DTTM}" HorizontalAlignment="Center" Format="yyyy-MM-dd HH:mm" />
                                        <c1:DataGridDateTimeColumn      Header="종료일시"     Binding = "{Binding  END_DTTM}" HorizontalAlignment="Center" Format="yyyy-MM-dd HH:mm" />
                                        <c1:DataGridTextColumn          Header="소요시간(분)"     Binding = "{Binding DURATION_MINUTE}" IsReadOnly="True"  HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn          Header="작업조"       Binding="{Binding SHFT_ID}"     IsReadOnly="True"         HorizontalAlignment="Center"   Visibility="Collapsed" />
                                        <c1:DataGridTextColumn          Header="작업조"       Binding="{Binding SHFT_NAME}"    IsReadOnly="True"          HorizontalAlignment="Center" />
                                        <c1:DataGridTextColumn          Header="작업자"       Binding="{Binding ACTUSER}"     IsReadOnly="True"         HorizontalAlignment="Center"   Visibility="Collapsed" />
                                        <c1:DataGridTextColumn          Header="작업자"       Binding="{Binding ACTUSERNAME}"    IsReadOnly="True"          HorizontalAlignment="Center" />

                                        <c1:DataGridTextColumn          Header="MDLLOT_ID"       Binding = "{Binding MDLLOT_ID}"  IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn          Header="NOTE"       Binding="{Binding PACK_NOTE}"    IsReadOnly="True"          HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn          Header="EQPTID"       Binding="{Binding EQPTID}"    IsReadOnly="True"          HorizontalAlignment="Center" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn          Header="EQPTNAME"       Binding="{Binding EQPTNAME}"    IsReadOnly="True"          HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn          Header="PACKDTTM"       Binding="{Binding PACKDTTM}"    IsReadOnly="True"          HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn          Header="상태조회용컬럼"     Binding = "{Binding BOXSTAT_LIST}"  IsReadOnly="True" HorizontalAlignment="Center"   Visibility="Collapsed" />

                                    </c1:C1DataGrid.Columns>
                                    <c1:C1GroupingWithSummariesBehavior.GroupingWithSummariesBehavior>
                                        <c1:C1GroupingWithSummariesBehavior/>
                                    </c1:C1GroupingWithSummariesBehavior.GroupingWithSummariesBehavior>

                                    <c1:C1DataGrid.BottomRows>
                                        <c1:DataGridSummaryRow x:Name="summary_Pallet" RowStyle="{StaticResource SummaryRowStyle}"/>
                                    </c1:C1DataGrid.BottomRows>
                                </c1:C1DataGrid>
                            </Grid>
                        </Grid>
                    </Grid>
                    <!--Content Top End-->

                    <GridSplitter Grid.Row="3" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                    <Grid x:Name="ContentBot" Grid.Row="4">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="5*"/>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="7*"/>
                            </Grid.ColumnDefinitions>


                            <Border Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>

                            <c1:C1TabControl  Grid.Row="0" Grid.Column="0" Margin="3">
                                <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='투입팔레트'}" >
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="34"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="34"/>
                                        </Grid.RowDefinitions>
                                        <!--<Border Grid.Row="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>-->

                                        <Grid Grid.Row="0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="4"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="180"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="80"/>
                                                <ColumnDefinition Width="0"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="4"/>
                                            </Grid.ColumnDefinitions>

                                            <TextBlock  Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='1차포장팔레트ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_TextBlockStyle}" FontWeight="SemiBold" HorizontalAlignment="Right"/>

                                            <TextBox  Grid.Column="2" Style="{StaticResource Content_InputForm_TextBoxStyle}" x:Name="txtInPalletID"  CharacterCasing="Upper" GotFocus="text_GotFocus" KeyDown="txtInPalletID_KeyDown" />


                                            <!--<TextBlock  Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='잔량', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_TextBlockStyle}" FontWeight="SemiBold" HorizontalAlignment="Right"/>-->
                                            <c1:C1NumericBox Grid.Row="1"  Grid.Column="5" x:Name="txtRemainQty"   ShowButtons="False"  Format="#,##0" Margin="3" Visibility="Collapsed"/>

                                            <uc:UcBaseButton      Grid.Column="7"  x:Name="btnRemain"    Click="btnRemain_Click"   Content="{Binding ConverterParameter='잔량처리',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" />

                                        </Grid>

                                        <c1:C1DataGrid Grid.Row="1" Grid.Column="0" x:Name="dgInPallet" IsReadOnly="False" FrozenBottomRowsCount="1"  HeadersVisibility="Column" SelectionMode="MultiRange" CanUserAddRows="True">
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridCheckBoxColumn   Width="45"        Binding = "{Binding CHK}"              IsReadOnly ="False"   HorizontalAlignment="Center"  />
                                                <c1:DataGridTextColumn Header="1차팔레트ID"    Binding = "{Binding BOXID}"            IsReadOnly ="True"    HorizontalAlignment="Center"  />
                                                <c1:DataGridTextColumn Header="조립LOTID"      Binding = "{Binding PKG_LOTID}"            IsReadOnly ="True"    HorizontalAlignment="Center"  />
                                                <c1:DataGridTextColumn Header="CELL수량"       Binding = "{Binding TOTAL_QTY2}"       IsReadOnly ="True"    HorizontalAlignment="Center"  />
                                                <c1:DataGridTextColumn Header="박스수량"       Binding = "{Binding BOXQTY}"           IsReadOnly ="True"    HorizontalAlignment="Center"  />
                                                <c1:DataGridTextColumn Header="CELL잔량"       Binding = "{Binding REST_TOTAL_QTY}"   IsReadOnly ="True"    HorizontalAlignment="Center"  />
                                                <c1:DataGridTextColumn Header="박스잔량"       Binding = "{Binding REST_BOXQTY}"      IsReadOnly ="True"    HorizontalAlignment="Center"  />
                                            </c1:C1DataGrid.Columns>
                                            <c1:C1GroupingWithSummariesBehavior.GroupingWithSummariesBehavior>
                                                <c1:C1GroupingWithSummariesBehavior/>
                                            </c1:C1GroupingWithSummariesBehavior.GroupingWithSummariesBehavior>

                                            <c1:C1DataGrid.BottomRows>
                                                <c1:DataGridSummaryRow x:Name="summary_InPallet" RowStyle="{StaticResource SummaryRowStyle}"/>
                                            </c1:C1DataGrid.BottomRows>
                                        </c1:C1DataGrid>

                                        <Grid Grid.Row="2">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="15"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="4"/>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="15"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock     Grid.Column="2" Text="{Binding ConverterParameter='출하처', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                                            <uc:UcBaseComboBox   Grid.Column="3" x:Name="cboShipTo" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                                            <uc:UcBaseButton     Grid.Column="5"  x:Name="btnChangeShipTo"       Content="{Binding ConverterParameter='변경',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnChangeShipTo_Click" />

                                        </Grid>
                                    </Grid>
                                </c1:C1TabItem>
                                <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='투입박스'}" >
