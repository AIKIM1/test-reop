<c1:C1Window x:Class="LGC.GMES.MES.BOX001.BOX001_240_LBL0085"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
		     xmlns:System="clr-namespace:System;assembly=mscorlib"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary" 
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"   
             xmlns:cmm001="clr-namespace:LGC.GMES.MES.CMM001;assembly=LGC.GMES.MES.CMM001"
             ctrlLib:LGCPopupWindowExtension.ApplyStyle="True"
             Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='Outbox 생성'}" 
             Tag="ProtoType01Windows01" Height="300" Width="400" IsResizable="True" Loaded="C1Window_Loaded" >

    <c1:C1Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;Component/ControlStyles.xaml"/>
                <ResourceDictionary Source="/LGC.GMES.MES.CMM001;component/AutoCompleteComboBoxStyle.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.CMM001;component/PopupFindControlStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </c1:C1Window.Resources>

    <Grid Name="grdMain">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="8" />
            <RowDefinition Height="*" />
            <RowDefinition Height="8" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

      
        <Grid Grid.Row="2">
            <Grid.RowDefinitions>
                <!--<RowDefinition Height="Auto"/>-->
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="99" Grid.RowSpan="99" BorderThickness="1" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>            
            <c1:C1DataGrid Grid.Row="0" Grid.Column="0" x:Name="dgResult" FrozenBottomRowsCount="1" IsReadOnly="False"  HeadersVisibility="Column" SelectionMode="MultiRange" CanUserAddRows="True">
                <c1:C1DataGrid.Columns>
                    <c1:DataGridTextColumn Header="InBox ID"        Binding = "{Binding BOXID}" HorizontalAlignment="Center"/>
                    <c1:DataGridTextColumn Header="Outbox ID"          Binding = "{Binding OUTER_BOXID2}" HorizontalAlignment="Center"/>
                    <!--컬럼속성 정리 필요-->
                </c1:C1DataGrid.Columns>
                <!--<c1:C1GroupingWithSummariesBehavior.GroupingWithSummariesBehavior>
                    <c1:C1GroupingWithSummariesBehavior/>
                </c1:C1GroupingWithSummariesBehavior.GroupingWithSummariesBehavior>

                <c1:C1DataGrid.BottomRows>
                    <c1:DataGridSummaryRow x:Name="summary_InPallet" RowStyle="{StaticResource SummaryRowStyle}"/>
                </c1:C1DataGrid.BottomRows>-->
            </c1:C1DataGrid>

        </Grid>
        <!--button area-->
        <Grid Grid.Row ="4">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="5"/>
                <ColumnDefinition Width="*"/>                
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="5"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>

            <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="99" Grid.RowSpan="99" BorderThickness="1" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
            <uc:UcBaseButton x:Name="btnSave"    Grid.Column="2" Content="{Binding ConverterParameter='Print', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"   Click="btnSave_Click" Style="{DynamicResource Content_MainButtonSpecialStyle}"  />
            <uc:UcBaseButton x:Name="btnClose" Grid.Column="4" Content="{Binding ConverterParameter='닫기', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Click="btnClose_Click" Style="{DynamicResource Content_MainButtonStyle}"  />
        </Grid>

        <ctrlLib:LoadingIndicator x:Name="loadingIndicator"  />
    </Grid>
</c1:C1Window>
