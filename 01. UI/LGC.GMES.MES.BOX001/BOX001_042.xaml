<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.BOX001.BOX001_042"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <Grid.Resources>
            <DataTemplate x:Key="ItemTemplate">
                <WrapPanel>
                    <Image Width="138" Height="138" Stretch="Fill" Source="{Binding}" Margin="3,3,3,3" />
                </WrapPanel>
            </DataTemplate>
            <DataTemplate x:Key="ResultTemplate">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="1" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>
        </Grid.Resources>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="TTI 라벨 발행">
            <ctrlLib:LGCLayout.Content>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="4"/>
                        <RowDefinition Height="35"/>
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="45"/>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="3*"/>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="3*"/>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="4*"/>
                        </Grid.RowDefinitions>
                        <Grid x:Name="SearchArea" Grid.Row="0">
                            <Border Grid.Row="0" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="4"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="13"/>
                                </Grid.ColumnDefinitions>
                                <Border Grid.Row="0" Grid.ColumnSpan="9" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock Margin="15,9,0,8" Grid.Column="0" Text="{Binding ConverterParameter='포장 실적 정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_Search_LabelStyle}" HorizontalAlignment="Left" Grid.ColumnSpan="2" Visibility="Collapsed"/>
                                <TextBlock Grid.Column="3" x:Name ="soShip" Text="{Binding ConverterParameter='Ship No', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_Search_LabelStyle}" HorizontalAlignment="Left" Grid.ColumnSpan="2" Margin="0,9,0,8"/>
                                <TextBox   Grid.Column="5" Style="{StaticResource Content_InputForm_TextBoxStyle}" x:Name="txtShipNo" Width="150" KeyDown="txtShipNo_KeyDown" />
                                <uc:UcBaseButton  Grid.Column="7" x:Name="btnSearch_SO" Content="{Binding ConverterParameter='SEARCH', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_SO_Click" />
                            </Grid>



                        </Grid>
                        <!--Search Area End-->
                        <Grid x:Name="grdInPallet" Grid.Row="2">
                            <Grid Grid.Row="0" Grid.Column="0">
                                <Border Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                <Grid Grid.Column="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Border Grid.Row="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <Grid   Grid.Row="0" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="15"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="13"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock      Grid.Row="0"  Grid.Column="1" Text="{Binding ConverterParameter='S/O 정보', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                    </Grid>
                                    <Grid Grid.Row="2">
                                        <c1:C1DataGrid Grid.Row="1" Grid.Column="1" x:Name="dgSalesOrder" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="SingleRow" MouseLeftButtonUp="dgSalesOrder_MouseLeftButtonUp" FrozenColumnCount="1">
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridTemplateColumn  EditOnSelection="True" Name="CHK" Width="Auto" CanUserFilter="False" CanUserSort="False" CanUserResize="False"  >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <RadioButton GroupName="dgSalesOrderChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgSalesOrderChoice" HorizontalAlignment="Center" Checked="dgSalesOrderChoice_Checked" />
                                                        </DataTemplate>
                                                    </c1:DataGridTemplateColumn.CellTemplate>
                                                </c1:DataGridTemplateColumn>
                                                <c1:DataGridTextColumn Header="Part No(Ordering Code)"     Binding = "{Binding CUST_MTRLID}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header="Part Name"     Binding = "{Binding CUST_MTRLNAME}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header="Shipping Note"     Binding = "{Binding SHIPMENT_NO}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header="Purchase"     Binding = "{Binding PO_NO}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header="Total Quantity"     Binding = "{Binding TOTL_QTY}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                                <c1:DataGridTextColumn Header="Vender Code"     Binding = "{Binding CUSTOMERID}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header="Vender Name"     Binding = "{Binding CUSTOMERNAME}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header="Manufacturer PartNumber"     Binding = "{Binding PRODID}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header="Print Manufacturer"     Binding = "{Binding PRODNAME}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header="SHIPTO_ID"     Binding = "{Binding SHIPTO_ID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn Header="인도처"     Binding = "{Binding ERP_SHIPTO_ID}" HorizontalAlignment="Center" />
                                            </c1:C1DataGrid.Columns>
                                        </c1:C1DataGrid>

                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>
                        <GridSplitter Grid.Row="3" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                        <Grid x:Name="grdSalesOrder" Grid.Row="4">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Border Grid.Row="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                            <Grid Grid.Row="0" Grid.Column="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="150"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="13"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock    Margin="15,0,0,0"  Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='포장 실적 정보', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Left" Style="{StaticResource Content_Search_LabelStyle}" />
                                <Grid Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column="0"  Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" VerticalAlignment="Center" TextAlignment="Right"/>
                                    <uc:UcBaseComboBox  Grid.Column="1"  x:Name="cboArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>
                                <Grid Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column="0"  Text="{Binding ConverterParameter='조립 생산일', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" VerticalAlignment="Center" TextAlignment="Right"/>
                                    <ctrlLib:LGCDatePicker Grid.Column="1" x:Name="dtpDateFrom"  DatepickerType="Date"/>
                                    <TextBlock Grid.Column="2" Text="~" VerticalAlignment="Center"/>
                                    <ctrlLib:LGCDatePicker Grid.Column="3" x:Name="dtpDateTo"  DatepickerType="Date"/>

                                </Grid>
                                <Grid Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock      Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='Pallet ID', Converter={StaticResource GlobalObjectDicConverter},  Mode=oneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <TextBox        Grid.Row="0" Grid.Column="1" Style="{StaticResource Content_InputForm_TextBoxStyle}" x:Name="txtPalletID" KeyDown="txtPalletID_KeyDown"/>

                                    <TextBlock      Grid.Row="0" Grid.Column="3" Text="{Binding ConverterParameter='LOT ID', Converter={StaticResource GlobalObjectDicConverter},  Mode=oneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <TextBox        Grid.Row="0" Grid.Column="4" Style="{StaticResource Content_InputForm_TextBoxStyle}" x:Name="txtLotID" CharacterCasing="Upper" KeyDown="txtLotID_KeyDown"/>

                                    
                                </Grid>
                                <uc:UcBaseButton Grid.Row="1" Grid.Column="5" x:Name="btnSearch" Content="{Binding ConverterParameter='SEARCH', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand"  Click="btnSearch_Click"/>
                            </Grid>

                            <c1:C1DataGrid Grid.Row="1" Grid.Column="1"  x:Name="dgInpallet" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="SingleRow" MouseLeftButtonUp="dgInpallet_MouseLeftButtonUp" FrozenColumnCount="1">
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridTemplateColumn  EditOnSelection="True" Name="CHK" Width="Auto" CanUserFilter="False" CanUserSort="False" CanUserResize="False"  >
                                        <c1:DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <RadioButton GroupName="dgPrintListChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgPrintListChoice" HorizontalAlignment="Center" Checked="dgPrintListChoice_Checked" />
                                            </DataTemplate>
                                        </c1:DataGridTemplateColumn.CellTemplate>
                                    </c1:DataGridTemplateColumn>
                                    <c1:DataGridTextColumn Header="AREA"     Binding = "{Binding AREANAME}" HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn Header="LINE"     Binding = "{Binding LINE_NO}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="PRODID"     Binding = "{Binding PRODID}" HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn Header="PRINT_MODEL_NAME"     Binding = "{Binding PRINT_MODEL_NAME}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="PROJECT"     Binding = "{Binding PROJECT}" HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn Header="LOTID"     Binding = "{Binding PKG_LOTID}" HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn Header="일자"     Binding = "{Binding PRINTDATE}" HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn Header="등급"     Binding = "{Binding PRDT_GRD_CODE}" HorizontalAlignment="Center" />
                                    <c1:DataGridTextColumn Header="CAPA"     Binding = "{Binding CAPA}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                    <c1:DataGridTextColumn Header="양품 수량"     Binding = "{Binding TOTAL_CELL_QTY}" HorizontalAlignment="Center" Format="###,###,##0.##"/>
                                    <c1:DataGridTextColumn Header="VERSION"     Binding = "{Binding PACK_VER}" HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn Header="Pallet ID"     Binding = "{Binding BOXID}" HorizontalAlignment="Center"/>

                                    <c1:DataGridTextColumn Header="AREAID"     Binding = "{Binding AREAID}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn Header="LOTID_PRODDATE"     Binding = "{Binding LOTID_PRODDATE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn Header="BOXID_BCD"     Binding = "{Binding BOXID_BCD}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn Header="BOXID_TEXT"     Binding = "{Binding BOXID_TEXT}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                </c1:C1DataGrid.Columns>
                            </c1:C1DataGrid>

                        </Grid>
                        <GridSplitter Grid.Row="5" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                        <Grid x:Name="grdLabel" Grid.Row="6">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Column="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Margin="15,0,0,0" Grid.Column="0" Text="{Binding ConverterParameter='OUTBOX 정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_Search_LabelStyle}" HorizontalAlignment="Left" Grid.ColumnSpan="2"/>
                                    <TextBlock Grid.Column="2" Text="{Binding ConverterParameter='발행 수량', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <c1:C1NumericBox  Grid.Column="4" Style="{StaticResource C1NumericBoxStyle}" x:Name="nbPrtQty_Box"  Minimum="1" Value="1"  Width="70" FontSize="20"/>
                                    <uc:UcBaseButton x:Name="btnCancelPrint" Grid.Column="6" Content="{Binding ConverterParameter='발행 취소', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource  Content_MainButtonNoMinWidthSpecialStyle}"  Click="btnCancelPrint_Click"/>
                                    <uc:UcBaseButton x:Name="btnInBoxPrint" Grid.Column="8" Content="{Binding ConverterParameter='INBOX 발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource  Content_MainButtonNoMinWidthSpecialStyle}"  Click="btnInBoxPrint_Click"/>
                                    <uc:UcBaseButton x:Name="btnInBoxAutoPrint" Grid.Column="10" Content="{Binding ConverterParameter='INBOX 자동발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource  Content_MainButtonNoMinWidthSpecialStyle}"  Click="btnInBoxAutoPrint_Click"/>
                                    <uc:UcBaseButton x:Name="btnOutBoxPrint" Grid.Column="12" Content="{Binding ConverterParameter='OUTBOX 발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource  Content_MainButtonNoMinWidthSpecialStyle}" Click="btnOutBoxPrint_Click"/>
                                </Grid>
                                <Grid Grid.Row="1">
                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Grid.RowSpan="99" Background="White"/>
                                    <Grid Margin="10">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="3*"/>
                                        </Grid.RowDefinitions>
                                        <Grid Grid.Row="0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="5*"/>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="4*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="10,0,0,0"  Text="Model" VerticalAlignment="Center" FontSize="17" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbOutModel" Margin="10,0,0,0"  VerticalAlignment="Center" FontSize="15" FontWeight="Bold"/>

                                            </Border>
                                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="10,0,0,0"  Text="Grade" VerticalAlignment="Center"  FontSize="17" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="3" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbOutGrade" Margin="10,0,0,0"  VerticalAlignment="Center" FontSize="15" FontWeight="Bold"/>
                                            </Border>

                                        </Grid>
                                        <Grid Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="5*"/>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="4*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="10,0,0,0"  Text="Line" VerticalAlignment="Center" FontSize="17" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbOutLine" Margin="10,0,0,0"  VerticalAlignment="Center" FontSize="15" FontWeight="Bold"/>

                                            </Border>
                                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="10,0,0,0"  Text="Lot" VerticalAlignment="Center"  FontSize="17" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="3" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbOutLot" Margin="10,0,0,0"   VerticalAlignment="Center" FontSize="15" FontWeight="Bold"/>
                                            </Border>

                                        </Grid>
                                        <Grid Grid.Row="2">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="5*"/>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="4*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="10,0,0,0"  Text="Quantity" VerticalAlignment="Center" FontSize="17" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <StackPanel Orientation="Horizontal">
                                                    <c1:C1NumericBox x:Name="tbOutQty" VerticalAlignment="Center" FontWeight="Bold" FontSize="15" HorizontalAlignment="Left" Width="60" Value="0" LostFocus="tbOutQty_LostFocus"/>
                                                    <TextBlock Margin="5,0,0,0"  Grid.Column="1" Text="pcs"  FontWeight="Bold" FontSize="15" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Left"/>
                                                </StackPanel>
                                            </Border>
                                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="10,0,0,0"  Text="Date" VerticalAlignment="Center"  FontSize="17" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="3" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbOutDate" Margin="10,0,0,0"   VerticalAlignment="Center" FontSize="15" FontWeight="Bold"/>
                                            </Border>

                                        </Grid>
                                        <Grid Grid.Row="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="5*"/>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="4*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="10,0,0,0"  Text="Capacity" VerticalAlignment="Center" FontSize="17" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <StackPanel Orientation="Horizontal">
                                                    <TextBlock x:Name="tbOutCapa" Margin="10,0,0,0"  VerticalAlignment="Center" FontSize="15" FontWeight="Bold"  />
                                                    <TextBlock x:Name="tbOutCapaCode" Margin="10,0,0,0"  VerticalAlignment="Center" FontSize="15" FontWeight="Bold"  Visibility="Collapsed"/>
                                                    <uc:UcBaseComboBox x:Name="cbOutCapa" Margin="10,0,0,0"  VerticalAlignment="Center" FontSize="15" FontWeight="Bold" SelectedValuePath="CODE" DisplayMemberPath="CODE_NAME1"  Visibility="Collapsed" SelectedValueChanged="cbOutCapa_SelectedValueChanged"/>
                                                </StackPanel>
                                            </Border>
                                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="10,0,0,0"  Text="TTI P/N" VerticalAlignment="Center"  FontSize="17" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="3" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbOutPN" Margin="10,0,0,0" VerticalAlignment="Center" FontSize="15" FontWeight="Bold"/>
                                            </Border>
                                        </Grid>
                                        <Grid Grid.Row="4">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="5*"/>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="4*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="10,0,0,0"  Text="PO No" VerticalAlignment="Center" FontSize="17" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbOutPONo" Margin="10,0,0,0"   VerticalAlignment="Center" FontSize="15" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="10,0,0,0"  Text="Version" VerticalAlignment="Center"  FontSize="17" FontWeight="Bold"/>
                                            </Border>
                                            <Border Grid.Column="3" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbOutVer" Margin="10,0,0,0"   VerticalAlignment="Center" FontSize="15"  FontWeight="Bold"/>
                                            </Border>
                                        </Grid>
                                        <Grid Grid.Row="5">
                                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                                                <c1:C1BarCode  x:Name="bcBoxID"  HorizontalAlignment="Stretch"  VerticalAlignment="Center" BorderBrush="Black" Foreground="Black" CodeType="Code_128auto" ModuleSize="2" />
                                                <TextBlock x:Name="tbOutBcd"  HorizontalAlignment="Center" FontWeight="Bold" FontSize="17"/>
                                            </StackPanel>
                                        </Grid>
                                        <Border Grid.ColumnSpan="99" Grid.RowSpan="99" BorderThickness="2" BorderBrush="Black"/>

                                    </Grid>
                                </Grid>
                            </Grid>
                            <GridSplitter Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                            <Grid Grid.Column="2">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Margin="15,0,0,0" Grid.Column="0" Text="{Binding ConverterParameter='PALLET 정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_Search_LabelStyle}" HorizontalAlignment="Left"/>
                                    <StackPanel Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right">
                                        <TextBlock  Text="{Binding ConverterParameter='총 페이지', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <c1:C1NumericBox    Margin="4,0,0,0" Style="{StaticResource C1NumericBoxStyle}" x:Name="nbPrtQty_Pallet"  Minimum="1"  Width="50" ValueChanged="nbPrtQty_Pallet_ValueChanged"/>
                                        <TextBlock   Margin="4,0,0,0" Text="{Binding ConverterParameter='시작 페이지', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <uc:UcBaseComboBox  Margin="4,0,0,0" x:Name="cboPageNum1"  MinWidth="60"  DisplayMemberPath="Value" SelectedValuePath="Key" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" IsDropDownOpenChanged="cboPageNum1_IsDropDownOpenChanged" SelectedValueChanged="cboPageNum1_SelectedValueChanged"/>
                                        <TextBlock  Margin="4,0,0,0"  Text="{Binding ConverterParameter='끝 페이지', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <uc:UcBaseComboBox Margin="4,0,0,0" x:Name="cboPageNum2" MinWidth="60"  DisplayMemberPath="Value" SelectedValuePath="Key" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" IsDropDownOpenChanged="cboPageNum2_IsDropDownOpenChanged" SelectedValueChanged="cboPageNum2_SelectedValueChanged"/>
                                        <uc:UcBaseButton x:Name="btnPrint_Plt" Margin="4,0,0,0" Content="{Binding ConverterParameter='Pallet 발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource  Content_MainButtonNoMinWidthSpecialStyle}" Click="btnPrint_Plt_Click"/>
                                    </StackPanel>
                                </Grid>
                                <Grid Grid.Row="1">
                                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Grid.RowSpan="99" Background="White"/>
                                    <Grid Margin="5">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Grid.RowSpan="99" Background="White" BorderThickness="1" BorderBrush="Black"/>
                                        <Grid Grid.Row="0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="3*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="Customer" VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>
                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBox x:Name="txtPltCustomer" Text ="TTi ( MACAO COMMERCIAL OFFSHORE )" FontSize="16" BorderBrush="Gray" TextAlignment="Left" Padding="1"/>
                                            </Border>
                                        </Grid>
                                        <Grid Grid.Row="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="Supplier" VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>
                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBox x:Name="txtPltSupplier" Text ="LG Chem" FontSize="16" BorderBrush="Gray" TextAlignment="Left" Padding="1"/>
                                            </Border>
                                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="Part No" VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>
                                            </Border>
                                            <Border Grid.Column="3" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbPltPartNo" Margin="5,0,0,0" VerticalAlignment="Center" FontSize="16" FontWeight="Thin"/>
                                            </Border>
                                        </Grid>
                                        <Grid Grid.Row="2">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="PO No" VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>
                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto">
                                                    <TextBlock x:Name="tbPltPONo" Margin="5,0,0,0"  VerticalAlignment="Center" FontSize="15" FontWeight="Thin" TextWrapping="Wrap" LineStackingStrategy="BlockLineHeight" LineHeight="14" />
                                                </ScrollViewer>
                                            </Border>
                                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="Version" VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>
                                            </Border>
                                            <Border Grid.Column="3" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbPltVer" Margin="5,0,0,0" VerticalAlignment="Center" FontSize="16" FontWeight="Thin"/>
                                            </Border>
                                        </Grid>
                                        <Grid Grid.Row="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="Cell Code" VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>
                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock x:Name="tbPltModel" Margin="5,0,0,0"  VerticalAlignment="Center" FontSize="16" FontWeight="Thin" TextWrapping="Wrap"/>
                                            </Border>
                                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="Pallet No" VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>
                                            </Border>
                                            <Border Grid.Column="3" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBox x:Name="txtPltPalltNo"  FontSize="16" BorderBrush="Gray" TextAlignment="Left" Padding="1"/>

                                            </Border>
                                        </Grid>
                                        <Grid Grid.Row="4">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="Qty(Outer Box)"  VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>
                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <c1:C1NumericBox x:Name="txtPltBoxQty" Value="96" Margin="5,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" MinWidth="60" FontSize="16" BorderBrush="Gray" TextAlignment="Left" Padding="1"/>
                                            </Border>
                                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="Gross Wt" VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>
                                            </Border>
                                            <Border Grid.Column="3" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBox x:Name="txtPltWeight" HorizontalAlignment="Left" MinWidth="40" Text ="820" FontSize="16" BorderBrush="Gray" TextAlignment="Left" Padding="1"/>
                                            </Border>
                                            <TextBlock Grid.Column="3"  HorizontalAlignment="RIGHT" VerticalAlignment="Center" MinWidth="40" Text ="KG" FontSize="16" TextAlignment="LEFT"/>
                                        </Grid>
                                        <Grid Grid.Row="5">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="Qty(Cell)" VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>

                                            </Border>
                                            <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <c1:C1NumericBox x:Name="txtPltCellQty" Value="19200" Margin="5,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" MinWidth="80" FontSize="16" BorderBrush="Gray" TextAlignment="Left" Padding="1"/>
                                            </Border>
                                            <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBlock Margin="5,0,0,0"  Text="Origin" VerticalAlignment="Center" FontSize="16" FontWeight="UltraBold"/>
                                            </Border>
                                            <Border Grid.Column="3" BorderBrush="Black" BorderThickness="1,0,0,1">
                                                <TextBox x:Name="txtPltOrigin" Text ="South Korea" FontSize="16" BorderBrush="Gray" TextAlignment="Left" Padding="1"/>
                                            </Border>
                                        </Grid>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>
                    </Grid>

                    <Grid x:Name="Worker" Grid.Row="2">
                        <Border Grid.Row="6" Background="#e8ebed" BorderThickness="1" BorderBrush="#8a8a8a" />
                        <Grid Grid.Row="6" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="13"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34" />
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="0"/>
                                </Grid.ColumnDefinitions>
                                <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='작업조', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                </StackPanel>
                                <TextBox Grid.Row="0" Grid.Column="1" x:Name="txtShift" IsReadOnly="True"  HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" VerticalAlignment="Center" Margin="3, 3, 10, 3" />
                                <StackPanel Grid.Row="0" Grid.Column="2" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='작업시간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                </StackPanel>
                                <TextBox Grid.Row="0" Grid.Column="3" x:Name="txtShiftDateTime" IsReadOnly="True"  HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" VerticalAlignment="Center" Margin="3, 3, 10, 3" />
                                <StackPanel Grid.Row="0" Grid.Column="4" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}" />
                                    <TextBlock Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='작업자', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_Search_LabelStyle}" />
                                </StackPanel>
                                <TextBox Grid.Row="0" Grid.Column="5" x:Name="txtWorker" IsReadOnly="True"  HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" VerticalAlignment="Center" Margin="3, 3, 3, 3" />
                                <uc:UcBaseButton x:Name="btnShift" Grid.Column="6" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_SearchButtonStyle}" HorizontalAlignment="Left" IsCancel="True" Click="btnShift_Click"/>
                                <TextBox Grid.Row="0" Grid.Column="7" x:Name="txtShiftStartTime" IsReadOnly="True"  HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="3, 3, 3, 3" Visibility="Collapsed" />
                                <TextBox Grid.Row="0" Grid.Column="7" x:Name="txtShiftEndTime" IsReadOnly="True"  HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="3, 3, 3, 3" Visibility="Collapsed" />
                            </Grid>
                        </Grid>
                    </Grid>
                </Grid>
            </ctrlLib:LGCLayout.Content>
        </ctrlLib:LGCLayout>

        <ctrlLib:LoadingIndicator x:Name="loadingIndicator" Grid.Row="999" Grid.Column="999" />
    </Grid>
</UserControl>
