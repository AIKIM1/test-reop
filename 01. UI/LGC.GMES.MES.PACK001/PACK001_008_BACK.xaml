<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.PACK001.PACK001_008_BACK"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="공정진척(Pack)_Pack 포장 박스라벨 발행">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="6" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Border Grid.Row="0"  Grid.RowSpan="2" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>

                <!-- Content(Tab) -->
                <c1:C1TabControl x:Name="tcMain"   Grid.Column="1" Grid.Row="1">
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='312H'}" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="46"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>

                            <!--백그라운드-->
                            <Border  Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3"  BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>


                            <Grid Grid.Row="0" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="200"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>


                                <TextBlock    Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='BOX Label Layout',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>

                                <!--<Button    Grid.Column="1" Grid.Row="0" x:Name="btnExcelActHistory"       Content="{Binding ConverterParameter=EXCEL,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnExcelActHistory_Click"/>-->
                            </Grid>

                            <Grid Grid.Row="2" Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="34" />
                                    <RowDefinition Height="8" />
                                </Grid.RowDefinitions>

                                <!--centent 영역-->
                                <Grid  Grid.Row="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <Border  Grid.Row="0" Grid.Column="0"  BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <!--왼쪽 바코드 영역-->
                                    <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1" >
                                        <Grid x:Name="content_left" Grid.Column="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="2*" />
                                                <RowDefinition Height="0.8*" />
                                                <RowDefinition Height="2.2*" />
                                            </Grid.RowDefinitions>

                                            <!--전체 외곽선-->
                                            <Border Grid.Row="0" BorderBrush="Black" BorderThickness="1" />
                                            <Border Grid.Row="1" BorderBrush="Black" BorderThickness="1" />
                                            <Border Grid.Row="2" BorderBrush="Black" BorderThickness="1" />

                                            <!-- 왼쪽 바코드 영역 - 1번째 row-->
                                            <Grid x:Name="content_left_1row"  Grid.Row="0" >
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="1*" />
                                                    <RowDefinition Height="1*" />
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="1*" />
                                                    <ColumnDefinition Width="1*" />
                                                </Grid.ColumnDefinitions>

                                                <!--외곽선-->
                                                <Border Grid.Row="0" Grid.Column="0" BorderBrush="Black" BorderThickness="1" />
                                                <Border Grid.Row="1" Grid.Column="0" BorderBrush="Black" BorderThickness="1" />
                                                <Border Grid.Row="0" Grid.Column="1" BorderBrush="Black" BorderThickness="1" />
                                                <Border Grid.Row="1" Grid.Column="1" BorderBrush="Black" BorderThickness="1" />

                                                <!--Receiver -->
                                                <Grid x:Name="receive" Grid.Row="0" Grid.Column="0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="6" />
                                                        <RowDefinition Height="2*" />
                                                        <RowDefinition Height="3*" />
                                                        <RowDefinition Height="6" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="10" />
                                                        <ColumnDefinition Width="1*" />
                                                        <ColumnDefinition Width="10" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock    Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Receive',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" Margin="0,-16,0,3"/>
                                                    <TextBox      Grid.Row="2" Grid.Column="1" x:Name="txtReceive" TextAlignment="Left" VerticalAlignment="Stretch"  Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  Height="Auto" BorderBrush="Black" BorderThickness="1" Margin="20,0,9,0"/>

                                                </Grid>

                                                <!--Advice Note No (N) -->
                                                <Grid x:Name="advice" Grid.Row="1" Grid.Column="0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="0" />
                                                        <RowDefinition Height="40" />
                                                        <RowDefinition Height="3*" />
                                                        <RowDefinition Height="6" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="10" />
                                                        <ColumnDefinition Width="130" />
                                                        <ColumnDefinition Width="10" />
                                                        <ColumnDefinition Width="1*" />
                                                        <ColumnDefinition Width="10" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock    Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Advice Note No (N)',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                                    <TextBox      Grid.Row="1" Grid.Column="3" x:Name="txtAdvice" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  BorderBrush="Black" BorderThickness="1" TextChanged="txtAdvice_TextChanged"/>
                                                    <c1:C1BarCode Grid.Row="2" Grid.Column="1" x:Name="bcAdvice"  Grid.ColumnSpan="3" HorizontalAlignment="Stretch"  VerticalAlignment="Center" BorderBrush="Black" Foreground="Black"  CodeType="Code39x"/>
                                                    <!--<Image        Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="3" HorizontalAlignment="Stretch" Stretch="Fill"  Source="/LGC.GMES.MES.PACK001;component/Images/bar1.png" Margin="20,0,9,0" />-->
                                                    <!--<Image Source="/LGC.GMES.MES.CMM001;component/Images/icon_input_search.png" Width="18" Height="18" />-->


                                                </Grid>

                                                <!--Dock / Gate -->
                                                <Grid x:Name="docGate" Grid.Row="0" Grid.Column="1">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="6" />
                                                        <RowDefinition Height="30" />
                                                        <RowDefinition Height="1*" />
                                                        <RowDefinition Height="6" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="10" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="10" />
                                                        <ColumnDefinition Width="*" />
                                                        <ColumnDefinition Width="*" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock    Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Dock / Gate',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                                    <TextBox      Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="4" x:Name="txtDock" Style="{StaticResource SearchCondition_TextBoxStyle}"  Height="Auto" Width="Auto" FontSize="30" FontStretch="Normal" FontWeight="Bold" MinWidth="80"
                                                                  TextAlignment="Center" VerticalContentAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center"  HorizontalAlignment="Center" >
                                                        <TextBox.BorderBrush>
                                                            <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ActiveCaptionTextColorKey}}"/>
                                                        </TextBox.BorderBrush>
                                                    </TextBox>
                                                </Grid>

                                                <!--두줄로 다시 나눔(Supplier address / Net.weight (Kg)-->
                                                <Grid x:Name="content_reRowSeperate" Grid.Row="1" Grid.Column="1" >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="1*" />
                                                        <RowDefinition Height="1*" />
                                                    </Grid.RowDefinitions>

                                                    <!--외곽선-->
                                                    <Border Grid.Row="0" BorderBrush="Black" BorderThickness="1" />
                                                    <Border Grid.Row="1" BorderBrush="Black" BorderThickness="1" />

                                                    <!--Supplier address-->
                                                    <Grid x:Name="supplier" Grid.Row="0" >
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="0" />
                                                            <RowDefinition Height="15" />
                                                            <RowDefinition Height="*" />
                                                            <RowDefinition Height="0" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="10" />
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="10" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock    Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Supplier address',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                                        <TextBox      Grid.Row="2" Grid.Column="1" x:Name="txtSupplierAddress" TextAlignment="Left" VerticalAlignment="Stretch" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  Height="Auto"  BorderBrush="Black" BorderThickness="1" IsReadOnly="True"/>

                                                    </Grid>

                                                    <!--3col으로 다시 분리 ( Net.weight (Kg) / Gross weight (Kg) / No. of Boxes-->
                                                    <Grid x:Name="netGrossBoxes_seperate" Grid.Row="1" >
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="1*" />
                                                        </Grid.ColumnDefinitions>

                                                        <!--외곽선-->
                                                        <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1" />
                                                        <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1" />
                                                        <Border Grid.Column="2" BorderBrush="Black" BorderThickness="1" />

                                                        <!-- Net.weight (Kg) -->
                                                        <Grid x:Name="netWeight" Grid.Column="0" >
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="0" />
                                                                <RowDefinition Height="15" />
                                                                <RowDefinition Height="3*" />
                                                                <RowDefinition Height="0" />
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="10" />
                                                                <ColumnDefinition Width="1*" />
                                                                <ColumnDefinition Width="10" />
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock    Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Text="{Binding ConverterParameter='Net.weight (Kg)',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center" FontSize="10"/>
                                                            <TextBox      Grid.Row="2" Grid.Column="1" x:Name="txtNetWeight" TextAlignment="Left" VerticalAlignment="Stretch" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  Height="Auto" BorderBrush="Black" BorderThickness="1" IsReadOnly="True"/>

                                                        </Grid>

                                                        <!-- Gross weight (Kg) -->
                                                        <Grid x:Name="grossWeight" Grid.Column="1" >
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="0" />
                                                                <RowDefinition Height="15" />
                                                                <RowDefinition Height="3*" />
                                                                <RowDefinition Height="0" />
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="10" />
                                                                <ColumnDefinition Width="1*" />
                                                                <ColumnDefinition Width="10" />
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock    Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Text="{Binding ConverterParameter='Gross weight (Kg)',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center" FontSize="10" />
                                                            <TextBox      Grid.Row="2" Grid.Column="1" x:Name="txtGrossWeight" TextAlignment="Left" VerticalAlignment="Stretch" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  Height="Auto"  BorderBrush="Black" BorderThickness="1" IsReadOnly="True"/>

                                                        </Grid>

                                                        <!-- No. of Boxes -->
                                                        <Grid x:Name="noOfBoxes" Grid.Column="2" >
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="0" />
                                                                <RowDefinition Height="15" />
                                                                <RowDefinition Height="*" />
                                                                <RowDefinition Height="0" />
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="10" />
                                                                <ColumnDefinition Width="1*" />
                                                                <ColumnDefinition Width="10" />
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock    Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Text="{Binding ConverterParameter='No. of Boxes',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center" FontSize="10"/>
                                                            <TextBox      Grid.Row="2" Grid.Column="1" x:Name="txtBoxes" TextAlignment="Left" VerticalAlignment="Stretch" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  Height="Auto" BorderBrush="Black" BorderThickness="1" IsReadOnly="True"/>

                                                        </Grid>

                                                    </Grid>
                                                </Grid>

                                            </Grid>

                                            <!-- 왼쪽 바코드 영역 - 2번째 row-->
                                            <Grid x:Name="content_left_2row"  Grid.Row="1">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="1*" />
                                                    <ColumnDefinition Width="1*" />
                                                </Grid.ColumnDefinitions>

                                                <!--Part Number (P)-->
                                                <Grid x:Name="partNumber" Grid.Column="0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="0" />
                                                        <RowDefinition Height="30" />
                                                        <RowDefinition Height="3*" />
                                                        <RowDefinition Height="6" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="10" />
                                                        <ColumnDefinition Width="140" />
                                                        <ColumnDefinition Width="10" />
                                                        <ColumnDefinition Width="1*" />
                                                        <ColumnDefinition Width="10" />
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock    Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Part Number (P)',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                                    <TextBox      Grid.Row="1" Grid.Column="3" x:Name="txtpartNumber" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch" BorderBrush="Black" BorderThickness="1" TextChanged="txtpartNumber_TextChanged" />
                                                    <c1:C1BarCode Grid.Row="2" Grid.Column="1" x:Name="bcpartNumber"  Grid.ColumnSpan="3" HorizontalAlignment="Stretch"  VerticalAlignment="Center" BorderBrush="Black" Foreground="Black" CodeType="Code39x" />
                                                    <!--<Image        Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="3" HorizontalAlignment="Stretch" Stretch="Fill" Source="/LGC.GMES.MES.PACK001;component/Images/bar2.png" Margin="20,0,9,0"  />-->
                                                </Grid>
                                            </Grid>

                                            <!-- 왼쪽 바코드 영역 - 3번째 row-->
                                            <Grid x:Name="content_left_3row"  Grid.Row="2" >
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="1*" />
                                                    <ColumnDefinition Width="1*" />
                                                </Grid.ColumnDefinitions>

                                                <!--외곽선-->
                                                <Border Grid.Column="0" BorderBrush="Black" BorderThickness="1" />
                                                <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1" />

                                                <!--왼쪽 바코드 영역 - 3번째 row -왼쪽 col-->
                                                <Grid x:Name="content_left_3row_1col" Grid.Column="0" >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="1*" />
                                                        <RowDefinition Height="1*" />
                                                        <RowDefinition Height="1*" />
                                                    </Grid.RowDefinitions>

                                                    <!--외곽선-->
                                                    <Border Grid.Row="0" BorderBrush="Black" BorderThickness="1" />
                                                    <Border Grid.Row="1" BorderBrush="Black" BorderThickness="1" />
                                                    <Border Grid.Row="2" BorderBrush="Black" BorderThickness="1" />

                                                    <!-- Quantity(Q) -->
                                                    <Grid x:Name="quantity" Grid.Row="0" >
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="0" />
                                                            <RowDefinition Height="30" />
                                                            <RowDefinition Height="*" />
                                                            <RowDefinition Height="6" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="10" />
                                                            <ColumnDefinition Width="140" />
                                                            <ColumnDefinition Width="10" />
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="10" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock    Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Quantity(Q)',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                                        <TextBox      Grid.Row="1" Grid.Column="3" x:Name="txtquantity" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch" Height="Auto"  BorderBrush="Black" BorderThickness="1" IsReadOnly="True" TextChanged="txtquantity_TextChanged"/>
                                                        <c1:C1BarCode Grid.Row="2" Grid.Column="1" x:Name="bcquantity"  Grid.ColumnSpan="3" HorizontalAlignment="Stretch"  VerticalAlignment="Center" BorderBrush="Black" Foreground="Black"  CodeType="Code39x"/>
                                                        <!--<Image        Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="3" HorizontalAlignment="Stretch" Stretch="Fill" Source="/LGC.GMES.MES.PACK001;component/Images/bar3.png" Height="Auto" Margin="20,0,9,0"/>-->

                                                    </Grid>

                                                    <!--Supplier ID (V) -->
                                                    <Grid x:Name="supplier_id" Grid.Row="1" >
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="6" />
                                                            <RowDefinition Height="24" />
                                                            <RowDefinition Height="3*" />
                                                            <RowDefinition Height="6" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="10" />
                                                            <ColumnDefinition Width="140" />
                                                            <ColumnDefinition Width="10" />
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="10" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Supplier ID (V)',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" />
                                                        <TextBox      Grid.Row="1" Grid.Column="3" x:Name="txtSupplierID" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch" Height="Auto" BorderBrush="Black" BorderThickness="1" TextChanged="txtSupplierID_TextChanged"/>
                                                        <c1:C1BarCode Grid.Row="2" Grid.Column="1" x:Name="bcSupplierID"  Grid.ColumnSpan="3" HorizontalAlignment="Stretch"  VerticalAlignment="Center" BorderBrush="Black" Foreground="Black" CodeType="Code39x" />
                                                        <!--<Image        Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="3" HorizontalAlignment="Stretch" Stretch="Fill"  Source="/LGC.GMES.MES.PACK001;component/Images/bar4.png" Height="Auto" Margin="20,0,9,0"/>-->

                                                    </Grid>

                                                    <!--Serial Number (S) -->
                                                    <Grid x:Name="serial" Grid.Row="2" >
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="0" />
                                                            <RowDefinition Height="30" />
                                                            <RowDefinition Height="3*" />
                                                            <RowDefinition Height="6" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="10" />
                                                            <ColumnDefinition Width="140" />
                                                            <ColumnDefinition Width="10" />
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="10" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock    Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Serial Number (S)',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                                        <TextBox      Grid.Row="1" Grid.Column="3" x:Name="txtSerial" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch" Height="Auto" BorderBrush="Black" BorderThickness="1" TextChanged="txtSerial_TextChanged"/>
                                                        <c1:C1BarCode Grid.Row="2" Grid.Column="1" x:Name="bcSerial"  Grid.ColumnSpan="3" HorizontalAlignment="Stretch"  VerticalAlignment="Center" BorderBrush="Black" Foreground="Black" CodeType="Code39x" />
                                                        <!--<Image        Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="3" HorizontalAlignment="Stretch" Stretch="Fill" Source="/LGC.GMES.MES.PACK001;component/Images/bar5.png" Margin="20,0,9,0" />-->

                                                    </Grid>

                                                </Grid>

                                                <!--왼쪽 바코드 영역 - 3번째 row -오른쪽 col-->
                                                <Grid x:Name="content_left_3row_2col" Grid.Column="1" >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="1.2*" />
                                                        <RowDefinition Height="1.7*" />
                                                        <RowDefinition Height="1.2*" />
                                                        <RowDefinition Height="1.4*" />
                                                    </Grid.RowDefinitions>

                                                    <!--외곽선-->
                                                    <Border Grid.Row="0" BorderBrush="Black" BorderThickness="1" />
                                                    <Border Grid.Row="1" BorderBrush="Black" BorderThickness="1" />
                                                    <Border Grid.Row="2" BorderBrush="Black" BorderThickness="1" />
                                                    <Border Grid.Row="3" BorderBrush="Black" BorderThickness="1" />

                                                    <!-- Description -->
                                                    <Grid x:Name="discription" Grid.Row="0" >
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="0" />
                                                            <RowDefinition Height="15" />
                                                            <RowDefinition Height="3*" />
                                                            <RowDefinition Height="0" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="10" />
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="10" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock    Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Description',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                                        <TextBox      Grid.Row="2" Grid.Column="1" x:Name="txtDescription1" TextAlignment="Left" VerticalAlignment="Stretch" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  Height="Auto" BorderBrush="Black" BorderThickness="1" IsReadOnly="True" />
                                                        <TextBox      Grid.Row="2" Grid.Column="2" x:Name="txtDescription2" TextAlignment="Left" VerticalAlignment="Stretch" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  Height="Auto" BorderBrush="Black" BorderThickness="1" IsReadOnly="True" />

                                                    </Grid>

                                                    <!--Logistic Reference -->
                                                    <Grid x:Name="reference" Grid.Row="1" >
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="0" />
                                                            <RowDefinition Height="15" />
                                                            <RowDefinition Height="3*" />
                                                            <RowDefinition Height="3*" />
                                                            <RowDefinition Height="0" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="10" />
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="10" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock    Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Logistic Reference',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                                                        <TextBox      Grid.Row="2" Grid.Column="1" x:Name="txtLogistic1" TextAlignment="Left" VerticalAlignment="Stretch" Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  Height="Auto" BorderBrush="Black" BorderThickness="1" Margin="3,1,3,1"/>
