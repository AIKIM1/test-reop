<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.PACK001.PACK003_045"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" d:DesignHeight="768" d:DesignWidth="1024" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.PACK001;component/Style/ControlStyles_PACK.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="Cell Pallet 반송 명령 생성 현황">
            <c1:C1TabControl x:Name="tabControlMain" >
                <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='현황 조회'}" >
                    <Grid x:Name="CellRequestList">
                        <Grid Grid.Row="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="51"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Grid x:Name="SearchArea" Grid.Row="0">
                                <Border Grid.Row="1" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                                <Grid   Grid.Row="1" Grid.Column="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="2.3*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="13"/>
                                    </Grid.ColumnDefinitions>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="10"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="10"/>
                                    </Grid.RowDefinitions>

                                    <!--조회조건-->
                                    <Grid Grid.Row="1" Grid.Column="1" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock              Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='일자', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <Grid Grid.Column="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>
                                            <ctrlLib:LGCDatePicker  Grid.Column="0" x:Name="dtpDateFrom" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center"/>
                                            <TextBlock              Grid.Column="1" Text="{Binding ConverterParameter='~', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                            <ctrlLib:LGCDatePicker  Grid.Column="2" x:Name="dtpDateTo" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center"/>
                                        </Grid>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock              Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='요청동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <ctrlLib:MultiSelectionBox  Grid.Row="1" Grid.Column="1" x:Name="mboReqArea1" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Margin="2,0,-2,0" SelectionChanged="mboReqArea1_SelectionChanged" />
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock                  Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='응답동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <ctrlLib:MultiSelectionBox  Grid.Row="1" Grid.Column="1" x:Name="mboSplyArea1" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Margin="2,0,-2,0" SelectionChanged="mboSplyArea1_SelectionChanged" />
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="7">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock                  Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='Cell 제품 ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <ctrlLib:MultiSelectionBox  Grid.Row="1" Grid.Column="1" x:Name="mboCellProdId1" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Margin="2,0,-2,0"/>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="9">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>

                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="9" HorizontalAlignment="Right">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="8"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <Button  Grid.Column="1" x:Name="btnCancelRequest"   Content="{Binding ConverterParameter=요청취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}"  Click="btnCancelRequest_Click" Cursor="Hand"/>
                                        <Button  Grid.Column="3" x:Name="btnChangeQty"       Content="{Binding ConverterParameter=수량변경,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}"  Click="btnChangeQty_Click"     Cursor="Hand"/>
                                        <Button  Grid.Column="5" x:Name="btnSearch"          Content="{Binding ConverterParameter=SEARCH,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource SearchButtonStyle}"               Click="btnSearch_Click"        Cursor="Hand"/>
                                    </Grid>

                                </Grid>
                            </Grid>

                            <Grid x:Name="GridMain" Grid.Row="5">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="0.*"/>
                                    <RowDefinition Height="0"/>
                                </Grid.RowDefinitions>


                                <Grid Grid.Row="1" Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>

                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Grid.ColumnSpan="5" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <TextBlock x:Name="tbWipListCount" Grid.Column="0" Text="[ 0 건 ]" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10,0,0,8"/>
                                    <!--<Button  Grid.Column="4" x:Name="btnExcel"  Content="{Binding ConverterParameter=EXCEL,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelExportStyle}" Margin="0,0,10,0" />-->
                                </Grid>

                                <!--조회 결과 부분-->
                                <Grid x:Name="resultTitle" Grid.Column="2" Grid.Row="0" Grid.ColumnSpan="2">
                                    <Border Grid.Row="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회 결과', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                </Grid>

                                <Grid x:Name="GridRequstRslt" Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="8"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="8"/>
                                    </Grid.RowDefinitions>

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="8" />
                                    </Grid.ColumnDefinitions>


                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <c1:C1DataGrid x:Name="dgCellReq" Grid.Row="1" Grid.Column="1" HeadersVisibility="None" IsReadOnly="True" CanUserFilter="False" CanUserReorderColumns="False" 
                                                   SelectionMode="None"  ColumnHeaderHeight="40" MouseOverBrush="Transparent" HorizontalScrollBarVisibility="Visible"
                                                   VerticalScrollBarVisibility="Visible"  FontSize="11" ClipboardPasteMode="None" FrozenTopRowsCount="1" >
                                        <c1:C1DataGrid.TopRows>
                                            <c1:DataGridColumnHeaderRow />
                                        </c1:C1DataGrid.TopRows>

                                        <c1:C1DataGrid.Columns >
                                            <c1:DataGridTextColumn     Binding = "{Binding CELL_SPLY_REQ_ID}"  HorizontalAlignment="Center"    MaxWidth="0"   HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI"/>
                                            <c1:DataGridTemplateColumn x:Name="rdoChk" Width="35" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI">
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <RadioButton IsChecked="{Binding CHK, Mode=OneWay}" Name="chk" HorizontalAlignment="Center" Checked="rdoChk_Checked" Cursor="Hand"/>
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>
                                            <c1:DataGridTextColumn       Header = "[공급요청ID]"     Binding = "{Binding CELL_SPLY_REQ_ID}"            HorizontalAlignment="Center"    Width="Auto"   HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn       Header = "[요청상태]"       Binding = "{Binding CELL_SPLY_STAT_CODE}"         HorizontalAlignment="Center"    Width="Auto"   HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn       Header = "[CELL PJT]"      Binding = "{Binding CELL_A_PRJT}"                 HorizontalAlignment="Center"    Width="Auto"   HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn       Header = "[PRODID]"        Binding = "{Binding PRODID}"                      HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn       Header = "[PRODID]"        Binding = "{Binding ASSYP_PRODID}"                HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn       Header = "[PRODID]"        Binding = "{Binding PACKP_MODLID}"                HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn       Header = "[요청동]"         Binding = "{Binding PACK_AREAID}"                 HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridNumericColumn    Header = "[요청수량]"       Binding = "{Binding REQ_QTY}"                     HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" Format="#,###0"/>
                                            <c1:DataGridTextColumn       Header = "[반송요청유형]"    Binding = "{Binding AUTO_FLAG}"                   HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[요청자]"         Binding = "{Binding REQ_USER}"                    HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn       Header = "[요청일시]"       Binding = "{Binding CELL_SPLY_REQ_DTTM}"          HorizontalAlignment="Center"     Width="*"     HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn       Header = "[요청비고]"       Binding = "{Binding PACK_NOTE}"                   HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="VERTICALHIERARCHI" />
                                            <c1:DataGridTextColumn       Header = "[RSPN ID]"       Binding = "{Binding CELL_SPLY_RSPN_ID}"           HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[공급상태]"       Binding = "{Binding CELL_SPLY_RSPN_STAT_CODE}"    HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[공급동]"         Binding = "{Binding ASSY_AREAID}"                 HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridNumericColumn    Header = "[응답수량]"       Binding = "{Binding CELL_SPLY_RSPN_QTY}"          HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" Format="#,###0"/>
                                            <c1:DataGridTextColumn       Header = "[공급자]"         Binding = "{Binding RSPN_USER}"                   HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[공급일시]"       Binding = "{Binding CELL_SPLY_RSPN_DTTM}"         HorizontalAlignment="Center"     Width="*"     HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[공급비고]"       Binding = "{Binding ASSY_NOTE}"                   HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[경과시간]"       Binding = "{Binding ELAPSED_TIME}"                HorizontalAlignment="Center"     Width="*"     HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                </Grid>

                                <!--조회 결과 부분-->
                                <Grid x:Name="shipRcvTitle" Grid.Column="2" Grid.Row="3" Grid.ColumnSpan="2">
                                    <Border Grid.Row="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                    <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회 결과', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                </Grid>

                                <Grid x:Name="GridShipRcv" Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="8"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="8"/>
                                    </Grid.RowDefinitions>

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="8" />
                                    </Grid.ColumnDefinitions>


                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                    <c1:C1DataGrid x:Name="dgShipRcv" Grid.Row="1" Grid.Column="1" HeadersVisibility="None" IsReadOnly="True" CanUserFilter="False" CanUserReorderColumns="False" 
                                                   SelectionMode="None"  ColumnHeaderHeight="40" MouseOverBrush="Transparent" HorizontalScrollBarVisibility="Visible"
                                                   VerticalScrollBarVisibility="Visible"  FontSize="11" ClipboardPasteMode="None" FrozenTopRowsCount="1" >

                                        <c1:C1DataGrid.TopRows>
                                            <c1:DataGridColumnHeaderRow />
                                            <c1:DataGridColumnHeaderRow />
                                        </c1:C1DataGrid.TopRows>

                                        <c1:C1DataGrid.Columns >
                                            <c1:DataGridTextColumn       Header = "[요청,공급요청ID]"       Binding = "{Binding CELL_SPLY_REQ_ID}"            HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridNumericColumn    Header = "[요청,요청수량]"         Binding = "{Binding REQ_QTY}"                     HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" Format="#,###0"/>
                                            <c1:DataGridTextColumn       Header = "[공급,RSPN ID]"          Binding = "{Binding CELL_SPLY_RSPN_ID}"           HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridNumericColumn    Header = "[공급,공급수량]"         Binding = "{Binding CELL_SPLY_RSPN_QTY}"          HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" Format="#,###0"/>
                                            <c1:DataGridTextColumn       Header = "[출고,출고동ID]"         Binding = "{Binding CELL_SPLY_RSPN_AREAID}"       HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[출고,출고일시]"         Binding = "{Binding ISS_DTTM}"                    HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[출고,출고ID]"           Binding = "{Binding SHIP_RCV_ISS_ID}"             HorizontalAlignment="Center"     Width="*"     HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[출고,출고상태]"         Binding = "{Binding RCV_ISS_TYPE_CODE}"           HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridNumericColumn    Header = "[출고,출고수량]"         Binding = "{Binding ISS_QTY}"                     HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" Format="#,###0"/>
                                            <c1:DataGridTextColumn       Header = "[출고,출고자]"           Binding = "{Binding INSUSER}"                     HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[출고,경과시간]"         Binding = "{Binding SPLY_SHIP_ELAPSED_TIME}"      HorizontalAlignment="Center"     Width="*"     HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[입고,PACK동]"           Binding = "{Binding AREAID}"                      HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[입고,입고일시]"         Binding = "{Binding RCV_DTTM}"                    HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[입고,입고ID]"           Binding = "{Binding RCV_RCV_ISS_ID}"              HorizontalAlignment="Center"     Width="*"     HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[입고,입고상태]"         Binding = "{Binding RCV_ISS_STAT_CODE}"           HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridNumericColumn    Header = "[입고,입고수량]"         Binding = "{Binding RCV_QTY}"                     HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" Format="#,###0"/>
                                            <c1:DataGridTextColumn       Header = "[입고,입고자]"           Binding = "{Binding ACT_USERID}"                  HorizontalAlignment="Center"     Width="Auto"  HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                            <c1:DataGridTextColumn       Header = "[입고,경과시간]"         Binding = "{Binding SHIP_RCV_ELAPSED_TIME}"       HorizontalAlignment="Center"     Width="*"     HeaderStyle="{StaticResource CenterGridHeaderStyle_14}" ctrlLib:DataGridMergeExtension.MergeMode="NONE" />
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                </Grid>
                            </Grid>
                        </Grid>
                    </Grid>
                </c1:C1TabItem>
                <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='이력 조회'}" Margin="0" >
                    <Grid x:Name="CellRespondList">
                        <Grid Grid.Row="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="51"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Grid x:Name="SearchArea2" Grid.Row="0">
                                <Border Grid.Row="1" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                                <Grid   Grid.Row="1" Grid.Column="0" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="2.3*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="13"/>
                                    </Grid.ColumnDefinitions>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="10"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="10"/>
                                    </Grid.RowDefinitions>

                                    <!--조회조건-->
                                    <Grid Grid.Row="1" Grid.Column="1" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock              Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='일자', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                        <Grid Grid.Column="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>
                                            <ctrlLib:LGCDatePicker  Grid.Column="0" x:Name="dtpDateFromHis" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center"/>
                                            <TextBlock              Grid.Column="1" Text="{Binding ConverterParameter='~', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                            <ctrlLib:LGCDatePicker  Grid.Column="2" x:Name="dtpDateToHis" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center"/>
                                        </Grid>
                                    </Grid>

                                    <Grid Grid.Row="1" Grid.Column="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4*"/>
                                            <ColumnDefinition Width="6*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock                  Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='요청동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
