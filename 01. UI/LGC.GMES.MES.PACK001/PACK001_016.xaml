<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.PACK001.PACK001_016" 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/C1DataGridStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="공정진척(Pack 포장)_수동 Pack 포장">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="8" />
                    <ColumnDefinition Width="1*" />
                </Grid.ColumnDefinitions>

                <!--백그라운드용-->
                <Border   Grid.Column="0" Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                <Border   Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}" />


                <!--<Border    Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>-->
                <!--<Border Grid.Row="4" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />-->

                <!--왼쪽영역-->
                <Grid Grid.Column="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="34" />
                        <RowDefinition Height="8" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="8" />
                        <RowDefinition Height="70" />
                        <RowDefinition Height="8" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="8" />
                        <RowDefinition Height="40" />
                        <RowDefinition Height="8" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="8" />
                    </Grid.RowDefinitions>

                    <!--백그라운드용-->
                    <Border Grid.Row="0"  BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                    <!--왼쪽 타이틀(작업자입력)-->
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <TextBlock  Grid.Column="1" Style="{StaticResource Content_ModuleTitleStyle}" ><Run Text="{Binding ConverterParameter='작업자입력', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" /></TextBlock>

                        <!--LOT정보(TEXTBLOCK)-->
                        <TextBlock    Grid.Column="3" x:Name="tbLotInform" Text="{Binding ConverterParameter='SCAN ID정보 : ',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />

                        <!--포장 제품 : CLASS(TEXTBLOCK)-->
                        <TextBlock    Grid.Column="4" x:Name="txtBoxInfo" Text="" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>

                    </Grid>

                    <!--왼쪽 PLLET ID 자동생성 영역-->
                    <Grid Grid.Row="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8" />
                            <RowDefinition Height="1*" />
                            <RowDefinition Height="1*" />
                            <RowDefinition Height="8" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <!--백그라운드용-->
                        <Border Grid.Row="0" Grid.RowSpan="4" Grid.Column="1"   Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                        <!--Pallete id checkbox-->
                        <Grid Grid.Row="1" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="145" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="4" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>

                            <!--PALLETID 자동생성(TEXTBLOCK)-->
                            <CheckBox    Grid.Column="1"  x:Name="chkPalletId"   Content="{Binding ConverterParameter=PALLETID 자동생성,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  HorizontalAlignment="Left" IsChecked="True" IsEnabled="False" Click="chkPalletId_Click" />

                            <!--고객사 Palletid(TextBlock)-->
                            <TextBlock    Grid.Column="3" x:Name="tbConfPalletId" Text="GTLID" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" FontWeight="Bold" Visibility="Hidden" />

                            <!--고객사 Palletid(TEXTBOX)-->
                            <TextBox      Grid.Column="4" x:Name="txtConfPalletId" Text=""    Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  BorderBrush="Black" BorderThickness="1" Visibility="Hidden"/>

                            <!--저장(BUTTON)-->
                            <Button       Grid.Column="5" x:Name="btnSave"    Content="{Binding ConverterParameter=저장,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                 Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" HorizontalAlignment="Center" Click="btnSave_Click" Visibility="Hidden" />

                            <!--작업초기화(BUTTON)-->
                            <Button  Grid.Column="7" x:Name="btnReset"    Content="{Binding ConverterParameter=작업초기화,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                 Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" HorizontalAlignment="Right" Click="btnReset_Click"  />
                        </Grid>

                        <!--PALLET ID TEXT-->
                        <Grid Grid.Row="2"  Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>

                            <!--PALLETID(TEXTBLOCK)-->
                            <TextBlock    Grid.Column="1" Text="{Binding ConverterParameter='PALLETID',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>

                            <!--PALLETID 입력(TEXTBOX)-->
                            <TextBox      Grid.Column="2" x:Name="txtPalletId" Text=""    Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  BorderBrush="Black" BorderThickness="1" KeyDown="txtPalletId_KeyDown" PreviewKeyDown="txtPalletId_PreviewKeyDown" IsEnabled="False">
                                <TextBox.ContextMenu>
                                    <ContextMenu>
                                        <MenuItem Header="{Binding ConverterParameter='COPY',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Click="MenuItem_PltId_Copy_Click"/>
                                        <MenuItem Header="{Binding ConverterParameter='CUT',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Click="MenuItem_PltId_Cut_Click" />
                                    </ContextMenu>
                                </TextBox.ContextMenu>
                            </TextBox>
                        </Grid>
                    </Grid>

                    <!--왼쪽 작업구분 영역-->
                    <Grid Grid.Row="4">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*" />
                            <RowDefinition Height="1*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <!--백그라운드용-->
                        <Border Grid.Row="0" Grid.RowSpan="2" Grid.Column="1"   Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                        <!--작업구분-->
                        <Grid Grid.Row="0"  Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>

                            <!--작업구분(TEXTBLOCK)-->
                            <TextBlock       Grid.Column="1" Text="{Binding ConverterParameter='작업구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                            <!--AREA(COMBO)-->
                            <c1:C1ComboBox   Grid.Column="2" x:Name="cboGubun" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" HorizontalAlignment="Left" DisplayMemberPath="KEY" SelectedValuePath="VALUE" SelectedIndex="1" SelectedValueChanged="cboGubun_SelectedValueChanged"/>

                            <!--포장수량(TEXTBLOCK)-->
                            <TextBlock       Grid.Column="3" Text="{Binding ConverterParameter='포장수량',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>

                            <!--포장수량(NUMERICBOX)-->
                            <c1:C1NumericBox Grid.Column="4" Value="1" x:Name="nbBoxingCnt"   Style="{StaticResource C1NumericBoxStyle}" Height="Auto" VerticalAlignment="Center" Minimum="1" Maximum="9999" ValueChanged="nbBoxingCnt_ValueChanged"  />
                        </Grid>

                        <!--BOX ID-->
                        <Grid Grid.Row="1"  Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>

                            <!--BOXID(TEXTBLOCK)-->
                            <TextBlock    Grid.Column="1" x:Name="tbBoxLotID" Text="{Binding ConverterParameter='Box ID',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>

                            <!--BOXID 입력(TEXTBOX)-->
                            <TextBox      Grid.Column="2" x:Name="txtBoxLOTID" Text=""    Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  BorderBrush="Black" BorderThickness="1" KeyDown="txtBoxLotID_KeyDown" PreviewKeyDown="txtBoxLotID_PreviewKeyDown" >
                                <TextBox.ContextMenu>
                                    <ContextMenu>
                                        <MenuItem Header="{Binding ConverterParameter='COPY',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Click="MenuItem_Copy_Click"/>
                                        <MenuItem Header="{Binding ConverterParameter='CUT',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Click="MenuItem_Cut_Click" />
                                    </ContextMenu>
                                </TextBox.ContextMenu>
                            </TextBox>

                            <!--남은 수량(TextBlock)-->
                            <TextBlock    Grid.Column="4" x:Name="tbCount" Text="1" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" Foreground="Red" FontWeight="Bold"  />

                        </Grid>
                    </Grid>

                    <!--포장정보 영역-->
                    <Grid Grid.Row="6">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <!--백그라운드용-->
                        <Border Grid.Row="0" Grid.RowSpan="2" Grid.Column="1"   Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                        <!--포장제품-->
                        <Grid Grid.Row="0" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>

                            <!--포장제품-->
                            <TextBlock Grid.Column="1"  Text="{Binding ConverterParameter='포장제품', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  Style="{StaticResource Content_InputForm_LabelStyle}" 
                                       HorizontalAlignment="Right"/>
                            <!--포장제품(TEXTBOX)-->
                            <TextBox   Grid.Column="2" x:Name="txtBoxingProd"     Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" />
                            <TextBox   Grid.Column="99" x:Name="txtLotProd"     Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" Visibility="Collapsed" />

                            <!--공정군-->
                            <TextBlock Grid.Column="3"  Text="{Binding ConverterParameter='공정군', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  Style="{StaticResource Content_InputForm_LabelStyle}" 
                                       HorizontalAlignment="Right"/>
                            <!--공정군(TEXTBOX)-->
                            <TextBox   Grid.Column="4" x:Name="txtBoxingPcsg"     Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" />
                            <TextBox   Grid.Column="100" x:Name="txtLotingPcsg"     Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" Visibility="Collapsed"/>


                            <!--포장모델-->
                            <TextBlock Grid.Column="5"  Text="{Binding ConverterParameter='포장모델', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  Style="{StaticResource Content_InputForm_LabelStyle}" 
                                       HorizontalAlignment="Right"/>
                            <!--포장모델(TEXTBOX)-->
                            <TextBox   Grid.Column="6" x:Name="txtBoxingModel"     Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" />

                        </Grid>

                        <!--포장오더-->
                        <Grid Grid.Row="1" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>

                            <!--포장오더-->
                            <TextBlock Grid.Column="1"  Text="{Binding ConverterParameter='WORKORDER', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>

                            <!--포장오더(TEXTBOX)-->
                            <TextBox   Grid.Column="2" x:Name="txtReworkWOID"     Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" />

                            <!--오더조회-->
                            <Button    Grid.Column="3" x:Name="btnWorkOroderSearch" Content="{Binding ConverterParameter=조회,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"     
                                       Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnWorkOroderSearch_Click" IsEnabled="False" Visibility="Hidden"/>
                        </Grid>

                        <Grid Grid.Row="2" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="90" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />

                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*" />

                                <ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>

                            <!-- 라인 -->
                            <TextBlock Grid.Column="1"  Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  Style="{StaticResource Content_InputForm_LabelStyle}"  HorizontalAlignment="Right"/>

                            <!--라인(TEXTBOX)-->
                            <TextBox   Grid.Column="2" x:Name="txtEqsgID"     Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" Visibility="Hidden" />
                            <TextBox   Grid.Column="3" x:Name="txtEqsgname"     Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" />

                            <!--제품군-->
                            <TextBlock Grid.Column="5"  Text="{Binding ConverterParameter='제품군', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>

                            <!--제품군(TEXTBOX)-->
                            <TextBox   Grid.Column="6" x:Name="txtProdClass"     Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" />
                        </Grid>
                    </Grid>

                    <!--왼쪽 버튼묶음 영역-->
                    <Grid Grid.Row="8">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="1" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <!--백그라운드용-->
                        <Border  Grid.Column="1" Grid.ColumnSpan="9"  Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                        <!--작업상태(TEXTBLOCK)-->
                        <TextBlock    Grid.Column="1" x:Name="txtcnt" Text="{Binding ConverterParameter=포장대기,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" />

                        <!--UNPACK(BUTTON)-->
                        <Button  Grid.Column="1" x:Name="btnUnPack"    Content="{Binding ConverterParameter=포장해제,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                 Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" HorizontalAlignment="Left" Click="btnUnPack_Click" Margin="8,0,0,0" Visibility="Hidden"/>

                        <!--포장단위: BOX/LOT (TEXTBLOCK)-->
                        <TextBlock       Grid.Column="2" x:Name="tbGubun" Text="{Binding ConverterParameter='포장단위(BOX/LOT)', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                         Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center" Foreground="Blue" FontWeight="Bold"/>

                        <!--선택취소(BUTTON)-->
                        <Button  Grid.Column="4" x:Name="btnSelectCacel"    Content="{Binding ConverterParameter=선택취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                 Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" HorizontalAlignment="Center" Click="btnSelectCacel_Click" />

                        <!--취소(BUTTON)-->
                        <Button  Grid.Column="6" x:Name="btncancel"    Content="{Binding ConverterParameter=취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                 Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" HorizontalAlignment="Center" Click="btncancel_Click" />

                        <!--PACK(BUTTON)-->
                        <Button  Grid.Column="8" x:Name="btnPack"    Content="{Binding ConverterParameter=포장,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                 Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" HorizontalAlignment="Center" Click="btnConfig_Click" />

                    </Grid>

                    <!--카운트 영역-->
                    <Grid Grid.Row="10" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock x:Name="tbBoxingWait_cnt" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,0,5"/>
                    </Grid>

                    <!--왼쪽 BOX ID 그리드 영역-->
                    <Grid Grid.Row="11">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="6" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="6" />
                        </Grid.ColumnDefinitions>

                        <c1:C1DataGrid x:Name="dgPalletBox" Grid.Column="1" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleCell" CanUserRemoveRows="True" CanUserAddRows="True" NewRowVisibility="Bottom" 
                                       MouseLeftButtonUp="dgPalletBox_MouseLeftButtonUp" MouseDoubleClick="dgPalletBox_MouseDoubleClick" Width="Auto" ClipboardPasteMode="None" >
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridCheckBoxColumn Header = "선택"          Binding = "{Binding CHK}"               HorizontalAlignment="Center"   Width="40"  />
                                <c1:DataGridTextColumn     Header = "PALLETID"      Binding = "{Binding PALLETID}"          HorizontalAlignment="Center"   IsReadOnly="True"  Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "BOXID"         Binding = "{Binding BOXID}"             HorizontalAlignment="Center"   IsReadOnly="True"  Width="Auto" />
                                <c1:DataGridTextColumn     Header = "BOX생성일시"   Binding = "{Binding PACKDTTM}"          HorizontalAlignment="Center"   IsReadOnly="True"  Width="Auto"/>
                                <!--<c1:DataGridTextColumn     Header = "검사ID"       Binding = "{Binding OQC_INSP_REQ_ID}"   HorizontalAlignment="Center" IsReadOnly="True"  Width="0"/>-->
                                <c1:DataGridTextColumn     Header = "검사결과"      Binding = "{Binding JUDG_NAME}"         HorizontalAlignment="Center"   IsReadOnly="True"  Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "전송W/O"       Binding = "{Binding WOID}"              HorizontalAlignment="Center"   IsReadOnly="True"  Width="Auto" />
                                <c1:DataGridTextColumn     Header = "ERP전송번호"   Binding = "{Binding ERP_TRNF_SEQNO}"    HorizontalAlignment="Center"   IsReadOnly="True"  Width="Auto" />
                                <c1:DataGridTextColumn     Header = "CUST_LOTID"   Binding = "{Binding CUST_LOTID}"    HorizontalAlignment="Center"   IsReadOnly="True"  Width="Auto" />
                                <c1:DataGridTextColumn     Header = "OCOP_RTN_FLAG"   Binding = "{Binding OCOP_RTN_FLAG}"    HorizontalAlignment="Center"   IsReadOnly="True"  Width="Auto" />                           
                            </c1:C1DataGrid.Columns>
                        </c1:C1DataGrid>
                    </Grid>
                </Grid>

                <GridSplitter Grid.Column="1" Grid.Row="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                <!--오른쪽영역-->
                <Grid Grid.Column="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="34" />
                        <RowDefinition Height="8" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="8" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="8" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="8" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="8" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="8" />
                    </Grid.ColumnDefinitions>

                    <!--백그라운드용-->
                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>


                    <!--백그라운드용-->
                    <Border Grid.Row="2" Grid.Column="1"   Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                    <Border Grid.Row="7" Grid.Column="1"   Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>


                    <!--오른쪽 타이틀-->
                    <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <!--이력조회(TEXBLOCK)-->
                        <TextBlock  Grid.Column="1" Style="{StaticResource Content_ModuleTitleStyle}" ><Run Text="{Binding ConverterParameter='이력조회', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" /></TextBlock>
                    </Grid>

                    <!--오른쪽 조회영역-->
                    <!--<Grid Grid.Row="2" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="30" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="8" />

                        </Grid.ColumnDefinitions>

                        -->
                    <!--포장일자(TEXTBLOCK)-->
                    <!--
                        <TextBlock              Grid.Column="0" Text="{Binding ConverterParameter='포장일', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                        -->
                    <!--FROMDATE(DATEPICKER)-->
                    <!--
                        <ctrlLib:LGCDatePicker  Grid.Column="1" x:Name="dtpDateFrom" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center"/>

                        -->
                    <!-- ~ (TEXTBLOCK)-->
                    <!--
                        <TextBlock              Grid.Column="2" Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center"/>

                        -->
                    <!--TODATE(DATEPICKER)-->
                    <!--
                        <ctrlLib:LGCDatePicker  Grid.Column="3" x:Name="dtpDateTo" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center"/>

                        -->
                    <!--PALLET포장일(CHECKBOX) 히든-->
                    <!--
                        <CheckBox               Grid.Column="4"  x:Name="chkPalletConfig"   Content="PALLET포장일"  HorizontalAlignment="Left" IsChecked="True" IsEnabled="True" Visibility="Hidden" />

                        -->
                    <!--구성날자(DATEPICKER)-->
                    <!--
                        <ctrlLib:LGCDatePicker  Grid.Column="5" x:Name="dtpPalletConfig" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center" IsReanOnly="True" Visibility="Hidden"/>

                        -->
                    <!--EXCEL(BUTTON)-->
                    <!--
                        <Button                 Grid.Column="7" x:Name="btnExcel"    Content="{Binding ConverterParameter=Export,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                Style="{DynamicResource Content_ModuleButtonExcelExportStyle}" HorizontalAlignment="Left" Click="btnExcel_Click" />
                        -->
                    <!--SEARCH(BUTTON)-->
                    <!--
                        <Button                 Grid.Column="9" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                Style="{StaticResource SearchButtonStyle}" Cursor="Hand" HorizontalAlignment="Right" Click="btnSearch_Click"/>

                    </Grid>-->
                    <Grid Grid.Row="2" Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="11" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="4" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="4" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="4" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="11" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="30" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="13" />
                        </Grid.ColumnDefinitions>

                        <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="7"  Grid.ColumnSpan="6" Style="{StaticResource Condition_BorderStyle}"></Border>

                        <!--조회영역 첫번째 row-->
                        <Grid Grid.Row="1" Grid.Column="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="2*" />
                            </Grid.ColumnDefinitions>
                            <!--AREA(TEXTBLOCK)-->
                            <TextBlock       Grid.Column="0" Text="{Binding ConverterParameter='AREA', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                            <!--AREA(COMBO)-->
                            <c1:C1ComboBox   Grid.Column="1" x:Name="cboArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="KEY" SelectedValuePath="VALUE" SelectedIndex="1"/>
                        </Grid>

                        <Grid Grid.Row="1" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="2*" />
                            </Grid.ColumnDefinitions>
                            <!--모델(TEXTBLOCK)-->
                            <TextBlock              Grid.Column="0" Text="{Binding ConverterParameter='모델', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                            <!--모델(COMBO)-->
                            <c1:C1ComboBox          Grid.Column="1" x:Name="cboProductModel" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="KEY" SelectedValuePath="VALUE" SelectedIndex="1"/>
                        </Grid>

                        <Grid Grid.Row="1" Grid.Column="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="2*" />
                            </Grid.ColumnDefinitions>
                            <!--제품(TEXTBLOCK)-->
                            <TextBlock       Grid.Column="0" Text="{Binding ConverterParameter='제품', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                            <!--제품(COMBO)-->
                            <c1:C1ComboBox   Grid.Column="1" x:Name="cboProduct" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="KEY" SelectedValuePath="VALUE" SelectedIndex="1"/>
                        </Grid>

                        <!--조회영역 두번째 row-->
                        <Grid Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="5*" />
                            </Grid.ColumnDefinitions>
                            <!--포장일자(TEXTBLOCK)-->
                            <TextBlock Grid.Column="0" Text="{Binding ConverterParameter='포장일', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                            <Grid Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="3" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="3" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="3" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="3" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                <ctrlLib:LGCDatePicker  Grid.Column="0" x:Name="dtpDateFrom" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center"/>
                                <c1:C1ComboBox Grid.Column="2" x:Name="cboTimeFrom" Height="21" MinWidth="40" Width="Auto" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                                <TextBlock              Grid.Column="4" Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center"/>
                                <ctrlLib:LGCDatePicker  Grid.Column="6" x:Name="dtpDateTo" DatepickerType="Date"  HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Center"/>
                                <c1:C1ComboBox Grid.Column="8" x:Name="cboTimeTo" Height="21" MinWidth="40" Width="Auto" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                            </Grid>
                        </Grid>

                        <!--조회 버튼-->
                        <Button    Grid.Row="5"   Grid.Column="4" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                            Style="{StaticResource SearchButtonStyle}" Cursor="Hand" HorizontalAlignment="Right" Click="btnSearch_Click"/>
                    </Grid>

                    <Grid Grid.Row="4" Grid.Column="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="150" />
                                <ColumnDefinition Width="8" />
                            </Grid.ColumnDefinitions>

                            <!--조회건수-->
                            <TextBlock x:Name="tbBoxHistory_cnt" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,0,5"/>

                            <!--BOXID(TEXTBLOCK)-->
                            <TextBlock    Grid.Column="3" Text="{Binding ConverterParameter='PALLET검색',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />

                            <!--BOXID입력(TEXTBOX)-->
                            <TextBox      Grid.Column="4" x:Name="txtSearchBox" Text=""  Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  BorderBrush="Black" BorderThickness="1" KeyDown="txtSearchBox_KeyDown" />
                        </Grid>
                    </Grid>

                    <!--결과 그리드-->
                    <Grid Grid.Row="5" Grid.Column="1" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="173*"/>
                            <ColumnDefinition Width="3*"/>
                            <ColumnDefinition Width="50*"/>
                        </Grid.ColumnDefinitions>
                        <c1:C1DataGrid x:Name="dgPallethistory" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="MultiRange" 
                                       MouseLeftButtonUp="dgPallethistory_MouseLeftButtonUp" LoadedCellPresenter="dgPallethistory_LoadedCellPresenter" MouseDoubleClick="dgPalletBox_MouseDoubleClick" Width="Auto" Grid.ColumnSpan="3" >
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTemplateColumn Header="재포장" Width="Auto" >
                                    <c1:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Button Content="{Binding ConverterParameter='적용',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  Click="Button_Click" Height="Auto" HorizontalAlignment="Stretch" Width="Auto"></Button>
                                        </DataTemplate>
                                    </c1:DataGridTemplateColumn.CellTemplate>
                                </c1:DataGridTemplateColumn>
                                <c1:DataGridTextColumn     Header = "CARRIERID" Binding = "{Binding CARRIERID}"           HorizontalAlignment="Left"   Width="Auto" Visibility="Collapsed" />
                                <c1:DataGridTextColumn     Header = "PALLETID"       Binding = "{Binding PALLETID}"           HorizontalAlignment="Left"   Width="Auto" />
                                <c1:DataGridTextColumn     Header = "PALLET구성일시" Binding = "{Binding PACKDTTM}"           HorizontalAlignment="Left"   Width="Auto" />
                                <!--<c1:DataGridTextColumn     Header = "포장가능수량" Binding = "{Binding BOXMAXCNT}"     HorizontalAlignment="Left"   Width="Auto" />-->
                                <c1:DataGridTextColumn     Header = "포장수량(BOX)"  Binding = "{Binding PALLETBOXCNT}"       HorizontalAlignment="Left"   Width="Auto"  />
                                <c1:DataGridTextColumn     Header = "포장수량(LOT)"  Binding = "{Binding TOTAL_QTY}"          HorizontalAlignment="Left"   Width="Auto"/>
                                <!--<c1:DataGridTextColumn     Header = "BOX ID "       Binding = "{Binding BOXID}"         HorizontalAlignment="Left"   Width="Auto" />
                                <c1:DataGridTextColumn     Header = "BOX 생성일시"  Binding = "{Binding PACKDTTM}"      HorizontalAlignment="Left"   Width="Auto" />-->
                                <c1:DataGridTextColumn     Header = "출고 ID"        Binding = "{Binding RCV_ISS_ID}"         HorizontalAlignment="Left"   Width="Auto" />
                                <c1:DataGridTextColumn     Header = "출하검사ID"     Binding = "{Binding OQC_INSP_REQ_ID}"    HorizontalAlignment="Left"   Width="Auto" />
                                <!--<c1:DataGridTextColumn     Header = "출하검사결과"   Binding = "{Binding OQC_INSP_RESULT}"    HorizontalAlignment="Left"   Width="Collapsed" />-->
                                <c1:DataGridTextColumn     Header = "출하검사결과"   Binding = "{Binding OQC_INSP_RESULT}"    HorizontalAlignment="Left"   Width="Auto" Visibility="Collapsed" />
                                <c1:DataGridTextColumn     Header = "출하검사결과"   Binding = "{Binding OQC_INSP_NAME}"      HorizontalAlignment="Left"   Width="Auto" />
                                <c1:DataGridTextColumn     Header = "QMS HOLD"       Binding = "{Binding QMS_HOLD_FLAG}"      HorizontalAlignment="Left"   Width="Auto" />
                                <c1:DataGridTextColumn     Header = "고객사PLTID"    Binding = "{Binding OUTER_BOXID2}"       HorizontalAlignment="Left"   Width="Auto" />
                                <c1:DataGridTextColumn     Header = "동"             Binding = "{Binding AREANAME}"           HorizontalAlignment="Left"   Width="Auto" />
                                <c1:DataGridTextColumn     Header = "라인"           Binding = "{Binding EQSGNAME}"             HorizontalAlignment="Left"   Width="Auto"/>

                                <c1:DataGridTextColumn     Header = "PRODID"         Binding = "{Binding PRODID}"             HorizontalAlignment="Left"   Visibility="Hidden" Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "PROCID"         Binding = "{Binding PROCID}"             HorizontalAlignment="Left"   Visibility="Hidden" Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "EQSGID"         Binding = "{Binding EQSGID}"             HorizontalAlignment="Left"   Visibility="Hidden" Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "EQPTID"         Binding = "{Binding EQPTID}"             HorizontalAlignment="Left"   Visibility="Hidden" Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "PRDCLASS"       Binding = "{Binding PRDCLASS}"           HorizontalAlignment="Left"   Visibility="Hidden" Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "고객사PALLETID" Binding = "{Binding OUTER_BOXID2}"       HorizontalAlignment="Left"   Visibility="Hidden" Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "PCSGID"         Binding = "{Binding PCSGID}"             HorizontalAlignment="Left"   Visibility="Hidden" Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "MODEL"          Binding = "{Binding MODEL}"              HorizontalAlignment="Left"   Visibility="Hidden" Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "PCSGID_C"         Binding = "{Binding PCSGID_C}"             HorizontalAlignment="Left"   Visibility="Hidden" Width="Auto"/>
                                <c1:DataGridTextColumn     Header = "PRODID_C"          Binding = "{Binding PRODID_C}"              HorizontalAlignment="Left"   Visibility="Hidden" Width="Auto"/>
                                <c1:DataGridTextColumn      Header = "QMS_FAIL_YN"  Binding = "{Binding QMS_FAIL_YN}"       HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                            </c1:C1DataGrid.Columns>
                        </c1:C1DataGrid>

                    </Grid>


                    <!--오른쪽 하단 영역-->
                    <Grid Grid.Row="7" Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="2" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="250" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="4" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="8" Style="{StaticResource Condition_Header_BorderStyle}"/>

                        <!--PALLETID(TEXTBLOCK)-->
                        <TextBlock  Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='PALLETID',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>

                        <!--PALLETID(TEXTBOX)-->
                        <TextBox    Grid.Row="1"  Grid.Column="2" x:Name="txtPalleyIdR" Text=""    Style="{StaticResource TextBoxBaseStyle}" HorizontalAlignment="Stretch"  BorderBrush="Black" BorderThickness="1" IsReadOnly="True"/>

                        <!--EXCELLOAD(BTTON)-->
                        <Button     Grid.Row="1"  Grid.Column="4" x:Name="btnExcelLoad"    Content="{Binding ConverterParameter=Excel Upload,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                 Style="{DynamicResource Content_ModuleButtonExcelImportStyle}" HorizontalAlignment="Right" Click="btnExcelLoad_Click" Visibility="Hidden"/>

                        <!--BOXLABEL(BUTTON)-->
                        <Button     Grid.Row="1"  Grid.Column="6" x:Name="btnPalletLabel"    Content="{Binding ConverterParameter=발행,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                     Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" HorizontalAlignment="Right" Click="btnPalletLabel_Click" />

                        <!--포장취소(BUTTON)-->
                        <Button     Grid.Row="1" Grid.Column="8" x:Name="btnConfigCancel"    Content="{Binding ConverterParameter=포장취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                     Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" HorizontalAlignment="Right" Click="btnConfigCancel_Click" Margin="0,6" />

                    </Grid>

                </Grid>
            </Grid>

        </ctrlLib:LGCLayout>

        <ctrlLib:LoadingIndicator x:Name="loadingIndicator" Grid.Row="999" Grid.Column="999" />
    </Grid>
</UserControl>
