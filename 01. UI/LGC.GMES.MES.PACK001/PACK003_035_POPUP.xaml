                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="479*"/>
                                <ColumnDefinition Width="13*"/>
                                <ColumnDefinition Width="8"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Row="0" Grid.ColumnSpan="4" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                            <Grid Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="2">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <TextBlock     Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='AREA', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Width="50" HorizontalAlignment="Right" TextAlignment="Right" />
                                <c1:C1ComboBox Grid.Row="0" Grid.Column="2" x:Name="t2_cboSnapArea" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="True"   Tag="SNAP_AREA" Width="180" IsEnabled="False" />
                                <TextBlock     Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Width="50" HorizontalAlignment="Right" TextAlignment="Right"/>
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="5" x:Name="t2_cboSnapEqsg" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" Height="Auto" VerticalAlignment="Center" Width="180" SelectionChanged="t2_cboSnapEqsg_Chamged" />
                                <TextBlock     Grid.Row="0" Grid.Column="7" Text="{Binding ConverterParameter='Rack ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Width="70" HorizontalAlignment="Right" TextAlignment="Right"/>
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="8" x:Name="t2_comMtrlPort" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" Height="Auto" VerticalAlignment="Center" Width="180" />
                                <Button        Grid.Row="0" Grid.Column="10" x:Name="t2_btnSearch"      Content="{Binding ConverterParameter=조회,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource SearchButtonStyle}" Click="t2_btnSearch_Click" Width ="80"  />
                            </Grid>
                            <!-- grdMain(tp1) start -->
                            <c1:C1DataGrid Grid.Row="2" Grid.Column="1" x:Name="grdMainTp2" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="SingleRow" Grid.ColumnSpan="2" FrozenColumnCount ="5">
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridTextColumn  Header="동"       Binding="{Binding AREANAME}" HorizontalAlignment="Center" Width="Auto" IsReadOnly="False" />
                                    <c1:DataGridTextColumn  Header="라인"    Binding="{Binding EQSGNAME}" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="자재 RACK ID"      Binding="{Binding MTRL_PORT_ID}" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="자재코드"      Binding="{Binding MTRLID}" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="REPACK_WH_ID"      Binding="{Binding REPACK_WH_ID}" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="ON_HAND"       Binding="{Binding ON_HAND}" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="요청 완료"  Binding="{Binding REQ}" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="IN_TRANSIT"  Binding="{Binding DEL}" HorizontalAlignment="Center" Width="Auto" />
                                </c1:C1DataGrid.Columns>
                            </c1:C1DataGrid>
                            <!-- grdMain(tp1) end -->
                        </Grid>
                        <!-- main(tp1) end -->

                    </Grid>
                    <!-- content(tp1) end -->
