                            </Grid>
                            <!-- grdMain(tp1) start -->
                            <c1:C1DataGrid Grid.Row="2" Grid.Column="1" x:Name="grdMainTp1" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="SingleRow" Grid.ColumnSpan="2" BeginningRowEdit="grdMainTp1_BeginningRowEdit" FrozenColumnCount ="6" MouseDoubleClick="t1_grdDetail_MouseDoubleClick" >
