                                    <c1:DataGridTextColumn  Header="KEP_BOX_QTY"       Binding="{Binding KEP_BOX_QTY}" HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed" IsReadOnly="True"/>
                                    <c1:DataGridTextColumn  Header="CATN_BOX_QTY"       Binding="{Binding CATN_BOX_QTY}" HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed" IsReadOnly="True"/>
                                    <c1:DataGridTextColumn  Header="DNGR_BOX_QTY"       Binding="{Binding DNGR_BOX_QTY}" HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed" IsReadOnly="True"/>
                                    <c1:DataGridTextColumn  Header="DIHAND"       Binding="{Binding DIHAND}" HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed" IsReadOnly="True"/>
                                </c1:C1DataGrid.Columns>

                            </c1:C1DataGrid>
