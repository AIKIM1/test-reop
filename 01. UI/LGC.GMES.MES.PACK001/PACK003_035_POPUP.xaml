                                    <c1:DataGridTextColumn  Header="REPACK_WH_ID"      Binding="{Binding REPACK_WH_ID}" HorizontalAlignment="Center" Width="Auto" IsReadOnly="True"/>
                                    <c1:DataGridTextColumn  Header="ON_HAND"       Binding="{Binding ONHAND}" HorizontalAlignment="Center" Width="Auto" IsReadOnly="True"/>
