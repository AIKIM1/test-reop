                                        </Grid>
                                    </Grid>
                                </Grid>
                            </Border>
                        </Popup>
                        <!-- alert popup end -->
                    </Grid>
                    <!-- content(tp1) end -->
                </c1:C1TabItem>
                <!-- tp1: end-->
                <!-- tp2: start-->
                <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='사전 자재현황'}" >
                    <!-- content(tp1) start -->
                    <Grid x:Name="ctt_tp2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0"/>
