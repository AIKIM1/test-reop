                                <TextBlock     Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Width="60" HorizontalAlignment="Right" TextAlignment="Right"/>
