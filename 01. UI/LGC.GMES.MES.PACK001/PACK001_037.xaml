<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.PACK001.PACK001_037"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1280" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/C1DataGridStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="IWMS 반품/재 입고">
            <c1:C1TabControl x:Name="tabControlMain">
                <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='IWMS 반품'}" Margin="0" >

                    <Grid x:Name="Content">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0"/>
                            <RowDefinition Height="0"/>
                            <RowDefinition Height="0"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="40*"/>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition Width="60*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>

                        <!--Input-->
                        <Border Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                        <Grid x:Name="ContentTop" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="33*"/>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="33*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="33*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="15"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="6"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="6"/>
                            </Grid.RowDefinitions>

                        </Grid>

                        <!--Left-->
                        <Border Grid.Row="3" Grid.Column="1" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                        <Grid x:Name="ContentLeft" Grid.Row="3" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="190"/>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="6"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="6"/>
                            </Grid.ColumnDefinitions>

                            <!--Left 1Row-->
                            <Border Grid.Row="0" Grid.ColumnSpan="3" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                            <TextBlock  Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_ModuleTitleStyle}">
                            <Run Text="{Binding ConverterParameter='IWMS 반품', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                            </TextBlock>

                            <!--Left 2Row-->
                            <Border Grid.Row="1" Grid.Column="1" Style="{StaticResource Condition_Header_BorderStyle}"/>
                            <!--<Border Grid.Row="2" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />-->
                            <Grid Grid.Row="2" Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="15*"/>
                                    <ColumnDefinition Width="30*"/>
                                    <ColumnDefinition Width="15*"/>
                                    <ColumnDefinition Width="15*"/>
                                    <ColumnDefinition Width="25*"/>
                                </Grid.ColumnDefinitions>


                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <CheckBox  x:Name="chkInputCell_Return_YN" Content="{Binding ConverterParameter='투입Cell반품포함',     Converter={StaticResource GlobalObjectDicConverter}}"  Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" Checked="chkInputCell_Return_YN_Checked" Margin="15,8,8,8"/>
                                </Border>

                                <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                    <TextBlock  x:Name="txtReturnLabel" Text="{Binding ConverterParameter='ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    <!--<TextBlock Grid.Row="0" Grid.Column="0" x:Name="txtReturnLabel" Text="{Binding ConverterParameter='ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>-->
                                </Border>
                                <Border Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <TextBox   Grid.Row="0" Grid.Column="1"   x:Name="txtReturnID"     Style="{StaticResource SearchCondition_TextBoxStyle}" Height="28" KeyDown="txtReturnID_KeyDown"/>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <!--<RadioButton Grid.Column="0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=입출고ID}" x:Name="rdoRcvIss" GroupName="RadioButton_Equipment Loss" Style="{StaticResource SearchCondition_RadioButtonStyle}" IsChecked="True" Margin="8,0,8,0" Checked="RadioButton_Checked"/>-->
                                        <RadioButton Grid.Column="0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=Pallet ID}" x:Name="rdoPallet" GroupName="RadioButton_Equipment Loss" Style="{StaticResource SearchCondition_RadioButtonStyle}" IsChecked="True" Margin="4,0,8,0" Checked="RadioButton_Checked"/>
                                        <RadioButton Grid.Column="1" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=Cell ID}"   x:Name="rdoCell" GroupName="RadioButton_Equipment Loss" Style="{StaticResource SearchCondition_RadioButtonStyle}"   Margin="0,0,8,0" Checked="RadioButton_Checked" />
                                    </Grid>
                                </Border>

                                <Border Grid.Row="2" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                    <TextBlock Text="{Binding ConverterParameter='사유', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    <!--<TextBlock Grid.Row="1" Grid.Column="5" Text="{Binding ConverterParameter='사유', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>-->
                                </Border>
                                <Border Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <TextBox   Grid.Row="1" Grid.Column="4"   x:Name="txtReturnResn"     Style="{StaticResource SearchCondition_TextBoxStyle}"  KeyDown="txtReturnResn_KeyDown" Margin="3,7,203,7" />
                                </Border>


                                <!-- 트라이 트라이 DisplayMemberPath, SelectedValuePath, SelectedValueChanged에 무슨 값이 들어가야 할까?-->
                                <Border Grid.Row="2" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                    <TextBlock Text="{Binding ConverterParameter='반품유형', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                </Border>
                                <Border Grid.Row="2" Grid.Column="4" Grid.ColumnSpan="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <c1:C1ComboBox   x:Name="cboTagetCategory"     Style="{StaticResource C1ComboBoxStyle}"  DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  MinWidth="90" SelectedValueChanged="cboTagetRoute_SelectedValueChanged" HorizontalAlignment="Left" Margin="3,7,0,7" Width="116"/>
                                </Border>

                                <Border Grid.Row="3" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                    <TextBlock Text="{Binding ConverterParameter='파일명', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    <!--<TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='파일명', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>-->
                                </Border>
                                <Border Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <Grid Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="50*"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBox   Grid.Row="0" Grid.Column="0" x:Name="txtReturnFileName"  IsReadOnly="True"   Style="{StaticResource SearchCondition_TextBoxStyle}"  />
                                        <Button    Grid.Row="0" Grid.Column="1" x:Name="btnReturnFileUpload"  IsEnabled="False"    Content="{Binding ConverterParameter='Excel Upload',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelImportStyle}" Margin="0,0,8,0" Click="btnReturnFileUpload_Click_1"/>
                                    </Grid>
                                </Border>


                                <Border Grid.Row="4" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                    <TextBlock Text="{Binding ConverterParameter='반품번호', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    <!--<TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='반품번호', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>-->
                                </Border>
                                <Border Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBox  x:Name="txtReturnNumber"  IsReadOnly="True"   Style="{StaticResource SearchCondition_TextBoxStyle}"   />
                                        <Button    Grid.Row="1" Grid.Column="7" x:Name="btnReturnNew"      Content="{Binding ConverterParameter=신규,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnReturnNew_Click" Margin="0,0,8,0"/>
                                    </Grid>

                                </Border>
                                <!--<Border Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">    
                </Border>-->
                                <Border Grid.Row="4" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                    <TextBlock Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    <!--<TextBlock     Grid.Row="4"  Grid.Column="2" Text="{Binding ConverterParameter='반품창고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>-->
                                </Border>
                                <Border Grid.Row="4" Grid.Column="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <!--<c1:C1ComboBox Grid.Row="4"  Grid.Column="3" x:Name="cboSLocTo" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  />-->
                                    <TextBox  x:Name="txtPRODID"  IsReadOnly="True"   Style="{StaticResource SearchCondition_TextBoxStyle}"   />
                                </Border>

                                <Border Grid.Row="5" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                    <TextBlock Text="{Binding ConverterParameter='출고창고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    <!--<TextBlock     Grid.Row="4"  Grid.Column="0" Text="{Binding ConverterParameter='출고창고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>-->
                                </Border>
                                <Border Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <!--<c1:C1ComboBox Grid.Row="4"  Grid.Column="1" x:Name="cboSLocFrom" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  />-->
                                    <TextBox  x:Name="txtSLocFrom"  IsReadOnly="True"   Style="{StaticResource SearchCondition_TextBoxStyle}"   />
                                </Border>


                                <Border Grid.Row="5" Grid.Column="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}">
                                    <TextBlock Text="{Binding ConverterParameter='반품창고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    <!--<TextBlock     Grid.Row="4"  Grid.Column="2" Text="{Binding ConverterParameter='반품창고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>-->
                                </Border>
                                <Border Grid.Row="5" Grid.Column="4"  Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <!--<c1:C1ComboBox Grid.Row="4"  Grid.Column="3" x:Name="cboSLocTo" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  />-->
                                    <TextBox  x:Name="txtSLocTo"  IsReadOnly="True"   Style="{StaticResource SearchCondition_TextBoxStyle}" />
                                </Border>
                            </Grid>

                            <Grid Grid.Row="3" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock x:Name="tbTagetListCount" Grid.Column="1" Text="[ 0 건 ]" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,0,5"/>
                                <Button    Grid.Column="3" x:Name="btnTagetSelectCancel"      Content="{Binding ConverterParameter=선택취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnTagetSelectCancel_Click"/>
                                <Button    Grid.Column="5" x:Name="btnTagetCancel"      Content="{Binding ConverterParameter=취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnTagetCancel_Click"/>
                                <Button    Grid.Column="7" x:Name="btnTagetInputComfirm"      Content="{Binding ConverterParameter=등록,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnTagetInputComfirm_Click"/>
                                <!--<Button    Grid.Column="1" x:Name="btnTagetSelectCancel"      Content="{Binding ConverterParameter=선택취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnTagetSelectCancel_Click" />
                <Button    Grid.Column="3" x:Name="btnTagetCancel"            Content="{Binding ConverterParameter=취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"     Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnTagetCancel_Click"/>
                <Button    Grid.Column="5" x:Name="btnTagetInputComfirm"      Content="{Binding ConverterParameter=입고,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"     Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnTagetInputComfirm_Click"/>-->
                            </Grid>

                            <!--Left 3Row-->

                            <c1:C1DataGrid Grid.Row="4" Grid.Column="1" x:Name="dgTagetList" IsReadOnly="True" HeadersVisibility="Column" SelectionMode="MultiRange"  >
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridTemplateColumn Name="CHK"  Header="선택" Width="40">
                                        <c1:DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                            </DataTemplate>
                                        </c1:DataGridTemplateColumn.CellTemplate>
                                    </c1:DataGridTemplateColumn>
                                    <c1:DataGridTextColumn        Header = "반품번호"       Binding = "{Binding RETURN_ID}"       HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "입고 ID"         Binding = "{Binding RCV_ISS_ID}"      HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "입고 Pallet ID"   Binding = "{Binding BOXID}"           HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "Cell ID"         Binding = "{Binding LOTID}"           HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "사유"           Binding = "{Binding RTN_RSN_NOTE}"    HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "FROM_AREAID"    Binding = "{Binding FROM_AREAID}"     HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "FROM_SLOC_ID"   Binding = "{Binding FROM_SLOC_ID}"    HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "FROM_SLOC_NAME" Binding = "{Binding FROM_SLOC_NAME}"  HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "TO_AREAID"      Binding = "{Binding TO_AREAID}"       HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "TO_SLOC_ID"     Binding = "{Binding TO_SLOC_ID}"      HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "TO_SLOC_NAME"   Binding = "{Binding TO_SLOC_NAME}"    HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "공정투입여부"   Binding = "{Binding PROC_INPUT_FLAG}"    HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                </c1:C1DataGrid.Columns>
                            </c1:C1DataGrid>


                        </Grid>

                        <GridSplitter Grid.Row="3" Grid.Column="2" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                        <!--Right-->
                        <Border Grid.Row="3" Grid.Column="3" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                        <Grid x:Name="ContentRight" Grid.Row="3" Grid.Column="3">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="81"/>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="71"/>
                                <RowDefinition Height="8"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="8"/>
                            </Grid.ColumnDefinitions>

                            <!--Right 1Row-->
                            <Border Grid.Row="0" Grid.ColumnSpan="3" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                            <TextBlock  Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_ModuleTitleStyle}">
                            <Run Text="{Binding ConverterParameter='반품정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                            </TextBlock>

                            <!--Right 2Row-->
                            <Border Grid.Row="2" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />
                            <Grid Grid.Row="2" Grid.Column="1" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>

                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>

                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>

                                    <ColumnDefinition Width="0"/>
                                    <ColumnDefinition Width="0"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="13"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="8"/>
                                </Grid.RowDefinitions>
                                <TextBlock     Grid.Row="2"  Grid.Column="1" Text="{Binding ConverterParameter='출고창고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                                <c1:C1ComboBox Grid.Row="2"  Grid.Column="2" x:Name="cboSearchSLocFrom" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_NAME" />
                                <TextBlock     Grid.Row="2"  Grid.Column="3" Text="{Binding ConverterParameter='반품창고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                                <c1:C1ComboBox Grid.Row="2"  Grid.Column="4" x:Name="cboSearchSLocTo" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_NAME" />
                                <TextBlock     Grid.Row="2"  Grid.Column="5" Text="{Binding ConverterParameter='현상태', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                                <c1:C1ComboBox Grid.Row="2"  Grid.Column="6" x:Name="cboReturStatus" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_NAME" />

                                <TextBlock     Grid.Row="1"  Grid.Column="1" Text="{Binding ConverterParameter='등록일자', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                                <Grid Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <ctrlLib:LGCDatePicker Grid.Column="0" x:Name="dtpDateFrom" DatepickerType="Date"/>
                                    <!--<TextBlock              Grid.Column="3" Text="{Binding ConverterParameter='~', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>-->
                                    <ctrlLib:LGCDatePicker Grid.Column="1" x:Name="dtpDateTo" DatepickerType="Date"/>
                                </Grid>


                                <Button Grid.Row="1" Grid.Column="11" x:Name="btnSearch"      Content="{Binding ConverterParameter=SEARCH,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource SearchButtonStyle}" Click="btnSearch_Click"/>
                            </Grid>

                            <Grid Grid.Row="3" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="0" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="15" />
                                </Grid.ColumnDefinitions>
                                <TextBlock x:Name="tbSearchListCount" Grid.Column="1" Text="[ 0 건 ]" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,0,5"/>
                                <Button    Grid.Column="3" x:Name="btnExcel"      Content="{Binding ConverterParameter=Excel,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelExportStyle}" Click="btnExcel_Click"/>
                                <!--<Button    Grid.Column="5" x:Name="btnReturnPrint"  Content="{Binding ConverterParameter=반품TAG발행,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnReturnPrint_Click"/>-->
                            </Grid>

                            <!--Right 3Row-->
