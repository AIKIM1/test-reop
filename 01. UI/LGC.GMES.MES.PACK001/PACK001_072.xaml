<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.PACK001.PACK001_072"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d"  
             d:DesignHeight="768" d:DesignWidth="1024" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="전산재고">
            <Grid>
                <c1:C1TabControl x:Name="tabStckCnt">
                    <c1:C1TabItem x:Name="tabStckCntSnap" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='전산재고'}" Margin="0">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="51" />
                            </Grid.RowDefinitions>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <!--조회영역-->
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35"/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="0*"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0"  Grid.RowSpan="2" Grid.ColumnSpan="20" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <!--ROW1-->
                                <TextBlock              Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="1" x:Name="cboAreaShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" SelectedValueChanged="cboAreaShot_SelectedValueChanged_1" />

                                <TextBlock              Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <!--<ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="3" x:Name="mboEqsgShot" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>-->
                                <c1:C1ComboBox Grid.Row="0" Grid.Column="3" x:Name="cboEqsgShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>

                                <TextBlock              Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <!--<ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="5" x:Name="mboModelShot" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />-->
                                <c1:C1ComboBox Grid.Row="0" Grid.Column="5" x:Name="cboModelShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />

                                <TextBlock              Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='제품구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="7" x:Name="cboSnapTypeShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"  />

                                <TextBlock     Grid.Row="0" Grid.Column="8" Text="{Binding ConverterParameter='WH_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="9" x:Name="mboSnapWhId" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" SelectionChanged="mboSnapWhId_SelectionChanged_1"/>
                                <!--<c1:C1ComboBox Grid.Row="0" Grid.Column="9" x:Name="cboSnapWhId" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>-->

                                <TextBlock     Grid.Row="0" Grid.Column="10" Text="{Binding ConverterParameter='RACKID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="11" x:Name="mboSnapRackId" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                                <!--<c1:C1ComboBox Grid.Row="0" Grid.Column="11" x:Name="cboSnapRackId" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />-->

                                <TextBlock              Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='기준월', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:LGCDatePicker  Grid.Row="1" Grid.Column="1" x:Name="ldpMonthShot"  DatepickerType="Month" SelectedDataTimeChanged="ldpMonthShot_SelectedDataTimeChanged" />

                                <TextBlock              Grid.Row="1" Grid.Column="2" Text="{Binding ConverterParameter='차수', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="1" Grid.Column="3" x:Name="cboStockSeqShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStockSeqShot_SelectedValueChanged"/>

                                <CheckBox               Grid.Row="1" Grid.Column="5" x:Name="chkFinlwip" Content="{Binding ConverterParameter='SNAP변경수량', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" />

                                <TextBlock              Grid.Row="1" Grid.Column="6" Text="{Binding ConverterParameter='STCK_CNT_EXCL_FLAG', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="1" Grid.Column="7" x:Name="cboExclFlagShot" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  />

                                <TextBlock              Grid.Row="1" Grid.Column="8" Text="{Binding ConverterParameter='CSTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="1" Grid.Column="9" x:Name="txtCSTIdShot" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" />

                                <TextBlock              Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="1" Grid.Column="11" x:Name="txtLotIdShot" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" />

                                <Button     Grid.Row="1" Grid.Column="14" x:Name="btnDegreeClose" Content="{Binding ConverterParameter=차수마감,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialPackStyle}" Click="btnDegreeClose_Click" />
                                <Button     Grid.Row="1" Grid.Column="14" x:Name="btnDegreeCloseCancel" Content="{Binding ConverterParameter=마감취소,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDegreeCloseCancel_Click" Height="29" Visibility="Collapsed"/>
                                <Button     Grid.Row="1" Grid.Column="16" x:Name="btnDegreeAdd"   Content="{Binding ConverterParameter=차수추가,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialPackStyle}" Click="btnDegreeAdd_Click"  />
                                <!--조회 버튼-->
                                <Button     Grid.Row="1" Grid.Column="18" x:Name="btnSearchShot" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchShot_Click" Margin="4,0,0,0"/>


                            </Grid>

                            <!--리스트-->
                            <Grid Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="8"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='리스트', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <TextBlock x:Name="snapNote" Grid.Column="2" Text="" Style="{StaticResource Content_ModuleTitleStyle}" Foreground="Red" Background="Yellow"/>

                                    <TextBlock Grid.Column="4" x:Name="txtStckCntCmplFlagShot" Text="{Binding Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_Number_LabelStyle}"/>

                                    <Button     Grid.Column="6"  x:Name="btnExcel_Snap"     Content="{Binding ConverterParameter=Excel,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelExportStyle}" HorizontalAlignment="Right" Click="btnExcel_Snap_Click" Visibility="Collapsed" />
                                </Grid>

                                <c1:C1DataGrid x:Name="dgListShot" Grid.Row="1" IsReadOnly="False" HeadersVisibility="Column" FrozenColumnCount="2" SelectionMode="SingleRow"  LoadedColumnHeaderPresenter="dgListShot_LoadedColumnHeaderPresenter" Margin="8">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTemplateColumn Name="CHK" Width="40">
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center"  Click="chkHeader_SNAP_Click"/>
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "동"                   Binding = "{Binding AREANAME}"                       HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "동"                   Binding = "{Binding AREAID}"                         HorizontalAlignment="Center"     IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "라인"                 Binding = "{Binding EQSGNAME}"                       HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "공정"                 Binding = "{Binding PROCNAME}"                       HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "창고"                 Binding = "{Binding WH_NAME}"                        HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "위치"                 Binding = "{Binding RACK_ID}"                        HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "PJT"                  Binding = "{Binding PRJT_NAME}"                      HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "모델"                 Binding = "{Binding MODELID}"                        HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "제품구분"             Binding = "{Binding PRDTYPE}"                        HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "제품ID"               Binding = "{Binding PRODID}"                         HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "LOT 유형"             Binding = "{Binding LOTTYPE}"                        HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "단위"                 Binding = "{Binding LOTUID}"                         HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "LOTID"                Binding = "{Binding LOTID}"                          HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "재공상태"             Binding = "{Binding WIPSTAT}"                        HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "출하 포장 여부"       Binding = "{Binding ISS_FLAG}"                       HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "HOLD 여부"            Binding = "{Binding WIP_HOLD_FLAG}"                  HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "CSTID"                Binding = "{Binding PANCAKRE_GR_ID}"                 HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "BOXID"                Binding = "{Binding BOXID}"                          HorizontalAlignment="Center"     IsReadOnly="True" Visibility='Collapsed'/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "PALLETID"             Binding = "{Binding OUTER_BOXID}"                    HorizontalAlignment="Center"     IsReadOnly="True" Visibility='Collapsed'/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "시장유형"             Binding = "{Binding MKT_TYPE_CODE}"                  HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridNumericColumn   Width="Auto" Header = "차수"                 Binding = "{Binding STCK_CNT_SEQNO}"                 HorizontalAlignment="Right"      IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "전산재고 저장시점"    Binding = "{Binding SNAP_DTTM}"                      HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "기준월"               Binding = "{Binding STCK_CNT_YM}"                    HorizontalAlignment="Center"     IsReadOnly="True" Visibility="Collapsed"/>
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>

                            <!--제외버튼-->
                            <Border Grid.Row="5" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"/>
                            <Grid Grid.Row="7">
                                <Grid.RowDefinitions >
                                    <RowDefinition Height="4" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="4" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <TextBlock  Grid.Row="1" Grid.Column="0" x:Name="tblExcludeNote_SNAP" Text="{Binding ConverterParameter='비고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox    Grid.Row="1" Grid.Column="1" x:Name="txtExcludeNote_SNAP" Style="{StaticResource SearchCondition_TextBoxStyle}" Height="Auto"  AcceptsReturn="True" />
                                <Button     Grid.Row="1" Grid.Column="3" x:Name="btnExclude_SNAP"            Content="{Binding ConverterParameter='제외',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnExclude_SNAP_Click"/>
                                <Button     Grid.Row="1" Grid.Column="3" x:Name="btnExclude_SNAP_Cancel"     Content="{Binding ConverterParameter='제외취소',     Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnExclude_SNAP_Cancel_Click" Visibility="Collapsed"/>
                                <Button     Grid.Row="1" Grid.Column="5" x:Name="btnRowReSet"                Content="{Binding ConverterParameter=선택재고변경,  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnRowReSet_Click"/>

                            </Grid>
                        </Grid>
                    </c1:C1TabItem>
                    <c1:C1TabItem x:Name="tabStckCntRslt" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='실물재고조사'}" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="51"/>
                            </Grid.RowDefinitions>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>

                            <!--조회영역-->
                            <Grid Grid.Row="1" x:Name="tabRsltHeader">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="1*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="0*"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0"   Grid.RowSpan="2" Grid.ColumnSpan="20" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <!--ROW1-->
                                <TextBlock              Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="1" x:Name="cboRsltArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboRsltArea_SelectedValueChanged" />

                                <TextBlock              Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <!--<ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="3" x:Name="mboEqsgRslt" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" ItemsSource="{Binding Path=CBO_CODE}"  />-->
                                <c1:C1ComboBox Grid.Row="0" Grid.Column="3" x:Name="cboRsltEqsg" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />

                                <TextBlock              Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <!--<ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="5" x:Name="mboModelRslt" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" ItemsSource="{Binding Path=CBO_CODE}"  />-->
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="5" x:Name="cboRsltModel" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  />

                                <TextBlock              Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='제품구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="7" x:Name="cboRsltTypeRslt" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"   />

                                <TextBlock              Grid.Row="0" Grid.Column="8" Text="{Binding ConverterParameter='WH_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="9" x:Name="mboRsltWhId" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" ItemsSource="{Binding Path=CBO_CODE}" SelectionChanged="mboRsltWhId_SelectionChanged_1" />
                                <!--<c1:C1ComboBox          Grid.Row="0" Grid.Column="9" x:Name="cboRsltWhId" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />-->

                                <TextBlock              Grid.Row="0" Grid.Column="10" Text="{Binding ConverterParameter='RACKID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="11" x:Name="mboRsltRackId" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" ItemsSource="{Binding Path=CBO_CODE}"  />
                                <!--<c1:C1ComboBox          Grid.Row="0" Grid.Column="11" x:Name="cboRsltRackId" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  />-->

                                <CheckBox  Grid.Row="0" Grid.Column="18" x:Name="chkOverlap" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='LOT 중복 제거'}"    Style="{StaticResource SearchCondition_CheckBoxStyle}"/>


                                <TextBlock              Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='기준월', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <ctrlLib:LGCDatePicker  Grid.Row="1" Grid.Column="1" x:Name="ldpMonthUpload"  DatepickerType="Month" SelectedDataTimeChanged="ldpMonthUpload_SelectedDataTimeChanged"/>

                                <TextBlock              Grid.Row="1" Grid.Column="2" Text="{Binding ConverterParameter='차수', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox          Grid.Row="1" Grid.Column="3" x:Name="cboStockSeqUpload" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboStockSeqUpload_SelectedValueChanged"/>

                                <TextBlock              Grid.Row="1" Grid.Column="8" Text="{Binding ConverterParameter='CSTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="1" Grid.Column="9" x:Name="txtCSTIdUpload" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" />

                                <TextBlock              Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="1" Grid.Column="11" x:Name="txtLotIdUpload" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" Margin="3,8,3,4" />

                                <StackPanel Grid.Row="0" Grid.Column="14" Grid.ColumnSpan="3" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <RadioButton Name="rdoLot"  Margin="8,0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=LOT}"         GroupName="RadioButton_Group" Style="{StaticResource SearchCondition_RadioButtonStyle}"  IsChecked="True" Visibility="Collapsed"/>
