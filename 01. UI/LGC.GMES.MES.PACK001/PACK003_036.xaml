                                <TextBlock     Grid.Row="3" Grid.Column="11" Text="{Binding ConverterParameter='자재 RACK ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Width="100" HorizontalAlignment="Right" TextAlignment="Right"/>
                                <!--<c1:C1ComboBox Grid.Row="3" Grid.Column="13" x:Name="cboMtrlRack" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="KEY" SelectedValuePath="VALUE" SelectedIndex="1"/>-->
                                <ctrlLib:MultiSelectionBox  Grid.Row="3" Grid.Column="13" x:Name="cboMtrlRack" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" Height="Auto" VerticalAlignment="Center" Width="180" />
                                <Border Grid.Row="3" Grid.Column="15" >
                                    <TextBlock  Text="{Binding ConverterParameter='자재코드', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                                </Border>
                                <Border Grid.Row="3" Grid.Column="17" >
                                    <TextBox x:Name="txtMtrlID" Style="{StaticResource SearchCondition_TextBoxStyle}" HorizontalAlignment="Left" VerticalAlignment="Center" MinWidth="120" KeyDown="txtMtrlID_KeyDown" />
                                </Border>
                                <Border Grid.Row="3" Grid.Column="21" >
                                    <Button  x:Name="btnSearch"      Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource SearchButtonStyle}" Click="btnSearch_Click"/>
                                </Border>
                                <Border Grid.Row="3" Grid.Column="22" />
                            </Grid>
                            <Grid Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="{Binding ConverterParameter='조회 결과', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,0,5"/>
                                <TextBlock Grid.Column="2" Text=":  " Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,0,5"/>
                                <TextBlock x:Name="tbGrdMainTp2Cnt" Grid.Column="3" Text="0" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,0,5"/>
                            </Grid>
                            <!-- grdMain(tp2) start -->
                            <c1:C1DataGrid Grid.Row="5" Grid.Column="1" x:Name="grdMainTp2" IsReadOnly="True" HeadersVisibility="None" SelectionMode="MultiRange" Grid.ColumnSpan="2" FrozenTopRowsCount="2">
                                <c1:C1DataGrid.TopRows>
                                    <c1:DataGridColumnHeaderRow />
                                    <c1:DataGridColumnHeaderRow />
                                </c1:C1DataGrid.TopRows>
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridTextColumn  Header="동"      				            Binding="{Binding AREANAME}" 				HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="라인"       			            Binding="{Binding EQSGNAME}" 				HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="자재 RACK ID"     		            Binding="{Binding MTRL_PORT_ID}" 			HorizontalAlignment="Left"   Width="Auto" />
                                    <c1:DataGridTextColumn  Header="TO_SLOC_ID"     		            Binding="{Binding TO_SLOC_ID}" 				HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="자재코드"     			            Binding="{Binding MTRLID}" 					HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="REPACK_WH_ID"     		            Binding="{Binding REPACK_WH_ID_NAME}" 		HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="요청번호"     			            Binding="{Binding REQ_NO}" 					HorizontalAlignment="Left"   Width="Auto" />
