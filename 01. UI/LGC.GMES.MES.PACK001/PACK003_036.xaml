                                    <c1:DataGridTextColumn  Header="[SUPERMARKET,SUPERMARKET_MTRL_QTY]" Binding="{Binding SUPERMARKET_MTRL_QTY}"    HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="[SUPERMARKET,SUPERMARKET_BOX_QTY]"  Binding="{Binding SUPERMARKET_BOX_QTY}"     HorizontalAlignment="Center" Width="Auto" />
