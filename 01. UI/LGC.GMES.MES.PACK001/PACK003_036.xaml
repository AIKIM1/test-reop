                                    <c1:DataGridTextColumn  Header="RIORITY"     			            Binding="{Binding PRIORITY}" 				HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="KANBAN_ID"     		                Binding="{Binding KANBAN_ID}" 			    HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="자재 BOX ID"     		            Binding="{Binding REPACK_BOX_ID}" 			HorizontalAlignment="Left"   Width="Auto" />
                                    <c1:DataGridTextColumn  Header="상태"    				            Binding="{Binding REQ_STAT_CODE}" 			HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="W/O"    				            Binding="{Binding ERP_TRNF_WOID}" 			HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="반품상태"                           Binding="{Binding ISS_STAT_CODE}" 		    HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="RACK_TYPE"    			            Binding="{Binding RACK_TYPE}" 				HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="PACK_PRDT_GR_CODE"    	            Binding="{Binding PACK_PRDT_GR_CODE}" 		HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="PLLT_ID"    			            Binding="{Binding PLLT_ID}" 				HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="자재수량"    			            Binding="{Binding ISS_QTY}" 				HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn  Header="사용수량"    			            Binding="{Binding USE_QTY}" 				HorizontalAlignment="Center" Width="Auto" />
