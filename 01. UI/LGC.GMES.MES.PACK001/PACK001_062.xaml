<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.PACK001.PACK001_062"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/C1DataGridStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="공정진척(Pack)_Pack 공정진척(자동발행)" Grid.ColumnSpan="2">
            <ctrlLib:LGCLayout.Content>
                <Grid>
                    <Grid x:Name="ButtonArea" Margin="0,-48,0,0">
                        <Grid VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="1*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="tbTitle" Grid.Column="1" Text="{Binding ConverterParameter='', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleTitleStyle}" HorizontalAlignment="Center"/>
                            <Grid Grid.Column="2">
                                <Grid VerticalAlignment="Top">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="34" />
                                        <ColumnDefinition Width="8" />
                                    </Grid.ColumnDefinitions>
                                    <Button  Grid.Row="0" Grid.Column="1" x:Name="btnProcessSelect" Content="{Binding ConverterParameter='공정선택',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl01Style}" Click="btnProcessSelect_Click" />
                                </Grid>

                            </Grid>
                        </Grid>
                    </Grid>

                    <Grid x:Name="Content" Grid.Row="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0" />
                            <ColumnDefinition Width="350" />
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="7*" />
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="0*" />
                        </Grid.ColumnDefinitions>

                        <Grid x:Name="ContentLeft" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="0"/>
                                <RowDefinition Height="0"/>
                            </Grid.RowDefinitions>

                            <!--Start 작업정보-->
                            <Grid x:Name="ContentLeftTop" Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="8"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <!-- 1row-->
                                <!--<Border Grid.Row="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>-->
                                <Border Grid.Row="0" Grid.ColumnSpan="3" BorderThickness="1,1,1,1" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_ModuleTitleStyle}">
                                    <Run Text="{Binding ConverterParameter='작업정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                </TextBlock>

                                <Border Grid.Row="1" Grid.ColumnSpan="3" Grid.RowSpan="5" Style="{StaticResource Content_InputForm_GroupBorderStyle}" BorderThickness="1,0,1,1"/>
                                <Grid x:Name="dgWorkInfo" Grid.Row="2" Grid.Column="1">

                                </Grid>
                                
                            </Grid>
                            <!--end 작업정보-->

                            <Grid Grid.Row="2">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="2"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="8"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <Border Grid.Row="0" Grid.ColumnSpan="3" BorderThickness="1,1,1,1" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock     Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_ModuleTitleStyle}">
                                    <Run Text="{Binding ConverterParameter='설정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                </TextBlock>

                                <Border Grid.Row="1" Grid.ColumnSpan="3" Grid.RowSpan="5" Style="{StaticResource Content_InputForm_GroupBorderStyle}" BorderThickness="1,0,1,1"/>
                                <Border Grid.Row="2" Grid.Column="1" Style="{StaticResource Condition_Header_BorderStyle}"/>
                                <Grid Grid.Row="3" Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="8"/>
                                        <RowDefinition Height="0*"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Row="0" Grid.Column="0"  Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,0" >
                                        <TextBlock Name="AutoPrint" Visibility="Visible" Text="{Binding ConverterParameter='자동발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,9,0"/>
                                    </Border>
                                    <Border Grid.Row="0" Grid.Column="1" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,1,0">
                                        <Grid Grid.Column="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <RadioButton Grid.Column="0" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=시작}"   x:Name="rdoPageFixed"    GroupName="PageFixed" Style="{StaticResource SearchCondition_RadioButtonStyle}"   Height="23" Margin="3" Checked="chkPageFixed_Checked" Unchecked="chkPageFixed_Unchecked"/>
                                            <RadioButton Grid.Column="1" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter=종료}"   x:Name="rdoPageUnFixed"    GroupName="PageFixed" Style="{StaticResource SearchCondition_RadioButtonStyle}" IsChecked="True"   Height="23" Margin="3" />
                                        </Grid>
                                    </Border>

                                    <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,0">
                                        <TextBlock  Text="{Binding ConverterParameter='바코드발행', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    </Border>
                                    <Border Grid.Row="1" Grid.Column="1" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,1,0">
                                        <c1:C1ComboBox  Grid.Column="8" x:Name="cboPrintYn" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  />
                                    </Border>

                                    <Border Grid.Row="2" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,0">
                                        <TextBlock  Text="{Binding ConverterParameter='갱신주기(sec)', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    </Border>
                                    <Border Grid.Row="2" Grid.Column="1" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,1,0">
                                        <c1:C1NumericBox x:Name="cboAutoSearchTime" Grid.Column="5" Value="3" Style="{StaticResource C1NumericBoxStyle}" TextAlignment="Center"/>
                                    </Border>
                                    <Border Grid.Row="3" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,1">
                                        <TextBlock  Text="{Binding ConverterParameter='조회건수', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    </Border>
                                    <Border Grid.Row="3" Grid.Column="1" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,1,1">
                                        <c1:C1NumericBox x:Name="cboSearchCount"  Increment="10" Value="50" Style="{StaticResource C1NumericBoxStyle}" TextAlignment="Center" />
                                    </Border>

                                    <c1:C1DataGrid Grid.Row="5"  Grid.Column="0" Grid.ColumnSpan="2" x:Name="dgLabelPortSetting" FontWeight="Normal" CanUserAddRows="True" CanUserRemoveRows="True"> <!--CurrentCellChanged="dgComPort_CurrentCellChanged">-->
                                        <!--ContextMenuOpening="dgComPort_ContextMenuOpening" BeginningEdit="dgComPort_BeginningEdit" CommittedEdit="dgComPort_CommittedEdit"-->
                                        <c1:C1DataGrid.Resources>
                                            <ResourceDictionary>
                                            </ResourceDictionary>
                                        </c1:C1DataGrid.Resources>
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridComboBoxColumn      Header="PRINTERNAME"    Binding="{Binding Path=PRINTERNAME}" x:Name="colPrinterName"  CellContentStyle="{StaticResource Grid_CellContentStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" EditOnSelection="True" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn          Header="포트명"          Binding="{Binding Path=PORTNAME}"    Width="Auto" IsReadOnly="True"/>
                                            <c1:DataGridTextColumn          Header="라벨명"          Binding="{Binding Path=LABEL_CODE}"    Width="Auto" IsReadOnly="True"/>
                                            <c1:DataGridNumericColumn       Header="발행수"          Binding="{Binding Path=LABEL_CNT}"   ShowButtons="False"    Width="60"/>
                                            <c1:DataGridComboBoxColumn      Header="DPI"             Binding="{Binding Path=DPI}"  x:Name="colLabelDPI"  Width="Auto" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" CellContentStyle="{StaticResource Grid_CellContentStyle}" EditOnSelection="True"/>
                                            <c1:DataGridTextColumn          Header="X"               Binding="{Binding Path=X}"    Width="Auto" />
                                            <c1:DataGridTextColumn          Header="Y"               Binding="{Binding Path=Y}"    Width="Auto" />
                                            <c1:DataGridTextColumn          Header="DARKNESS"        Binding="{Binding Path=DARKNESS}"    Width="Auto" />
                                            
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>

                                </Grid>

                            </Grid>
                            
                            
                        </Grid>

                        <Grid x:Name="ContentRight" Grid.Column="3">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>  <!--헤더-->
                                <RowDefinition Height="8"/>    <!--공백-->
                                <RowDefinition Height="45"/>   <!--조회-->
                                <RowDefinition Height="8"/>     <!--공백-->
                                <RowDefinition Height="34"/>    <!--헤더-->
                                <RowDefinition Height="200"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="34"/>     <!--헤더-->
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="8"/>
                            </Grid.ColumnDefinitions>

                            <!--<Border Grid.Row="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>-->
                            <Border Grid.Row="0" Grid.Column="0"  Grid.ColumnSpan="3" BorderThickness="1,1,1,1" Style="{StaticResource BorderSubTitleStyle}"/>
                            <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="13"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock  Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}">
                                    <Run Text="{Binding ConverterParameter='바코드발행정보', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                </TextBlock>
                               
                            </Grid>
                            
                            <Border Grid.Row="1" Grid.ColumnSpan="10" Grid.RowSpan="9" Style="{StaticResource Content_InputForm_GroupBorderStyle}" BorderThickness="1,0,1,1"/>

                            <!--2Row-->
                            <!--<Border Grid.Row="2" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />-->

                            <Border  Style="{StaticResource Condition_BorderStyle}"></Border>
                            <Grid x:Name="ContentWorkSelect" Grid.Row="2" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="13"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto" MinWidth="70"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto" MinWidth="50"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="13"/>
                                </Grid.ColumnDefinitions>

                                <Button    Grid.Row="0" Grid.Column="9" x:Name="btnProdutLotSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnProdutLotSearch_Click"/>
                            </Grid>

                            <Border Grid.Row="3" Grid.ColumnSpan="3" Grid.RowSpan="11" Style="{StaticResource Content_InputForm_GroupBorderStyle}" BorderThickness="1,0,1,1"/>
                            <Border Grid.Row="3" Grid.ColumnSpan="3" BorderThickness="1,1,1,1" Style="{StaticResource BorderSubTitleStyle}"/>
                            
                            <Grid Grid.Row="4" Grid.Column="1" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="8"/>
                                </Grid.RowDefinitions>
                                
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>
                                
                                <TextBlock x:Name="tbPrintWip" Grid.Row="1" Grid.Column="1" Text="바코드 발행 대기" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,0,5"/>
                                
                                
                            </Grid>

                            <c1:C1DataGrid Grid.Row="5" Grid.Column="1" x:Name="dgWipPrint" IsReadOnly="True" HeadersVisibility="Column" MouseUp="dgWipPrint_MouseUp"  LoadedCellPresenter="dgWipPrint_LoadedCellPresenter" MouseDoubleClick="dgWipPrint_MouseDoubleClick" Margin="8,0,0,0" >
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridTextColumn        Header = "SCAN_ID"              Binding = "{Binding SCAN_ID}"         HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "발행요청번호"         Binding = "{Binding PRT_REQ_SEQNO}"   HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "LOTID"                Binding = "{Binding LOTID}"           HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "제품ID"               Binding = "{Binding PRODID}"          HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "제품명"               Binding = "{Binding PRODNAME}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "제품설명"             Binding = "{Binding PRODDESC}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "PROCID"               Binding = "{Binding PROCID}"          HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "현재공정"             Binding = "{Binding PROCNAME}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "WIPSTAT"              Binding = "{Binding WIPSTAT}"         HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "상태"                 Binding = "{Binding WIPSNAME}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "라벨 발행여부"        Binding = "{Binding PRT_FLAG}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "라벨 발행일시"        Binding = "{Binding PRT_DTTM}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "라벨 발행요청일시"    Binding = "{Binding INSDTTM}"         HorizontalAlignment="Center" Width="Auto"/>
                                </c1:C1DataGrid.Columns>
                            </c1:C1DataGrid>

                            <Grid x:Name="PrintInputKeyPart" Grid.Row="7" Grid.Column="1" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="2"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto" MinWidth="100"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto" MinWidth="100"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto" MinWidth="100"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>

                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="11" Style="{StaticResource Condition_Header_BorderStyle}"></Border>

                                <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,1" Margin="4,0,0,0">
                                    <TextBlock  Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="1" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,0,1" >
                                    <TextBox x:Name="txtCurrentLot" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,1">
                                    <TextBlock  Text="{Binding ConverterParameter='발행 요청번호', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="3"  Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,0,1">
                                    <TextBox x:Name="txtCurrentReqNo" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="4" Grid.ColumnSpan="4" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,1">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="13"/>
                                        </Grid.ColumnDefinitions>

                                        <Button x:Name="btnDelete" Grid.Column="1" Content="{Binding ConverterParameter=삭제,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" HorizontalAlignment="Right" Click="btnDelete_Click" />
                                    </Grid>
                                </Border>
                            </Grid>
                            
                            <Border Grid.Row="8" Grid.ColumnSpan="3" BorderThickness="1,1,1,1" Style="{StaticResource BorderSubTitleStyle}"/>
                            <Grid Grid.Row="9" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock x:Name="tbWipListCount" Grid.Column="1" Text="[ 0 건 ]" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,0,5"/>
                            </Grid>                            

                            <!--3Row-->
                            <c1:C1DataGrid Grid.Row="10" Grid.Column="1" x:Name="dgWipList" IsReadOnly="True" HeadersVisibility="Column"  MouseUp="dgWipList_MouseUp" LoadedCellPresenter="dgWipList_LoadedCellPresenter" MouseDoubleClick="dgWipList_MouseDoubleClick" GotFocus="dgWipList_GotFocus"  Margin="8,0,0,0">
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridTextColumn        Header = "SCAN_ID"              Binding = "{Binding SCAN_ID}"         HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "발행요청번호"         Binding = "{Binding PRT_REQ_SEQNO}"   HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "LOTID"                Binding = "{Binding LOTID}"           HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "WIPSTAT"              Binding = "{Binding EQSGID}"         HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "제품ID"               Binding = "{Binding PRODID}"          HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <!--<c1:DataGridTextColumn        Header = "제품명"        Binding = "{Binding PRODNAME}"        HorizontalAlignment="Center" Width="Auto"/>-->
                                    <c1:DataGridTextColumn        Header = "제품설명"             Binding = "{Binding PRODDESC}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "PROCID"               Binding = "{Binding PROCID}"          HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "현재공정"             Binding = "{Binding PROCNAME}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "WIPSTAT"              Binding = "{Binding WIPSTAT}"         HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "상태"                 Binding = "{Binding WIPSNAME}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "라벨 발행여부"        Binding = "{Binding PRT_FLAG}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "라벨 발행일시"        Binding = "{Binding PRT_DTTM}"        HorizontalAlignment="Center" Width="Auto"/>
                                    <c1:DataGridTextColumn        Header = "라벨 발행요청일시"    Binding = "{Binding INSDTTM}"         HorizontalAlignment="Center" Width="Auto"/>
                                </c1:C1DataGrid.Columns>
                            </c1:C1DataGrid>

                            <!--5Row-->
                            <!--<Border Grid.Row="6" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />-->
                            <!--<Border Grid.Row="6" Grid.Column="1" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>-->
                            <Grid x:Name="ContentInputKeyPart" Grid.Row="12" Grid.Column="1" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="2"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto" MinWidth="100"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto" MinWidth="100"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto" MinWidth="100"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="0*"/>
                                    <ColumnDefinition Width="0*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>

                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="11" Style="{StaticResource Condition_Header_BorderStyle}"></Border>

                                <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,1" Margin="4,0,0,0">
                                    <TextBlock  Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="1" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,0,1">
                                    <TextBox x:Name="txtLabelLot" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="2" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,1">
                                    <TextBlock  Text="{Binding ConverterParameter='품명', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="3"  Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,0,1">
                                    <TextBox x:Name="txtLabelProduct" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                                </Border>

                                <Border Grid.Row="1" Grid.Column="3"  Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,0,1" Visibility="Collapsed">
                                    <TextBox x:Name="txtLabelProductId" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                                </Border>

                                <Border Grid.Row="1" Grid.Column="4" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,1">
                                    <TextBlock Text="{Binding ConverterParameter='LABELCODE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" Visibility="Collapsed"/>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="5" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,0,1">
                                    <c1:C1ComboBox  x:Name="cboLabelCode" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" AutoComplete="False" IsEditable="False"  Visibility="Collapsed"/>
                                </Border>

                                <Border Grid.Row="1" Grid.Column="6" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="13"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBox x:Name="txtLabelScanID" Grid.Column="0" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" Visibility="Collapsed"/>
                                        <Button x:Name="btnLabel" Grid.Column="1" Content="{Binding ConverterParameter=재발행,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnLabel_Click"/>
                                    </Grid>
                                </Border>

                                <!--<TextBlock Grid.Column="1" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>-->
                                <!--<TextBox   Grid.Column="2"   x:Name="txtLabelLot"  IsReadOnly="True"    Style="{StaticResource SearchCondition_TextBoxStyle}"   Height="30" />-->

                                <!--<TextBlock Grid.Column="4" Text="{Binding ConverterParameter='품명', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>-->
                                <!--<TextBox   Grid.Column="5"   x:Name="txtLabelProduct"  IsReadOnly="True"   Style="{StaticResource SearchCondition_TextBoxStyle}"   Height="30" />-->
                                <!--<Button    Grid.Column="6" x:Name="btnLabel"      Content="{Binding ConverterParameter=재발행,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonStyle}"/>-->
                            </Grid>
                        </Grid>

                        <Grid x:Name="ContentRight_KeyPart" Grid.Column="5">
                            <!--Start Ketpart-->
                            <Grid.RowDefinitions>
                                <RowDefinition Height="34"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="8"/>
                            </Grid.ColumnDefinitions>

                            <!--<Border Grid.Row="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>-->
                            <Border Grid.Row="0" Grid.ColumnSpan="3" BorderThickness="1,1,1,1" Style="{StaticResource BorderSubTitleStyle}"/>

                            <!--1Row-->
                            <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="13"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock  Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}">
                                        <Run Text="{Binding ConverterParameter='KEYPART', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                                </TextBlock>
                                <Button    Grid.Row="0" Grid.Column="1" x:Name="btnKeyPartCopy"       Content="{Binding ConverterParameter=이력복사,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnKeyPartCopy_Click" />

                            </Grid>

                            <Border Grid.Row="1" Grid.ColumnSpan="3" Grid.RowSpan="5" Style="{StaticResource Content_InputForm_GroupBorderStyle}" BorderThickness="1,0,1,1"/>
                            <c1:C1TreeView Grid.Row="2" Grid.Column="1" x:Name="trvKeypartList" >
                                <c1:C1TreeView.ItemTemplate>
                                    <c1:C1HierarchicalDataTemplate  ItemsSource="{Binding Relations}">
                                        <StackPanel Orientation="Horizontal">
                                            <!--<CheckBox Visibility="{Binding VISIBLE_CHKECK, Converter={StaticResource  GlobalBoolToVisibilityConverter}}" >
                                                <CheckBox.LayoutTransform>
                                                    <ScaleTransform ScaleX="0.8" ScaleY="0.8" />
                                                </CheckBox.LayoutTransform>
                                            </CheckBox>-->
                                            <TextBlock Text="{Binding INPUT_LOTID}" />
                                        </StackPanel>
                                    </c1:C1HierarchicalDataTemplate>
                                </c1:C1TreeView.ItemTemplate>
                            </c1:C1TreeView>
                            <!--End Ketpart-->
                        </Grid>
                    </Grid>
                </Grid>
            </ctrlLib:LGCLayout.Content>
        </ctrlLib:LGCLayout>

        <ctrlLib:LoadingIndicator x:Name="loadingIndicator" Grid.ColumnSpan="2" Grid.RowSpan="1" />
    </Grid>
</UserControl>
