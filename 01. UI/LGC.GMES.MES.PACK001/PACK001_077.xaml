<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"
             x:Class="LGC.GMES.MES.PACK001.PACK001_077"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d"
             d:DesignHeight="768" d:DesignWidth="1024">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
                <ResourceDictionary Source="/lgc.gmes.mes.controlslibrary;component/controltemplates/C1DataGridStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="실적집계">
            <Grid x:Name="grdLayout">
                <c1:C1TabControl>
                    <c1:C1TabItem x:Name="tabInputResult" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='Input실적'}">
                        <Grid x:Name="grdInputResult">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="8" />
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="1" x:Name="grdInputSearch">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="35" />
                                </Grid.RowDefinitions>

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>
                                <!-- 백그라운드용 -->
                                <Border                    Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />

                                <!--동-->
                                <TextBlock                 Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox             Grid.Row="0" Grid.Column="2" x:Name="cboInputAreaID" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboInputAreaID_SelectedValueChanged" />

                                <!--라인-->
                                <TextBlock                 Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='Line', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox             Grid.Row="0" Grid.Column="5" x:Name="cboInputEquipmentSegmentID" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboInputEquipmentSegmentID_SelectedValueChanged" />

                                <!--공정-->
                                <TextBlock                 Grid.Row="0" Grid.Column="7" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="8" x:Name="cboMultiInputProcessID" />

                                <!--집계구분-->
                                <TextBlock          Grid.Row="0" Grid.Column="10" Text="{Binding ConverterParameter='집계구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox      Grid.Row="0" Grid.Column="11" x:Name="cboInputActID" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                <!--ERP 미전송대상 제외-->
                                <CheckBox           Grid.Row="0" Grid.Column="12" x:Name="chkInputExclFlag" Content="{Binding ConverterParameter='ERP 미전송대상 제외', Converter={StaticResource GlobalObjectDicConverter}}"  Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" Margin="10,0,0,0" />

                                <!--전기일-->
                                <TextBlock          Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='전기일',Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" />

                                <!--DATE(DATEPICKER)-->
                                <Grid               Grid.Row="1" Grid.Column="2" HorizontalAlignment="Left" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <ctrlLib:LGCDatePicker  Grid.Row="0" Grid.Column="0" x:Name="dtpDateFromInput" DatepickerType="Date" />
                                    <TextBlock              Grid.Row="0" Grid.Column="1" Text="~" HorizontalAlignment="Center" Style="{StaticResource SearchCondition_LabelStyle}" />
                                    <ctrlLib:LGCDatePicker  Grid.Row="0" Grid.Column="2" x:Name="dtpDateToInput" DatepickerType="Date" />
                                </Grid>

                                <!--PJT약어-->
                                <TextBlock          Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox      Grid.Row="1" Grid.Column="5" x:Name="cboInputProjectAbbreviationName" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />

                                <!--ERP IF 자재ID-->
                                <TextBlock          Grid.Row="1" Grid.Column="7" Text="{Binding ConverterParameter='ERP I/F 자재ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <TextBox            Grid.Row="1" Grid.Column="8" x:Name="txtInputERPInterfaceMaterialID" Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtInputERPInterfaceMaterialID_KeyDown" MinWidth="120" />

                                <!--투입LOT유형-->
                                <TextBlock          Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter='투입LOT유형', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox      Grid.Row="1" Grid.Column="11" x:Name="cboInputLotType" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />


                                <!--조회 버튼-->
                                <Button             Grid.Row="0" Grid.Column="13" Grid.RowSpan="2" x:Name="btnInputSearch" Content="{Binding ConverterParameter=Search, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"
                                                    Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnInputSearch_Click" />
                            </Grid>
                            <!-- grdInputSearch -->

                            <Grid Grid.Row="3" x:Name="grdInputResultList">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border             Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                <Border             Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                <TextBlock          Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}" >
                                    <Run Text="{Binding ConverterParameter='목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" />
                                </TextBlock>
                                <TextBlock     Grid.Row="1" Grid.Column="0" x:Name="txtInputResultCount" Text="[ 0 건 ]" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" VerticalAlignment="Center" />
                                <c1:C1DataGrid      Grid.Row="2" x:Name="dgListInputResult" HeadersVisibility="None" FrozenTopRowsCount="2" FrozenColumnCount="6" SelectionMode="SingleRow">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn Width="Auto" Header="라인"                           Binding="{Binding EQSGNAME}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="공정"                           Binding="{Binding PROCNAME}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="W/O"                            Binding="{Binding WOID}"                HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="집계구분"                       Binding="{Binding ACTID}"               HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="제품ID"                         Binding="{Binding PRODID}"              HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="자재ID"                         Binding="{Binding MTRLID}"              HorizontalAlignment="Center" IsReadOnly="True" />

                                        <c1:DataGridTextColumn Width="Auto" Header="투입LOT유형"                    Binding="{Binding INPUT_LOT_TYPE_CODE}" HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="KEY_PART_FLAG"                  Binding="{Binding KEY_PART_FLAG}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="집계일시"                       Binding="{Binding SUM_DTTM}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="전기일"                         Binding="{Binding CALDATE}"             HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="ERP 자재ID"                     Binding="{Binding ERP_IF_MTRLID}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="처리수량"                       Binding="{Binding ACTQTY2}"             HorizontalAlignment="Right"  IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="LOTTYPE"                        Binding="{Binding LOTTYPE_NM}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="출고저장위치"                   Binding="{Binding ISS_SLOC_ID}"         HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="처리저장위치"                   Binding="{Binding PRCS_SLOC_ID}"        HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="FROM_SLOC_ID"                   Binding="{Binding FROM_SLOC_ID}"        HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="TO_SLOC_ID"                     Binding="{Binding TO_SLOC_ID}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="ERP전송W/O"                     Binding="{Binding ERP_TRNF_WOID}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[정상W/O실적처리,ERP전송번호]"  Binding="{Binding ERP_TRNF_SEQNO}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[정상W/O실적처리,ERP전송결과]"  Binding="{Binding ERP_STAT_CODE}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[정상W/O실적처리,ERP처리결과]"  Binding="{Binding ERP_ERR_CODE}"        HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[정상W/O실적처리,ERP오류내용]"  Binding="{Binding ERP_ERR_CAUSE_CNTT}"  HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[정상W/O실적처리,ERP오류유형]"  Binding="{Binding ERP_ERR_TYPE}"        HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[이전전기,ERP전송번호]"         Binding="{Binding ERP_TRNF_SEQNO2}"     HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[이전전기,ERP전송결과]"         Binding="{Binding ERP_STAT_CODE2}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[이전전기,ERP처리결과]"         Binding="{Binding ERP_ERR_CODE2}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[이전전기,ERP오류내용]"         Binding="{Binding ERP_ERR_CAUSE_CNTT2}" HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[이전전기,ERP오류유형]"         Binding="{Binding ERP_ERR_TYPE2}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[PPAR실적처리,ERP전송번호]"     Binding="{Binding ERP_TRNF_SEQNO3}"     HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[PPAR실적처리,ERP전송결과]"     Binding="{Binding ERP_STAT_CODE3}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[PPAR실적처리,ERP처리결과]"     Binding="{Binding ERP_ERR_CODE3}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[PPAR실적처리,ERP오류내용]"     Binding="{Binding ERP_ERR_CAUSE_CNTT3}" HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[PPAR실적처리,ERP오류유형]"     Binding="{Binding ERP_ERR_TYPE3}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                    </c1:C1DataGrid.Columns>

                                    <c1:C1DataGrid.TopRows>
                                        <c1:DataGridColumnHeaderRow />
                                        <c1:DataGridColumnHeaderRow />
                                    </c1:C1DataGrid.TopRows>
                                </c1:C1DataGrid>
                            </Grid>
                            <!--grdInputResultList-->
                        </Grid>
                        <!-- grdInputResult -->
                    </c1:C1TabItem>

                    <c1:C1TabItem x:Name="tabOutputResult" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='Output실적'}">
                        <Grid x:Name="grdOutputResult">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="8" />
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="1" x:Name="grdOutputSearch">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="35" />
                                </Grid.RowDefinitions>

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>
                                <!-- 백그라운드용 -->
                                <Border             Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />

                                <!--동-->
                                <TextBlock          Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox      Grid.Row="0" Grid.Column="2" x:Name="cboOutputAreaID" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboOutputAreaID_SelectedValueChanged" />

                                <!--라인-->
                                <TextBlock          Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='Line', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox      Grid.Row="0" Grid.Column="5" x:Name="cboOutputEquipmentSegmentID" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboOutputEquipmentSegmentID_SelectedValueChanged" />

                                <!--공정-->
                                <TextBlock          Grid.Row="0" Grid.Column="7" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <ctrlLib:MultiSelectionBox       Grid.Row="0" Grid.Column="8" x:Name="cboMultiOutputProcessID" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />

                                <!--집계구분-->
                                <TextBlock          Grid.Row="0" Grid.Column="10" Text="{Binding ConverterParameter='집계구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox      Grid.Row="0" Grid.Column="11" x:Name="cboOutputActID" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />

                                <!--ERP 미전송대상 제외-->
                                <CheckBox           Grid.Row="0" Grid.Column="12" x:Name="chkOutputExclFlag" Content="{Binding ConverterParameter='ERP 미전송대상 제외',     Converter={StaticResource GlobalObjectDicConverter}}"  Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" Margin="10,0,0,0" />

                                <!--전기일-->
                                <TextBlock          Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='전기일',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" />

                                <!--DATE(DATEPICKER)-->
                                <Grid               Grid.Row="1" Grid.Column="2" HorizontalAlignment="Left" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <ctrlLib:LGCDatePicker  Grid.Row="0" Grid.Column="0" x:Name="dtpDateFromOutput" DatepickerType="Date" />
                                    <TextBlock              Grid.Row="0" Grid.Column="1" Text="~" HorizontalAlignment="Center" Style="{StaticResource SearchCondition_LabelStyle}" />
                                    <ctrlLib:LGCDatePicker  Grid.Row="0" Grid.Column="2" x:Name="dtpDateToOutput" DatepickerType="Date" />
                                </Grid>

                                <!--PJT약어-->
                                <TextBlock          Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox      Grid.Row="1" Grid.Column="5" x:Name="cboOutputProjectAbbreviationName" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />

                                <!--ERP IF 자재ID-->
                                <TextBlock          Grid.Row="1" Grid.Column="7" Text="{Binding ConverterParameter='ERP I/F 자재ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <TextBox            Grid.Row="1" Grid.Column="8" x:Name="txtOutputERPInterfaceMaterialID" Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtOutputERPInterfaceMaterialID_KeyDown" MinWidth="120" />


                                <!--조회 버튼-->
                                <Button             Grid.Row="0" Grid.Column="13" Grid.RowSpan="2" x:Name="btnOutputSearch" Content="{Binding ConverterParameter=Search, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"
                                                    Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnOutputSearch_Click" />
                            </Grid>
                            <!-- grdOutputSearch -->

                            <Grid Grid.Row="3" x:Name="grdOutputResultList">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border             Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                <Border             Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                <TextBlock          Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}" >
                                    <Run Text="{Binding ConverterParameter='목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" />
                                </TextBlock>
                                <TextBlock     Grid.Row="1" Grid.Column="0" x:Name="txtOutputResultCount" Text="[ 0 건 ]" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" VerticalAlignment="Center" />
                                <c1:C1DataGrid      Grid.Row="2" x:Name="dgListOutputResult" HeadersVisibility="None" FrozenTopRowsCount="2" FrozenColumnCount="6" SelectionMode="SingleRow">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn Width="Auto" Header="라인"                           Binding="{Binding EQSGNAME}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="공정"                           Binding="{Binding PROCNAME}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="W/O"                            Binding="{Binding WOID}"                HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="집계구분"                       Binding="{Binding ACTID}"               HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="제품ID"                         Binding="{Binding PRODID}"              HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="자재ID"                         Binding="{Binding MTRLID}"              HorizontalAlignment="Center" IsReadOnly="True" />

                                        <c1:DataGridTextColumn Width="Auto" Header="집계일시"                       Binding="{Binding SUM_DTTM}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="실적차수"                       Binding="{Binding RSLT_SEQS}"           HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="전기일"                         Binding="{Binding CALDATE}"             HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="ERP 자재ID"                     Binding="{Binding ERP_IF_MTRLID}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="처리수량"                       Binding="{Binding ACTQTY2}"             HorizontalAlignment="Right"  IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="불량수량"                       Binding="{Binding CNFM_DFCT_QTY2}"      HorizontalAlignment="Right"  IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="LOTTYPE"                        Binding="{Binding LOTTYPE_NM}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="FROM공정"                       Binding="{Binding FROM_PROCID}"         HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="TO공정"                         Binding="{Binding TO_PROCID}"           HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="입고저장위치"                   Binding="{Binding RCV_SLOC_ID}"         HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="출고저장위치"                   Binding="{Binding ISS_SLOC_ID}"         HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="처리저장위치"                   Binding="{Binding PRCS_SLOC_ID}"        HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="FROM_SLOC_ID"                   Binding="{Binding FROM_SLOC_ID}"        HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="TO_SLOC_ID"                     Binding="{Binding TO_SLOC_ID}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="ERP전송W/O"                     Binding="{Binding ERP_TRNF_WOID}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[정상W/O실적처리,ERP전송번호]"  Binding="{Binding ERP_TRNF_SEQNO}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[정상W/O실적처리,ERP전송결과]"  Binding="{Binding ERP_STAT_CODE}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[정상W/O실적처리,ERP처리결과]"  Binding="{Binding ERP_ERR_CODE}"        HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[정상W/O실적처리,ERP오류내용]"  Binding="{Binding ERP_ERR_CAUSE_CNTT}"  HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[정상W/O실적처리,ERP오류유형]"  Binding="{Binding ERP_ERR_TYPE}"        HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[이전전기,ERP전송번호]"         Binding="{Binding ERP_TRNF_SEQNO2}"     HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[이전전기,ERP전송결과]"         Binding="{Binding ERP_STAT_CODE2}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[이전전기,ERP처리결과]"         Binding="{Binding ERP_ERR_CODE2}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[이전전기,ERP오류내용]"         Binding="{Binding ERP_ERR_CAUSE_CNTT2}" HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[이전전기,ERP오류유형]"         Binding="{Binding ERP_ERR_TYPE2}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[PPAR실적처리,ERP전송번호]"     Binding="{Binding ERP_TRNF_SEQNO3}"     HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[PPAR실적처리,ERP전송결과]"     Binding="{Binding ERP_STAT_CODE3}"      HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[PPAR실적처리,ERP처리결과]"     Binding="{Binding ERP_ERR_CODE3}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[PPAR실적처리,ERP오류내용]"     Binding="{Binding ERP_ERR_CAUSE_CNTT3}" HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn Width="Auto" Header="[PPAR실적처리,ERP오류유형]"     Binding="{Binding ERP_ERR_TYPE3}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                    </c1:C1DataGrid.Columns>

                                    <c1:C1DataGrid.TopRows>
                                        <c1:DataGridColumnHeaderRow />
                                        <c1:DataGridColumnHeaderRow />
                                    </c1:C1DataGrid.TopRows>
                                </c1:C1DataGrid>
                            </Grid>
                            <!--grdOutputResultList-->
                        </Grid>
                        <!-- grdOutputResult -->
                    </c1:C1TabItem>

                    <c1:C1TabItem x:Name="tabEndOutputResult" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='완성LOT실적'}">
                        <Grid x:Name="grdEndOutputResult">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="8" />
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="1" x:Name="grdEndOutputSearch">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="35" />
                                </Grid.RowDefinitions>

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>
                                <!-- 백그라운드용 -->
                                <Border                    Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Grid.ColumnSpan="18" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />

                                <!--전기일-->
                                <TextBlock                 Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='전기일', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" />

                                <!--DATE(DATEPICKER)-->
                                <Grid                      Grid.Row="0" Grid.Column="2" HorizontalAlignment="Left">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <ctrlLib:LGCDatePicker  Grid.Row="0" Grid.Column="0" x:Name="dtpDateFromEndOutput" DatepickerType="Date" />
                                    <TextBlock              Grid.Row="0" Grid.Column="1" Text="~" HorizontalAlignment="Center" Style="{StaticResource SearchCondition_LabelStyle}" />
                                    <ctrlLib:LGCDatePicker  Grid.Row="0" Grid.Column="2" x:Name="dtpDateToEndOutput" DatepickerType="Date" />
                                </Grid>

                                <!--동-->
                                <TextBlock                 Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1ComboBox             Grid.Row="0" Grid.Column="5" x:Name="cboEndOutputAreaID" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboEndOutputAreaID_SelectedValueChanged" />

                                <!--라인-->
                                <TextBlock                 Grid.Row="0" Grid.Column="7" Text="{Binding ConverterParameter='Line', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <ctrlLib:MultiSelectionBox Grid.Row="0" Grid.Column="8" x:Name="cboMultiEndOutputEquipmentSegmentID" MinWidth="80" isAllUsed="True" Height="Auto" VerticalAlignment="Center" SelectionChanged="cboMultiEndOutputEquipmentSegmentID_SelectionChanged" />


                                <!--P5200 적용 라인-->
                                <CheckBox                  Grid.Row="0" Grid.Column="10" Grid.ColumnSpan="2" x:Name="chkEndOutputExclFlag" Content="{Binding ConverterParameter='P5200 적용 라인', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" Margin="10,0,0,0" Checked="chkEndOutputP5200ApplyLine_Checked" Unchecked="chkEndOutputP5200ApplyLine_Unchecked" />

                                <!--공정-->
                                <TextBlock                 Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <ctrlLib:MultiSelectionBox Grid.Row="1" Grid.Column="2" x:Name="cboMultiEndOutputProcessID" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" SelectionChanged="cboMultiEndOutputProcessID_SelectionChanged" />

                                <!--실적구분-->
                                <TextBlock                 Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='실적구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <ctrlLib:MultiSelectionBox Grid.Row="1" Grid.Column="5" x:Name="cboMultiEndOutputActID" MinWidth="80" isAllUsed="True" Height="Auto" VerticalAlignment="Center" />

                                <!--LOT 유형-->
                                <TextBlock                 Grid.Row="1" Grid.Column="7" Text="{Binding ConverterParameter='LOTTYPE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <ctrlLib:MultiSelectionBox Grid.Row="1" Grid.Column="8" x:Name="cboMultiEndOutputLotType" MinWidth="80" isAllUsed="True" Height="Auto" VerticalAlignment="Center" />

                                <!--PJT약어-->
                                <TextBlock                 Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <ctrlLib:MultiSelectionBox Grid.Row="1" Grid.Column="11" x:Name="cboMultiEndOutputProjectAbbrName" MinWidth="80" isAllUsed="True" Height="Auto" VerticalAlignment="Center" />

                                <!--ERP IF 자재ID-->
                                <TextBlock                 Grid.Row="1" Grid.Column="13" Text="{Binding ConverterParameter='ERP I/F 자재ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <TextBox                   Grid.Row="1" Grid.Column="14" x:Name="txtEndOutputErpInterfaceMaterialID" Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtEndOutputERPInterfaceMaterialID_KeyDown"  MinWidth="120" />

                                <!--조회 버튼-->
                                <Button                    Grid.Row="0" Grid.Column="16" Grid.RowSpan="2" x:Name="btnEndOutputSearch" Content="{Binding ConverterParameter=Search, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"
                                                           Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnEndOutputSearch_Click" />
                            </Grid>

                            <Grid Grid.Row="3" x:Name="grdEndOutputResultList">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="35" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="6*" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="4*" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border        Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}" />
                                <Border        Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                <TextBlock     Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}">
                                    <Run Text="{Binding ConverterParameter='Summary', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" />
                                </TextBlock>
                                <TextBlock     Grid.Row="1" Grid.Column="0" x:Name="txtEndOutputResultCount" Text="[ 0 건 ]" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" VerticalAlignment="Center" />
                                <c1:C1DataGrid Grid.Row="2" Grid.Column="0" x:Name="dgListEndOutputResult" HeadersVisibility="None" FrozenTopRowsCount="2" SelectionMode="SingleRow"
                                               MouseDoubleClick="dgListEndOutputResult_MouseDoubleClick"
                                               LoadedCellPresenter="dgListEndOutputResult_LoadedCellPresenter"
                                               MergingCells="dgListEndOutputResult_MergingCells"
                                               LoadedRowPresenter="dgListEndOutputResult_LoadedRowPresenter">

                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn Width="Auto" Header="전기일"     Binding="{Binding CALDATE}"           HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn Width="Auto" Header="전기일"     Binding="{Binding CALDATENAME}"       HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="라인"       Binding="{Binding EQSGID}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn Width="Auto" Header="라인"       Binding="{Binding EQSGNAME}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="공정"       Binding="{Binding PROCID}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn Width="Auto" Header="공정"       Binding="{Binding PROCNAME}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="W/O"        Binding="{Binding WOID}"              HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn Width="Auto" Header="W/O"        Binding="{Binding WONAME}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="실적구분"   Binding="{Binding ACTID}"             HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn Width="Auto" Header="실적구분"   Binding="{Binding ACTNAME}"           HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="PROG_SEQS"  Binding="{Binding PROG_SEQS}"         HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn Width="Auto" Header="RSLT_SEQS"  Binding="{Binding RSLT_SEQS}"         HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn Width="Auto" Header="ERP 자재ID" Binding="{Binding PRODID}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn Width="Auto" Header="ERP 자재ID" Binding="{Binding PRODNAME}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="LOTTYPE"    Binding="{Binding LOTTYPE}"           HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn Width="Auto" Header="LOTTYPE"    Binding="{Binding LOTYNAME}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="PJT"        Binding="{Binding PROJECT_ABBR}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn Width="Auto" Header="PJT"        Binding="{Binding PROJECT_ABBR_NAME}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="처리수량"   Binding="{Binding ACTQTY}"            HorizontalAlignment="Right"  IsReadOnly="True" Visibility="Visible" />
                                    </c1:C1DataGrid.Columns>

                                    <c1:C1DataGrid.TopRows>
                                        <c1:DataGridColumnHeaderRow />
                                        <c1:DataGridColumnHeaderRow />
                                    </c1:C1DataGrid.TopRows>
                                </c1:C1DataGrid>

                                <!-- Splitter -->
                                <GridSplitter Grid.Row="0" Grid.Column="1" Grid.RowSpan="3" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                <Border        Grid.Row="0" Grid.Column="2" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                <TextBlock     Grid.Row="0" Grid.Column="2" Style="{StaticResource Content_ModuleTitleStyle}">
                                    <Run Text="{Binding ConverterParameter='Detail', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" />
                                </TextBlock>
                                <Grid Grid.Row="1" Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="8" />
                                    </Grid.ColumnDefinitions>

                                    <TextBlock     Grid.Column="0" x:Name="txtEndOutputDetailCount" Text="[ 0 건 ]" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" VerticalAlignment="Center" />
                                    <Button        Grid.Column="2" x:Name="btnExcel"     Content="{Binding ConverterParameter=Excel,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelExportStyle}" HorizontalAlignment="Right" Click="btnExcel_Click"  />
                                </Grid>
                                <c1:C1DataGrid Grid.Row="2" Grid.Column="2" x:Name="dgListEndOutputDetail" HeadersVisibility="None" FrozenTopRowsCount="2" SelectionMode="SingleRow">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn Width="Auto" Header="전기일"     Binding="{Binding CALDATE}"   HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="LOTID"      Binding="{Binding LOTID}"     HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="ERP 자재ID" Binding="{Binding PRODID}"    HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="W/O"        Binding="{Binding WOID}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="진행차수"   Binding="{Binding PROG_SEQS}" HorizontalAlignment="Right"  IsReadOnly="True" Visibility="Visible" />
                                        <c1:DataGridTextColumn Width="Auto" Header="실적차수"   Binding="{Binding RSLT_SEQS}" HorizontalAlignment="Right"  IsReadOnly="True" Visibility="Visible" />
                                    </c1:C1DataGrid.Columns>

                                    <c1:C1DataGrid.TopRows>
                                        <c1:DataGridColumnHeaderRow />
                                        <c1:DataGridColumnHeaderRow />
                                    </c1:C1DataGrid.TopRows>
                                </c1:C1DataGrid>
                            </Grid>
                            <!--grdEndOutputResultList-->
                        </Grid>
                        <!-- grdEndOutputResult -->
                    </c1:C1TabItem>
                </c1:C1TabControl>
            </Grid>
            <!-- grdLayout -->
        </ctrlLib:LGCLayout>
        <ctrlLib:LoadingIndicator x:Name="loadingIndicator"/>
    </Grid>
    <!-- grdMain -->
</UserControl>
