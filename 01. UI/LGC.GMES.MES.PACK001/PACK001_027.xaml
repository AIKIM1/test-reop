<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.PACK001.PACK001_027"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="공정진척(포장)_포장 출고 및 이력 조회">
            <Grid>

                <c1:C1TabControl  Grid.Row="0">
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='포장출고'}" Margin="0" >

                        <Grid Grid.Row="0">

                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="138"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <Border Grid.Row="1" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                            <Grid x:Name="InfoArea" Grid.Row="1">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="13"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="13"/>
                                </Grid.ColumnDefinitions>

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="14" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="10" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="10" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="14" />
                                </Grid.RowDefinitions>

                                <Grid Grid.Row="1" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="150"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock   Grid.Row="0" Grid.Column="0"    Text="{Binding ConverterParameter='PALLETID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                        Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox Grid.Row="0" Grid.Column="1"  x:Name="txtPalletID" Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="100" KeyDown="txtPalletID_KeyDown" />

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='PALLET수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox Grid.Row="0" Grid.Column="1" x:Name="txtPalletQty" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"  MinWidth="100" />

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock  Grid.Row="0" Grid.Column="0"  Text="{Binding ConverterParameter='총제품수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <TextBox Grid.Row="0" Grid.Column="1" x:Name="txtCellQty" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" MinWidth="100" />

                                </Grid>


                                <Grid Grid.Row="3" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="1"  Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="1"  Grid.Column="1" x:Name="cboAreaAll" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                </Grid>

                                <Grid Grid.Row="3" Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="1"  Grid.Column="0" Text="{Binding ConverterParameter='출고창고', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="1"  Grid.Column="1" x:Name="cboLocFrom" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                </Grid>

                                <Grid Grid.Row="3" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="1"  Grid.Column="0" Text="{Binding ConverterParameter='입고창고', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="1"  Grid.Column="1" x:Name="cboLocTo" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="3" Grid.Column="4">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="1"  Grid.Column="0" Text="{Binding ConverterParameter='출하지', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="1"  Grid.Column="1" x:Name="cboComp" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>

                                <Grid Grid.Row="5" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="5*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock  Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='작업자', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" Visibility="Hidden" />
                                    <TextBox    Grid.Row="0" Grid.Column="1" x:Name="txtUserID" Style="{StaticResource SearchCondition_TextBoxStyle}"  MinWidth="100" IsReadOnly="True" Visibility="Hidden"/>
                                    <Button     Grid.Row="0" Grid.Column="2" x:Name="btnWorker"    Content="{Binding ConverterParameter=Title17,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_SearchButtonStyle}" Click="btnWorker_Click" Visibility="Hidden"/>
                                </Grid>

                                <Grid Grid.Row="5" Grid.Column="3" Grid.ColumnSpan="2" HorizontalAlignment="Right">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>

                                    <Button     Grid.Row="0" Grid.Column="0" x:Name="btnFileReg"   Content="{Binding ConverterParameter=PALLETID파일등록,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                    Style="{DynamicResource Content_MainButtonStyle}" Click="btnFileReg_Click"/>

                                    <!--<Button     Grid.Row="0" Grid.Column="7" x:Name="btnDelete"   Content="{Binding ConverterParameter=삭제,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                    Style="{DynamicResource Content_MainButtonStyle}" Click="btnDelete_Click"/>-->

                                    <Button     Grid.Row="0" Grid.Column="2" x:Name="btnRefresh"   Content="{Binding ConverterParameter=초기화,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                    Style="{DynamicResource Content_MainButtonStyle}" Click="btnRefresh_Click"/>

                                    <Button     Grid.Row="0" Grid.Column="4" x:Name="btnPackOut"   Content="{Binding ConverterParameter=포장출고,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                    Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnPackOut_Click"/>

                                </Grid>



                            </Grid>

                            <Grid x:Name="TitleArea" Grid.Row="3">
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회결과', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                            </Grid>

                            <Grid x:Name="OutArea" Grid.Row="4">
                                <Border        Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                                <c1:C1DataGrid x:Name="dgPackOut" IsReadOnly="True" HeadersVisibility="Column" Margin="8" CanUserRemoveRows="True" CanUserAddRows="True"
                                               SelectionMode="MultiRange" Grid.ColumnSpan="2">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn Header = "PALLETID"       Binding = "{Binding BOXID}"         HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "Project"        Binding = "{Binding PROJECTNAME}"   HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "제품ID"           Binding = "{Binding PRODID}"        HorizontalAlignment="Center"/>
                                        <!--<c1:DataGridTextColumn Header = "제품설명"       Binding = "{Binding PRODDESC}"      HorizontalAlignment="Center"/>-->
                                        <c1:DataGridTextColumn Header = "수량"           Binding = "{Binding TOTAL_QTY}"     HorizontalAlignment="Center" Format="#,##0"/>
                                        <c1:DataGridTextColumn Header = "OCOP_RTN_CNT"           Binding = "{Binding OCOP_RTN_CNT}"     HorizontalAlignment="Center" Format="#,##0"/>
                                        <c1:DataGridTextColumn Header = "비고"           Binding = "{Binding PACK_NOTE}"     HorizontalAlignment="Center"/>
                                        <c1:DataGridTextColumn Header = "BOXTYPE"        Binding = "{Binding BOXTYPE}"       HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "INNER_BOXTYPE"  Binding = "{Binding INNER_BOXTYPE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "TO_SLOC_ID"     Binding = "{Binding TO_SLOC_ID}"    HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "BOXSTAT"        Binding = "{Binding BOXSTAT}"       HorizontalAlignment="Center" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn Header = "고객사PLTID"    Binding = "{Binding OUTER_BOXID2}"  HorizontalAlignment="Left"   Width="Auto" />

                                        <c1:DataGridTemplateColumn Name="Delete" Width="Auto" Header="삭제">
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button  x:Name="btnDelete" Width="Auto" Content="{Binding ConverterParameter=삭제, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                                 Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDelete_Click"/>
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>

                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>

                        </Grid>

                    </c1:C1TabItem>

                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='출고이력조회'}" Margin="0" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="45"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Border Grid.Row="1" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                            <!--조회 조건 영역-->
                            <Grid   Grid.Row="1" Grid.Column="0" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="13"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="13"/>
                                </Grid.ColumnDefinitions>
                                <!--기간-->
                                <Grid Grid.Row="0" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='기간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                                    Style="{StaticResource Content_InputForm_LabelStyle}" />

                                    <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1">
                                        <!--<DatePicker  x:Name="dtpDateFrom" />-->
                                        <ctrlLib:LGCDatePicker x:Name="dtpDateFrom" DatepickerType="Date" />
                                        <TextBlock  Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0"/>
                                        <!--<DatePicker  x:Name="dtpDateTo" />-->
                                        <ctrlLib:LGCDatePicker x:Name="dtpDateTo" DatepickerType="Date"/>
                                    </StackPanel>

                                </Grid>
                                <!--동-->
                                <Grid Grid.Row="0" Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                    <c1:C1ComboBox  Grid.Row="0"  Grid.Column="1" x:Name="cboAreaAll2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>

                                </Grid>
                                <!--팔레트/출고ID-->
                                <Grid Grid.Row="0" Grid.Column="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="5*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='팔레트/출고 ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                        Style="{StaticResource Content_InputForm_LabelStyle}" />

                                    <TextBox Grid.Row="0" Grid.Column="1"  x:Name="txtBoxID" Style="{StaticResource SearchCondition_TextBoxStyle}" />


                                </Grid>
                                <!--버튼-->
                                <Grid Grid.Row="0" Grid.Column="4" HorizontalAlignment="Right" >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="4"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>

                                    <Button     Grid.Row="0" Grid.Column="0" x:Name="btnCreate"   Content="{Binding ConverterParameter=출고파일생성,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                    Style="{DynamicResource Content_MainButtonStyle}" Click="btnCreate_Click" Visibility="Collapsed"/>

                                    <Button     Grid.Row="0" Grid.Column="2" x:Name="btnPrint"   Content="{Binding ConverterParameter=출고TAG재발행,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                    Style="{DynamicResource Content_MainButtonStyle}" Click="btnPrint_Click" />
                                    <Button     Grid.Row="0" Grid.Column="4" x:Name="btnCancel" Content="{Binding ConverterParameter=출고취소,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                Style="{DynamicResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnCancel_Click" />
                                    <Button     Grid.Row="0" Grid.Column="6" x:Name="btnSearch"   Content="{Binding ConverterParameter=SEARCH,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" 
                                                    Style="{StaticResource SearchButtonStyle}" Click="btnSearch_Click"/>


                                </Grid>
                            </Grid>
                            <!--조회 결과-->
                            <Grid x:Name="GridArea" Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="7*" />
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="3*" />
                                </Grid.ColumnDefinitions>

                                <!--왼쪽-->
                                <!-- 1Row -->
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회결과', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <!-- 2Row -->
                                <Grid Grid.Row="1" Grid.RowSpan="2">
                                    <Grid   Grid.Row="1" Grid.Column="1" >
                                        <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                        <c1:C1DataGrid x:Name="dgOutHist" IsReadOnly="True" HeadersVisibility="Column" Margin="8" CanUserSort="True" CanUserFilter="True"
                                               SelectionMode="MultiRange" Grid.ColumnSpan="2">
                                            <c1:C1DataGrid.Columns>
                                                <!--<c1:DataGridTextColumn Header = "선택"          Binding = "{Binding NUMBER}" HorizontalAlignment="Center"/>-->

                                                <!--<c1:DataGridTemplateColumn Name="CHK" Width="50" Header="선택" >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" Click="CheckBox_Click_1" />
                                                        </DataTemplate>
                                                    </c1:DataGridTemplateColumn.CellTemplate>
                                                </c1:DataGridTemplateColumn>-->

                                                <c1:DataGridTemplateColumn  Header="선택" EditOnSelection="True" Name="CHK" Width="Auto" HorizontalAlignment="Center" >
                                                    <c1:DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <RadioButton GroupName="dgOutHist_ChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgOutHist_Choice" HorizontalAlignment="Center" Checked="dgOutHist_Choice_Checked"  />
                                                        </DataTemplate>
                                                    </c1:DataGridTemplateColumn.CellTemplate>
                                                </c1:DataGridTemplateColumn>

                                                <c1:DataGridTextColumn Header = "출고LOTID"     Binding = "{Binding RCV_ISS_ID}"        HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "출고상태"      Binding = "{Binding RCV_ISS_STAT_NAME}" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "출고상태"      Binding = "{Binding RCV_ISS_STAT_CODE}" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn Header = "제품ID"        Binding = "{Binding PRODID}"            HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "Project"       Binding = "{Binding PROJECTNAME}"       HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "모델"          Binding = "{Binding MODLID}"            HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "수량"          Binding = "{Binding TOTALQTY}"          HorizontalAlignment="Center" Format="#,##0"/>
                                                <c1:DataGridTextColumn Header = "PALLET수량"    Binding = "{Binding PALLETQTY}"         HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "출하지"        Binding = "{Binding SHIPTO_NAME}"       HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "출고일시"      Binding = "{Binding INSDTTM}"           HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "출고창고"      Binding = "{Binding FROM_SLOC_ID}"      HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "입고창고"      Binding = "{Binding TO_SLOC_ID}"        HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "작업자"        Binding = "{Binding INSUSERNAME}"       HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "BOXTYPE"       Binding = "{Binding BOXTYPE}"           HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            </c1:C1DataGrid.Columns>
                                        </c1:C1DataGrid>
                                    </Grid>
                                </Grid>
                                 
                                <!--오른쪽-->
                                <!--1Row-->
                                <Border Grid.Row="0" Grid.Column="2" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <TextBlock Grid.Row="0" Grid.Column="2" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='구성Pallet조회', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <!--2Row-->
                                <Border Grid.Row="1" Grid.Column="2" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                                <Grid Grid.Row="1" Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="7*" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="8"/>
                                    </Grid.ColumnDefinitions>
                                    <Button Grid.Row="0" Grid.Column="1" x:Name="btnExcel" Content="{Binding ConverterParameter=EXCEL,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelExportStyle}" Click="btnExcel_Click"/>
                                </Grid>
                                <!--3Row-->
                                <Grid Grid.Row="2" Grid.Column="2">
                                    <!--<Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="8"/>
                                    </Grid.ColumnDefinitions>-->
                                    <Grid   Grid.Row="1" Grid.Column="1" >
                                        <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                        <c1:C1DataGrid x:Name="dgOutDetail" IsReadOnly="True" HeadersVisibility="Column" Margin="8"
                                               SelectionMode="MultiRange" Grid.ColumnSpan="2">
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridTextColumn Header = "제품ID"      Binding = "{Binding PRODID}"     HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn Header = "수량"        Binding = "{Binding TOTAL_QTY}"  HorizontalAlignment="Center" Format="#,##0"/>
                                                <c1:DataGridTextColumn Header = "PALLETID"    Binding = "{Binding BOXID}"      HorizontalAlignment="Center"/>
                                                <!--2018.07.02-->
                                                <c1:DataGridTextColumn Header = "LOTID"    Binding = "{Binding LOTID}"      HorizontalAlignment="Center"/>
                                                <!--2022.07.04-->
                                                <c1:DataGridTextColumn Header = "OCOP_RTN_FLAG"    Binding = "{Binding OCOP_RTN_FLAG}"      HorizontalAlignment="Center"/>
                                            </c1:C1DataGrid.Columns>
                                        </c1:C1DataGrid>
                                    </Grid>

                                </Grid>

                            </Grid>

                        </Grid>
                    </c1:C1TabItem>

                    <c1:C1TabItem x:Name="ShippingHistoryGBT" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='출고이력 GBT'}" Margin="0" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Border Grid.Row="1" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                            <!--조회 조건 영역-->
                            <Grid   Grid.Row="1" Grid.Column="0" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                
                                <!--기간-->
                                <Grid Grid.Row="0" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="2*"/>
                                        <ColumnDefinition Width="6*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock      Grid.Row="0"  Grid.Column="0" Text="{Binding ConverterParameter='기간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" 
                                                    Style="{StaticResource Content_InputForm_LabelStyle}" />

                                    <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1">
                                        <ctrlLib:LGCDatePicker x:Name="dtpDateFrom1" DatepickerType="Date" />
                                        <TextBlock  Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0"/>
                                        <ctrlLib:LGCDatePicker x:Name="dtpDateTo1" DatepickerType="Date"/>
                                    </StackPanel>

                                </Grid>
