<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.PACK001.PACK001_018"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1280" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/C1DataGridStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="LOT 정보 조회(PACK)">
            <c1:C1TabControl x:Name="tabControlMain">
                <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='현재LOT정보'}" >
                    <Grid x:Name="Content">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0"/>
                            <RowDefinition Height="100"/>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="34"/>
                            <RowDefinition Height="34"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Border Grid.Row="2" Grid.Column="1" Grid.RowSpan="4" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>
                        <!--<Border Grid.Row="8" Grid.Column="1" Grid.RowSpan="3" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>-->

                        <!--<Border Grid.Row="1" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />-->
                        <Border Grid.Row="1" Grid.Column="1" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>
                        <!--<Border Grid.Row="7" Grid.Column="1" Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>-->
                        <!--<Border Grid.Row="7" Grid.Column="1" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"></Border>-->

                        <!--1ROW-->
                        <Grid x:Name="ContentTop" Grid.Row="1" Grid.Column="1" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="15"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="25*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="25*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="20*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="20*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="20*"/>
                                <ColumnDefinition Width="18"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="15"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8"/>
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="15"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="20"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>

                                <!--<StackPanel Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Right" >
                                <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}"  Margin="10,0,-10,0"/>
                                <TextBlock Text="{Binding ConverterParameter='생성일시', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                                <c1:C1ComboBox     Grid.Row="0"     Grid.Column="0" x:Name="cboPalletConfig" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="KEY" SelectedValuePath="VALUE" SelectedIndex="0"/>
                            </StackPanel>-->
                                <c1:C1ComboBox     Grid.Row="0"     Grid.Column="0" x:Name="cboDateConfig" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="KEY" SelectedValuePath="VALUE" SelectedIndex="0"/>
                                <ctrlLib:LGCDatePicker  Grid.Column="1" x:Name="dtpDateFrom" />
                                <c1:C1ComboBox Grid.Column="2" x:Name="cboTimeFrom" Height="21" MinWidth="40" Width="Auto" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                                <ctrlLib:LGCDatePicker  Grid.Column="4" x:Name="dtpDateTo" DatepickerType="Date" />
                                <c1:C1ComboBox Grid.Column="5" x:Name="cboTimeTo" Height="21" MinWidth="40" Width="Auto" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                                <CheckBox               Grid.Column="7" x:Name="chkToday" Content="{Binding ConverterParameter='현재일자',     Converter={StaticResource GlobalObjectDicConverter}}"  Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" Checked="chkToday_Checked" Unchecked="chkToday_Unchecked"/>

                            </Grid>

                            <StackPanel Grid.Row="1" Grid.Column="5" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}" Margin="10,0,-10,0"/>
                                <TextBlock Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                            </StackPanel>
                            <c1:C1ComboBox  Grid.Row="1"   Grid.Column="6" x:Name="cboAreaByAreaType" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />


                            <StackPanel  Grid.Row="1"  Grid.Column="7" Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBlock Style="{StaticResource SearchCondition_MandatoryMarkStyle}" Margin="10,0,-10,0"/>
                                <TextBlock Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                            </StackPanel>
                            <c1:C1ComboBox  Grid.Row="1"      Grid.Column="8" x:Name="cboEquipmentSegment" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>

                            <TextBlock      Grid.Row="1"      Grid.Column="9" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                            <c1:C1ComboBox  Grid.Row="1"       Grid.Column="10" x:Name="cboProcess" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>

                            <Button   Grid.Row="1" Grid.Column="12" x:Name="btnSearch" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click"/>


                            <TextBlock     Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='모델', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                            <c1:C1ComboBox Grid.Row="2" Grid.Column="2" x:Name="cboProductModel" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                            <TextBlock     Grid.Row="2" Grid.Column="3" Text="{Binding ConverterParameter='제품분류', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                            <c1:C1ComboBox Grid.Row="2" Grid.Column="4" x:Name="cboPrdtClass" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                            <TextBlock     Grid.Row="2" Grid.Column="5" Text="{Binding ConverterParameter='제품', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                            <c1:C1ComboBox Grid.Row="2" Grid.Column="6" x:Name="cboProduct" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                            <TextBlock     Grid.Row="2" Grid.Column="7" Text="{Binding ConverterParameter='작업조', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                            <c1:C1ComboBox Grid.Row="2" Grid.Column="8" x:Name="cboShift" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                            <TextBlock     Grid.Row="2" Grid.Column="9" Text="{Binding ConverterParameter='상태', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right"/>
                            <ctrlLib:MultiSelectionBox  Grid.Row="2" Grid.Column="10" x:Name="cboWipState" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                        </Grid>

                        <!--2ROW-->
                        <Border Grid.Row="3" Grid.ColumnSpan="3" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                        <TextBlock     Grid.Row="3" Grid.Column="1" Style="{StaticResource Content_ModuleTitleStyle}">
                                    <Run Text="{Binding ConverterParameter='조회 결과', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/>
                        </TextBlock>

                        <Grid Grid.Row="4" Grid.Column="1" x:Name="gdContentTitle">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="290"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="15"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="290"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="8"/>
                            </Grid.ColumnDefinitions>
                            <ToggleButton   Grid.Column="1"  x:Name="btnLeft" Style="{StaticResource btnLeftTopConerStyleHorizontal}" IsChecked="False" Checked="btnLeft_Checked" Unchecked="btnLeft_Unchecked" Margin="0,5"/>
                            <TextBlock x:Name="tbLotListCount" Grid.Column="3" Text="[0건]" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,0,5"/>
                            <TextBlock  Grid.Column="5"  Text="{Binding ConverterParameter='최신', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Right" Margin="3,3,0,5" VerticalAlignment="Bottom"/>
                            <c1:C1ComboBox Grid.Column="6" x:Name="cboListCount" Height="18" MinWidth="50" Width="Auto" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" SelectedValueChanged="cboListCount_SelectedValueChanged" VerticalAlignment="Bottom"/>
                            <TextBlock  Grid.Column="7"  Text="{Binding ConverterParameter='건', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"  Margin="0,3,0,5" VerticalAlignment="Bottom"/>
                            <Button   Grid.Column="9" x:Name="btnExcel"  Content="{Binding ConverterParameter=EXCEL,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelExportStyle}" Click="btnExcel_Click"/>
                            <ToggleButton   Grid.Column="11"  x:Name="btnRight" Style="{StaticResource btnLeftTopConerStyleHorizontal}" IsChecked="True" Checked="btnRight_Checked" Unchecked="btnRight_Unchecked" Margin="0,5"/>
                        </Grid>

                        <Grid x:Name="gdContent" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="300"/>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="300"/>
                            </Grid.ColumnDefinitions>

                            <Border Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_InputForm_GroupBorderStyle}" BorderThickness="1,0,1,1"/>

                            <Grid Grid.Column="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="2"/>
                                    <RowDefinition Height="40"/>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="8"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" Style="{StaticResource Condition_Header_BorderStyle}"/>

                                <Button Grid.Row="1" Grid.Column="3" x:Name="btnSearch_id" Content="{Binding ConverterParameter='SEARCH', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_id_Click"  Margin="5"/>

                                <Border Grid.Row="2" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,0">
                                    <TextBlock  Text="{Binding ConverterParameter='ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                    <!--<c1:C1ComboBox  x:Name="cboID_Type" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="KEY" SelectedValuePath="VALUE" SelectedIndex="0"/>-->
                                </Border>
                                <Border Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,1,0">
                                    <TextBox x:Name="txtIDInput"     Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtIDInput_KeyDown" />
                                </Border>

                                <Border Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,0">
                                    <TextBlock  Text="{Binding ConverterParameter='ID목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold"  HorizontalAlignment="Center" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                </Border>
                                <Border Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,1,0">
                                    <RichTextBox   Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="2"  x:Name="txtNote_IDList" Margin="3" ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Auto" >
                                        <FlowDocument LineHeight="1" />
                                    </RichTextBox>
                                </Border>
                            </Grid>

                            <!--3ROW-->
                            <c1:C1DataGrid Grid.Column="2" x:Name="dgLotList" IsReadOnly="False" HeadersVisibility="Column" SelectionMode="MultiRange" MouseUp="dgLotList_MouseUp" MouseDoubleClick="dgLotList_MouseDoubleClick" LoadedCellPresenter="dgLotList_LoadedCellPresenter" LoadedColumnHeaderPresenter="dgLotList_LoadedColumnHeaderPresenter">
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridCheckBoxColumn                              Binding = "{Binding CHK}"  Width="50"/>
                                    <c1:DataGridTextColumn        Header = "LOT ID"         Binding = "{Binding LOTID}"     IsReadOnly="True"    HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "AREAID"         Binding = "{Binding AREAID}"    IsReadOnly="True" HorizontalAlignment="Center" Width="Auto"  Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "동"             Binding = "{Binding AREANAME}"  IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "EQSGID"         Binding = "{Binding EQSGID}"    IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "Line"           Binding = "{Binding EQSGNAME}"  IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "PROCID"         Binding = "{Binding PROCID}"    IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "공정"           Binding = "{Binding PROCNAME}"  IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "EQPTID"         Binding = "{Binding EQPTID}"    IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "설비"           Binding = "{Binding EQPTNAME}"  IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "작업조"         Binding = "{Binding SHIFT}"     IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "MODLID"         Binding = "{Binding MODLID}"    IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed" />
                                    <c1:DataGridTextColumn        Header = "모델"           Binding = "{Binding MODLNAME}"  IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "PRDT_CLSS_CODE" Binding = "{Binding PRDT_CLSS_CODE}" IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed"/>
                                    <c1:DataGridTextColumn        Header = "제품분류"       Binding = "{Binding PRDT_CLSS_CODE}" IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "제품ID"         Binding = "{Binding PRODID}"       IsReadOnly="True"   HorizontalAlignment="Center" Width="Auto" />
                                    <!--<c1:DataGridTextColumn        Header = "제품명"         Binding = "{Binding PRODNAME}"    IsReadOnly="True"  HorizontalAlignment="Center" Width="Auto" />-->
                                    <c1:DataGridTextColumn        Header = "제품설명"       Binding = "{Binding PRODDESC}"    IsReadOnly="True"  HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "WIPSTAT"        Binding = "{Binding WIPSTAT}"     IsReadOnly="True"  HorizontalAlignment="Center" Width="Auto" Visibility="Collapsed" />
                                    <c1:DataGridTextColumn        Header = "상태"           Binding = "{Binding WIPSNAME}"    IsReadOnly="True"  HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "생성일시"       Binding = "{Binding LOTDTTM_CR}"  IsReadOnly="True"  HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "변경일시"       Binding = "{Binding WIPSDTTM}"    IsReadOnly="True"  HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "Hold 여부"      Binding = "{Binding WIPHOLD}"     IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "Hold 일시"      Binding = "{Binding WIPDTTM_HD}"  IsReadOnly="True"    HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "폐기여부"       Binding = "{Binding WIPSCRP}"     IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "폐기일시"       Binding = "{Binding SCRP_DTTM}"   IsReadOnly="True"   HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "비용구분"       Binding = "{Binding WRC_ACTID}"     IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "고객사제품번호" Binding = "{Binding CUST_LOTID}"   IsReadOnly="True"   HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "VALIDATION"     Binding = "{Binding LOT_VALID}"    IsReadOnly="True"   HorizontalAlignment="Center" Width="Auto" />
                                    <c1:DataGridTextColumn        Header = "작업지시"       Binding = "{Binding WOID}"     IsReadOnly="True" HorizontalAlignment="Center" Width="Auto" />
                                </c1:C1DataGrid.Columns>
                            </c1:C1DataGrid>

                            <Border Grid.Row="0" Grid.Column="4" Style="{StaticResource Content_InputForm_GroupBorderStyle}" BorderThickness="1,0,1,1"/>
                            <Grid Grid.Column="4">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="2"/>
                                    <RowDefinition Height="40"/>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="8"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" Style="{StaticResource Condition_Header_BorderStyle}"/>

                                <Button    Grid.Row="1" Grid.Column="3" x:Name="btnLabel"      Content="{Binding ConverterParameter=출력,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonStyle}" Click="btnLabel_Click" />

                                <Border Grid.Row="2" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,0">
                                    <TextBlock  Text="{Binding ConverterParameter='ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                </Border>
                                <Border Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,1,0">
                                    <TextBox x:Name="txtPrintIDInput"     Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtPrintIDInput_KeyDown" />
                                </Border>
                                <Border Grid.Row="3" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,0">
                                    <TextBlock  Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                </Border>
                                <Border Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,1,0">
                                    <TextBox   Grid.Column="4"   x:Name="txtLabelProdID" IsReadOnly="True"    Style="{StaticResource SearchCondition_TextBoxStyle}" />
                                </Border>
                                <Border Grid.Row="4" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,0">
                                    <TextBlock  Text="{Binding ConverterParameter='LABELCODE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                </Border>
                                <Border Grid.Row="4" Grid.Column="2" Grid.ColumnSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,0,1">
                                    <c1:C1ComboBox  Grid.Column="8" x:Name="cboLabelCode" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                                </Border>
                                <Border Grid.Row="5" Grid.Column="1" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,0">
                                    <TextBlock  Text="{Binding ConverterParameter='DPI', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold"  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="15,0,9,0"/>
                                </Border>
                                <Border Grid.Row="5" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="1,1,0,1" Grid.ColumnSpan="2">
                                    <c1:C1ComboBox  Grid.Column="8" x:Name="cboLabelDPI" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>
                                </Border>
                                <!--<Border Grid.Row="5" Grid.Column="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}" BorderThickness="0,1,1,1" >
                                <Button    Grid.Row="1" Grid.Column="3" x:Name="btnLabel_view"      Content="{Binding ConverterParameter=미리보기,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthStyle}" Click="btnLabel_view_Click" Margin="0,0,3,0" />
                            </Border>-->

                                <Border Grid.Row="7" Grid.Column="1" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_LabelBorderStyle}" BorderThickness="1,1,0,0">
                                    <TextBlock  Text="{Binding ConverterParameter='ID목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" FontWeight="Bold"  HorizontalAlignment="Center" VerticalAlignment="Center" Margin="15,0,9,0"/>
