                                                <c1:DataGridTextColumn Header = "제품ID"          Binding = "{Binding PRODID}"                VerticalAlignment="Center"   HorizontalAlignment="Center"  Width="150" />
                                                <c1:DataGridTextColumn Header = "제품명"          Binding = "{Binding PROCNAME}"              VerticalAlignment="Center"   HorizontalAlignment="Center"  Width="150" />
                                                <c1:DataGridTextColumn Header = "색상"            Binding = "{Binding COLOR_FLAG}"            VerticalAlignment="Center"   HorizontalAlignment="Center"  Width="150" Visibility="Collapsed"/>
                                            </c1:C1DataGrid.Columns>
                                        </c1:C1DataGrid>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>
                    </c1:C1TabItem>
