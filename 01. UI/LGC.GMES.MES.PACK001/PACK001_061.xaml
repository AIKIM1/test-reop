                                                </Grid>
                                                <Grid Grid.Column="1" Background="LightGray">
                                                    <TextBlock Grid.Row="1" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center"><Run Text="{Binding ConverterParameter=미만일, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                                </Grid>
                                                <Grid Grid.Column="2" Background="YellowGreen">
                                                    <TextBlock Grid.Row="1" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center"><Run Text="{Binding ConverterParameter=당일, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                                </Grid>
