                                    <Grid Grid.Column="1">

                                        <c1:C1DataGrid x:Name="dgListSnapStock"  IsReadOnly="True" HeadersVisibility="Column" CanUserRemoveRows="True" CanUserAddRows="True" AlternatingRowBackground="White" SelectionMode="MultiRange" >
