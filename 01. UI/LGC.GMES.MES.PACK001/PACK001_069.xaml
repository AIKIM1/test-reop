                                                <c1:DataGridTextColumn      Width="Auto" Header = "HOLD 여부"            Binding = "{Binding WIP_HOLD_FLAG}"                  HorizontalAlignment="Center"     IsReadOnly="True" Visibility="Collapsed"/>
                                            </c1:C1DataGrid.Columns>
                                        </c1:C1DataGrid>
                                    </Grid>
                                </Grid>
                            </Grid>

                            <Grid Grid.Row="5">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <TextBlock Grid.Column="0" x:Name="tblExcludeNote_SNAP" Text="{Binding ConverterParameter='비고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                
                                <RichTextBox  Grid.Column="2" x:Name="txtExcludeNote_SNAP" Margin="3">
                                  <FlowDocument LineHeight="1" />
                                </RichTextBox>
