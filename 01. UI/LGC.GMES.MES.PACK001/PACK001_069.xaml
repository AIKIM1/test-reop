                                                <c1:DataGridTextColumn      Width="Auto" Header = "PRJT_NICKNAME"          Binding = "{Binding PRJT_NAME}"                      HorizontalAlignment="Center"     IsReadOnly="True"/>
                                                <c1:DataGridTextColumn      Width="Auto" Header = "모델"                Binding = "{Binding MODELID}"                        HorizontalAlignment="Center"     IsReadOnly="True"/>
                                                <c1:DataGridTextColumn      Width="Auto" Header = "제품ID"               Binding = "{Binding PRODID}"                         HorizontalAlignment="Center"     IsReadOnly="True"/>
                                                <c1:DataGridTextColumn      Width="Auto" Header = "LOT 유형"             Binding = "{Binding LOTTYPE}"                        HorizontalAlignment="Center"     IsReadOnly="True"/>
