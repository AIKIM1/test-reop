                                                                  BorderBrush="Black" BorderThickness="1" />-->

                                            <RichTextBox        Grid.Row="1" Grid.Column="1" x:Name="txtTextProd" Height="100" BorderThickness="1" BorderBrush="#FFC3C3C3" Margin="3">
                                                <RichTextBox.Resources>
                                                    <Style TargetType="{x:Type Paragraph}">
                                                        <Setter Property="Margin" Value="5"/>
                                                    </Style>
                                                </RichTextBox.Resources>
                                                <FlowDocument LineHeight="10" >
                                                    <Paragraph FontSize="10pt" LineHeight="10pt"  LineStackingStrategy="BlockLineHeight" Tag="TEST">
