                                <TextBlock              Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='제외 유무', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="7" x:Name="cboExcludeYNTp2" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />

                                <TextBlock              Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='기준월', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:LGCDatePicker  Grid.Row="1" Grid.Column="1" x:Name="dpStckCntYmTp2"  DatepickerType="Month" SelectedDataTimeChanged="dpStckCntYmTp2_SelectedDataTimeChanged"  />

                                <TextBlock              Grid.Row="1" Grid.Column="2" Text="{Binding ConverterParameter='차수', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <c1:C1ComboBox          Grid.Row="1" Grid.Column="3" x:Name="cboSeqNoTp2" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" AutoComplete="False" />

                                <CheckBox               Grid.Row="1" Grid.Column="7" x:Name="chkRackIDGap" Content="{Binding ConverterParameter='Rack ID 상이', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" />

                                <CheckBox               Grid.Row="1" Grid.Column="9" x:Name="chkStatus" Content="{Binding ConverterParameter='Not Complete', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" />

                                <!--조회 버튼-->
                                <Button     Grid.Row="1" Grid.Column="18"  x:Name="btnSearchTp2" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchTp2_Click"/>
                            </Grid>
                            <!-- top(tp2) end -->
                            <!-- main(tp2) start -->
                            <Grid Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock x:Name="tbGrdMainTp2Cnt" Grid.Column="0" Text="[ 0 건 ]" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <TextBlock x:Name="rsltNote" Grid.Column="2" Text="" Style="{StaticResource Content_ModuleTitleStyle}" Foreground="Red" Background="Yellow"/>
                                    <TextBlock Grid.Column="7" x:Name="txtStckCntCmplFlagUpload" Text="{Binding Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_Number_LabelStyle}"/>
                                    <Button     Grid.Column="9"  x:Name="btnExcel_Rslt"     Content="{Binding ConverterParameter=Excel,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelExportStyle}" HorizontalAlignment="Right"  Visibility="Collapsed"/>
                                </Grid>

                                <!-- grdMain(tp2) start -->
                                <c1:C1DataGrid x:Name="grdMainTp2" Grid.Row="1" IsReadOnly="False" HeadersVisibility="Column" FrozenColumnCount="2" SelectionMode="SingleRow" Margin="8">
                                    <c1:C1DataGrid.Columns>
