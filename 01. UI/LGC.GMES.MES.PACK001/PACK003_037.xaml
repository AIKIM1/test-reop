                                        <c1:DataGridTextColumn      Width="Auto" Header = "SCAN_WRKR"                Binding = "{Binding SCAN_WRKR}"                 HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "REAL_MTRL_PORT_ID"        Binding = "{Binding REAL_MTRL_PORT_ID}"         HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "REAL_REPACK_BOX_ID"       Binding = "{Binding REAL_REPACK_BOX_ID}"        HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "라인"                     Binding = "{Binding EQSGNAME}"                  HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "자재 RACK ID"             Binding = "{Binding MTRL_PORT_ID}"              HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "자재 ID"                  Binding = "{Binding MTRLID}"                    HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "요청번호"                 Binding = "{Binding REQ_NO}"                    HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "KANBAN_ID"                Binding = "{Binding KANBAN_ID}"                 HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "자재 BOX ID"              Binding = "{Binding REPACK_BOX_ID}"             HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "자재수량"                 Binding = "{Binding ISS_QTY}"                   HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "상태"                     Binding = "{Binding REQ_STAT_CODE}"             HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "RACK_TYPE"                Binding = "{Binding RACK_TYPE}"                 HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "ISS_RACK_LOAD_WRKR"       Binding = "{Binding ISS_RACK_LOAD_WRKR}"        HorizontalAlignment="Center"     IsReadOnly="True"/>
