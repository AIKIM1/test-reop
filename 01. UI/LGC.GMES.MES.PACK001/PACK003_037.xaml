<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.PACK001.PACK003_037"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d"  
             d:DesignHeight="768" d:DesignWidth="1024" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <!-- content start -->
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="자재 재고실사(Box)">
            <Grid>
                <!-- tab start-->
                <c1:C1TabControl x:Name="tabControlMain">
                    <!-- tp1: start-->
                    <c1:C1TabItem x:Name="tp1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='전산재고'}" Margin="0">
                        <!-- content(tp1) start -->
                        <Grid  x:Name="ctt_tp1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="51" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <!-- top(tp1) start -->
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="35"/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="48*"/>
                                    <ColumnDefinition Width="15*"/>
                                    <ColumnDefinition Width="53*"/>
                                    <ColumnDefinition Width="48*"/>
                                    <ColumnDefinition Width="68*"/>
                                    <ColumnDefinition Width="48*"/>
                                    <ColumnDefinition Width="68*"/>
                                    <ColumnDefinition Width="48*"/>
                                    <ColumnDefinition Width="68*"/>
                                    <ColumnDefinition Width="48*"/>
                                    <ColumnDefinition Width="67*"/>
                                    <ColumnDefinition Width="49*"/>
                                    <ColumnDefinition Width="68*"/>
                                    <ColumnDefinition Width="0*"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0"  Grid.RowSpan="2" Grid.ColumnSpan="21" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <!--ROW1-->
                                <TextBlock              Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="-4,10,7,9"/>
                                <ctrlLib:MultiSelectionBox  Grid.Row="0" Grid.Column="1" x:Name="cboLine" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" Height="Auto" VerticalAlignment="Center" SelectionChanged="cboLine_SelectionChanged" Grid.ColumnSpan="2" Margin="0,3" />

                                <TextBlock              Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='자재 Rack ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Grid.ColumnSpan="2" Margin="0,10,7,9"/>
                                <ctrlLib:MultiSelectionBox  Grid.Row="0" Grid.Column="4" x:Name="cboMtrlPort" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" Height="Auto" VerticalAlignment="Center" Margin="0,3" />

                                <TextBlock              Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='자재 ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Grid.ColumnSpan="2" Margin="0,10,8,9"/>
                                <TextBox                Grid.Row="0" Grid.Column="6" x:Name="txtMtrkIdTp1" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" Margin="3,6" />

                                <TextBlock              Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='Box ID / KANBAN ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Grid.ColumnSpan="2" Margin="0,10,8,9"/>
                                <TextBox                Grid.Row="1" Grid.Column="6" x:Name="txtRepackBoxIdTp1" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" Margin="3,6" />

                                <TextBlock              Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='제외 유무', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Grid.ColumnSpan="2" Margin="0,10,7,9"/>
                                <c1:C1ComboBox          Grid.Row="0" Grid.Column="8" x:Name="cboExcludeYNTp1" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Grid.ColumnSpan="2" Margin="3,6,13,6" />

                                <TextBlock              Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='기준월', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="-16,10,7,9"/>
                                <ctrlLib:LGCDatePicker  Grid.Row="1" Grid.Column="1" x:Name="dpStckCntYmTp1"  DatepickerType="Month" SelectedDataTimeChanged="dpStckCntYmTp1_SelectedDataTimeChanged" Grid.ColumnSpan="2" Margin="3,6"  />

                                <TextBlock              Grid.Row="1" Grid.Column="2" Text="{Binding ConverterParameter='차수', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Grid.ColumnSpan="2" Margin="0,10,7,9"/>
                                <c1:C1ComboBox          Grid.Row="1" Grid.Column="4" x:Name="cboSeqNoTp1" Style="{StaticResource C1ComboBoxStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" AutoComplete="False" Grid.ColumnSpan="2" Margin="3,6,13,6" />



                                <Button     Grid.Row="1" Grid.Column="15" x:Name="btnSeqNoCloseTp1" Content="{Binding ConverterParameter=차수마감,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialPackStyle}" Click="btnSeqNoCloseTp1_Click" Margin="0,3"/>
                                <Button     Grid.Row="1" Grid.Column="17" x:Name="btnSeqNoAddTp1"   Content="{Binding ConverterParameter=차수추가,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialPackStyle}" Click="btnSeqNoAddTp1_Click" Margin="0,3"/>
                                <!--조회 버튼-->
                                <Button     Grid.Row="1" Grid.Column="19" x:Name="btnSearchTp1" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Margin="4,3,0,3" Click="btnSearchTp1_Click"/>
                            </Grid>
                            <!-- top(tp1) end -->
                            <!-- main(tp1) start -->
                            <Grid Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="8"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="8"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock x:Name="tbGrdMainTp1Cnt" Grid.Column="0" Text="[ 0 건 ]" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <TextBlock x:Name="snapNote" Grid.Column="2" Text="" Style="{StaticResource Content_ModuleTitleStyle}" Foreground="Red" Background="Yellow"/>

                                    <TextBlock Grid.Column="4" x:Name="txtStckCntCmplFlagShot" Text="{Binding Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_Search_Number_LabelStyle}"/>

                                    <Button     Grid.Column="6"  x:Name="btnExcel_Snap"     Content="{Binding ConverterParameter=Excel,    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_ModuleButtonExcelExportStyle}" HorizontalAlignment="Right" Visibility="Collapsed" />
                                </Grid>
                                <!-- grdMain(tp1) start -->
                                <c1:C1DataGrid x:Name="grdMainTp1" Grid.Row="1" IsReadOnly="False" HeadersVisibility="Column" FrozenColumnCount="2" SelectionMode="SingleRow" Margin="8">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTemplateColumn Name="CHK" Width="40">
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_ORD_DTTM"           Binding = "{Binding STCK_CNT_ORD_DTTM}"             HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "라인"                        Binding = "{Binding EQSGNAME}"                      HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "자재 RACK ID"                Binding = "{Binding MTRL_PORT_ID}"                  HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "자재 ID"                     Binding = "{Binding MTRLID}"                        HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "요청번호"                    Binding = "{Binding REQ_NO}"                        HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "KANBAN_ID"                   Binding = "{Binding KANBAN_ID}"                     HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "자재 BOX ID"                 Binding = "{Binding REPACK_BOX_ID}"                 HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "자재수량"                    Binding = "{Binding ISS_QTY}"                       HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "상태"                        Binding = "{Binding REQ_STAT_CODE}"                 HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "RACK_TYPE"                   Binding = "{Binding RACK_TYPE}"                     HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "ISS_RACK_LOAD_WRKR"          Binding = "{Binding ISS_RACK_LOAD_WRKR}"            HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "ISS_RACK_LOAD_DTTM"          Binding = "{Binding ISS_RACK_LOAD_DTTM}"            HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_EXCL_FLAG"          Binding = "{Binding STCK_CNT_EXCL_FLAG}"            HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_EXCL_WRKR"          Binding = "{Binding STCK_CNT_EXCL_WRKR}"            HorizontalAlignment="Center"     IsReadOnly="True"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_EXCL_DTTM"          Binding = "{Binding STCK_CNT_EXCL_DTTM}"            HorizontalAlignment="Center"     IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_EXCL_NOTE"          Binding = "{Binding STCK_CNT_EXCL_NOTE}"            HorizontalAlignment="Center"     IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_YM"                 Binding = "{Binding STCK_CNT_YM}"                   HorizontalAlignment="Center"     Visibility="Hidden"/>
                                        <c1:DataGridTextColumn      Width="Auto" Header = "STCK_CNT_SEQNO"              Binding = "{Binding STCK_CNT_SEQNO}"                HorizontalAlignment="Center"     Visibility="Hidden"/>
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                                <!-- grdMain(tp1) end -->
                            </Grid>
                            <!-- main(tp1) end -->
                            <!-- bottom(tp1) start -->
                            <Border Grid.Row="5" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"/>
                            <Grid Grid.Row="7">
                                <Grid.RowDefinitions >
                                    <RowDefinition Height="4" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="4" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <TextBlock  Grid.Row="1" Grid.Column="0" x:Name="tblExcludeNote_SNAP" Text="{Binding ConverterParameter='비고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox    Grid.Row="1" Grid.Column="1" x:Name="txtExcludeNote_SNAP" Style="{StaticResource SearchCondition_TextBoxStyle}" Height="Auto"  AcceptsReturn="True" />
                                <Button     Grid.Row="1" Grid.Column="3" x:Name="btnExclude1"            Content="{Binding ConverterParameter='제외',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonSpecialStyle}" Click="btnExclude1_Click" />

                            </Grid>
                            <!-- bottom(tp1) end -->
                        </Grid>
                        <!-- content(tp1) end -->
                    </c1:C1TabItem>
                    <!-- tp1: end-->
                    <!-- tp2: start-->
                    <c1:C1TabItem x:Name="tp2" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='실물재고'}" >
                        <!-- content(tp2) start -->
                        <Grid x:Name="cct_tp2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="70"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="51"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <!-- top(tp2) start -->
                            <Grid Grid.Row="1" x:Name="tabRsltHeader">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1*"/>
                                    <RowDefinition Height="1*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="3*"/>
                                    <ColumnDefinition Width="6*"/>
                                    <ColumnDefinition Width="0*"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="8"/>
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0"   Grid.RowSpan="2" Grid.ColumnSpan="20" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <!--ROW1-->
                                <TextBlock              Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:MultiSelectionBox  Grid.Row="0" Grid.Column="1" x:Name="cboLine2" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" Height="Auto" VerticalAlignment="Center" SelectionChanged="cboLine2_SelectionChanged" />

                                <TextBlock              Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='자재 Rack ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <ctrlLib:MultiSelectionBox  Grid.Row="0" Grid.Column="3" x:Name="cboMtrlPort2" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  isAllUsed="True" Height="Auto" VerticalAlignment="Center" />

                                <TextBlock              Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='자재 ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBox                Grid.Row="0" Grid.Column="5" x:Name="txtMtrkIdTp2" Style="{StaticResource Content_InputForm_TextBoxStyle}" CharacterCasing="Upper" />

                                <TextBlock              Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='Box ID / KANBAN ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
