<UserControl x:Class="LGC.GMES.MES.FCS002.FCS002_031"
             xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1524"  Loaded="UserControl_Loaded" >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <Grid.Resources>
            <DataTemplate x:Key="ItemTemplate">
                <WrapPanel>
                    <Image Width="138" Height="138" Stretch="Fill" Source="{Binding}" Margin="3,3,3,3" />
                </WrapPanel>
            </DataTemplate>
            <DataTemplate x:Key="ResultTemplate">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <!--<ColumnDefinition Width="*"/>-->
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>
            <DataTemplate x:Key="ResultTemplateSum">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="6"  Text="{Binding ConverterParameter='합계', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>
        </Grid.Resources>

        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="상온/출하 Aging 예약" >
            <ctrlLib:LGCLayout.Content>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <Border Grid.Row="1" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"/>
                    <Grid   Grid.Row="1" Grid.Column="0" >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="4" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="4" />
                        </Grid.RowDefinitions>

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="13"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="13"/>
                        </Grid.ColumnDefinitions>

                        <!--조회조건 ROW1-->
                        <TextBlock                            Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='AGING_TYPE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <c1:C1ComboBox  x:Name="cboAgingType" Grid.Row="1" Grid.Column="2" MinHeight="23" Style="{StaticResource C1ComboBoxStyle_Mandatory}" AutoComplete="False" SelectedValueChanged="cboAgingType_SelectedValueChanged" />

                        <TextBlock                            Grid.Row="1" Grid.Column="9" Text="{Binding ConverterParameter='NEXT_OP', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" Visibility="Collapsed"/>
                        <c1:C1ComboBox  x:Name="cboNextOp"    Grid.Row="1" Grid.Column="10" MinHeight="23" Style="{StaticResource C1ComboBoxStyle_Mandatory}" AutoComplete="False" Visibility="Collapsed" />

                        <TextBlock                            Grid.Row="1" Grid.Column="3" Text="{Binding ConverterParameter='FLAG', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <c1:C1ComboBox  x:Name="cboPriority"  Grid.Row="1" Grid.Column="4" MinHeight="23" Style="{StaticResource C1ComboBoxStyle_Mandatory}" AutoComplete="False" SelectionCommitted="cboPriority_SelectionCommitted" />

                        <TextBlock                            Grid.Row="1" Grid.Column="5" Text="{Binding ConverterParameter='SC_LINE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <c1:C1ComboBox  x:Name="cboSCLine"    Grid.Row="1" Grid.Column="6" MinHeight="23" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                        <CheckBox       Grid.Row="1" Grid.Column="7"  x:Name="chkOutDate" Content="{Binding ConverterParameter='예정일 무시', Converter={StaticResource GlobalObjectDicConverter}}"  
                                                Style="{StaticResource SearchCondition_CheckBoxStyle}" MinWidth="100" Margin="3" IsChecked="False"  />
                        
                        <CheckBox       x:Name="chkLotHold"   Grid.Row="1" Grid.Column="12" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" Background="LightSalmon" IsChecked="False" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Stretch" FlowDirection="RightToLeft"/>
                        <TextBlock                            Grid.Row="1" Grid.Column="13" Text="{Binding ConverterParameter='LOT_HOLD', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" Background="LightSalmon" MinHeight="23" TextAlignment="Left" HorizontalAlignment="Stretch" Margin="4,0,0,0" />

                        <CheckBox       x:Name="chkLowVolt"   Grid.Row="1" Grid.Column="14" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" FlowDirection="RightToLeft" Visibility="Collapsed" />
                        <TextBlock                            Grid.Row="1" Grid.Column="15" Text="{Binding ConverterParameter='LOW_VOLT_JUDG_CHK', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" Background="LightSlateGray" MinHeight="23" TextAlignment="Left" HorizontalAlignment="Stretch" Margin="4,0,0,0" Visibility="Collapsed" />

                        <!--조회조건 ROW2-->
                        <TextBlock                            Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='LINE_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <uc:UcBaseComboBox x:Name="cboLine"   Grid.Row="2" Grid.Column="2" MinHeight="23" Style="{StaticResource C1ComboBoxStyle_Mandatory}" AutoComplete="False" />

                        <TextBlock                            Grid.Row="2" Grid.Column="3" Text="{Binding ConverterParameter='MODEL', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <uc:UcBaseComboBox x:Name="cboModel"  Grid.Row="2" Grid.Column="4" MinHeight="23" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                        <TextBlock                            Grid.Row="2" Grid.Column="5" Text="{Binding ConverterParameter='ROUTE_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <uc:UcBaseComboBox x:Name="cboRoute"  Grid.Row="2" Grid.Column="6" MinHeight="23" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                        <TextBlock                            Grid.Row="2" Grid.Column="7" Text="{Binding ConverterParameter='출고예정일', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <StackPanel Grid.Row="2" Grid.Column="8" Orientation="Horizontal">
                            <ctrlLib:LGCDatePicker x:Name="dtpOutDate" DatepickerType="Date" Width="100"/>
                        </StackPanel>

                        <CheckBox       x:Name="chkSpecialShipBan"   Grid.Row="2" Grid.Column="12" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" FlowDirection="RightToLeft"/>
                        <TextBlock                                   Grid.Row="2" Grid.Column="13" Text="{Binding ConverterParameter='SPECIAL_SHIP_BAN', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" Background="Plum" Foreground="Red" MinHeight="23" TextAlignment="Left" HorizontalAlignment="Stretch" Margin="4,0,0,0"/>

                        <CheckBox       x:Name="chkMinTime"          Grid.Row="2" Grid.Column="14" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" FlowDirection="RightToLeft" Visibility="Collapsed" />
                        <TextBlock                                   Grid.Row="2" Grid.Column="15" Text="{Binding ConverterParameter='MIN_TIME', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" Background="Yellow" MinHeight="23" TextAlignment="Left" HorizontalAlignment="Stretch" Margin="4,0,0,0" Visibility="Collapsed"/>

                        <!--조회조건 ROW3-->
                        <TextBlock                            Grid.Row="3" Grid.Column="1" Text="{Binding ConverterParameter='일괄선택', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <CheckBox       x:Name="chkAll"       Grid.Row="3" Grid.Column="2" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False"
                                        Checked="chkHeaderAll_Checked" Unchecked="chkHeaderAll_Unchecked"/>
                        <TextBlock                            Grid.Row="3" Grid.Column="3" Text="{Binding ConverterParameter='ASSEMBLY_LOT_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <uc:UcBaseComboBox x:Name="cboLotId"     Grid.Row="3" Grid.Column="4" MinHeight="23" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                        <TextBox        x:Name="txtLotID"     Grid.Row="3" Grid.Column="4" MinHeight="23" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" />

                        <TextBlock                            Grid.Row="3" Grid.Column="5" Text="{Binding ConverterParameter='WORK_OP', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <c1:C1ComboBox  x:Name="cboOper"      Grid.Row="3" Grid.Column="6" MinHeight="23" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                        <TextBlock                            Grid.Row="3" Grid.Column="7" Text="{Binding ConverterParameter='SPECIAL_YN', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <c1:C1ComboBox  x:Name="cboSpecial"   Grid.Row="3" Grid.Column="8" MinHeight="23" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                        <TextBlock                                Grid.Row="3" Grid.Column="9" Text="{Binding ConverterParameter='SEL_CNT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <c1:C1ComboBox  x:Name="cboSearchChount"  Grid.Row="3" Grid.Column="10" MinHeight="23" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                        <CheckBox       x:Name="chkSpecial"       Grid.Row="3" Grid.Column="12" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" HorizontalAlignment="Stretch" VerticalContentAlignment="Center" FlowDirection="RightToLeft"/>
                        <TextBlock                                Grid.Row="3" Grid.Column="13" Text="{Binding ConverterParameter='SPECIAL', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" Background="White" Foreground="Red" MinHeight="23" TextAlignment="Left" HorizontalAlignment="Stretch" Margin="4,0,0,0" />

                        <!--조회 버튼-->
                        <Button         x:Name="btnSearch"        Grid.Row="3" Grid.Column="15" Margin="2" Content="{Binding ConverterParameter='조회', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                        <Button         x:Name="btnReservation"   Grid.Row="3" Grid.Column="16" Margin="2" Content="{Binding ConverterParameter='RESV', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnReservation_Click" />
                        <Button         x:Name="btnCancel"        Grid.Row="3" Grid.Column="16" Margin="2" Content="{Binding ConverterParameter='RESV_CANCEL', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnReservation_Click" />
                    </Grid>

                    <!--조회 영역-->
                    <Border Grid.Row="3" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffbebebe"></Border>
                    <Grid   Grid.Row="3" Grid.Column="0" >
                        <c1:C1DataGrid x:Name="dgAgingReserv" Grid.Row="0" Grid.Column="0" Margin="8" IsReadOnly="True" HeadersVisibility="Row" RowHeaderWidth="33" FrozenColumnCount="13" FrozenTopRowsCount="2" SelectionMode="MultiRange" MouseDoubleClick="dgAgingReserv_MouseDoubleClick" PreviewMouseLeftButtonDown="dgAgingReserv_PreviewMouseLeftButtonDown"
                                       LoadedCellPresenter="dgAgingReserv_LoadedCellPresenter" UnloadedCellPresenter="dgAgingReserv_UnloadedCellPresenter" LoadedRowHeaderPresenter="dgAgingReserv_LoadedRowHeaderPresenter"  >
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTextColumn      Header = "[TRAY_NO,TRAY_NO]"                      Binding = "{Binding LOTID}"               HorizontalAlignment="Center" Visibility ="Collapsed" />
                                <c1:DataGridTextColumn      Header = "[LOW_VOLT_JUDGE_YN,LOW_VOLT_JUDGE_YN]"  Binding = "{Binding LOW_VOLT_JUDGE_YN}"   HorizontalAlignment="Center" Visibility ="Collapsed" />
                                <c1:DataGridTextColumn      Header = "[LOT_HOLD_YN,LOT_HOLD_YN]"              Binding = "{Binding LOT_HOLD_YN}"         HorizontalAlignment="Center" Visibility ="Collapsed" />
                                <c1:DataGridTextColumn      Header = "[PQC_CHECK,PQC_CHECK]"                  Binding = "{Binding PQC_CHECK}"           HorizontalAlignment="Center" Visibility ="Collapsed" />

                                <c1:DataGridCheckBoxColumn     Binding = "{Binding CHK, Mode=TwoWay}" Width="Auto"  HorizontalAlignment="Center" CanUserSort="False" >
                                    <c1:DataGridCheckBoxColumn.Header>
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                                        </StackPanel>
                                    </c1:DataGridCheckBoxColumn.Header>
                                </c1:DataGridCheckBoxColumn>

                                <c1:DataGridTextColumn      Header = "[ASSEMBLY_LOT_ID,ASSEMBLY_LOT_ID]"                Binding = "{Binding PROD_LOTID}"          HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn      Header = "[WORK_UNIT,WORK_UNIT]"                  Binding = "{Binding EQPTID}"              HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn      Header = "[SC_LINE,SC_LINE]"                      Binding = "{Binding EQPTNAME}"            HorizontalAlignment="Left" />
                                <c1:DataGridTextColumn      Header = "[TRAY_ID,TRAY_ID]"                      Binding = "{Binding CSTID}"               HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn      Header = "[INSDTTM,INSDTTM]"                      Binding = "{Binding LOTDTTM_CR}"          HorizontalAlignment="Center" Format ="yyyy-MM-dd HH:mm:ss" />
                                <c1:DataGridTextColumn      Header = "[ROUTE_ID,ROUTE_ID]"                    Binding = "{Binding ROUTID}"              HorizontalAlignment="Center" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplateSum}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>

                                <c1:DataGridNumericColumn   Header = "[QTY,INPUT]"                            Binding = "{Binding INPUT_SUBLOT_QTY}"    HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[QTY,재공]"                             Binding = "{Binding GOOD_SUBLOT_QTY}"     HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridTextColumn      Header = "[JOB_TIME,START]"                       Binding = "{Binding WIPDTTM_ST}"          HorizontalAlignment="Center" Format ="yyyy-MM-dd HH:mm:ss" />
                                <c1:DataGridTextColumn      Header = "[JOB_TIME,SHIPNG_PLAN]"                 Binding = "{Binding AGING_ISS_SCHD_DTTM}" HorizontalAlignment="Center" Format ="yyyy-MM-dd HH:mm:ss" />
                                <c1:DataGridNumericColumn   Header = "[GRADE_A,GRADE_A]"                      Binding = "{Binding A_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_B,GRADE_B]"                      Binding = "{Binding B_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_C,GRADE_C]"                      Binding = "{Binding C_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_D,GRADE_D]"                      Binding = "{Binding D_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_E,GRADE_E]"                      Binding = "{Binding E_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_F,GRADE_F]"                      Binding = "{Binding F_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_G,GRADE_G]"                      Binding = "{Binding G_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_H,GRADE_H]"                      Binding = "{Binding H_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_I,GRADE_I]"                      Binding = "{Binding I_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_J,GRADE_J]"                      Binding = "{Binding J_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_K,GRADE_K]"                      Binding = "{Binding K_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_L,GRADE_L]"                      Binding = "{Binding L_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_M,GRADE_M]"                      Binding = "{Binding M_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_N,GRADE_N]"                      Binding = "{Binding N_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridNumericColumn>

                                <c1:DataGridNumericColumn   Header = "[GRADE_O,GRADE_O]"                      Binding = "{Binding O_GRD_QTY}"           HorizontalAlignment="Right" Format="###,###,##0" >
