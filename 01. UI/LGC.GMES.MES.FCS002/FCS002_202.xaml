<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  
             x:Class="LGC.GMES.MES.FCS002.FCS002_202"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:Controls="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1524"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="Stocler Rack 현황">

            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="8"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>

                <!--조회 영역-->
                <Border Grid.Row="0" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"/>

                <Grid Grid.Row="0" Grid.Column="0" >
                    <Grid.RowDefinitions>
                        <RowDefinition Height="11"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="11"/>
                    </Grid.RowDefinitions>

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="13"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="13"/>
                    </Grid.ColumnDefinitions>

                    <TextBlock                            Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Stocker 구분', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                    <c1:C1ComboBox  x:Name="cboStockerType"      Grid.Row="1" Grid.Column="2" Style="{StaticResource C1ComboBoxStyle_Mandatory}" AutoComplete="False" SelectionCommitted="cboLane_SelectionCommitted" Margin="10,3" />

                    <TextBlock                            Grid.Row="1" Grid.Column="3" Text="{Binding ConverterParameter=호기, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                    <c1:C1ComboBox  x:Name="cboSCLine"       Grid.Row="1" Grid.Column="4" Style="{StaticResource C1ComboBoxStyle_Mandatory}" AutoComplete="False" Margin="10,3"  />

                    <TextBlock                            Grid.Row="1" Grid.Column="5" Text="{Binding ConverterParameter=열, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                    <c1:C1ComboBox  x:Name="cboRow"       Grid.Row="1" Grid.Column="6" Style="{StaticResource C1ComboBoxStyle_Mandatory}" AutoComplete="False" Margin="10,3"  />

                    <TextBlock                              Grid.Row="1" Grid.Column="7" Text="{Binding ConverterParameter=COL, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                    <Controls:UcBaseTextBox x:Name="txtCol" Grid.Row="1" Grid.Column="8" MinWidth="100" Margin="10,4,10,2" Style="{StaticResource SearchCondition_TextBoxStyle}" />

                    <TextBlock                            Grid.Row="1" Grid.Column="9" Text="{Binding ConverterParameter=TRAY ID, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                    <Controls:UcBaseTextBox x:Name="txtSearchTray" Grid.Row="1" Grid.Column="10" MinWidth="100" Margin="10,4,10,2"  Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" KeyDown="txtSearchTray_KeyDown" CharacterCasing="Upper" MaxLength="10"/>

                    <Button x:Name="btnTraySearch" Margin="10,5,26,3"  Content="{Binding ConverterParameter=검색, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonNoMinWidthSpecialStyle}" Cursor="Hand" Click="btnTraySearch_Click" Grid.Column="11" Grid.Row="1" Visibility="Collapsed" />

                    <Button         x:Name="btnSearch"    Grid.Row="1" Grid.Column="14" Margin="4"  Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Click="btnSearch_Click" Cursor="Hand" />
                    <Button         x:Name="btnTrayList" Grid.Column="13" Margin="4" Content="{Binding ConverterParameter=Tray List, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnTrayList_Click" Grid.Row="1"  />


                </Grid>

                <!--조회 영역-->

                <Grid Grid.Row="2" Grid.Column="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="4*"/>
                        <ColumnDefinition Width="8"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>

                    <!--조회 영역-->
                    <Border Grid.Row="0" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#fff2f2f2"/>
                    <Grid   Grid.Row="0" Grid.Column="0" Margin="0" >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="3"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <!--색 범례 영역-->
                        <Grid Grid.Row="1" Grid.Column="0" x:Name="dgColor"  >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="4"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="4"/>
                            </Grid.ColumnDefinitions>

                        </Grid>




                        <Grid Grid.Row="2" Grid.Column="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="4"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="4"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock                            Grid.Row="0"  Grid.Column="1" Text="{Binding ConverterParameter=ALL_RATE, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                            <TextBox    x:Name="txtTotalUseRate"  Grid.Row="0"  Grid.Column="2" MinWidth="50" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True"  TextAlignment="Right"/>
                            <TextBlock                            Grid.Row="0"  Grid.Column="3" Text="{Binding ConverterParameter=ROW_RATE, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" />
                            <TextBox    x:Name="txtRowUseRate"    Grid.Row="0"  Grid.Column="4" MinWidth="50" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" TextAlignment="Right"/>
                            <TextBlock                            Grid.Row="0"  Grid.Column="5" Text="{Binding ConverterParameter=전체적재율(%), Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}"  Visibility="Collapsed"/>
                            <TextBox    x:Name="txtAllLoad"       Grid.Row="0"  Grid.Column="6" MinWidth="50" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True"  TextAlignment="Right" Visibility="Collapsed"/>
                            <TextBlock                            Grid.Row="0"  Grid.Column="7" Text="{Binding ConverterParameter=적재율(%), Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}"  Visibility="Collapsed"/>
                            <TextBox    x:Name="txtLoad"          Grid.Row="0"  Grid.Column="8" MinWidth="50" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" TextAlignment="Right" Visibility="Collapsed"/>
                            <TextBlock                            Grid.Row="0"  Grid.Column="9" Text="{Binding ConverterParameter=EMPTY_TRAY_INPUT_QTY, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}"  Visibility="Collapsed"/>
                            <TextBox    x:Name="txtEmptyCnt"      Grid.Row="0"  Grid.Column="10" MinWidth="50" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" TextAlignment="Right" Visibility="Collapsed"/>
                            <TextBlock                            Grid.Row="0"  Grid.Column="11" Text="{Binding ConverterParameter=EMPTY_TRAY__LOAD_RATE, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}"  Visibility="Collapsed"/>
                            <TextBox    x:Name="txtEmptyload"     Grid.Row="0"  Grid.Column="12" MinWidth="50" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" TextAlignment="Right" Visibility="Collapsed"/>

                        </Grid>

                        <Grid Grid.Row="4" Grid.Column="0">
                            <c1:C1DataGrid x:Name="dgStockerRack" Grid.Row="0" Grid.Column="0" Margin="8" IsReadOnly="True" HeadersVisibility="None" SelectionMode="MultiRange" LoadedCellPresenter="dgStockerRack_LoadedCellPresenter" MouseUp="dgStockerRack_MouseUp"  MouseDoubleClick="dgStockerRack_MouseDoubleClick"/>
                        </Grid>
                    </Grid>

                    <GridSplitter Grid.Row="0" Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>

                    <Grid Grid.Row="0" Grid.Column="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <Border Grid.Row="0" Grid.Column="0" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"/>
                        <Grid Grid.Row="0" Grid.Column="0">
                            <c1:C1TabControl Grid.Row="0" x:Name="tcRackInfo">
                                <!--Rack 정보 -->
                                <c1:C1TabItem Header="{Binding ConverterParameter=RACK정보, Converter={StaticResource GlobalObjectDicConverter}}" IsSelected="True" >
                                    <Grid Grid.Row="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="4"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="4"/>
                                        </Grid.RowDefinitions>

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="66"/>
                                            <ColumnDefinition Width="140"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="4"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock                            Grid.Row="1"  Grid.Column="2" Text="{Binding ConverterParameter=RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,6,8,7"  />
                                        <TextBox    x:Name="txtRackCnt"       Grid.Row="1"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3,4,3,2" TextAlignment="Right"/>
                                        <TextBlock                            Grid.Row="2"  Grid.Column="2" Text="{Binding ConverterParameter=1_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,7,8,6"  />
                                        <TextBox    x:Name="txt1RackCnt"      Grid.Row="2"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3,4,3,2" TextAlignment="Right" />
                                        <TextBlock                            Grid.Row="3"  Grid.Column="2" Text="{Binding ConverterParameter=2_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,6,8,7" />
                                        <TextBox    x:Name="txt2RackCnt"      Grid.Row="3"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3,4,3,2"  TextAlignment="Right"/>
                                        <TextBlock                            Grid.Row="4"  Grid.Column="2" Text="{Binding ConverterParameter=3_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,7,8,6"  />
                                        <TextBox    x:Name="txt3RackCnt"      Grid.Row="4"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3,4,3,2"  TextAlignment="Right"/>
                                        <TextBlock                            Grid.Row="5"  Grid.Column="2" Text="{Binding ConverterParameter=4_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,6,8,7"  />
                                        <TextBox    x:Name="txt4RackCnt"      Grid.Row="5"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3,4,3,2"  TextAlignment="Right"/>
                                        <TextBlock                            Grid.Row="6"  Grid.Column="2" Text="{Binding ConverterParameter=5_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,7,8,6" />
                                        <TextBox    x:Name="txt5RackCnt"      Grid.Row="6"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3,4,3,2"  TextAlignment="Right"/>
                                        <TextBlock                            Grid.Row="7"  Grid.Column="2" Text="{Binding ConverterParameter=6_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,6,8,7"  />
                                        <TextBox    x:Name="txt6RackCnt"      Grid.Row="7"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3,4,3,2" TextAlignment="Right"/>
                                        <TextBlock                            Grid.Row="8"  Grid.Column="2" Text="{Binding ConverterParameter=7_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,7,8,6"  />
                                        <TextBox    x:Name="txt7RackCnt"      Grid.Row="8"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3,4,3,2"  TextAlignment="Right"/>
                                        <TextBlock                            Grid.Row="9"  Grid.Column="2" Text="{Binding ConverterParameter=8_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,6,8,7"  />
                                        <TextBox    x:Name="txt8RackCnt"      Grid.Row="9"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3,4,3,2"  TextAlignment="Right"/>
                                        <TextBlock                            Grid.Row="10"  Grid.Column="2" Text="{Binding ConverterParameter=EMPTY_TRAY_RCV_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,7,8,6"  Visibility="Collapsed" />
                                        <TextBox    x:Name="txtEmptytrayCnt"  Grid.Row="10"  Grid.Column="3" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" TextAlignment="Right" Visibility="Collapsed"/>
                                        <TextBlock                            Grid.Row="11"  Grid.Column="2" Text="{Binding ConverterParameter=RCV_POS_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,6,8,7"  />
                                        <TextBox    x:Name="txtPossibleCnt"   Grid.Row="11"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3" TextAlignment="Right" />
                                        <TextBlock                            Grid.Row="12"  Grid.Column="2" Text="{Binding ConverterParameter=RCV_BAN_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,7,8,6"  />
                                        <TextBox    x:Name="txtImpossibleCnt" Grid.Row="12"  Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" Margin="3" TextAlignment="Right"/>
                                        <TextBlock                            Grid.Row="13"  Grid.Column="2" Text="{Binding ConverterParameter=RCV_ABNORM_RACK_CNT, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,6,8,7"  Visibility="Collapsed" />
                                        <TextBox    x:Name="txtTroubleCnt"    Grid.Row="13"  Grid.Column="3" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" TextAlignment="Right" Visibility="Collapsed"/>
                                        <TextBlock                            Grid.Row="14"  Grid.Column="2" Text="{Binding ConverterParameter=RCV_BAN_RATE, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,7,8,6" Visibility="Collapsed"  />
                                        <TextBox    x:Name="txtImpossible"    Grid.Row="14"  Grid.Column="3" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" TextAlignment="Right" Visibility="Collapsed"/>
                                        <TextBlock                            Grid.Row="15"  Grid.Column="2" Text="{Binding ConverterParameter=RCV_ABNORM_RATE, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Height="16" Margin="0,6,8,7"  Visibility="Collapsed" />
                                        <TextBox    x:Name="txtTrouble"       Grid.Row="15"  Grid.Column="3" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" TextAlignment="Right" Visibility="Collapsed"/>
                                    </Grid>
                                </c1:C1TabItem>

                                <!--선택한 Rack 정보 -->
                                <c1:C1TabItem Header="{Binding ConverterParameter=SEL_RACK_INFO, Converter={StaticResource GlobalObjectDicConverter}}" IsSelected="False" >
                                    <Grid Grid.Row="0" >
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="4"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="4"/>
                                        </Grid.RowDefinitions>

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="4"/>
                                            <ColumnDefinition Width="62"/>
                                            <ColumnDefinition Width="92"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="4"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock                            Grid.Row="1"  Grid.Column="2" Text="{Binding ConverterParameter=STG1, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,8,6" />
                                        <TextBlock                            Grid.Row="2"  Grid.Column="2" Text="{Binding ConverterParameter=STG2, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,8,6" />
                                        <TextBlock                            Grid.Row="3"  Grid.Column="2" Text="{Binding ConverterParameter=STG3, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,8,6" />
                                        <TextBlock                            Grid.Row="4"  Grid.Column="2" Text="{Binding ConverterParameter=STG4, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,8,6" />
                                        <TextBlock                            Grid.Row="5"  Grid.Column="2" Text="{Binding ConverterParameter=STG5, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,8,6" />
                                        <TextBlock                            Grid.Row="6"  Grid.Column="2" Text="{Binding ConverterParameter=STG6, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,8,6" />
                                        <TextBlock                            Grid.Row="7"  Grid.Column="2" Text="{Binding ConverterParameter=STG7, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,8,6" />
                                        <TextBlock                            Grid.Row="8"  Grid.Column="2" Text="{Binding ConverterParameter=STG8, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,8,6" />
                                        <TextBlock                            Grid.Row="9"  Grid.Column="2" Text="{Binding ConverterParameter=MODEL, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,8,6" />
                                        <TextBlock                            Grid.Row="10" Grid.Column="2" Text="{Binding ConverterParameter=ROUTE_ID, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,7,6" />
                                        <TextBlock                            Grid.Row="11" Grid.Column="2" Text="{Binding ConverterParameter=RCV_DATE, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,7,8,6" />
                                        <TextBlock                            Grid.Row="12" Grid.Column="1" Text="{Binding ConverterParameter=SHIPNG_PLAN_DATE, Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource Content_InputForm_LabelStyle}" Grid.ColumnSpan="2" Margin="0,7,7,6" />
