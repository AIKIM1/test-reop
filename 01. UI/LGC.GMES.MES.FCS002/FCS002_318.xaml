<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.FCS002.FCS002_318"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:CMM001="clr-namespace:LGC.GMES.MES.CMM001;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <Grid.Resources>
            <DataTemplate x:Key="ItemTemplate">
                <WrapPanel>
                    <Image Width="138" Height="138" Stretch="Fill" Source="{Binding}" Margin="3,3,3,3" />
                </WrapPanel>
            </DataTemplate>
            <DataTemplate x:Key="ResultTemplate">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <!--<ColumnDefinition Width="*"/>-->
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>

        </Grid.Resources>

        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="공정별 생산실적 조회" Grid.ColumnSpan="2">
            <Grid>

                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="8" />
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>

                <!--조회영역-->
                <Grid Grid.Row="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="120" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="120" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="120" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="120" />

                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="120"/>
                        <ColumnDefinition Width="Auto"/>
                        <!--10-->
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>

                        <!--<ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>-->

                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="8" />
                    </Grid.ColumnDefinitions>

                    <!--백그라운드용-->
                    <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                    <!--조회조건-->
                    <TextBlock                            Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동',         Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='LINE',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='공정',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='설비',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock  x:Name="cboTopBackTiltle" Grid.Row="0" Grid.Column="8" Text="{Binding ConverterParameter='Top/Back',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='작업일',     Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='투입LOT',    Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="1" Grid.Column="6" Text="{Binding ConverterParameter='경로유형',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock x:Name="cboElecTypeTiltle" Grid.Row="1" Grid.Column="8" Text="{Binding ConverterParameter='극성',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter='시장유형',  Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>


                    <TextBlock                            Grid.Row="2" Grid.Column="0" Text="{Binding ConverterParameter='LOTID',      Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="2" Grid.Column="2" Text="{Binding ConverterParameter='모델',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="2" Grid.Column="4" Text="{Binding ConverterParameter='제품ID',     Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="2" Grid.Column="6" Text="{Binding ConverterParameter='프로젝트명', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                    <TextBlock                            Grid.Row="2" Grid.Column="8" Text="{Binding ConverterParameter='생산구분',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                    <c1:C1ComboBox x:Name="cboArea"                Grid.Row="0" Grid.Column="1"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    <c1:C1ComboBox x:Name="cboEquipmentSegment"    Grid.Row="0" Grid.Column="3"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    <c1:C1ComboBox x:Name="cboProcess"             Grid.Row="0" Grid.Column="5"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    <c1:C1ComboBox x:Name="cboEquipment"           Grid.Row="0" Grid.Column="7"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    <c1:C1ComboBox x:Name="cboTopBack"             Grid.Row="0" Grid.Column="9"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    <ctrlLib:LGCDatePicker x:Name="dtpDateFrom"    Grid.Row="1" Grid.Column="1"  DatepickerType="Date"   />
                    <TextBlock                                     Grid.Row="1" Grid.Column="2" Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment='Center' />
                    <ctrlLib:LGCDatePicker x:Name="dtpDateTo"      Grid.Row="1" Grid.Column="3"  DatepickerType="Date"   />

                    <TextBox       x:Name="txtPRLOTID"             Grid.Row="1" Grid.Column="5"  Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtLotId_KeyDown" />
                    <c1:C1ComboBox x:Name="cboFlowType"            Grid.Row="1" Grid.Column="7"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                    <c1:C1ComboBox x:Name="cboElecType"            Grid.Row="1" Grid.Column="9"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                    <c1:C1ComboBox x:Name="cboMKTtype"             Grid.Row="1" Grid.Column="11" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>



                    <TextBox       x:Name="txtLotId"               Grid.Row="2" Grid.Column="1"  Style="{StaticResource SearchCondition_TextBoxStyle}" KeyDown="txtLotId_KeyDown" />
                    <CMM001:AutoCompleteTextBox x:Name="txtModlId" Grid.Row="2" Grid.Column="3"  Threshold="2" DelayTime="100" Height="30" MaxWidth="120" GotFocus="txtModlId_GotFocus"/>
                    <TextBox       x:Name="txtProdId"              Grid.Row="2" Grid.Column="5"  Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140" />
                    <TextBox       x:Name="txtPrjtName"            Grid.Row="2" Grid.Column="7"  Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140" KeyDown="txtPrjtName_KeyDown" />
                    <c1:C1ComboBox  x:Name="cboProductDiv"         Grid.Row="2" Grid.Column="9"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"/>


                    <CheckBox      x:Name="chkProc"                Grid.Row="2" Grid.Column="10"  Grid.ColumnSpan="2"   Content="{Binding ConverterParameter='진행중인Lot 포함', Converter={StaticResource GlobalObjectDicConverter}}"  
                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" HorizontalAlignment="Right" />
                    <CheckBox      x:Name="chkPtnLen"              Grid.Row="2" Grid.Column="12" Content="{Binding ConverterParameter='M수량환산', Converter={StaticResource GlobalObjectDicConverter}}"  
                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" HorizontalAlignment="Center" Checked="chkPtnLen_Checked" Unchecked="chkPtnLen_Unchecked" Margin="10,0,0,0"/>
                    <CheckBox      x:Name="chkModel"               Grid.Row="2" Grid.Column="13"   Content="{Binding ConverterParameter='모델합산', Converter={StaticResource GlobalObjectDicConverter}}"  
                                      Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" HorizontalAlignment="Left" Checked="chkModel_Checked" Unchecked="chkModel_Unchecked" Margin="10,0,0,0" />
                    <Button     Grid.Row="0" Grid.Column="15"      Grid.RowSpan="2" Grid.ColumnSpan="3" x:Name="btnSearchHold" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" 
                                Cursor="Hand" Click="btnSearch_Click" HorizontalAlignment="Right"/>

                </Grid>

                <Grid Grid.Row="2">

                    <Border Grid.Row="2" Grid.Column="0"  Style="{StaticResource Content_InputForm_GroupBorderStyle}"/>

                    <c1:C1TabControl x:Name="tbcList" Grid.Column="0" Margin="3" SelectionChanged="tbcList_SelectionChanged">
                        <c1:C1TabItem x:Name="Lot" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='LOT'}" >
                            <Grid>

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="8" />
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <GridSplitter Grid.Row="1" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                <!--LOT 리스트-->
                                <Grid Grid.Row="0" Grid.Column="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="34"/>
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="42*" />
                                        <ColumnDefinition Width="463*"/>
                                        <ColumnDefinition Width="8" />
                                        <ColumnDefinition Width="505*" />
                                    </Grid.ColumnDefinitions>

                                    <!--백그라운드용-->
                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="4" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" Grid.ColumnSpan="4" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                    <TextBlock Grid.Row="0" Grid.ColumnSpan="4" Style="{StaticResource Content_ModuleTitleStyle}" Margin="15,9,0,8"><Run Text="{Binding ConverterParameter='LOT리스트', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                    <c1:C1DataGrid x:Name="dgLotList" Grid.Row="1" Grid.ColumnSpan="4" IsReadOnly="False" HeadersVisibility="Column" FrozenColumnCount="3" FrozenBottomRowsCount="1" MouseDoubleClick="dgLotList_DoubleClick" SelectionMode="MultiRange"  Margin="8,8,8,8">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTemplateColumn  Header="선택" EditOnSelection="True" Name="CHK" Width="60" >
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <RadioButton GroupName="dgProductLotChoiceGroup" IsChecked="{Binding CHK, Mode=OneWay}" Name="dgProductLotChoice" HorizontalAlignment="Center" Checked="dgProductLotChoice_Checked" />
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>
                                            <c1:DataGridTextColumn      Header = "코터 생산호기"   Binding = "{Binding EQPTID_COATER}"       IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "투입LOT"         Binding = "{Binding PR_LOTID}"            IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "투입CarrierID"   Binding = "{Binding PR_CSTID}"            IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "LOTID"           Binding = "{Binding LOTID}"               IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "H/S Side"        Binding = "{Binding HALF_SLIT_SIDE}"      IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTemplateColumn  Header = "이력" Width="Auto"  Name="HIST_LIST" >
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Button x:Name="btnHist" Style="{DynamicResource Content_SearchButtonStyle}" VerticalAlignment="Center" Click="btnHist_Click"  />
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>
                                            <c1:DataGridTextColumn      Header = "CarrierID"        Binding="{Binding CSTID}"                 IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "대차LOTID"        Binding = "{Binding WINDING_RUNCARD_ID}"  IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "Assembly LOT"     Binding = "{Binding LOTID_AS}"            IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "Foil LOT"         Binding = "{Binding FOIL_LOTID}"          IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "AUTOSTOP"         Binding = "{Binding AUTO_STOP_FLAG}"      IsReadOnly="True" HorizontalAlignment="Center" />
                                            <!--<c1:DataGridTextColumn      Header = "압연차수"      Binding = "{Binding ROLLPRESS_SEQNO}"   IsReadOnly="True" HorizontalAlignment="Center" />-->
                                            <c1:DataGridTextColumn      Header = "압연차수"         Binding = "{Binding ROLLPRESS_COUNT}"     IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "WIPSEQ"           Binding = "{Binding WIPSEQ}"              IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header = "제품"             Binding = "{Binding PRODID}"              IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "제품명"           Binding = "{Binding PRODNAME}"            IsReadOnly="True" HorizontalAlignment="Left" />
                                            <c1:DataGridTextColumn      Header = "모델"             Binding = "{Binding MODLID}"              IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "PJT"              Binding = "{Binding PRJT_NAME}"           IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "LOT 유형"         Binding = "{Binding LOTYNAME}"            IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "시장유형"         Binding = "{Binding MKT_TYPE_CODE}"       IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "FastTrack"        Binding = "{Binding FAST_TRACK_FLAG}"     IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <c1:DataGridTextColumn      Header = "HOLD"             Binding = "{Binding WIPHOLD}"             IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "QMS 판정결과"     Binding = "{Binding JUDG_NAME}"           IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                                            <c1:DataGridTextColumn      Header = "이전시장유형"     Binding = "{Binding FRST_MKT_TYPE_CODE}"  IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" x:Name="FRST_MKT"  />
                                            <c1:DataGridTextColumn      Header = "단위"             Binding = "{Binding UNIT_CODE}"           IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "제품모델 변경 최초 생산 LOT 여부"          Binding = "{Binding MODL_CHG_FRST_PROD_LOT_FLAG}"           IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "REWINDER"         Binding = "{Binding EQPT_END_PSTN_ID}"    IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridNumericColumn   Header = "설비양품량"       Binding = "{Binding EQPT_END_QTY}"        IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>
                                            <c1:DataGridNumericColumn   Header = "설비양품량"    Binding = "{Binding EQPT_END_QTY_EA}"      IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>

                                            <c1:DataGridNumericColumn   Header = "TAG수"    Binding = "{Binding TAG_QTY}"      IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>

                                            <c1:DataGridNumericColumn   Header = "설비수집TAG수"    Binding = "{Binding EQP_TAG_QTY}"      IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>

                                            <!-- ======================================== 원형,초소형 관련 ================================== -->
                                            <c1:DataGridNumericColumn   Header = "차이량"     Binding = "{Binding DIFF_QTY}"         IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>

                                            <!-- ======================================== 원형,초소형 관련 ================================== -->

                                            <c1:DataGridNumericColumn   Header = "투입량"     Binding = "{Binding INPUT_QTY}"         IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>
                                            <c1:DataGridNumericColumn   Header = "투입량"     Binding = "{Binding INPUT_QTY_EA}"         IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>

                                            <!-- ======================================== 원형,초소형 관련 ================================== -->
                                            <c1:DataGridNumericColumn   Header = "재투입"     Binding = "{Binding INPUT_DIFF_QTY_WS}"         IsReadOnly="True" HorizontalAlignment="Right" Format="###,###,##0.##">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>

                                            <!-- ======================================== 원형,초소형 관련 ================================== -->

                                            <c1:DataGridNumericColumn     Header = "양품량"               Binding = "{Binding WIPQTY_ED}"   HorizontalAlignment="Right" Format="###,###,##0.##" IsReadOnly="True">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>
                                            <c1:DataGridNumericColumn     Header = "양품량"               Binding = "{Binding WIPQTY_ED_EA}"   HorizontalAlignment="Right" Format="###,###,##0.##" IsReadOnly="True">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>

                                            <c1:DataGridNumericColumn     Header = "불량량"               Binding = "{Binding CNFM_DFCT_QTY}"   HorizontalAlignment="Right" Format="###,###,##0.##" IsReadOnly="True">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>
                                            <c1:DataGridNumericColumn     Header = "불량량"               Binding = "{Binding CNFM_DFCT_QTY_EA}"   HorizontalAlignment="Right" Format="###,###,##0.##" IsReadOnly="True">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                                    </c1:DataGridAggregatesCollection>
                                                </c1:DataGridAggregate.AggregateFunctions>
                                            </c1:DataGridNumericColumn>

                                            <c1:DataGridNumericColumn     Header = "LOSS량"               Binding = "{Binding CNFM_LOSS_QTY}"   HorizontalAlignment="Right" Format="###,###,##0.##" IsReadOnly="True">
                                                <c1:DataGridAggregate.AggregateFunctions>
                                                    <c1:DataGridAggregatesCollection>
                                                        <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
