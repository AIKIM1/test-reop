<UserControl xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.FCS002.FCS002_313"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="재작업 대상 재공 이동">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <c1:C1TabControl>
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='활성화 재공 이동'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8" />
                                <RowDefinition />
                                <RowDefinition Height="8" />
                                <RowDefinition />
                                <RowDefinition Height="8" />
                                <RowDefinition Height="100" />
                            </Grid.RowDefinitions>


                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="231*"/>
                                <ColumnDefinition Width="155*"/>
                                <ColumnDefinition Width="8"/>
                                <ColumnDefinition Width="122*" />

                            </Grid.ColumnDefinitions>
                            <GridSplitter Grid.Row="4" Grid.ColumnSpan="4" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                            <GridSplitter Grid.Row="5" Grid.Column="2" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                            <!--조회영역-->
                            <Grid Grid.Row="1" Grid.ColumnSpan="4">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="13" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="0" Grid.Column="2" Text="{Binding ConverterParameter='라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="0" Grid.Column="6" Text="{Binding ConverterParameter='구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>

                                <TextBlock  Grid.Row="1" Grid.Column="0" Text="{Binding ConverterParameter='PJT명', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="2" Text="{Binding ConverterParameter='제품', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="4" Text="{Binding ConverterParameter='조립LOTID',       Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <TextBlock  Grid.Row="1" Grid.Column="6" Text="{Binding ConverterParameter='PalletID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />

                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="1" x:Name="cboAreaInput"             Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"/>
                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="3" x:Name="cboEquipmentSegmentInput"          Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="5" x:Name="cboProcessInput"             Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboProcessInput_SelectedValueChanged" />
                                <c1:C1ComboBox   Grid.Row="0" Grid.Column="7" x:Name="cboQlty"             Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                <TextBox         Grid.Row="1" Grid.Column="1" x:Name="txtPrjtNameInput"             Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140"/>
                                <TextBox         Grid.Row="1" Grid.Column="3" x:Name="txtProdidInput"              Style="{StaticResource SearchCondition_TextBoxStyle}" />
                                <TextBox         Grid.Row="1" Grid.Column="5" x:Name="txtLotRTDInput"               Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140"/>
                                <TextBox         Grid.Row="1" Grid.Column="7" x:Name="txtPalletIdInput"             Style="{StaticResource SearchCondition_TextBoxStyle}" MinWidth="140" KeyDown="txtPalletIdInput_KeyDown" PreviewKeyDown="txtPalletIdInput_PreviewKeyDown"/>

                                <!--조회 버튼-->
                                <Button     Grid.Row="0" Grid.Column="9"  Grid.RowSpan="2" x:Name="btnReSet" Content="{Binding ConverterParameter=초기화,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnReSet_Click" Height="29"/>
                                <Button     Grid.Row="0" Grid.Column="11" Grid.RowSpan="2" x:Name="btnSearchInput" Content="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchInput_Click" />

                            </Grid>

                            <!--대상목록-->
                            <Grid Grid.Row="3" Grid.ColumnSpan="4">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='조회목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <c1:C1DataGrid x:Name="dgListInput" Grid.Row="1" HeadersVisibility="Column" FrozenColumnCount="2" SelectionMode="SingleRow" Margin="8,8,8,8" MouseLeftButtonUp="dgListInput_MouseLeftButtonUp" >
                                    <c1:C1DataGrid.Columns>
                                        <!--<c1:DataGridTemplateColumn Name="CHK" Width="40" Header = "선택"  >
                                            <c1:DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    -->
                                        <!--<RadioButton IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" Click="chkList_Click" />-->
                                        <!--
                                                    <RadioButton GroupName="dgLotInputCheck" IsChecked="{Binding CHK, Mode=OneWay}" x:Name="dgLotInputChoice" Width="Auto" HorizontalAlignment="Center" Checked="dgLotInputCheck_Checked"/>
                                                </DataTemplate>
                                            </c1:DataGridTemplateColumn.CellTemplate>
                                        </c1:DataGridTemplateColumn>-->
                                        <c1:DataGridCheckBoxColumn  Header = "선택"                  Binding = "{Binding CHK}"                 HorizontalAlignment="Center" Width="40" />
                                        <c1:DataGridTextColumn      Header = "FORM_WRK_TYPE_CODE"    Binding = "{Binding FORM_WRK_TYPE_CODE}"  HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "동"                    Binding = "{Binding AREANAME}"            HorizontalAlignment="Center" IsReadOnly="True"  />
                                        <c1:DataGridTextColumn      Header = "라인"                  Binding = "{Binding EQSGNAME}"            HorizontalAlignment="Center" IsReadOnly="True"  />
                                        <c1:DataGridTextColumn      Header = "공정"                  Binding = "{Binding PROCNAME}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "동"                    Binding = "{Binding AREAID}"              HorizontalAlignment="Center" IsReadOnly="True"  Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "라인"                  Binding = "{Binding EQSGID}"              HorizontalAlignment="Center" IsReadOnly="True"  Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "공정"                  Binding = "{Binding PROCID}"              HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "PalletID"              Binding = "{Binding PALLETID}"            HorizontalAlignment="Center" IsReadOnly="True"  />
                                        <c1:DataGridTextColumn      Header = "조립LOT"               Binding = "{Binding LOTID_RT}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "구분"                  Binding = "{Binding WIP_QLTY_TYPE_NAME}"  HorizontalAlignment="Center" IsReadOnly="True"  />
                                        <c1:DataGridTextColumn      Header = "LotType"               Binding = "{Binding LOTYNAME}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "PJT"                   Binding = "{Binding PJT}"                 HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "제품"                  Binding = "{Binding PRODID}"              HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridNumericColumn   Header = "수량"                  Binding = "{Binding WIPQTY}"              HorizontalAlignment="Right"  Format="###,###,##0.##" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "단위"                  Binding = "{Binding UNIT}"                HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "SOC"                   Binding = "{Binding SOC_VALUE}"           HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "GROUP"                 Binding = "{Binding WND_GR_CODE}"         HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "Winder 설비"           Binding = "{Binding WND_EQPTNM}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "시장유형"              Binding = "{Binding MKT_TYPE_NAME}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "용량등급"              Binding = "{Binding CAPA_GRD_CODE}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "전압등급"              Binding = "{Binding VLTG_GRD_CODE}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "저항등급"              Binding = "{Binding RSST_GRD_CODE}"       HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "완료일자"              Binding = "{Binding UPDDATE}"             HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "CMCODE"                Binding = "{Binding CMCODE}"              HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "양품/불량"             Binding = "{Binding QLTY_TYPE_NAME}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "WIPSEQ"                Binding = "{Binding WIPSEQ}"              HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "WIPHOLD"               Binding = "{Binding WIPHOLD}"             HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "PROC_LABEL_PRT_FLAG"   Binding = "{Binding PROC_LABEL_PRT_FLAG}" HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "EQPTID"                Binding = "{Binding EQPTID}"              HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "ROUTID"                Binding = "{Binding ROUTID}"              HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "FLOWID"                Binding = "{Binding FLOWID}"              HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "S04"                   Binding = "{Binding S04}"                 HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "CLSS3_CODE"            Binding = "{Binding CLSS3_CODE}"          HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>

                            </Grid>

                            <!--선택목록-->
                            <Grid Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="2">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상목록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <c1:C1DataGrid x:Name="dgSelectInput" Grid.Row="1" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="2" SelectionMode="SingleRow" >
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn      Header = "PalletID"       Binding = "{Binding PALLETID}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "조립LOT"        Binding = "{Binding LOTID_RT}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "PJT"            Binding = "{Binding PJT}"               HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "제품"           Binding = "{Binding PRODID}"            HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridNumericColumn   Header = "수량"           Binding = "{Binding WIPQTY}"            HorizontalAlignment="Right"  Format="###,###,##0.##" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "단위"           Binding = "{Binding UNIT}"              HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "생산일자"       Binding = "{Binding CALDATE}"           HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "동"             Binding = "{Binding AREAID}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "라인"           Binding = "{Binding EQSGID}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "공정"           Binding = "{Binding PROCID}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "ROUTID"         Binding = "{Binding ROUTID}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "FLOWID"         Binding = "{Binding FLOWID}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "S04"            Binding = "{Binding S04}"               HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                        <c1:DataGridTextColumn      Header = "CLSS3_CODE"     Binding = "{Binding CLSS3_CODE}"        HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>
                            </Grid>

                            <Grid Grid.Row="5" Grid.Column="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="40" />
                                </Grid.RowDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <Border Grid.Row="2" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Border Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>

                                <TextBlock Grid.Row="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='공정실적 차감 설정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <c1:C1DataGrid x:Name="dgSelectInput_Prcess" Grid.Row="1"  HeadersVisibility="Column" FrozenColumnCount="2" SelectionMode="SingleRow" Margin="8,8,8,8">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn      Header = "NO"             Binding = "{Binding ROWNUM}"          HorizontalAlignment="Center" IsReadOnly="True" />
                                        <c1:DataGridTextColumn      Header = "공정"           Binding = "{Binding PROCID}"    HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                        <c1:DataGridTextColumn      Header = "공정명"           Binding = "{Binding PROC_NAME}"    HorizontalAlignment="Left"  IsReadOnly="True" />
                                        <c1:DataGridCheckBoxColumn  Header = "실적차감"       Binding = "{Binding CHK}"         HorizontalAlignment="Center" IsReadOnly="False" />
                                    </c1:C1DataGrid.Columns>
                                </c1:C1DataGrid>

                                <TextBox   Grid.Row="2" x:Name="txtMessage" VerticalScrollBarVisibility="Auto" Height="Auto" Margin="1,1,1,1" AcceptsReturn="True" FontSize="10" TextWrapping="Wrap"/>
                            </Grid>
                            <!--사유영역-->
                            <Grid x:Name="TextArea" Grid.Row="7" Grid.ColumnSpan="4">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="34"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Grid.RowSpan="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <TextBlock Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='이동구분', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <c1:C1ComboBox  Grid.Column="1" x:Name="cboRework" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboRework_SelectedValueChanged"/>
                                <TextBlock Grid.Column="2" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <c1:C1ComboBox  Grid.Column="3" x:Name="cboTagetArea" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedValueChanged="cboTagetArea_SelectedValueChanged" IsEnabled="False" />
                                <TextBlock Grid.Column="4" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='대상라인', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <c1:C1ComboBox  Grid.Column="5" x:Name="cboTagetLine" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"  IsEnabled="False" SelectedValueChanged="cboTagetLine_SelectedValueChanged"/>
                                <TextBlock Grid.Column="6" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='이동공정', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <c1:C1ComboBox  Grid.Column="7" x:Name="cboReworkProcess" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False"  IsEnabled="False" SelectedValueChanged="cboReworkProcess_SelectedValueChanged"/>
                                <Button    Grid.Column="11" x:Name="btnReworkMove"    Content="{Binding ConverterParameter=공정이동,   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Margin="0,0,8,0" Click="btnReworkMove_Click"/>

                                <TextBlock Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}"><Run Text="{Binding ConverterParameter='Remark', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>
                                <TextBox   Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="9"  x:Name="txtRemark"  TextWrapping="Wrap" VerticalScrollBarVisibility="Auto"  Style="{StaticResource SearchCondition_TextBoxStyle}" Height="Auto" Margin="8,8,8,8" AcceptsReturn="True"/>

                            </Grid>
                        </Grid>
                    </c1:C1TabItem>
                    <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='활성화 재공 이동 이력'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="AUTO"/>
                                <RowDefinition Height="8" />
                                <RowDefinition Height="2.*"/>
                                <RowDefinition Height="8" />
                                <RowDefinition />
                            </Grid.RowDefinitions>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <GridSplitter Grid.Row="4" Grid.ColumnSpan="4" ResizeDirection="Rows" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle }" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                            <!--조회영역-->
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="AUTO" />
                                    <RowDefinition Height="AUTO" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />

                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>

                                <!--백그라운드용-->
                                <Border     Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="15" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>

                                <!--조회조건-->
                                <!--ROW1-->
                                <TextBlock  Grid.Row="0" Grid.Column="0" Text="{Binding ConverterParameter='기간', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="3">
                                    <ctrlLib:LGCDatePicker  x:Name="ldpDateFromHist"  DatepickerType="Date"/>
                                    <TextBlock  Text="~" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="3,0,3,0"/>
                                    <ctrlLib:LGCDatePicker  x:Name="ldpDateToHist"  DatepickerType="Date"/>
                                </StackPanel>
                                <TextBlock  Grid.Row="0" Grid.Column="4" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
