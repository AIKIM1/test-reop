<c1:C1Window x:Class="LGC.GMES.MES.FCS002.FCS002_054_DFCT_CELL_LIST"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
		     xmlns:System="clr-namespace:System;assembly=mscorlib"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"        
             ctrlLib:LGCPopupWindowExtension.ApplyStyle="True"
             Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='불량 CELL 목록'}" 
             Tag="ProtoType01Windows01" Height="600" Width="500" IsResizable="True" Loaded="C1Window_Loaded" >
    <c1:C1Window.Resources>
        <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;Component/ControlStyles.xaml"/>
    </c1:C1Window.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="Tray 선택">
            <c1:C1DataGrid x:Name="dgCellList" Grid.Row="0" IsReadOnly="False" HeadersVisibility="All" SelectionMode="MultiRange" LoadedRowHeaderPresenter="dgCellList_LoadedRowHeaderPresenter" >
                <c1:C1DataGrid.Columns>
                    <c1:DataGridTextColumn    Header = "CELL_ID"          Binding = "{Binding SUBLOTID}"     IsReadOnly="True" HorizontalAlignment="Center" />
                    <c1:DataGridTextColumn    Header = "불량명"           Binding = "{Binding DFCT_NAME}"    IsReadOnly="True" HorizontalAlignment="Center" />
                </c1:C1DataGrid.Columns>
            </c1:C1DataGrid>
        </ctrlLib:LGCLayout>
    </Grid>
</c1:C1Window>
    