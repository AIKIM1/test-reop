<UserControl x:Class="LGC.GMES.MES.FCS002.FCS002_005_02"
             xmlns:ss="http://schemas.grapecity.com/windows/spreadsheet/2012"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1920"  Loaded="UserControl_Loaded" >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <DataTemplate x:Key="ResultTemplate">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <!--<ColumnDefinition Width="*"/>-->
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>
            <DataTemplate x:Key="ResultTemplateSum">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0"  Text="{Binding ConverterParameter='합계', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" HorizontalAlignment="Right" Padding="0,0,5,0"  />
                </Grid>
            </DataTemplate>
        </ResourceDictionary>

    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <Grid.Resources>
            <DataTemplate x:Key="ItemTemplate">
                <WrapPanel>
                    <Image Width="138" Height="138" Stretch="Fill" Source="{Binding}" Margin="3,3,3,3" />
                </WrapPanel>
            </DataTemplate>
        </Grid.Resources>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="Tray List" Grid.ColumnSpan="2">
            <ctrlLib:LGCLayout.Content>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <Border Grid.Row="1" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1"/>
                    <Grid x:Name="gdConditionArea"  Grid.Row="1" Grid.Column="0" >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="14" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="1" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="1" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="14" />
                        </Grid.RowDefinitions>

                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="13" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="13"/>
                        </Grid.ColumnDefinitions>

                        <!--조회조건 ROW1-->
                        <TextBlock                          Grid.Row="1" Grid.Column="1" Margin="0,0,5,0" Text="{Binding ConverterParameter='생산라인', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <TextBox   x:Name="txtLine"         Grid.Row="1" Grid.Column="2" MinWidth="100" Style="{StaticResource SearchCondition_TextBoxStyle}" IsReadOnly="True" />
                        <TextBlock                          Grid.Row="1" Grid.Column="3" Margin="0,0,5,0" Text="{Binding ConverterParameter='모델', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <TextBox   x:Name="txtModel"        Grid.Row="1" Grid.Column="4" MinWidth="100" Style="{StaticResource SearchCondition_TextBoxStyle}" IsReadOnly="True" />
                        <TextBlock                          Grid.Row="1" Grid.Column="5" Margin="0,0,5,0" Text="{Binding ConverterParameter='공정경로', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <TextBox   x:Name="txtRoute"        Grid.Row="1" Grid.Column="6" MinWidth="100" Style="{StaticResource SearchCondition_TextBoxStyle}" IsReadOnly="True" />
                        <TextBlock                          Grid.Row="1" Grid.Column="7" Margin="0,0,5,0" Text="{Binding ConverterParameter='공정', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <TextBox   x:Name="txtOp"           Grid.Row="1" Grid.Column="8" MinWidth="100" Style="{StaticResource SearchCondition_TextBoxStyle}" IsReadOnly="True" />
                        <TextBlock                          Grid.Row="1" Grid.Column="9" Margin="0,0,5,0" Text="{Binding ConverterParameter='LOTTYPE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <TextBox   x:Name="txtLotType"      Grid.Row="1" Grid.Column="10" MinWidth="100" Style="{StaticResource SearchCondition_TextBoxStyle}" IsReadOnly="True" />
                        <Button    x:Name="btn1"            Grid.Row="1" Grid.Column="14" Margin="0,3,3,0" MinWidth="120" Content="{Binding ConverterParameter='특별', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Background="Red" Foreground="White" VerticalAlignment="Center" Height="29" Padding="12,-1,12,0" FontSize="12" FontFamily="#맑은고딕" />
                        <Button    x:Name="btn2"            Grid.Row="1" Grid.Column="15" Margin="0,3,3,0" MinWidth="120" Content="{Binding ConverterParameter='Aging 시간 미달', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Background="Magenta" Foreground="White" VerticalAlignment="Center" Height="29" Padding="12,-1,12,0" FontSize="12" FontFamily="#맑은고딕" />
                        <Button    x:Name="btn3"            Grid.Row="1" Grid.Column="16" Margin="0,3,3,0" MinWidth="120" Content="{Binding ConverterParameter='Dummy', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Background="Blue" Foreground="White" VerticalAlignment="Center" Height="29" Padding="12,-1,12,0" FontSize="12" FontFamily="#맑은고딕" />
                        <Button    x:Name="btn4"            Grid.Row="1" Grid.Column="17" Margin="0,3,3,0" MinWidth="120" Content="{Binding ConverterParameter='Sample', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Background="DarkViolet" Foreground="White" VerticalAlignment="Center" Height="29" Padding="12,-1,12,0" FontSize="12" FontFamily="#맑은고딕" />

                        <!--조회조건 ROW2-->
                        <TextBlock                          Grid.Row="3" Grid.Column="1" Margin="0,0,5,0" Text="{Binding ConverterParameter='호기', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <TextBox   x:Name="txtAgingEqpName" Grid.Row="3" Grid.Column="2" MinWidth="100" Style="{StaticResource SearchCondition_TextBoxStyle}" IsReadOnly="True" />
                        <TextBlock                          Grid.Row="3" Grid.Column="3" Margin="0,0,5,0" Text="{Binding ConverterParameter='특별관리내용', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox   x:Name="txtSpecialYN"    Grid.Row="3" Grid.Column="4" MinWidth="100" Style="{StaticResource SearchCondition_TextBoxStyle}" IsReadOnly="True" />
                        <TextBlock                          Grid.Row="3" Grid.Column="5" Margin="0,0,5,0" Text="{Binding ConverterParameter='상태', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox   x:Name="txtStatus"       Grid.Row="3" Grid.Column="6" MinWidth="100" Style="{StaticResource SearchCondition_TextBoxStyle}" IsReadOnly="True" />
                        <CheckBox  x:Name="chkDetail"       Grid.Row="3" Grid.Column="8" Content="{Binding ConverterParameter='상세보기', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" Checked="chkDetail_Checked" Unchecked="chkDetail_Unchecked" Visibility="Collapsed" />
                        <CheckBox  x:Name="chkEWGrade"      Grid.Row="3" Grid.Column="10" Content="{Binding ConverterParameter='E/W 등급 복수 존재 대상', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" Checked="chkEWGrade_Checked" Unchecked="chkEWGrade_Unchecked" Visibility="Collapsed"/>
                        <TextBlock x:Name="lblMsg"          Grid.Row="3" Grid.Column="12" Margin="0,0,5,0" Text="{Binding ConverterParameter='선택한 저전압 팔레트 수량은', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Visibility="Collapsed" />
                        <Button    x:Name="btnLowVolt"      Grid.Row="3" Grid.Column="14" Margin="0,3,3,0" Content="{Binding ConverterParameter='저전압선택', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnLowVolt_Click" Visibility="Collapsed" />
                         <Button    x:Name="btnManualOut"      Grid.Row="3" Grid.Column="13" Margin="0,3,3,0" Content="{Binding ConverterParameter='MANUAL_OUT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnManualOut_Click" Visibility="Collapsed" />

                        <Button    x:Name="btnForceOut"     Grid.Row="3" Grid.Column="14" Margin="0,3,3,0" Content="{Binding ConverterParameter='강제출고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnForceOut_Click" />
                        <Button    x:Name="btnSampleOut"    Grid.Row="3" Grid.Column="15" Margin="0,3,3,0" Content="{Binding ConverterParameter='Sample 출고', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnSampleOut_Click" />
                        <Button    x:Name="btnSampleRel"    Grid.Row="3" Grid.Column="16" Margin="0,3,3,0" Content="{Binding ConverterParameter='Sample 출고해제', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnSampleRel_Click" />

                        <!--조회조건 ROW3-->
                        <TextBlock                          Grid.Row="5" Grid.Column="1" Margin="0,0,5,0" Text="{Binding ConverterParameter='일괄선택', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <CheckBox  x:Name="chkAll"          Grid.Row="5" Grid.Column="2" Content="{Binding Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" />
                        <TextBlock                          Grid.Row="5" Grid.Column="3" Margin="0,0,5,0" Text="{Binding ConverterParameter='Tray ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <TextBox   x:Name="txtTrayID"       Grid.Row="5" Grid.Column="4" MinWidth="100" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" KeyDown="txtTrayID_KeyDown" PreviewKeyDown="txtTrayID_PreviewKeyDown" CharacterCasing="Upper"/>
                        <TextBlock                          Grid.Row="5" Grid.Column="5" Margin="0,0,5,0" Text="{Binding ConverterParameter='ASSEMBLY_LOT_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox   x:Name="txtLotID"        Grid.Row="5" Grid.Column="6" MinWidth="100" Style="{StaticResource SearchCondition_TextBoxStyle}" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" KeyDown="txtLotID_KeyDown" />
                        <CheckBox  x:Name="chkCellLot"      Grid.Row="5" Grid.Column="8" Content="{Binding ConverterParameter='Cell의 Lot으로 찾기', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="False" Visibility="Collapsed" />
                        <CheckBox  x:Name="chkGroup"        Grid.Row="5" Grid.Column="8" Content="{Binding ConverterParameter='동일 Rack 선택', Converter={StaticResource GlobalObjectDicConverter}}" Style="{StaticResource SearchCondition_CheckBoxStyle}" IsChecked="True" />
                        <Button    x:Name="btnExcel"        Grid.Row="3" Grid.Column="17" Margin="0,3,3,0" Content="{Binding ConverterParameter=Excel , Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleButtonExcelImportStyle}" MinHeight="29" Cursor="Hand" Click="btnExcel_Click" />
                        <Button    x:Name="btnSearch"       Grid.Row="5" Grid.Column="17" Margin="0,3,3,0" Content="{Binding ConverterParameter='조회', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" />
                        <Button    x:Name="btnAgingManualOut" Grid.Row="5" Grid.Column="14" Margin="0,3,3,0" Content="{Binding ConverterParameter='AGING_MANUAL_OUT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnAgingManualOut_Click" />
                        <Button    x:Name="btnSpecial"      Grid.Row="5" Grid.Column="15" Margin="0,3,3,0" Content="{Binding ConverterParameter='특별관리등록', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnSpecial_Click" />
                        <Button    x:Name="btnRoute"        Grid.Row="5" Grid.Column="16" Margin="0,3,3,0" Content="{Binding ConverterParameter='Route 변경', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnRoute_Click"/>
                    </Grid>

                    <!--조회 영역-->
                    <Border Grid.Row="3" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffbebebe"></Border>
                    <Grid   Grid.Row="3" Grid.Column="0" >
                        <uc:UcBaseDataGrid x:Name="dgTrayList" Grid.Row="0" Grid.Column="0" Margin="8" IsReadOnly="False" HeadersVisibility="Row" FrozenColumnCount="4" FrozenTopRowsCount="2" SelectionMode="MultiRange" 
                                           LoadedCellPresenter="dgTrayList_LoadedCellPresenter" 
                                           MouseDoubleClick="dgTrayList_MouseDoubleClick" 
                                           LoadedRowHeaderPresenter="dgTrayList_LoadedRowHeaderPresenter"
                                           ExecuteDataModify="dgTrayList_ExecuteDataModify"  ColumnHeaderClickAction="Select"
                                           ExecuteDataCompleted="dgTrayList_ExecuteDataCompleted" MergingCells="dgTrayList_MergingCells">
                            
                            <c1:C1DataGrid.Columns>
                                <c1:DataGridTemplateColumn Name="CHK" Width="40" Header = "선택"  >
                                    <c1:DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <CheckBox IsChecked="{Binding CHK, Mode=TwoWay}" VerticalAlignment="Center" HorizontalAlignment="Center" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked"/>
                                        </DataTemplate>
                                    </c1:DataGridTemplateColumn.CellTemplate>
                                </c1:DataGridTemplateColumn>
                                <c1:DataGridTextColumn      Header = "[ASSEMBLY_LOT_ID,ASSEMBLY_LOT_ID]"          Binding = "{Binding PROD_LOTID}"         IsReadOnly="True"  HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn      Header = "[Tray ID,Tray ID]"                Binding = "{Binding CSTID}"              IsReadOnly="True"  HorizontalAlignment="Center" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplateSum}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[TRAY_LOT_ID,TRAY_LOT_ID]"        Binding = "{Binding LOTID}"              IsReadOnly="True"  HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn      Header = "[BOX ID,BOX ID]"                  Binding = "{Binding EQP_ID}"             IsReadOnly="True"  HorizontalAlignment="Left"   />
                                <c1:DataGridTextColumn      Header = "[공정경로,공정경로]"              Binding = "{Binding ROUTID}"             IsReadOnly="True"  HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn      Header = "[현재공정,현재공정]"              Binding = "{Binding OP_NAME}"            IsReadOnly="True"  HorizontalAlignment="Left"   />
                                <c1:DataGridTextColumn      Header = "[공정 ID,공정 ID]"                Binding = "{Binding PROCID}"             IsReadOnly="True"  HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn      Header = "[차기공정,차기공정]"              Binding = "{Binding NEXT_OP_NAME}"       IsReadOnly="True"  HorizontalAlignment="Left"   />
                                <c1:DataGridTextColumn      Header = "[수량,투입]"                      Binding = "{Binding INPUT_SUBLOT_QTY}"   IsReadOnly="True"  HorizontalAlignment="Right"  >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[수량,재공]"                      Binding = "{Binding WIP_QTY}"            IsReadOnly="True"  HorizontalAlignment="Right" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[작업시간,PRE_OP_END_TIME]"       Binding = "{Binding WIPDTTM_ED}"         IsReadOnly="True"  HorizontalAlignment="Center" Format ="yyyy-MM-dd HH:mm:ss" />
                                <c1:DataGridTextColumn      Header = "[작업시간,현재공정 시작시간]"     Binding = "{Binding WIPDTTM_ST}"         IsReadOnly="True"  HorizontalAlignment="Center" Format ="yyyy-MM-dd HH:mm:ss" />
                                <c1:DataGridTextColumn      Header = "[작업시간,출고예정]"              Binding = "{Binding OP_PLAN_TIME}"       IsReadOnly="True"  HorizontalAlignment="Center" Format ="yyyy-MM-dd HH:mm:ss" />
                                <c1:DataGridTextColumn      Header = "[작업시간,DGS/EOL 시간]"          Binding = "{Binding ED_PLAN_TIME}"       IsReadOnly="True"  HorizontalAlignment="Center" Format ="yyyy-MM-dd HH:mm:ss" Visibility="Collapsed"/>
                                <c1:DataGridTextColumn      Header = "[특별관리여부,특별관리여부]"      Binding = "{Binding SPCL_TYPE_CODE}"     IsReadOnly="True"  HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn      Header = "[Dummy 여부,Dummy 여부]"          Binding = "{Binding DUMMY_FLAG}"         IsReadOnly="True"  HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn      Header = "[FITTED_JUDG_MODE,FITTED_JUDG_MODE]"          Binding = "{Binding FITTED_JUDG_MODE}"         IsReadOnly="True"  HorizontalAlignment="Center" />
                                <c1:DataGridTextColumn      Header = "[A,A]"                            Binding = "{Binding A_GRD_QTY}"          IsReadOnly="True"  HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[B,B]"                            Binding = "{Binding B_GRD_QTY}"          IsReadOnly="True"  HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[C,C]"                            Binding = "{Binding C_GRD_QTY}"          IsReadOnly="True"  HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[D,D]"                            Binding = "{Binding D_GRD_QTY}"          IsReadOnly="True"  HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[E,E]"                            Binding = "{Binding E_GRD_QTY}"          IsReadOnly="True"  HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[F,F]"                            Binding = "{Binding F_GRD_QTY}"          IsReadOnly="True"  HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[G,G]"                            Binding = "{Binding G_GRD_QTY}"          IsReadOnly="True"  HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[H,H]"                            Binding = "{Binding H_GRD_QTY}"          IsReadOnly="True"  HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[I,I]"                            Binding = "{Binding I_GRD_QTY}"          IsReadOnly="True"  HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
                                </c1:DataGridTextColumn>
                                <c1:DataGridTextColumn      Header = "[J,J]"                            Binding = "{Binding J_GRD_QTY}"          IsReadOnly="True"  HorizontalAlignment="Right" Format="###,###,##0" >
                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplate}" ></c1:DataGridAggregateSum>
                                        </c1:DataGridAggregatesCollection>
