<UserControl xmlns:ss="hhemas.grapecity.com/windows/spreadsheet/2012" x:Class="LGC.GMES.MES.FCS002.FCS002_059"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:Controls="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1920" Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="불량 셀 등록" Grid.ColumnSpan="3" >
            <Grid>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <c1:C1TabControl x:Name="tbcWip">
                        <!--등록 tabpag -->
                        <c1:C1TabItem x:Name="Create" Header="{Binding ConverterParameter=REGISTER, Converter={StaticResource GlobalObjectDicConverter}}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="8"/>
                                    <ColumnDefinition Width="5*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="8"/>
                                </Grid.RowDefinitions>
                                <Border Grid.Row="1" Grid.Column="0" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                <Grid x:Name="Content" Grid.Column="0" Grid.Row="1" >
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="14"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="auto"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="13"/>
                                            <ColumnDefinition Width="120"/>
                                            <ColumnDefinition Width="80"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="13"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock      Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left"  Text="{Binding ConverterParameter=불량 등록 작업일자, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <ctrlLib:LGCDatePicker Grid.Row="2" Grid.Column="1" x:Name="dtpFromDateT1"  DatepickerType="Date"/>
                                        <c1:C1DateTimePicker   Grid.Row="2" Grid.Column="2" x:Name="dtpFromTimeT1" CustomTimeFormat="HH:mm" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Center" EditMode="Time" Background="LightYellow"/>

                                        <TextBlock      Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="3" HorizontalAlignment="Left"  Text="{Binding ConverterParameter=DEFECT_KIND, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                        <c1:C1ComboBox  Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="3"  x:Name="cboInsertGroup"    MinHeight="30" Style="{StaticResource C1ComboBoxStyle_Mandatory}" AutoComplete="False" />

                                        <TextBlock      Grid.Row="7" Grid.Column="1" Grid.ColumnSpan="3" Text="{Binding ConverterParameter=DEFECT_ID, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                        <c1:C1ComboBox  Grid.Row="8" Grid.Column="1" Grid.ColumnSpan="3"  x:Name="cboInsertDefect"   MinHeight="30" Style="{StaticResource C1ComboBoxStyle_Mandatory}" AutoComplete="False" />

                                        <TextBlock      Grid.Row="10"  Grid.Column="1" Grid.ColumnSpan="3" Text="{Binding ConverterParameter=REMARK, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                        <Controls:UcBaseTextBox Grid.Row="11" Grid.Column="1" Grid.ColumnSpan="3" x:Name="txtRemark"        Style="{StaticResource Content_InputForm_TextBoxStyle}" TextAlignment="Left" VerticalScrollBarVisibility="Auto" TextWrapping="Wrap" Height="80" AcceptsReturn="True"/>

                                        <Grid Grid.Row="13" Grid.Column="1" Grid.ColumnSpan="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="auto"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Button  x:Name="btnSave" Grid.Column="1" Content="{Binding ConverterParameter=BAD_CELL_REG, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}" Cursor="Hand" Click="btnBadCellReg_Click" HorizontalAlignment="Center" />
                                        </Grid>
                                    </Grid>
                                </Grid>

                                <GridSplitter Grid.Row="1" Grid.Column="1" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                                <Border Grid.Row="1" Grid.Column="2" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>

                                <Grid Grid.Row="1"  Grid.Column="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="8"/>
                                        <RowDefinition Height="AUTO"/>
                                        <RowDefinition Height="4"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="8"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="13"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid Grid.Row="1" Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="3"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="3"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="3"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="3"/>
                                            <ColumnDefinition Width="auto"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock      Grid.Column="0"  Text="{Binding ConverterParameter=CELL_CNT_SUM, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                        <TextBox        Grid.Column="1"  x:Name="txtInsertCellCnt" MinWidth="60" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>

                                        <TextBlock      Grid.Column="3" Text="{Binding ConverterParameter=INFO_ERR, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                        <TextBox        Grid.Column="4"  x:Name="txtBadInsertRow" MinWidth="60"  Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>

                                        <TextBlock      Grid.Column="6" Text="{Binding ConverterParameter=ROW_NUM, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                        <TextBox        Grid.Column="8" MinWidth="60"  x:Name="txtRowCntInsertCell"  Style="{StaticResource Content_InputForm_TextBoxStyle}" Text="100" TextAlignment="Right"/>

                                        <Button  x:Name="btnRefresh"  Grid.Column="10"  Content="{Binding ConverterParameter=Refresh, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl03Style}" Cursor="Hand" Click="btnRefresh_Click" HorizontalAlignment="Center" />

                                        <Button  x:Name="btnExcel"  Grid.Column="12" Content="{Binding ConverterParameter=엑셀등록, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleButtonExcelImportStyle}" Cursor="Hand" Click="btnExcel_Click" HorizontalAlignment="Center" />
                                        <Button  x:Name="btnSearch"  Grid.Column="14"  Content="{Binding ConverterParameter=Search, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch_Click" HorizontalAlignment="Center" />
                                    </Grid>

                                    <Grid Grid.Row="3" Grid.Column="1">
                                        <c1:C1DataGrid x:Name="dgInputList" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="1" IsReadOnly="False" HeadersVisibility="None"  
                                                       FrozenColumnCount="3"
                                                       LoadedColumnHeaderPresenter="dgInputList_LoadedColumnHeaderPresenter" LoadedCellPresenter="dgInputList_LoadedCellPresenter">
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridCheckBoxColumn Width="Auto" Name = "CHK"  Binding="{Binding CHK}"                 HorizontalAlignment="Center" CanUserFilter="False" CanUserSort="False" CanUserResize="False" />
                                                <c1:DataGridTextColumn      Header = "[CELL_ID,CELL_ID]"            Binding = "{Binding SUBLOTID}"         IsReadOnly="False" HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn      Header = "[WORK_DATE,WORK_DATE]"        Binding = "{Binding WORK_DATE}"        IsReadOnly="True" HorizontalAlignment="Center" />
                                                <c1:DataGridComboBoxColumn  Header = "[SHIFT_NAME,SHIFT_NAME]"      Binding = "{Binding SHIFT}"            IsReadOnly="True" HorizontalAlignment="Center"  EditOnSelection="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Visibility="Visible" CellContentStyle="{StaticResource Grid_CellContentStyle}" />
                                                <c1:DataGridTextColumn      Header = "[GRADE,GRADE]"                Binding = "{Binding FINL_JUDG_CODE}"   IsReadOnly="True" HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn      Header = "[TRAY_ID,TRAY_ID]"            Binding = "{Binding CSTID}"            IsReadOnly="True" HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn      Header = "[ROUTE_ID,ROUTE_ID]"          Binding = "{Binding ROUTID}"           IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[LOT_ID,LOT_ID]"              Binding = "{Binding PROD_LOTID}"       IsReadOnly="True" HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn      Header = "[CELL_NO,CELL_NO]"            Binding = "{Binding CSTSLOT}"          IsReadOnly="True" HorizontalAlignment="Left" />
                                                <c1:DataGridTextColumn      Header = "[LANE_NAME,LANE_NAME]"        Binding = "{Binding LANE_NAME}"        IsReadOnly="True" HorizontalAlignment="Center" />
                                                <c1:DataGridTextColumn      Header = "[EQP_KIND_CD,EQP_KIND_CD]"    Binding = "{Binding S70}"              IsReadOnly="True"  HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[LANE_ID,LANE_ID]"            Binding = "{Binding S71}"              IsReadOnly="True" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn      Header = "[EQP_ID,EQP_ID]"              Binding = "{Binding EQPTID}"           IsReadOnly="True" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn      Header = "[EQP_NAME,EQP_NAME]"          Binding = "{Binding EQPTNAME}"         IsReadOnly="True" HorizontalAlignment="Left"/>
                                                <c1:DataGridTextColumn      Header = "[OP_NAME,OP_NAME]"            Binding = "{Binding PROCNAME}"         IsReadOnly="True" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn      Header = "[DEFECT,DEFECT_NAME]"         Binding = "{Binding DFCTNAME}"         IsReadOnly="True" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn      Header = "[DEFECT,EQP_NAME_DEFECT]"     Binding = "{Binding EQP_NAME_D}"       IsReadOnly="True" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn      Header = "[DEFECT,HOLD_DESC]"           Binding = "{Binding REMARKS_CNTT}"     IsReadOnly="True" HorizontalAlignment="Center"/>
                                                <c1:DataGridComboBoxColumn  Header = "[DEFECT,INPUT_POS_YN]"        Binding = "{Binding EQPT_INPUT_AVAIL_FLAG}"   IsReadOnly="True"  HorizontalAlignment="Left" EditOnSelection="False" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Visibility="Visible" CellContentStyle="{StaticResource Grid_CellContentStyle}" />
                                                <c1:DataGridTextColumn      Header = "[DEFECT,REMARK]"              Binding = "{Binding DENY_DESC}"        IsReadOnly="True" HorizontalAlignment="Center"/>
                                                <c1:DataGridTextColumn      Header = "[DEFECT,PROCESS_TYPE]"        Binding = "{Binding PROCESS_TYPE}"     IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn      Header = "[DEFECT,WORK_DTTM]"           Binding = "{Binding WORK_DTTM}"        IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            </c1:C1DataGrid.Columns>
                                            <c1:C1DataGrid.TopRows>
                                                <c1:DataGridColumnHeaderRow />
                                                <c1:DataGridColumnHeaderRow />
                                            </c1:C1DataGrid.TopRows>
                                        </c1:C1DataGrid>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </c1:C1TabItem>
                        <!--복구 tabpag -->
                        <c1:C1TabItem x:Name="Repair" Header="{Binding ConverterParameter=RESTORE, Converter={StaticResource GlobalObjectDicConverter}}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="4"/>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="8"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="13"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="13"/>
                                </Grid.ColumnDefinitions>
                                <Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                <Grid Grid.Row="2" Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="10"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock      Grid.Column="0"  Text="{Binding ConverterParameter=CELL_CNT_SUM, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                    <TextBox        Grid.Column="2"  x:Name="txtCellCount2"  Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" MinWidth="60"/>
                                    <Button  x:Name="btnRefresh2"  Grid.Column="4"  Content="{Binding ConverterParameter=Refresh, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl03Style}" Cursor="Hand" Click="btnRefresh2_Click" HorizontalAlignment="Center"  />
                                    <TextBlock      Grid.Column="6" Text="{Binding ConverterParameter=ROW_NUM, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left"/>
                                    <TextBox        Grid.Column="8" MinWidth="60"  x:Name="txtRowCntInsertCell2"  Style="{StaticResource Content_InputForm_TextBoxStyle}" Text="100" TextAlignment="Right"/>
                                    <Button  x:Name="btnExcel2"  Grid.Column="10" Content="{Binding ConverterParameter=Search, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_ModuleButtonExcelImportStyle}" Cursor="Hand" Click="btnExcel2_Click" HorizontalAlignment="Center" />
                                    <Button  x:Name="btnSearch2"  Grid.Column="12"  Content="{Binding ConverterParameter=Search, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearch2_Click" HorizontalAlignment="Center" />
                                    <Button  x:Name="btnBadCellRestore"  Grid.Column="14" Content="{Binding ConverterParameter=BAD_CELL_RESTORE, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl01Style}" Cursor="Hand" Click="btnBadCellResotre_Click" HorizontalAlignment="Center"/>
                                </Grid>
                                <Grid Grid.Row="4" Grid.Column="1">
                                    <c1:C1DataGrid x:Name="dgRecover" Grid.Column="0" IsReadOnly="False" HeadersVisibility="Column" FrozenColumnCount="3" SelectionMode="MultiRange" Margin="16,10,0,6"
                                                   LoadedColumnHeaderPresenter="dgRecover_LoadedColumnHeaderPresenter">
                                        <c1:C1DataGrid.Columns>
                                            <c1:DataGridTextColumn      Header = "CELL_ID"           Binding = "{Binding SUBLOTID}"      IsReadOnly="False" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "ROUTE_ID"          Binding = "{Binding ROUTID}"        IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTextColumn      Header = "ASSEMBLY_LOT_ID"        Binding = "{Binding PROD_LOTID}"    IsReadOnly="True" HorizontalAlignment="Center" />
                                            <c1:DataGridTemplateColumn>
                                                <c1:DataGridTemplateColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Button x:Name="btnDelete" Content="{Binding ConverterParameter=삭제, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainButtonSpecialStyle}"  Cursor="Hand" Click="btnDelete_Click"/>
                                                    </DataTemplate>
                                                </c1:DataGridTemplateColumn.CellTemplate>
                                            </c1:DataGridTemplateColumn>
                                        </c1:C1DataGrid.Columns>
                                    </c1:C1DataGrid>
                                </Grid>
                            </Grid>

                        </c1:C1TabItem>
                        <!--조회 tabpag -->
                        <c1:C1TabItem x:Name="Search" Header="{Binding ConverterParameter=SEARCH, Converter={StaticResource GlobalObjectDicConverter}}" Margin="0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="8"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="4"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="8" />
                                </Grid.RowDefinitions>
                                <Grid Grid.Row="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="4"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="4"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="4"/>
                                        <RowDefinition Height="auto"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="13"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="1.5*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="1.5*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="1.5*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="1.5*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="1.5*"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="2.5*"/>
                                        <ColumnDefinition Width="13"/>
                                    </Grid.ColumnDefinitions>

                                    <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                                    <StackPanel Grid.Row="1" Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right">
                                        <TextBlock Text="{Binding ConverterParameter=SEL_PERIOD, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    </StackPanel>
                                    
                                    <Grid Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="6">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="10"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <ctrlLib:LGCDatePicker  Grid.Column="0" x:Name="dtpFromDate"  DatepickerType="Date"/>
                                        <c1:C1DateTimePicker    Grid.Column="1" x:Name="dtpFromTime" CustomTimeFormat="HH:mm" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Center" EditMode="Time" Background="LightYellow"/>
                                        <TextBlock              Grid.Column="2" Text="~" Margin="10,0,10,0" Style="{StaticResource Content_InputForm_LabelStyle}"  HorizontalAlignment="Center" />
                                        <ctrlLib:LGCDatePicker  Grid.Column="3" x:Name="dtpToDate"  DatepickerType="Date"/>
                                        <c1:C1DateTimePicker    Grid.Column="4" x:Name="dtpToTime" CustomTimeFormat="HH:mm" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Center" EditMode="Time" Background="LightYellow"/>
                                        <RadioButton  Grid.Column="6" x:Name="rdoReg"  GroupName="rdoDateTime"  Style="{StaticResource SearchCondition_RadioButtonStyle}" Content="{Binding ConverterParameter=REG_DATE, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Tag="AT" Checked="rdoReg_Checked" IsChecked="True" />
                                        <RadioButton  Grid.Column="7" x:Name="rdoWork" GroupName="rdoDateTime"  Margin="10,0,0,0" Style="{StaticResource SearchCondition_RadioButtonStyle}" Content="{Binding ConverterParameter=WORK_DATE, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Tag="CT" Checked="rdoReg_Checked"/>
                                    </Grid>

                                    
                                    <StackPanel Grid.Row="1" Grid.Column="7" Orientation="Horizontal" HorizontalAlignment="Right">
                                        <TextBlock Text="{Binding ConverterParameter=OP, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    </StackPanel>
                                    <Controls:UcBaseComboBox Grid.Row="1" Grid.Column="8" x:Name="cboSearchOp" Style="{StaticResource C1ComboBoxStyle_Mandatory}" AutoComplete="False" />

                                    <TextBlock               Grid.Row="1" Grid.Column="10" Text="{Binding ConverterParameter=INPUT_POS_YN, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Margin="30,3,3,3" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <Controls:UcBaseComboBox Grid.Row="1" Grid.Column="11" x:Name="cboEqpInputYN"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                                    <!--Row2-->
                                    <TextBlock       Grid.Row="3" Grid.Column="1" Text="{Binding ConverterParameter=MODEL, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Margin="30,3,3,3" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <Controls:UcBaseComboBox Grid.Row="3" Grid.Column="2" x:Name="cboSearchModel"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                    <TextBlock       Grid.Row="3" Grid.Column="4" Text="{Binding ConverterParameter=LANE_ID, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Margin="30,3,3,3" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <Controls:UcBaseComboBox Grid.Row="3" Grid.Column="5" x:Name="cboSearchLane"    Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                    <TextBlock               Grid.Row="3" Grid.Column="7" Text="{Binding ConverterParameter=EQP, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Margin="30,3,3,3" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <Controls:UcBaseComboBox Grid.Row="3" Grid.Column="8" x:Name="cboSearchEqp"    Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                    <TextBlock               Grid.Row="3" Grid.Column="10" Text="{Binding ConverterParameter=SHIFT_NAME, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Margin="30,3,3,3" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <Controls:UcBaseComboBox Grid.Row="3" Grid.Column="11" x:Name="cboSearchShift"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                    <TextBlock       Grid.Row="3" Grid.Column="13" Text="{Binding ConverterParameter=DEFECT_ID, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Margin="30,3,3,3" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <Controls:UcBaseComboBox Grid.Row="3" Grid.Column="14" x:Name="cboSearchDefect"    Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />

                                    <!--Row3-->
                                    <TextBlock       Grid.Row="5" Grid.Column="1" Text="{Binding ConverterParameter=LOT_ID, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Margin="30,3,3,3" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <Controls:UcBaseTextBox Grid.Row="5" Grid.Column="2" x:Name="txtSearchLotId"  Style="{StaticResource Content_InputForm_TextBoxStyle}" />

                                    <TextBlock       Grid.Row="5" Grid.Column="4" Text="{Binding ConverterParameter=CELL_ID, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Margin="30,3,3,3" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <Controls:UcBaseTextBox Grid.Row="5" Grid.Column="5" x:Name="txtSearchCellId"  Style="{StaticResource Content_InputForm_TextBoxStyle}"/>

                                    <TextBlock       Grid.Row="5" Grid.Column="7" Text="{Binding ConverterParameter=HOLD_DESC, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Margin="30,3,3,3" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                                    <Controls:UcBaseTextBox Grid.Row="5" Grid.Column="8" x:Name="txtHoldDesc"  Style="{StaticResource Content_InputForm_TextBoxStyle}"/>

