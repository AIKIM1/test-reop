                                    <c1:DataGridAggregate.AggregateFunctions>
                                        <c1:DataGridAggregatesCollection>
                                            <c1:DataGridAggregateSum ResultTemplate="{StaticResource ResultTemplateSum}"/>
                                        </c1:DataGridAggregatesCollection>
                                    </c1:DataGridAggregate.AggregateFunctions>
