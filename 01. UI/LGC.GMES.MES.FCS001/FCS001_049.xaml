                            </c1:C1TabItem>
                            <c1:C1TabItem x:Name="TabTempGraph" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='TEMP_GRAPH'}" Margin="0" >
                                <Grid x:Name="grdChrtMain">
