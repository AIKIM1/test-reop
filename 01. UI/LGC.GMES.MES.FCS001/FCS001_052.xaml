                                <TextBlock       Grid.Row="2" Grid.Column="3" Text="{Binding ConverterParameter='~', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1NumericBox Grid.Row="2" Grid.Column="4" x:Name="nudPOCVE" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="True" CornerRadius="0" AllowNull="True" HandleUpDownKeys="False" Format="#,##0" SelectOnFocus="Always" Value="3" MinWidth="50" Minimum="1" Maximum="20" ValueChanged="nudPOCVE_ValueChanged"/>
                                <uc:UcBaseRadioButton     Grid.Row="2" Grid.Column="6" x:Name="rdoOCV1After" Margin="4"  GroupName="rbAfter" Style="{StaticResource SearchCondition_RadioButtonStyle}" Content="{Binding ConverterParameter='DEFAULT_MEAS_DATA', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" IsChecked="True" Checked="rdoOCV1After_Checked"/>
                                <uc:UcBaseButton          Grid.Row="2" Grid.Column="7" x:Name="btnSearchAfter" Content="{Binding ConverterParameter='SEARCH', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}"  Cursor="Hand" Click="btnSearchAfter_Click" />
