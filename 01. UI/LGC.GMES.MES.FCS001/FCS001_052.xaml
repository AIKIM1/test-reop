                                <c1:C1NumericBox Grid.Row="3" Grid.Column="2" x:Name="nudDOCVS" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="True" CornerRadius="0" AllowNull="True" HandleUpDownKeys="False" Format="#,##0" SelectOnFocus="Always" Value="1" MinWidth="50" Minimum="1" Maximum="20" ValueChanged="nudDOCVS_ValueChanged"/>
                                <TextBlock       Grid.Row="3" Grid.Column="3" Text="{Binding ConverterParameter='~', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                                <c1:C1NumericBox Grid.Row="3" Grid.Column="4" x:Name="nudDOCVE" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="True" CornerRadius="0" AllowNull="True" HandleUpDownKeys="False" Format="#,##0" SelectOnFocus="Always" Value="3" MinWidth="50" Minimum="1" Maximum="20" ValueChanged="nudDOCVE_ValueChanged"/>
