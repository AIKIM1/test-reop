                            <!--Cell검색-->
                            <Grid Grid.Row="1" Grid.Column="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="11" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="11" />
                                </Grid.RowDefinitions>

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="13"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="13"/>
                                </Grid.ColumnDefinitions>

                                <Border         Grid.Row="0" Grid.RowSpan="3" Grid.ColumnSpan="99" Background="#fff2f2f2" BorderThickness="1" BorderBrush="#ffd1d1d1" Margin="0"/>
                                <!--<TextBlock      Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='PKG_LOT_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,8,8,8" />
                                <TextBox        Grid.Row="1" Grid.Column="2" x:Name="txtLotbefore"  MinWidth="150" Style="{StaticResource Content_InputForm_MandatoryTextBoxStyle}" KeyDown="txtLotbefore_KeyDown"/>
                                <uc:UcBaseButton         Grid.Row="1" Grid.Column="3" x:Name="btnBeforeSearchLot" Content="{Binding ConverterParameter='SEARCH', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource SearchButtonStyle}" Cursor="Hand" Click="btnSearchLot_Click" />-->
                                <TextBlock       Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='INIT_ROW_NUM', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" Margin="0,8,8,8"/>
                                <c1:C1NumericBox Grid.Row="1" Grid.Column="2" x:Name="txtDegaasRowCntCell" Width="60" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="False" CornerRadius="0" AllowNull="True" HandleUpDownKeys="False" Format="#,##0" SelectOnFocus="Always" Value="100" MinWidth="100" Background="LightYellow" Margin="3,5" />
                                <uc:UcBaseButton          Grid.Row="1" Grid.Column="3" x:Name="btnDegasBCellReset" Content="{Binding ConverterParameter = 'REFRESH', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_MainControl03Style}" Cursor="Hand" Click="btnDegasBCellReset_Click" />
                            </Grid>


                            <!--검색리스트-->
                            <Grid Grid.Row="3" Grid.Column="0">
                                <Border    Grid.Row="0" Grid.Column="0" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"  />
                                <uc:UcBaseDataGrid x:Name="dgBeforeSerchList" IsReadOnly="False" SelectionMode="MultiRange" HeadersVisibility="All" FrozenColumnCount="1" RowHeaderWidth="40" ClipboardPasted="dgBeforeSerchList_ClipboardPasted" VerticalScrollBarVisibility="Visible" ClipboardPasteMode="None" IsUserConfigUse="False">
                                    <c1:C1DataGrid.Columns>
                                        <c1:DataGridTextColumn       Header = "CELL_ID"          Binding = "{Binding SUBLOTID}"   HorizontalAlignment="Center" />
                                    </c1:C1DataGrid.Columns>
                                </uc:UcBaseDataGrid>
                            </Grid>

                            <!--공정 체크박스-->
                            <Grid Grid.Row="1" Grid.Column="1" Grid.RowSpan="3" >
                                <Border    Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />
                                <c1:C1TreeView Grid.Row="1" Grid.Column="0" x:Name="tvBefore" Margin="8" ItemExpanded="trvData_ItemExpanded" VirtualizingPanel.VirtualizationMode="Recycling" VirtualizingPanel.IsVirtualizing="True"  >
                                    <c1:C1TreeView.ItemTemplate>
                                        <c1:C1HierarchicalDataTemplate DataType="{x:Type MenuItem}" ItemsSource="{Binding Items}">
                                            <StackPanel Orientation="Horizontal" Name="StackPanel1">
                                                <CheckBox IsChecked="{Binding IsChecked, Mode=TwoWay}" Name="chkbefore" HorizontalAlignment="Center" Checked="bfCheck_Checked" Unchecked="bfCheck_Unchecked" Tag="{Binding Tag}">
                                                    <TextBlock Text="{Binding Title}"/>
                                                </CheckBox>
                                            </StackPanel>
                                        </c1:C1HierarchicalDataTemplate>
                                    </c1:C1TreeView.ItemTemplate>
                                </c1:C1TreeView>
                            </Grid>

                            <!--조회리스트-->
                            <Grid x:Name="gBefore" Grid.Row="1" Grid.Column="2" Grid.RowSpan="3">
                                <Border    Grid.Row="0" Grid.Column="0" Grid.RowSpan="99" BorderThickness="1" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}" />

                                <uc:UcBaseDataGrid x:Name="dgBefore" IsReadOnly="True" SelectionMode="MultiRange" HeadersVisibility="Row" FrozenColumnCount="1" FrozenTopRowsCount="2" RowHeaderWidth="40" IsUserConfigUse="False">
                                    <c1:C1DataGrid.Columns>

                                    </c1:C1DataGrid.Columns>
                                </uc:UcBaseDataGrid>
                                <!---수정 완료-->
                            </Grid>
                        </Grid>
                    </c1:C1TabItem>

