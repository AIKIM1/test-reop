<UserControl xmlns:ss="hhemas.grapecity.com/windows/spreadsheet/2012"  x:Class="LGC.GMES.MES.FCS001.FCS001_021"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1920"
             Loaded="UserControl_Loaded">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grdMain">
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="Tray 정보조회" Grid.ColumnSpan="2">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="160"/>
                    <RowDefinition Height="8" />
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1.1*"/>
                    <ColumnDefinition Width="8"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>

                <!--조회영역-->
                <Grid x:Name="SearchArea" Grid.Row="0" Grid.Column="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="11" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="4" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="4" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="4" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="11" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="13"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="13"/>
                    </Grid.ColumnDefinitions>
                    <!--백그라운드용-->
                    <Border     Grid.Row="0" Grid.RowSpan="99" Grid.Column="0" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                    <!--조회조건 ROW1-->
                    <Grid Grid.Row="1" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width = "4*"/>
                            <ColumnDefinition Width = "6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock   Grid.Column="0" Text="{Binding ConverterParameter='TRAY_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <Grid Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBox     Grid.Column="0" x:Name="txtTrayID" Style="{StaticResource Content_InputForm_MandatoryTextBoxStyle}" KeyDown="txtTrayID_KeyDown"  CharacterCasing="Upper" />
                            <uc:UcBaseCheckBox   Grid.Row="1" Grid.Column="2" x:Name="chkHist" Style="{StaticResource SearchCondition_CheckBoxStyle}" />
                        </Grid>
                    </Grid>
                    <Grid Grid.Row="1" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width = "4*"/>
                            <ColumnDefinition Width = "6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Column="0"  Text="{Binding ConverterParameter='TRAY_LOT_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Column="1" x:Name="txtTrayNo"  Style="{StaticResource Content_InputForm_MandatoryTextBoxStyle}" KeyDown="txtTrayNo_KeyDown"/>
                    </Grid>
                    <Grid Grid.Row = "1" Grid.Column ="6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.8*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="20"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Column="0" Text="{Binding ConverterParameter='ROUTE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Column="1" x:Name="txtROUTID" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" MouseDoubleClick="txtROUTID_MouseDoubleClick"/>
                        <Button     Grid.Column="2" x:Name="btnRoutInfo" Width="20" Height="20" Padding="0" Click="btnRoutInfo_Click">
                            <Image Source="/LGC.GMES.MES.ControlsLibrary;component/Images/LGC/icon_cal.png" Stretch="Fill"/>
                        </Button>
                    </Grid>
                    <Grid Grid.Row="1" Grid.Column="8">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Column="0" Text="{Binding ConverterParameter='INPUT', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Column="1" x:Name="txtInCellCnt" MinWidth="50" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" HorizontalContentAlignment="Right" />
                    </Grid>
                    <!--조회조건 ROW2-->
                    <Grid Grid.Row="3" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock   Grid.Column="0"  Text="{Binding ConverterParameter='PKG_LOT_ID', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox     Grid.Column="1" Grid.ColumnSpan="2"  x:Name="txtLotID" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                    </Grid>
                    <Grid Grid.Row="3" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Column="0"  Text="{Binding ConverterParameter='PROD_CD', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Column="1"  x:Name="txtPRODID"  MinWidth="100"  Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                    </Grid>
                    <Grid Grid.Row="3" Grid.Column="6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8*"/>
                            <ColumnDefinition Width="2*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock   Grid.Column="0"  Text="{Binding ConverterParameter='DFCT_LIMIT_OVER_FLAG', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox     Grid.Column="1"  x:Name="txtDfctLmtOverFlag"   Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                    </Grid>
                    <Grid Grid.Row="3" Grid.Column="8">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock   Grid.Column="0"  Text="{Binding ConverterParameter='재공', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox     Grid.Column="1"  x:Name="txtNowCellCnt"   Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" HorizontalContentAlignment="Right"/>
                    </Grid>

                    <!--조회조건 ROW3-->
                    <Grid Grid.Row="5" Grid.Column="1" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0"  Text="{Binding ConverterParameter='CURR_OP', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <Grid Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1.5*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <TextBox  Grid.Column="0"  x:Name="txtOper" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" />
                            <TextBox  Grid.Column="1"   x:Name="txtTrayOpStatus" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" Padding="0" HorizontalContentAlignment="Center" />
                        </Grid>
                    </Grid>
                    <Grid Grid.Row="5" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock   Grid.Column="0"  Text="{Binding ConverterParameter='NEXT_OP', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox     Grid.Column="1"  x:Name="txtNextOp" Grid.ColumnSpan="2"  Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                    </Grid>
                    <Grid Grid.Row="5" Grid.Column="6" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.8*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock  Grid.Column="0"  Text="{Binding ConverterParameter='SHIPNG_PLAN_DATE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox    Grid.Column="1"  x:Name="txtOpPlanTime" MinWidth="80"  Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" Padding="0" HorizontalContentAlignment="Center"/>
                    </Grid>
                    <Grid Grid.Row="5" Grid.Column="8">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5*"/>
                            <ColumnDefinition Width="5*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock   Grid.Column="0"  Visibility="Hidden" Text="{Binding ConverterParameter='Good', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox     Grid.Column="1"  Visibility="Hidden" x:Name="txtGoodCellCnt"   Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                    </Grid>
                    <Grid Grid.Row="7" Grid.Column="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="6*"/>
                            <ColumnDefinition Width="4*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock   Grid.Column="0"  Text="{Binding ConverterParameter='ABNORM_TRF_RSN_CODE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" />
                        <TextBox     Grid.Column="1"  x:Name="txtAbnormTrfRsnCode"   Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                    </Grid>
                    <Grid Grid.Row="7" Grid.Column="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4*"/>
                            <ColumnDefinition Width="6*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock   Grid.Column="0"  Text="{Binding ConverterParameter='LOT_HOLD', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox     Grid.Column="1"  x:Name="txtLotHold" Grid.ColumnSpan="2"  Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                    </Grid>
                    <Grid Grid.Row="7" Grid.Column="6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.8*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock   Grid.Column="0"  Text="{Binding ConverterParameter='LOT_TYPE', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox     Grid.Column="1"  x:Name="txtLotTYPE" Grid.ColumnSpan="2"  Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                    </Grid>
                    <Grid Grid.Row="5" Grid.Column="8">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <uc:UcBaseCheckBox   Grid.Column="0" x:Name="chkCdcHold"  Style="{StaticResource SearchCondition_CheckBoxStyle}" IsEnabled="False" Visibility="Collapsed"/>
                        <uc:UcBaseTextBlock  Grid.Column="1"  Text="{Binding ConverterParameter='CDC_HOLD', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}" HorizontalAlignment="Left" Visibility="Collapsed"/>
                    </Grid>                    
                </Grid>
                
                <!--Tray 조회 리스트-->
                <Grid Grid.Row="2" Grid.Column="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="3*"/>
                        <RowDefinition Height="AUTO"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="0.75*"/>
                        <RowDefinition Height="0.7*"/>
                        <RowDefinition Height="0.7*" x:Name="CtGrd"/>
                    </Grid.RowDefinitions>
                    <!--백그라운드용-->
                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                    <Border Grid.Row="0" Grid.Column="0" BorderThickness="1" Grid.ColumnSpan="3" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                    <uc:UcBaseDataGrid x:Name="dgProcess" Grid.Row="0" Grid.ColumnSpan="3" IsReadOnly="True" HeadersVisibility="All" CanUserSort="False" SelectionMode="SingleRow" IsRowCountView="False"
                                   PreviewMouseLeftButtonDown="dgProcess_MouseLeftButtonDown" MouseLeftButtonDown="dgProcess_MouseLeftButtonDown" MouseRightButtonDown="dgProcess_MouseRightButtonDown"
                                   LoadedCellPresenter="dgProcess_LoadedCellPresenter" LoadedRowHeaderPresenter="dgProcess_LoadedRowHeaderPresenter">
                        <c1:C1DataGrid.Columns>
                            <c1:DataGridTextColumn         Header = "OP_NAME"         Binding = "{Binding PROCNAME}"      IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridDateTimeColumn     Header = "START_TIME"      Binding = "{Binding WIPDTTM_ST}"    IsReadOnly="True" HorizontalAlignment="Center" CustomDateFormat="yyyy-MM-dd HH:mm:ss" />
                            <c1:DataGridDateTimeColumn     Header = "END_TIME"        Binding = "{Binding WIPDTTM_ED}"    IsReadOnly="True" HorizontalAlignment="Center" CustomDateFormat="yyyy-MM-dd HH:mm:ss" />
                            <c1:DataGridTextColumn         Header = "MINUTE"          Binding = "{Binding WORKTIME}"      IsReadOnly="True" HorizontalAlignment="Center" Width="70" />
                            <c1:DataGridTextColumn         Header = "REMARK"          Binding = "{Binding EQP_ID}"        IsReadOnly="True" HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn         Header = "OP_ID"           Binding = "{Binding PROCID}"        IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed" />
                            <c1:DataGridTextColumn         Header = "CURR_YN"         Binding = "{Binding CURR_YN}"       IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn         Header = "JUDG_OP_YN"      Binding = "{Binding JUDG_OP_YN}"    IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn         Header = "IRR_HIST_YN"     Binding = "{Binding IRR_HIST_YN}"   IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                            <c1:DataGridTextColumn         Header = "TIME_OVER_YN"    Binding = "{Binding TIME_OVER_YN}"  IsReadOnly="True" HorizontalAlignment="Center" Visibility="Collapsed"/>
                        </c1:C1DataGrid.Columns>
                    </uc:UcBaseDataGrid>
                    <Border Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                    <Border Grid.Row="1" Grid.Column="0" BorderThickness="1" Grid.ColumnSpan="3" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                    <uc:UcBaseDataGrid x:Name="dgWetting" Height="70" Grid.Row="1" Grid.ColumnSpan="3" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="3" SelectionMode="SingleRow" IsRowCountView="False" Visibility="Collapsed"  >
                        <c1:C1DataGrid.Columns>
                            <c1:DataGridTextColumn      Header = "PRESSURE_TIME_SEC"   Binding = "{Binding PRESSURE_TIME}" IsReadOnly="True"   HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "PRESSURE_VAL"        Binding = "{Binding PRESS_VALUE}"   IsReadOnly="True"   HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "HOLD 시간"           Binding = "{Binding HOLD_TIME}"     IsReadOnly="True"   HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "VENT_TIME"           Binding = "{Binding VENT_TIME}"     IsReadOnly="True"   HorizontalAlignment="Center" />
                        </c1:C1DataGrid.Columns>
                    </uc:UcBaseDataGrid>
                    <Border Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                    <Border Grid.Row="2" Grid.Column="0" BorderThickness="1" Grid.ColumnSpan="3" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                    <uc:UcBaseDataGrid x:Name="dgDOCV" Grid.Row="2" Grid.ColumnSpan="3" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="3" IsRowCountView="False" SelectionMode="SingleRow" PreviewMouseLeftButtonDown="dgDOCV_MouseLeftButtonDown" MouseLeftButtonDown="dgDOCV_MouseLeftButtonDown" >
                        <c1:C1DataGrid.Columns>
                            <c1:DataGridTextColumn      Header = "DOCV_OP"        Binding = "{Binding DOCV_OP_NAME}"   IsReadOnly="True"   HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "START_OCV_OP"   Binding = "{Binding START_OP_NAME}"  IsReadOnly="True"   HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "END_OCV_OP"     Binding = "{Binding END_OP_NAME}"    IsReadOnly="True"   HorizontalAlignment="Center" />
                            <c1:DataGridDateTimeColumn  Header = "END_TIME"       Binding = "{Binding END_TIME}"       IsReadOnly="True"   HorizontalAlignment="Center" CustomDateFormat="yyyy-MM-dd HH:mm:ss" />
                        </c1:C1DataGrid.Columns>
                    </uc:UcBaseDataGrid>
                    <Border Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                    <Border Grid.Row="3" Grid.Column="0" BorderThickness="1" Grid.ColumnSpan="3" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                    <uc:UcBaseDataGrid x:Name="dgTemp" Grid.Row="3" Grid.ColumnSpan="3" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="3" SelectionMode="SingleRow" IsRowCountView="False" >
                        <c1:C1DataGrid.Columns>
                            <c1:DataGridTextColumn      Header = "INIT_TEMP"      Binding = "{Binding TEMP1}"    IsReadOnly="True"    HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "AVG_TEMP"       Binding = "{Binding TEMP2}"    IsReadOnly="True"    HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "MIN_TEMP"       Binding = "{Binding TEMP3}"    IsReadOnly="True"    HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "MAX_TEMP"       Binding = "{Binding TEMP4}"    IsReadOnly="True"    HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "END_TEMP"       Binding = "{Binding TEMP5}"    IsReadOnly="True"    HorizontalAlignment="Center" />
                        </c1:C1DataGrid.Columns>
                    </uc:UcBaseDataGrid>
                    <Border Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                    <Border Grid.Row="4" Grid.Column="0" BorderThickness="1" Grid.ColumnSpan="3" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                    <uc:UcBaseDataGrid x:Name="dgWLowVolt" Grid.Row="4" Grid.ColumnSpan="3" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="3" SelectionMode="SingleRow" IsRowCountView="False" >
                        <c1:C1DataGrid.Columns>
                            <c1:DataGridTextColumn      Header = "LOW_VOLT_OP"     Binding = "{Binding PROCNAME}"    IsReadOnly="True"   HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "WORK_USER"       Binding = "{Binding USERNAME}"    IsReadOnly="True"   HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "TIME"            Binding = "{Binding WIPDTTM_ED}"  IsReadOnly="True"   HorizontalAlignment="Center" />
                        </c1:C1DataGrid.Columns>
                    </uc:UcBaseDataGrid>
                    <Border Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_InputForm_ContentBorderStyle}"/>
                    <Border Grid.Row="5" Grid.Column="0" BorderThickness="1" Grid.ColumnSpan="3" BorderBrush="#ffbebebe" Style="{StaticResource BorderSubTitleStyle}"/>
                    <uc:UcBaseDataGrid x:Name="dgCT" Grid.Row="5" Grid.ColumnSpan="3" IsReadOnly="True" HeadersVisibility="Column" FrozenColumnCount="3" SelectionMode="SingleRow" IsRowCountView="False" PreviewMouseLeftButtonDown="dgCT_MouseLeftButtonDown" MouseLeftButtonDown="dgCT_MouseLeftButtonDown" >
                        <c1:C1DataGrid.Columns>
                            <c1:DataGridTextColumn      Header = "LINE_ID"     Binding = "{Binding EQSG_NAME}"    IsReadOnly="True"   HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "검사유형"       Binding = "{Binding INSP_MTHD_NAME}"    IsReadOnly="True"   HorizontalAlignment="Center" />
                            <c1:DataGridTextColumn      Header = "종료시간"            Binding = "{Binding ACTDTTM}"  IsReadOnly="True"   HorizontalAlignment="Center" />
                        </c1:C1DataGrid.Columns>
                    </uc:UcBaseDataGrid>
                </Grid>

                <Grid Grid.Row="0" Grid.RowSpan="3" Grid.Column="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8" />
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    
                    <!--작업-->
                    <Grid Grid.Row="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="5"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="4"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="4"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="4"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="4"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="4"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="6"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="13"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="1.2*"/>
                            <ColumnDefinition Width="13"/>
                        </Grid.ColumnDefinitions>

                        <!--백그라운드용-->
                        <Border     Grid.Row="0"  Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>
                        <!--작업 ROW1-->
                        <uc:UcBaseButton    Grid.Row="1"   Grid.Column="1" x:Name="btnSpecial"     FontSize="11"  Content="{Binding ConverterParameter='특별관리', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnSpecial_Click"/>
                        <TextBox   Grid.Row="1"   Grid.Column="3" Grid.ColumnSpan="5"     FontSize="11" x:Name="txtSpDes"  Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                        <TextBox   Grid.Row="1"   Grid.Column="9" Grid.ColumnSpan="3"     FontSize="11" x:Name="txtSpSelect"      Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}"/>
                        <!--작업 ROW2-->
                        <TextBlock Grid.Row="3"   Grid.Column="1" x:Name="lblSpecial"     FontSize="11"  Text="{Binding ConverterParameter='SPECIAL_NO', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox   Grid.Row="3"   Grid.Column="3" FontSize="11" x:Name="txtSpc"  Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" />
                        <TextBlock Grid.Row="3"   Grid.Column="5" FontSize="11" x:Name="lblSpclRelDate" Text="{Binding ConverterParameter='예상해제일', Converter={StaticResource GlobalObjectDicConverter},Mode=OneWay}" Style="{StaticResource Content_InputForm_LabelStyle}"/>
                        <TextBox   Grid.Row="3"   Grid.Column="7" FontSize="11" x:Name="txtSpclRelDate"  Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" />
                        <uc:UcBaseButton    Grid.Row="3"   Grid.Column="9" x:Name="btnSampleOut"   FontSize="11" Content="{Binding ConverterParameter=SAMPLE_SHIP, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnSampleOut_Click"/>
                        <TextBox   Grid.Row="3"   Grid.Column="11" x:Name="txtSample"     FontSize="11" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" />

                        <!--작업 ROW3-->
                        <uc:UcBaseButton    Grid.Row="5"   Grid.Column="1" x:Name="btnGrade"       FontSize="11" Content="{Binding ConverterParameter=GRADE_DISTR, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}"   Click="btnGrade_Click"/>
                        <uc:UcBaseButton    Grid.Row="5"   Grid.Column="3" x:Name="btnCell"        FontSize="11" Content="{Binding ConverterParameter=CELL_SEL, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}"   Click="btnCell_Click"/>
