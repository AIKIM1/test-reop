<c1:C1Window x:Class="LGC.GMES.MES.FCS001.FCS001_054_DFCT_CELL_LIST"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
		     xmlns:System="clr-namespace:System;assembly=mscorlib"
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             xmlns:uc="clr-namespace:LGC.GMES.MES.CMM001.Controls;assembly=LGC.GMES.MES.CMM001"
             ctrlLib:LGCPopupWindowExtension.ApplyStyle="True"
             Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='불량 CELL 목록'}" 
             Tag="ProtoType01Windows01" Height="600" Width="500" IsResizable="True" Loaded="C1Window_Loaded" >
    <c1:C1Window.Resources>
        <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;Component/ControlStyles.xaml"/>
    </c1:C1Window.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot" Tag="Tray 선택">
            <uc:UcBaseDataGrid x:Name="dgCellList" Grid.Row="0" IsReadOnly="False" HeadersVisibility="All" SelectionMode="MultiRange"
                               LoadedRowHeaderPresenter="dgCellList_LoadedRowHeaderPresenter"
                               ExecuteDataCompleted="dgCellList_ExecuteDataCompleted">
                <c1:C1DataGrid.Columns>
                    <c1:DataGridTextColumn    Header = "WORK_DATE"        Binding = "{Binding CALDATE}"      IsReadOnly="True" HorizontalAlignment="Center"  Visibility="Collapsed" />
                    <c1:DataGridTextColumn    Header = "작업조"           Binding = "{Binding SHFT_ID}"      IsReadOnly="True" HorizontalAlignment="Center"  Visibility="Collapsed" />
                    <c1:DataGridTextColumn    Header = "CELL_ID"          Binding = "{Binding SUBLOTID}"     IsReadOnly="True" HorizontalAlignment="Center" />
                    <c1:DataGridTextColumn    Header = "불량명"           Binding = "{Binding DFCT_NAME}"    IsReadOnly="True" HorizontalAlignment="Center" />
                </c1:C1DataGrid.Columns>
            </uc:UcBaseDataGrid>
        </ctrlLib:LGCLayout>
    </Grid>
</c1:C1Window>
    