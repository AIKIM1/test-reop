<UserControl x:Class="LGC.GMES.MES.ASSY006.Controls.UcAssemblyProductionResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LGC.GMES.MES.ASSY006.Controls"             
             xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="1800" 
             Initialized="UserControl_Initialized"
             Unloaded="UserControl_Unloaded">
    <UserControl.Resources>
        <ResourceDictionary> 
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlStyles.xaml" />
                <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;component/ControlTemplates/LGCDatePicker.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <ctrlLib:LGCLayout x:Name="LayoutRoot">
            <Grid >
                <Grid.Resources>
                    <DataTemplate x:Key="ResultTemplate">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="6"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right"></TextBlock>
                        </Grid>
                    </DataTemplate>
                </Grid.Resources>

                <!--start -->
                <Grid x:Name="grdPublic">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="8" />
                        <RowDefinition Height="1.0*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="8" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="8" />
                    </Grid.ColumnDefinitions>

                    <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Background="#e8ebed" BorderThickness="1" BorderBrush="#8a8a8a"></Border>
                    <GridSplitter Grid.Row="1" Grid.Column="0" ResizeDirection="Rows" Grid.ColumnSpan="3" ShowsPreview="True" Style="{StaticResource GridSplitterVerticalStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                    <Grid Grid.Row="0" Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="34"/>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="8"/>
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="8" />
                        </Grid.ColumnDefinitions>

                        <!-- Row 0 -->
                        <!--<TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Style="{StaticResource Content_ModuleTitleStyle}" ><Run Text="{Binding ConverterParameter='생산실적', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>-->
                        <!--<Grid Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="5" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                        </Grid>-->

                        <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="4">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="8" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource Content_ModuleTitleStyle}" ><Run Text="{Binding ConverterParameter='생산실적', Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}"/></TextBlock>

                            <Grid Grid.Row="0" Grid.Column="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="8" />
                                </Grid.ColumnDefinitions>
                                <Rectangle x:Name="recEquipment" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="99" Margin="0,5,0,5"/>
                                <TextBlock x:Name="txtEquipment" Grid.Row="1" Grid.Column="1" Style="{StaticResource SearchCondition_LabelStyle}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Black" />
                            </Grid>

                            <Grid Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Button x:Name="btnProductionUpdate"   Grid.Column="3" Content="{Binding ConverterParameter=저장, Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Width="100" Cursor="Hand" Click="btnProductionUpdate_Click"/>
                            </Grid>

                        </Grid>

                        <!-- Border -->
                        <Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="4" Grid.ColumnSpan="99" Background="#ffffffff" />

                        <!-- Row 2 -->
                        <Grid x:Name="grdLotInfo" Grid.Row="2" Grid.Column="2" Visibility="Visible">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="8"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="13" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                
                                <ColumnDefinition Width="13" />
                            </Grid.ColumnDefinitions>

                            <TextBlock                              Grid.Row="0" Grid.Column="1" Text="{Binding ConverterParameter='LOTID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold" />
                            <TextBox  x:Name="textLotId"            Grid.Row="0" Grid.Column="2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" FontWeight="Bold" Margin="8,0,8,0"/>
                            <TextBlock                              Grid.Row="0" Grid.Column="3" Text="{Binding ConverterParameter='작업시간', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox  x:Name="textWorkTime"         Grid.Row="0" Grid.Column="4" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>
                            <TextBlock                              Grid.Row="0" Grid.Column="5" Text="{Binding ConverterParameter='제품ID', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="textProdId"         Grid.Row="0" Grid.Column="6" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>
                            
                            <TextBlock                                Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='PJT', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="textProjectName"      Grid.Row="2" Grid.Column="2" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0"/>
                            <TextBlock  x:Name="tbInputQty"           Grid.Row="2" Grid.Column="3" Text="{Binding ConverterParameter='생산 수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="textInputQty"    Grid.Row="2" Grid.Column="4" ShowButtons="False" Format="#,##0" IsEnabled="False" FontWeight="Bold" Margin="8,0,8,0" />
                            <TextBlock  x:Name="tbWipstat"          Grid.Row="2" Grid.Column="3" Text="{Binding ConverterParameter='상태', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="txtWipstat"         Grid.Row="2" Grid.Column="4" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" FontWeight="Bold" Foreground="Red" Margin="8,0,8,0" />
                            <TextBlock  x:Name="tbGoodQty"            Grid.Row="2" Grid.Column="5" Text="{Binding ConverterParameter='양품수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="textGoodQty"     Grid.Row="2" Grid.Column="6" ShowButtons="False" Format="#,##0" IsEnabled="False" FontWeight="Bold" Margin="8,0,8,0" />
                            <TextBlock  x:Name="tbUnit"               Grid.Row="2" Grid.Column="5" Text="{Binding ConverterParameter='단위', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <TextBox    x:Name="txtUnit"              Grid.Row="2" Grid.Column="6" Style="{StaticResource Content_InputForm_ReadOnlyTextBoxStyle}" CharacterCasing="Upper" HorizontalContentAlignment="Center" Margin="8,0,8,0" />

                            <TextBlock  x:Name="tbInputQtyZtz"        Grid.Row="4" Grid.Column="1" Text="{Binding ConverterParameter='생산 수량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="textInputQtyZtz" Grid.Row="4" Grid.Column="2" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="False" Format="F2" FontWeight="Bold"  Background="Yellow" KeyDown="textInputQtyZtz_KeyDown" LostFocus="textInputQtyZtz_LostFocus" Margin="8,0,8,0" />
                            <TextBlock  x:Name="tbParentQty"          Grid.Row="4" Grid.Column="3" Text="{Binding ConverterParameter='모LOT투입량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="txtParentQty"    Grid.Row="4" Grid.Column="4" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="False" Format="F2" IsEnabled="False" FontWeight="Bold" SelectOnFocus="Always" Margin="8,0,8,0" />
                            <TextBlock  x:Name="tbRemainQty"          Grid.Row="4" Grid.Column="5" Text="{Binding ConverterParameter='잔량', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold"/>
                            <c1:C1NumericBox x:Name="txtRemainQty"    Grid.Row="4" Grid.Column="6" Style="{StaticResource C1NumericBoxStyle}" ShowButtons="False" Format="F2" IsReadOnly="True" FontWeight="Bold" Margin="8,0,8,0"/>
                            <TextBlock  x:Name="tbLane"               Grid.Row="4" Grid.Column="7" Text="{Binding ConverterParameter='Lane수', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" Style="{StaticResource SearchCondition_LabelStyle}" FontWeight="SemiBold" Visibility="Collapsed"/>
                            <c1:C1NumericBox  x:Name="txtLaneQty"     Grid.Row="4" Grid.Column="8" Style="{StaticResource C1NumericBoxStyle}" Format="F0" ShowButtons="False" IsEnabled="False" HorizontalContentAlignment="Center" Visibility="Collapsed"/>
                            <c1:C1NumericBox  x:Name="textTmpProdQty" Grid.Row="4" Grid.Column="7" Style="{StaticResource C1NumericBoxStyle}" Format="F0" ShowButtons="False" IsEnabled="False" Visibility="Collapsed"/>

                            <TextBox x:Name="txtProdVerCode" Grid.Row="0" Grid.Column="7" Style="{StaticResource Content_InputForm_TextBoxStyle}" MinWidth="60" TextAlignment="Right" IsReadOnly="True" Margin="4,0,0,0" Foreground="Red" Visibility="Collapsed"/>
                        </Grid>

                        <!-- Row 4 -->
                        <Grid x:Name="grdAddDefect" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="4" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="4" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="4" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="4" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="4" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="4" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="4" />
                            </Grid.ColumnDefinitions>
                            <TextBlock x:Name="tbPatternResult"  Grid.Column="1" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <TextBox x:Name="txtPatternResultQty" Grid.Column="3" Style="{StaticResource Content_InputForm_TextBoxStyle}" MinWidth="60" TextAlignment="Right" IsReadOnly="True" Margin="4,0,0,0" Foreground="Red"/>
                            <TextBlock x:Name="tbAssyResult"  Grid.Column="5" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <TextBox x:Name="txtAssyResultQty" Grid.Column="7" Style="{StaticResource Content_InputForm_TextBoxStyle}" MinWidth="60" TextAlignment="Right" IsReadOnly="True" Margin="4,0,0,0" Foreground="Red"/>
                            <Border x:Name="bdAddDefect" Grid.Column="9" Style="{StaticResource Content_Separator_BorderStyle}" />
                            <Button  Grid.Column="11" x:Name="btnSaveWipHistory" Content="{Binding ConverterParameter='실적임시저장',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" />
                        </Grid>

                        <Grid x:Name="LotList" Grid.Row="6" Grid.Column="2">
                            <c1:C1DataGrid x:Name="dgDefectDetail" HeadersVisibility="None" SelectionMode="SingleRow" FrozenTopRowsCount="2" LoadedCellPresenter="dgDefectDetail_LoadedCellPresenter" UnloadedCellPresenter="dgDefectDetail_UnloadedCellPresenter" CommittedEdit="dgDefectDetail_CommittedEdit"  >
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridNumericColumn     Header = "[투입수량, 투입수량]"          Binding = "{Binding INPUTQTY}"      HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" />
                                    <c1:DataGridNumericColumn     Header = "[재투입, 재투입]"              Binding = "{Binding REINPUTQTY}"    HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" ShowButtons="False" Minimum="0"/>
                                    <c1:DataGridNumericColumn     Header = "[생산 수량, 생산 수량]"        Binding = "{Binding OUTPUTQTY}"     HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" />
                                    <c1:DataGridTextColumn        Header = "[설비투입수량(M/EA), 설비투입수량(M/EA)]"  Binding = "{Binding EQPTQTY_M_EA}"   HorizontalAlignment="Right" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" />
                                    <c1:DataGridNumericColumn     Header = "[설비투입수량, 설비투입수량]"  Binding = "{Binding EQPTQTY}"       HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" Visibility="Collapsed" />
                                    <c1:DataGridNumericColumn     Header = "[양품수량, 양품수량]"          Binding = "{Binding GOODQTY}"       HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" ShowButtons="False" Minimum="0" />
                                    <c1:DataGridNumericColumn     Header = "[불량수량, 불량]"              Binding = "{Binding DTL_DEFECT}"    HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" />
                                    <c1:DataGridNumericColumn     Header = "[불량수량, LOSS]"              Binding = "{Binding DTL_LOSS}"      HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" />
                                    <c1:DataGridNumericColumn     Header = "[불량수량, 물품청구]"          Binding = "{Binding DTL_CHARGEPRD}" HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" />
                                    <c1:DataGridNumericColumn     Header = "[불량수량, 합계]"              Binding = "{Binding DEFECTQTY}"     HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" />
                                    <c1:DataGridNumericColumn     Header = "[차이수량, 재투입(+)]"         Binding = "{Binding ALPHAQTY_P}"    HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" />
                                    <c1:DataGridNumericColumn     Header = "[차이수량, 미기입불량(-)]"     Binding = "{Binding ALPHAQTY_M}"    HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" />
                                    <c1:DataGridNumericColumn     Header = "[BOX수량, BOX수량]"            Binding = "{Binding BOXQTY}"        HorizontalAlignment="Right" Format="#,##0" CanUserFilter="False" CanUserSort="False" CanUserGroup="False" IsReadOnly="True" />
                                </c1:C1DataGrid.Columns>
                                <c1:C1DataGrid.TopRows>
                                    <c1:DataGridColumnHeaderRow />
                                    <c1:DataGridColumnHeaderRow />
                                </c1:C1DataGrid.TopRows>
                            </c1:C1DataGrid>
                        </Grid>

                        <!--<Grid Grid.Row="4" Grid.Column="2">
                            <c1:C1DataGrid x:Name="dgDefectDetail" 
                                           IsReadOnly="True" 
                                           AlternatingRowBackground="White"
                                           HeadersVisibility="Column"
                                           SelectionMode="SingleRow">
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridTextColumn    Header="항목"             Binding="{Binding ITEM}"                    HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn    Header="합계"             Binding="{Binding DEFECTQTY}"               HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn    Header="불량"             Binding="{Binding DTL_DEFECT}"              HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn    Header="LOSS"             Binding="{Binding DTL_LOSS}"                HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn    Header="물품청구"         Binding="{Binding DTL_CHARGEPRD}"           HorizontalAlignment="Center"/>
                                    <c1:DataGridTextColumn    Header="투입수량"         Binding="{Binding INPUTQTY}"                HorizontalAlignment="Center" Visibility="Visible"/> -->
                        <!--Visibility="Collapsed"-->
                        <!--
                                    <c1:DataGridTextColumn    Header="재투입"           Binding="{Binding REINPUTQTY}"              HorizontalAlignment="Center" Visibility="Visible"/>
                                    <c1:DataGridTextColumn    Header="생산 수량"        Binding="{Binding OUTPUTQTY}"               HorizontalAlignment="Center" Visibility="Visible"/>
                                    <c1:DataGridTextColumn    Header="설비투입수량"     Binding="{Binding EQPTQTY}"                 HorizontalAlignment="Center" Visibility="Visible"/>
                                    <c1:DataGridTextColumn    Header="양품수량"         Binding="{Binding GOODQTY}"                 HorizontalAlignment="Center" Visibility="Visible"/>
                                    <c1:DataGridTextColumn    Header="BOX수량"          Binding="{Binding BOXQTY}"                  HorizontalAlignment="Center" Visibility="Visible"/>
                                    <c1:DataGridTextColumn    Header="차이수량"         Binding="{Binding DIFFQTY}"                 HorizontalAlignment="Center" Visibility="Visible"/>
                                </c1:C1DataGrid.Columns>
                            </c1:C1DataGrid>
                        </Grid>-->
                    </Grid>

                    <Grid x:Name="grdDataCollect" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="8" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>

                        <Border Grid.Row="0" Grid.Column="0" Background="#e8ebed" BorderThickness="1" BorderBrush="#8a8a8a"></Border>
                        <Border Grid.Row="0" Grid.Column="2" Background="#e8ebed" BorderThickness="1" BorderBrush="#8a8a8a"></Border>

                        <GridSplitter Grid.Row="0" Grid.Column="1" Grid.RowSpan="99" ResizeDirection="Columns" ShowsPreview="True" Style="{StaticResource GridSplitterStyle}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

                        <Grid x:Name="grdDataCollectLeft" Grid.Row="1" Grid.Column="0">
                            <c1:C1TabControl Name="tcDataCollectLeft" Grid.Row="1" >
                                <!--불량/Loss/물품청구-->
                                <c1:C1TabItem x:Name="tabDefect" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='불량/Loss/물품청구'}" >
                                    <Grid>
                                        <Grid.Resources>
                                            <DataTemplate x:Key="ResultTemplate">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*"/>
                                                        <ColumnDefinition Width="6"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock Grid.Column="0" Text="{Binding}" HorizontalAlignment="Right"></TextBlock>
                                                </Grid>
                                            </DataTemplate>
                                        </Grid.Resources>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="8"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="3"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <Button     Grid.Row="1" Grid.Column="2" x:Name="btnDefectSave"       Content="{Binding ConverterParameter='저장',   Converter={StaticResource GlobalObjectDicConverter}, Mode=OneWay}" Style="{DynamicResource Content_MainButtonNoMinWidthSpecialStyle}" Click="btnDefectSave_Click"/>
                                        <c1:C1DataGrid Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="3" 
                                                       x:Name="dgDefect" 
                                                       IsReadOnly="False" 
                                                       HeadersVisibility="Column" 
                                                       SelectionMode="SingleCell" 
                                                       LoadedCellPresenter ="dgDefect_LoadedCellPresenter" 
                                                       UnloadedCellPresenter="dgDefect_UnloadedCellPresenter"
                                                       BeginningEdit ="dgDefect_BeginningEdit" 
                                                       CommittedEdit="dgDefect_CommittedEdit"
                                                        >
                                            <c1:C1DataGrid.Columns>
                                                <c1:DataGridTextColumn        Header = "활동ID"         Binding = "{Binding ACTID}"               HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed" />
                                                <c1:DataGridTextColumn        Header = "활동명"         Binding = "{Binding ACTNAME}"             ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL" HorizontalAlignment="Center" IsReadOnly="True" Width="0.8*" />
                                                <c1:DataGridTextColumn        Header = "불량코드"       Binding = "{Binding RESNCODE}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn        Header = "GRCODENAME"     Binding = "{Binding CLSS_NAME1}"          ctrlLib:DataGridMergeExtension.MergeMode="VERTICAL" HorizontalAlignment="Center" IsReadOnly="True" Width="0.7*"/>
                                                <c1:DataGridTextColumn        Header = "활동사유명"     Binding = "{Binding RESNNAME}"            HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn        Header = "불량명"         Binding = "{Binding DFCT_CODE_DETL_NAME}" HorizontalAlignment="Left"   IsReadOnly="True" Width="1.0*"/>
                                                <c1:DataGridTextColumn        Header = "불량코드명"     Binding = "{Binding DFCT_CODE_NAME}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn        Header = "불량상세명"     Binding = "{Binding DFCT_DETL_NAME}"      HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
                                                <c1:DataGridTextColumn        Header = "부품"           Binding = "{Binding DFCT_PART_NAME}"      HorizontalAlignment="Left"   IsReadOnly="True" Width="1.0*"/>
                                                <c1:DataGridTextColumn        Header = "불량코드"       Binding = "{Binding DFCT_CODE}"           HorizontalAlignment="Center" IsReadOnly="True" Visibility="Collapsed"/>
