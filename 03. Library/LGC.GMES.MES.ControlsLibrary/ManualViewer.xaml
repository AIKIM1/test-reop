<c1:C1Window xmlns:UserControls="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
			 x:Class="LGC.GMES.MES.ControlsLibrary.ManualViewer"
