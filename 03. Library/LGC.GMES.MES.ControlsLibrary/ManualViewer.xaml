																				<GradientStop Color="#FFDCEEF3" Offset="0.5"/>
																				<GradientStop Color="#FFBFE1EA" Offset="0.5"/>
																				<GradientStop Color="#FF98CFDE" Offset="1"/>
																			</LinearGradientBrush>
