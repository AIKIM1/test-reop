<c1:C1Window x:Name="popup" x:Class="LGC.GMES.MES.MainFrame.DebugWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
        xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
        ctrlLib:LGCPopupWindowExtension.ApplyStyle="True"
        Width="500" Height="660"
        Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='GMES SFU Configuration'}">
    <c1:C1Window.Resources>
        <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;Component/ControlStyles.xaml"/>
    </c1:C1Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <TextBox     Grid.Row="0"  Grid.Column="0" x:Name="txtDebug" Text="Normal"    Style="{StaticResource Content_InputForm_TextBoxStyle}" AcceptsReturn="True" TextWrapping="Wrap" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>




    </Grid>
</c1:C1Window>
