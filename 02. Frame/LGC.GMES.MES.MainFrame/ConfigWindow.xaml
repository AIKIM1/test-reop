<c1:C1Window x:Name="popup" x:Class="LGC.GMES.MES.MainFrame.ConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
        xmlns:ctrlLib="clr-namespace:LGC.GMES.MES.ControlsLibrary;assembly=LGC.GMES.MES.ControlsLibrary"
        xmlns:self="clr-namespace:LGC.GMES.MES.MainFrame.ConfigWindows"
        ctrlLib:LGCPopupWindowExtension.ApplyStyle="True"
        Width="700" Height="530"
        Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='MES SFU Configuration'}">
    <c1:C1Window.Resources>
        <ResourceDictionary Source="/LGC.GMES.MES.ControlsLibrary;Component/ControlStyles.xaml"/>
    </c1:C1Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <c1:C1TabControl x:Name="tcConfig" Background="White">
            <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='일반'}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <GroupBox Grid.Row="1" Grid.Column="1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='Organization'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="9" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <!--<Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="99"  Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>-->
                            <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='공장', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="1" Grid.Column="3" x:Name="cboShop" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE"  Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                            <TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='동', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="2" Grid.Column="3" x:Name="cboArea" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                            <TextBlock Grid.Row="3" Grid.Column="1" Text="{Binding ConverterParameter='LINE', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="3" Grid.Column="3" x:Name="cboEquipmentSegment" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                            <TextBlock Grid.Row="4" Grid.Column="1" Text="{Binding ConverterParameter='PROCESS', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="4" Grid.Column="3" x:Name="cboProcess" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                            <TextBlock Grid.Row="5" Grid.Column="1" Text="{Binding ConverterParameter='EQUIPMENT', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="5" Grid.Column="3" x:Name="cboEquipment" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                        </Grid>
                    </GroupBox>
                    <GroupBox Grid.Row="3" Grid.Column="1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='Language'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="9" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <!--<Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />-->
                            <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Language', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="1" Grid.Column="3" x:Name="cboLang" DisplayMemberPath="LANGNAME" SelectedValuePath="LANGID" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" SelectedItemChanged="cboLang_SelectedItemChanged" />
                        </Grid>
                    </GroupBox>
                    <GroupBox Grid.Row="5" Grid.Column="1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='Default Menu'}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="9" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="5" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <!--<Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="99"  Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}"/>-->
                            <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Default Menu', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="1" Grid.Column="3" x:Name="cboDefaultMenu" DisplayMemberPath="FULLMENUNAME" SelectedValuePath="MENUID" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                        </Grid>
                    </GroupBox>
                </Grid>
            </c1:C1TabItem>
            <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='프린터'}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="200" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="200" />
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="5" />
                    </Grid.ColumnDefinitions>
                    <GroupBox Grid.Row="1" Grid.Column="1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='일반'}" Grid.ColumnSpan="2">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="9" />
                            </Grid.ColumnDefinitions>
                            <c1:C1ComboBox x:Name="cboGeneralPrinter" Grid.Row="1" Grid.Column="1" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="NAME" SelectedValuePath="CODE" />
                        </Grid>
                    </GroupBox>
                    <GroupBox Grid.Row="3" Grid.Column="1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='바코드'}" Grid.ColumnSpan="2">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="5" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="9" />
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Row="1" Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right">
                                <Button x:Name="btnAddBarcodePrinter" Style="{DynamicResource Content_ModuleButtonAddStyle}" Click="btnAdd_Click" />
                                <Button x:Name="btnRemoveBarcodePrinter" Style="{DynamicResource Content_ModuleButtonDeleteStyle}" Click="btnRemove_Click" Margin="5,0,0,0" />
                            </StackPanel>
                            <c1:C1DataGrid Grid.Row="3" Grid.Column="1" x:Name="dgBarcodePrinter" FontWeight="Normal" CanUserAddRows="True" CanUserRemoveRows="True" CurrentCellChanged="dgComPort_CurrentCellChanged">
                                <!--ContextMenuOpening="dgComPort_ContextMenuOpening" BeginningEdit="dgComPort_BeginningEdit" CommittedEdit="dgComPort_CommittedEdit"-->
                                <c1:C1DataGrid.Resources>
                                    <ResourceDictionary>
                                        <self:BaudRates x:Key="BaudRateList" />
                                        <self:ParityBits x:Key="ParityBitList" />
                                        <self:DataBits x:Key="DataBitList" />
                                        <self:StopBits x:Key="StopBitList" />
                                        <self:DPIs x:Key="DpiList" />
                                        <self:Darknesses x:Key="DarknessList" />
                                        <DataTemplate x:Key="tmpActiveColumn">
                                            <Grid>
                                                <Image Source="/LGC.GMES.MES.MainFrame;component/Images/icon_Active.png" Visibility="{Binding Path=ISACTIVE, Converter={StaticResource GlobalBoolToVisibilityConverter}}" Stretch="None" />
                                                <TextBlock Text="└" Margin="-3,-5,0,0" FontSize="25" FontWeight="ExtraBold" Visibility="{Binding Path=ISACTIVE, Converter={StaticResource GlobalReverseBoolToVisibilityConverter}}" />
                                            </Grid>
                                        </DataTemplate>
                                    </ResourceDictionary>
                                </c1:C1DataGrid.Resources>
                                <c1:C1DataGrid.Columns>
                                    <c1:DataGridCheckBoxColumn Header="Default" Binding="{Binding Path=DEFAULT}" IsReadOnly="True" />
                                    <c1:DataGridComboBoxColumn Header="Port Name" Binding="{Binding Path=PORTNAME}" CellContentStyle="{StaticResource Grid_CellContentStyle}" DisplayMemberPath="NAME" SelectedValuePath="CODE" />
                                    <c1:DataGridComboBoxColumn Header="Printer Name" Binding="{Binding Path=PRINTERNAME}" CellContentStyle="{StaticResource Grid_CellContentStyle}" DisplayMemberPath="NAME" SelectedValuePath="CODE" />
                                    <c1:DataGridComboBoxColumn Header="ZPL Type" Binding="{Binding Path=PRINTERTYPE}" CellContentStyle="{StaticResource Grid_CellContentStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                                    <c1:DataGridComboBoxColumn Header="DPI" Binding="{Binding Path=DPI}" ItemsSource="{StaticResource DpiList}" CellContentStyle="{StaticResource Grid_CellContentStyle}" />
                                    <c1:DataGridNumericColumn Header="X" Binding="{Binding Path=X}" ShowButtons="False" />
                                    <c1:DataGridNumericColumn Header="Y" Binding="{Binding Path=Y}" ShowButtons="False" />
                                    <c1:DataGridNumericColumn Header="Copies" Binding="{Binding Path=COPIES}" Minimum="1" x:Name="colBarcodeCopies" />
                                    <c1:DataGridComboBoxColumn Header="Darkness" Binding="{Binding Path=DARKNESS}" ItemsSource="{StaticResource DarknessList}" CellContentStyle="{StaticResource Grid_CellContentStyle}" />
                                    <c1:DataGridComboBoxColumn Header="Baud Rate" Binding="{Binding Path=BAUDRATE}" ItemsSource="{StaticResource BaudRateList}"  CellContentStyle="{StaticResource Grid_CellContentStyle}" />
                                    <c1:DataGridComboBoxColumn Header="Parity Bit" Binding="{Binding Path=PARITYBIT}" ItemsSource="{StaticResource ParityBitList}" CellContentStyle="{StaticResource Grid_CellContentStyle}" />
                                    <c1:DataGridComboBoxColumn Header="Data Bit" Binding="{Binding Path=DATABIT}" ItemsSource="{StaticResource DataBitList}" CellContentStyle="{StaticResource Grid_CellContentStyle}" />
                                    <c1:DataGridComboBoxColumn Header="Stop Bit" Binding="{Binding Path=STOPBIT}" ItemsSource="{StaticResource StopBitList}" CellContentStyle="{StaticResource Grid_CellContentStyle}" />
                                    <c1:DataGridComboBoxColumn Header="설비" Binding="{Binding Path=EQUIPMENT}" CellContentStyle="{StaticResource Grid_CellContentStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                                    <c1:DataGridComboBoxColumn Header="LABEL INFO" Binding="{Binding Path=LABELID}" CellContentStyle="{StaticResource Grid_CellContentStyle}" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" />
                                    <c1:DataGridCheckBoxColumn Header="Connectionless" Binding="{Binding Path=CONNECTIONLESS}" Visibility="Collapsed" />
                                    <c1:DataGridTextColumn Header="PRINTERKEY" Binding="{Binding Path=PRINTERKEY}" IsReadOnly="True" Visibility="Collapsed" />
                                    <c1:DataGridTextColumn Header="PARENTPRINTERKEY" Binding="{Binding Path=PARENTPRINTERKEY}" IsReadOnly="True" Visibility="Collapsed" />
                                    <c1:DataGridTemplateColumn Header="ACTIVECOLUMN" CellTemplate="{StaticResource tmpActiveColumn}" IsReadOnly="True" Visibility="Collapsed" CanUserResize="False" Width="30" />
                                    <c1:DataGridComboBoxColumn Header="Label Type" Binding="{Binding Path=LABEL_TYPE}" x:Name="colBarcodeLabelType" Visibility="Collapsed" CellContentStyle="{StaticResource Grid_CellContentStyle}" />
                                </c1:C1DataGrid.Columns>
                                <!--<c1:C1DataGrid.ContextMenu>
                                    <ContextMenu x:Name="cmComPort">
                                        <MenuItem Header="Add Standby Printer" Click="MenuItem_Click"/>
                                    </ContextMenu>
                                </c1:C1DataGrid.ContextMenu>-->
                            </c1:C1DataGrid>
                        </Grid>
                    </GroupBox>
                    <GroupBox Grid.Row="5" Grid.Column="1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='감열지'}" Grid.ColumnSpan="2">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="9" />
                            </Grid.ColumnDefinitions>
                            <c1:C1ComboBox x:Name="cboThermalPrinter" Grid.Row="1" Grid.Column="1" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" DisplayMemberPath="NAME" SelectedValuePath="CODE" />
                        </Grid>
                    </GroupBox>
                    <GroupBox Grid.Row="7" Grid.Column="1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='Thermal Printing Paper'}" Grid.ColumnSpan="2" Visibility="Hidden">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="9" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="5" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <!--<Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />-->
                            <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Left Margin', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1NumericBox Grid.Row="1" Grid.Column="3" x:Name="numLeftMargin" Style="{StaticResource C1NumericBoxStyle}" />
                            <TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='Top Margin', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1NumericBox Grid.Row="2" Grid.Column="3" x:Name="numTopMargin" Style="{StaticResource C1NumericBoxStyle}" />
                        </Grid>
                    </GroupBox>
                </Grid>
            </c1:C1TabItem>
            <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='라벨'}" Margin="0">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="25*" />
                        <ColumnDefinition Width="411*" />
                        <ColumnDefinition Width="5" />
                    </Grid.ColumnDefinitions>
                    <GroupBox Grid.Row="1" Grid.Column="1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='바코드'}" Grid.ColumnSpan="2">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="9" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="5" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='공정라벨 유형', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="1" Grid.Column="3" x:Name="cboLabelType" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                            <TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding ConverterParameter='Default 라벨 발행 매수', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1NumericBox Grid.Row="2" Grid.Column="3" x:Name="numLabelCopies" Style="{StaticResource C1NumericBoxStyle}" Minimum="1" />
                            <TextBlock Grid.Row="3" Grid.Column="1" Text="{Binding ConverterParameter='CUT 라벨 발행 여부', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="3" Grid.Column="3" x:Name="cboCutLabel" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                            <TextBlock Grid.Row="4" Grid.Column="1" Text="{Binding ConverterParameter='라벨 자동발행 여부', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="4" Grid.Column="3" x:Name="cboLabelAuto" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                            <TextBlock Grid.Row="5" Grid.Column="1" Text="{Binding ConverterParameter='첫번째 컷 라벨 발행 매수', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1NumericBox Grid.Row="5" Grid.Column="3" x:Name="numFirstCutCopies" Style="{StaticResource C1NumericBoxStyle}" Minimum="0" />
                            <TextBlock Grid.Row="6" Grid.Column="1" Text="{Binding ConverterParameter='Default 이력카드 발행 매수', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1NumericBox Grid.Row="6" Grid.Column="3" x:Name="numCardCopies" Style="{StaticResource C1NumericBoxStyle}" Minimum="1" />
                            <TextBlock Grid.Row="7" Grid.Column="1" Text="{Binding ConverterParameter='이력카드 자동발행 여부', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="7" Grid.Column="3" x:Name="cboCardAuto" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                            <TextBlock Grid.Row="8" Grid.Column="1" Text="{Binding ConverterParameter='이력카드 팝업 여부', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1ComboBox Grid.Row="8" Grid.Column="3" x:Name="cboCardPopup" DisplayMemberPath="CBO_NAME" SelectedValuePath="CBO_CODE" Style="{StaticResource C1ComboBoxStyle}" AutoComplete="False" />
                        </Grid>
                    </GroupBox>
                    <GroupBox Grid.Row="2" Grid.Column="1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='감열지'}" Grid.ColumnSpan="2">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="9" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="5" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding ConverterParameter='Default 라벨 발행 매수', Converter={StaticResource GlobalObjectDicConverter}, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Right" Style="{StaticResource SearchCondition_LabelStyle}" />
                            <c1:C1NumericBox Grid.Row="1" Grid.Column="3" x:Name="numThermalCopies" Style="{StaticResource C1NumericBoxStyle}" Minimum="1" />
                        </Grid>
                    </GroupBox>
                </Grid>
            </c1:C1TabItem>
            <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='로그'}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="25*" />
                        <ColumnDefinition Width="411*" />
                        <ColumnDefinition Width="5" />
                    </Grid.ColumnDefinitions>
                    <GroupBox Grid.Row="1" Grid.Column="1" Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='Logging'}" Grid.ColumnSpan="2">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="9" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="5" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="5" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="9" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="5" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="5" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="5" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <!--<Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="99" Grid.ColumnSpan="99" Style="{StaticResource Content_InputForm_LabelBorderStyle}" />-->
                            <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='UI Log'}" Style="{DynamicResource Content_InputForm_LabelStyle}" />
                            <CheckBox Grid.Row="1" Grid.Column="3" x:Name="chkUILog" IsChecked="False" />
                            <TextBlock Grid.Row="1" Grid.Column="5" Text="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='Frame Log'}" Style="{DynamicResource Content_InputForm_LabelStyle}" />
                            <CheckBox Grid.Row="1" Grid.Column="7" x:Name="chkFrameLog" IsChecked="False" />
                            <TextBlock Grid.Row="3" Grid.Column="1" Text="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='BizRule Log'}" Style="{DynamicResource Content_InputForm_LabelStyle}" />
                            <CheckBox Grid.Row="3" Grid.Column="3" x:Name="chkBizRuleLog" IsChecked="False" />
                        </Grid>
                    </GroupBox>
                </Grid>
            </c1:C1TabItem>
            <c1:C1TabItem Header="{Binding Converter={StaticResource GlobalObjectDicConverter}, ConverterParameter='기타'}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="25*" />
                        <ColumnDefinition Width="411*" />
                        <ColumnDefinition Width="5" />
                    </Grid.ColumnDefinitions>
